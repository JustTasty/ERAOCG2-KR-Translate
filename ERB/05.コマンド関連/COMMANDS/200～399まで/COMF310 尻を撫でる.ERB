@COM310
;엉덩이를 쓰다듬는다
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
;ボディタッチで발기UP
IF BASE:MASTER:발기 <= 1000
	TCVAR:MASTER:사정쾌락강도 = 600
	TCVAR:MASTER:사정대상캐릭터_1 = TARGET
	TCVAR:MASTER:사정부위플래그 = 8
ENDIF


LOCAL = RAND:110
LOCAL:1 = 90 + GET_SUCCESS_RATE() / 5
SIF LOCAL:1 > 109
	LOCAL:1 = 109
IF LOCAL <= 9
	;大成功
	TFLAG:SELECTCOM분기 = 1
ELSEIF LOCAL >= LOCAL:1
	;失敗
	TFLAG:SELECTCOM분기 = -1
ELSE
	;残りは成功
	TFLAG:SELECTCOM분기 = 0
ENDIF

;先制発生時は失敗しない
SIF TFLAG:COMABLE관리 == 3
	TFLAG:SELECTCOM분기 = MAX(TFLAG:SELECTCOM분기,0)
DOWNBASE:기력 += 50
DOWNBASE:MASTER:기력 += 50

;환락강도
LOCAL:0 = 150

IF TFLAG:SELECTCOM분기 == -1
	TIMES LOCAL:0 , 0.10
ELSEIF TFLAG:SELECTCOM분기 == 0
	TIMES LOCAL:0 , 1.00
ELSE
	TIMES LOCAL:0 , 2.00
ENDIF
CALL SOURCE_CALC_환락(TARGET, MASTER, LOCAL:0)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 100)
CALL SOURCE_CALC_노출(TARGET, MASTER, 150)

TIME += 10

RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------
;엉덩이를 쓰다듬는다
@COM_ABLE310
;엉덩이를 쓰다듬는다実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(310)
	RETURN RESULT
;친밀감강도1
SIF TCVAR:MASTER:친밀감 < 1
	RETURN 0
;수면中
SIF CFLAG:수면
	RETURN 0
;기력0
SIF BASE:MASTER:기력 <= 0
	RETURN 0
;腕사용중
SIF TFLAG:61
	RETURN 0
SIF GETBIT (TCVAR:26,0)
	RETURN 0
RETURN 1