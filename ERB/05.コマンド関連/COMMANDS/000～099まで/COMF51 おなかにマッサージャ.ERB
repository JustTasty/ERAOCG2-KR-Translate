;-------------------------------------------------
;배에 마사지기
;도구사용コマンド、レベル2
;-------------------------------------------------
@COM51
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 30
DOWNBASE:기력 += 150
IF TALENT:TARGET:포르치오성감
	CALL SOURCE_CALC_쾌Ｖ(TARGET, MASTER,600)
	CALL SOURCE_CALC_노출(TARGET, MASTER, 80)
	CALL SOURCE_CALC_접촉(TARGET, MASTER,200)
ELSE
	CALL SOURCE_CALC_쾌Ｖ(TARGET, MASTER,300)
	CALL SOURCE_CALC_노출(TARGET, MASTER, 40)
	CALL SOURCE_CALC_접촉(TARGET, MASTER, 30)
ENDIF
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 1
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 1
ENDIF
IF EXP:TARGET:포르치오경험 >= 50
	EXP:TARGET:포르치오경험 += 3
ELSE
	EXP:TARGET:포르치오경험 ++
ENDIF
EXP:MASTER:도구사용경험 ++

RETURN 1

