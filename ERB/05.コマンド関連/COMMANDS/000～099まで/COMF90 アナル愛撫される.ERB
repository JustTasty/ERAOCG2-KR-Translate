;-------------------------------------------------
;애널애무 받는다
;-------------------------------------------------
@COM90
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
TCVAR:MASTER:사정대상캐릭터_1 = TARGET
TCVAR:MASTER:사정부위플래그 = 9
TCVAR:MASTER:사정쾌락강도 = 400

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 10
DOWNBASE:기력 += 100

CALL SOURCE_CALC_쾌Ａ(MASTER, TARGET, 100)
CALL SOURCE_CALC_정복(TARGET, MASTER, 200)
CALL SOURCE_CALC_노출(TARGET, MASTER, 20)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 50)
CALL SOURCE_CALC_봉사(TARGET, MASTER, 150)
CALL SOURCE_CALC_불결(TARGET, MASTER, 400)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;대상의 手⇔조교者のＡの汚れが이동
STAIN:1 |= STAIN:MASTER:4
STAIN:MASTER:4 |= STAIN:1

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 7
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 7
ENDIF

STR:0 = 애널애무 받는다

RETURN 1

@CAN_COM90(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
SKIPDISP ARG
PRINTL 애널을 애무받는다
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------


;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER
素質確認 = RESULT:1
판정値 += RESULT
;ABL:욕망
IF ABL:욕망
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 1,')
	素質確認 = 1
ENDIF

;ABL:봉사정신
IF ABL:봉사정신
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV,ABL:봉사정신,'(,ABL:봉사정신 * 2,')
	素質確認 = 1
ENDIF

;ABL:새드끼
IF ABL:새드끼
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:새드끼 * 4
	PRINT 새드끼
	PRINTV 'LV,ABL:새드끼,'(,ABL:새드끼 * 2,')
	素質確認 = 1
ENDIF

IF MARK:쾌락각인
	SIF 素質確認
		PRINT  + 
	판정値 += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 1,')
	素質確認 = 1
ENDIF

;PALAM:욕정
IF GETPALAMLV(PALAM:욕정,5)
	SIF 素質確認
		PRINT  + 
	판정値 += GETPALAMLV(PALAM:욕정,5) * 1
	PRINT 욕정
	PRINTV 'LV,GETPALAMLV(PALAM:욕정,5),'(,GETPALAMLV(PALAM:욕정,5) * 1,')
	素質確認 = 1
ENDIF

;부끄럼쟁이
IF TALENT:수치심 > 0
	PRINT  - 
	판정値 -= 1
	PRINT 부끄러움
	PRINTV '(,1,')
	素質確認 = 1
ENDIF

;악취둔감
IF TALENT:악취내성 > 0
	SIF 素質確認
		PRINT  + 
	판정値 += 1
	PRINT 악취둔감
	PRINTV '(,1,')
	素質確認 = 1
ENDIF

;악취민감
IF TALENT:악취내성 < 0
	PRINT  - 
	판정値 -= 3
	PRINT 악취민감
	PRINTV '(,3,')
	素質確認 = 1
ENDIF

;헌신적
IF TALENT:헌신적
	SIF 素質確認
		PRINT  + 
	판정値 += 6
	PRINT 헌신적
	PRINTV '(,6,')
	素質確認 = 1
ENDIF

;쾌감의 부정（음란、쾌락광で無効）
IF TALENT:쾌감반응 < 0 && !TALENT:음란
	PRINT  - 
	판정値 -= 1
	PRINT 쾌감의 부정
	PRINTV '(,1,')
	素質確認 = 1
ENDIF

;男嫌い（연모、친애で無効）
IF TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1) && (!TALENT:연모 || MASTER != MASTER)
	PRINT  - 
	판정値 -= 12
	PRINT 남성혐오
	PRINTV '(,12,')
	素質確認 = 1
ENDIF

;연모
IF TALENT:연모
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 연모
	PRINTV '(,5,')
	素質確認 = 1
ENDIF

;친애
IF TALENT:연모 > 1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 친애
	PRINTV '(,5,')
	素質確認 = 1
ENDIF


;愛液の汚れ
SIF STAIN:MASTER:2 & 1
	汚れ値 += 1
;정액の汚れ
SIF STAIN:MASTER:2 & 4
	汚れ値 += 3
;애널の汚れ
SIF STAIN:MASTER:2 & 8
	汚れ値 += 7
SIF STAIN:MASTER:2 & 16
	汚れ値 += 15
;手での애무なので汚れがそれほど影響しないってことに
Y /= 3
汚れ値 = 汚れ値 * (2 - TALENT:악취내성) / 2

;더러움있음
IF 汚れ値
 	PRINT  - 
	판정値 -= 汚れ値
	PRINT 더러움 있음
	;악취둔감
	IF TALENT:악취내성 > 0
		PRINT 、악취둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT 、악취민감
	ENDIF
	PRINTV '(,汚れ値,')
	素質確認 = 1
ENDIF

;合計を표시(18以上で実行)
PRINT  = 
PRINTV 판정値

実行値 = 18
SIF 판정値 < 実行値
	PRINT  < 
SIF 판정値 == 実行値
	PRINT  = 
SIF 판정値 > 実行値
	PRINT  > 
PRINT 실행치
PRINTV 実行値

WAIT
SKIPDISP 0
;実行できない
SIF 판정値 < 実行値
	RETURN 0
RETURN 1


;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE90
;애널애무 받는다実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(90)
	RETURN RESULT
;하반신착의（조교者）
SIF TEQUIP:MASTER:0 & 4
	RETURN 0
RETURN 1