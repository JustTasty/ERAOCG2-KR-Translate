;-------------------------------------------------
;수음펠라
;봉사系파생コマンド
;-------------------------------------------------
@SCOM5
;펠라치오に準拠
CALL COM81

CALL SOURCE_CALC_봉사(TARGET, MASTER, 100)
CALL SOURCE_CALC_쾌Ｍ(TARGET, MASTER,  100)
;변경、추가部分
EXP:수음경험 += 1
TCVAR:MASTER:사정쾌락강도 = 2000
RETURN 1

@CAN_SCOM5(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
SKIPDISP ARG
PRINTL 수음펠라
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------


;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER
素質確認 = RESULT:1
판정値 += RESULT
;ABL:욕망
IF ABL:욕망
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 1,')
	素質確認 = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV,ABL:봉사정신,'(,ABL:봉사정신 * 4,')
	素質確認 = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:정액중독 * 3
	PRINT 정액중독
	PRINTV 'LV,ABL:정액중독,'(,ABL:정액중독 * 3,')
	素質確認 = 1
ENDIF

IF MARK:쾌락각인
	SIF 素質確認
		PRINT  + 
	판정値 += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 1,')
	素質確認 = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정,5)
	SIF 素質確認
		PRINT  + 
	판정値 += GETPALAMLV(PALAM:욕정,5) * 1
	PRINT 욕정
	PRINTV 'LV,GETPALAMLV(PALAM:욕정,5),'(,GETPALAMLV(PALAM:욕정,5) * 1,')
	素質確認 = 1
ENDIF
;恥じらい
IF TALENT:수치심 > 0
	PRINT  - 
	판정値 -= 1
	PRINT 恥じらい
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;汚臭鈍感
IF TALENT:악취내성 > 0
	SIF 素質確認
		PRINT  + 
	판정値 += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;汚臭敏感
IF TALENT:악취내성 < 0
	PRINT  - 
	판정値 -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	素質確認 = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF 素質確認
		PRINT  + 
	판정値 += 6
	PRINT 헌신적
	PRINTV '(,6,')
	素質確認 = 1
ENDIF
;쾌락の부정（음란、쾌락狂で無効）
IF TALENT:쾌감반응 < 0 && !TALENT:음란
	PRINT  - 
	판정値 -= 1
	PRINT 쾌락の부정
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;男嫌い（연모、친애で無効）
IF TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1) && (!TALENT:연모 || MASTER != MASTER)
	PRINT  - 
	판정値 -= 12
	PRINT 男嫌い
	PRINTV '(,12,')
	素質確認 = 1
ENDIF
;연모
IF TALENT:연모
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 연모
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;친애
IF TALENT:연모 > 1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 친애
	PRINTV '(,5,')
	素質確認 = 1
ENDIF

;조교者がふたなり
IF (TALENT:MASTER:성별 & 3) == 3
	SIF 素質確認
		PRINT  + 
	판정値 += 8
	PRINT ふたなり
	PRINTV '(,8,')
	素質確認 = 1
ENDIF


;愛液の汚れ
SIF STAIN:MASTER:2 & 1
	汚れ値 += 1
;정액の汚れ
SIF STAIN:MASTER:2 & 4
	汚れ値 += 3
;애널の汚れ
SIF STAIN:MASTER:2 & 8
	汚れ値 += 7
SIF STAIN:MASTER:2 & 16
	汚れ値 += 15

汚れ値 = 汚れ値 * (2 - TALENT:악취내성) / 2

;汚れあり
IF 汚れ値
 	PRINT  - 
	판정値 -= 汚れ値
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:악취내성 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:악취내성 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,汚れ値,')
	素質確認 = 1
ENDIF

;合計を표시(24以上で実行)
PRINT  = 
PRINTV 판정値

実行値 = 24
SIF 판정値 < 実行値
	PRINT  < 
SIF 판정値 == 実行値
	PRINT  = 
SIF 판정値 > 実行値
	PRINT  > 
PRINT 実行値
PRINTV 実行値

WAIT
SKIPDISP 0
;実行できない
SIF 판정値 < 実行値
	RETURN 0
RETURN 1


;-------------------------------------------------
;実行판정
;-------------------------------------------------

@COM_ABLE905
;수음펠라実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(905)
	RETURN RESULT
;기교Lv1以上必要
SIF ABL:기교 < 1
	RETURN 0
;고양이혀の奴隷の場合は기교Lv2以上必要
SIF TALENT:고양이혀 && ABL:기교 < 2
	RETURN 0
;볼개그사용중は불가
SIF TEQUIP:볼개그
	RETURN 0
;밧줄사용중/手足が使えないと불가
SIF TEQUIP:밧줄 || TEQUIP:사지없음
	RETURN 0
;조교者がオトコか半人半妖かふたなりじゃないとダメ
SIF !(TALENT:MASTER:성별 & 2)
	RETURN 0
RETURN 1