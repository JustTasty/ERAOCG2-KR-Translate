@GET_SUCCESS_RATE
#FUNCTION
VARSET LOCAL
;순종
LOCAL += ABL:순종 * 5
LOCAL += GETPALAMLV(PALAM:공순,5) * 3

;反抗的.素直
LOCAL -= TALENT:태도 * 5
;気丈
SIF TALENT:배짱 > 0
	LOCAL -= 5

;프라이드高い
SIF TALENT:프라이드 > 0
	LOCAL -= 15
;프라이드低い
SIF TALENT:프라이드 < 0
	LOCAL += 5
;目立ちたがり
SIF TALENT:자기과시욕 > 0
	LOCAL += 2
;目立ちたがり
SIF TALENT:자기과시욕 < 0
	LOCAL -= 5
;抑圧
SIF TALENT:자기애 < 0
	LOCAL -= 10
;저항
SIF TALENT:저항
	LOCAL -= 10
;즉시타락
SIF TALENT:즉시타락
	LOCAL += 10
;성별嗜好
SIF (TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1)) || (TALENT:성별기호 & 2 && TALENT:MASTER:성별 & 1)
	LOCAL -= 7

;조교者の素質
;매혹
SIF TALENT:MASTER:매혹
	LOCAL += 6
;謎の매력
SIF TALENT:MASTER:수수께끼의매력
	LOCAL += 6
;호감도
LOCAL += CFLAG:호감도 / 50

;연모
SIF TALENT:연모
	LOCAL += 20
;친애
SIF TALENT:연모 > 1
	LOCAL += 40
LOCAL += (CFLAG:MASTER:310 - CFLAG:TARGET:310) / 500

SIF LOCAL < 0
	LOCAL = 0

RETURNF LOCAL
