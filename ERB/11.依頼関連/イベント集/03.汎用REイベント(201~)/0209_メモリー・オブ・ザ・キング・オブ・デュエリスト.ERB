@IRAI_EVENT_209(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 메모이 오브 더 킹 오브 듀얼리스트(몇번이든 수주 가능)
	CASE "이벤트種"
		RETURN 이벤트_施設
	CASE "依頼先"
		RETURN 場所_カドショ
	CASE "詳細"
		PRINTL やってみよう、伝説のデュエルキングの덱と대상！
		PRINTL 
	CASE "条件"
		SIF 依頼_全完了횟수() < 3
			RETURN 0
		RETURN 1
	CASE "利用制限"
		SIF 依頼플래그:209 != 0
			RETURN 3
		RETURN 0
ENDSELECT

RETURN 0

@IRAI_ACT_209(参照)
#DIM DYNAMIC 参照
#DIM DYNAMIC 대상상대
#DIM DYNAMIC パック_종류
#DIM DYNAMIC カウンタ


IF 依頼_進行보존:209 == 0
	PRINTFORML 
	PRINTFORMW 『やってみよう、伝説のデュエルキングの덱と대상！』
	PRINTFORMW カードショップの壁に何かしらのキャンペーンのポスターが貼られていたので、店員に聞いてみた。
	PRINTFORMW 「お客様！ただいまあの伝説のデュエルキングの덱を再現した덱と대상するキャンペーンを開催中です！」
	PRINTFORMW 「勝利すると、伝説のデュエルキングをイメージした카드가 入ったパックが貰えます！」
	PRINTFORMW 「チャレンジ、どうですか？」
	PRINTFORML 
	PRINTFORML 
	PRINTFORML [0] やる
	PRINTFORML [1] やらない
	BINPUT
	IF RESULT == 0
		PRINTFORML 「分かりました、상대はあちらのデュエルマシンとなります！」
	ELSE
		PRINTFORML 「分かりました、ではまたどうぞ！」
		RETURN 0
	ENDIF

ELSE
	PRINTFORML 
	PRINTFORMW 「お客様！今日もキャンペーンにチャレンジしますか？」
	PRINTFORML 
	PRINTFORML 
	PRINTFORML [0] やる
	PRINTFORML [1] やらない
	BINPUT
	IF RESULT == 0
		PRINTFORML 「分かりました、상대はあちらのデュエルマシンとなります！」
	ELSE
		PRINTFORML 「分かりました、ではまたどうぞ！」
		RETURN 0
	ENDIF
	PRINTFORML 
	PRINTFORMW 今日はどの덱とデュエルしようか？
	PRINTFORML
	PRINTFORML [0] 騎士가이아덱
	SIF 依頼_進行보존:209 >= 1
		PRINTFORML [1] 카오스・ソルジャー덱
	SIF 依頼_進行보존:209 >= 2
		PRINTFORML [2] 有翼환수キマイラ덱
	SIF 依頼_進行보존:209 >= 3
		PRINTFORML [3] 마그넷 워리어덱
	SIF 依頼_進行보존:209 >= 4
		PRINTFORML [4] バスター・ブレーダー덱
	SIF 依頼_進行보존:209 >= 5
		PRINTFORML [5] ブラック・マジシャン덱
	SIF 依頼_進行보존:209 == 6
		PRINTFORML [6] ？？？덱
	SIF 依頼_進行보존:209 >= 7
		PRINTFORML [6] 三幻神덱
	PRINTFORML
	PRINTFORML [100] 돌아간다
	BINPUT
	依頼_完了횟수:209 = RESULT
ENDIF
IF 依頼_完了횟수:209 == 100
	依頼_完了횟수:209 = 0
	PRINTFORML しかしまだ準備ができていなかったので、一旦待ってもらうことにした。
	RETURN 0
ENDIF

TARGET = -1
SELECTCASE 依頼_完了횟수:209
	CASE 0, 1, 2
		CALL DUEL_SYSTEM(1,209,0,2)
	CASE 3, 4, 5, 6
		CALL DUEL_SYSTEM(1,209,0,1)
	CASEELSE
		RETURN 0
ENDSELECT
IF RESULT == 1
	PRINTFORML
	PRINTFORMW 勝った！
	PRINTFORMW 「おめでとうございます！ではこちら、特別パックになります！」
	PRINTFORML
	CALL CARD_PACK_SHORI(5,49)
	PRINTFORML
	IF 依頼_進行보존:209 == 0
		PRINTFORMW 「次回からは別の덱との대상も解禁されますので、是非またどうぞ！」
		PRINTFORMW ん？別の덱？
		PRINTFORMW 「はい。あの伝説のデュエルキングの덱を一つの덱で再現するのは難しく、分けて再現されたとの事なんです」
		PRINTFORMW そうだったのか。
		PRINTFORMW 「勝ち抜いていけば、いずれはあの神の카드가 入った덱とも대상できる……かも知れませんので、頑張ってくださいね！」
	ENDIF
	SIF 依頼_進行보존:209 <= 6 && 依頼_進行보존:209 == 依頼_完了횟수:209
		依頼_進行보존:209 += 1
	IF 依頼_進行보존:209 >= 6 && パック_解放:49 == 0
		PRINTFORMW 「そしてここまで勝ち抜いたボーナスとして、特別パックを通常販売いたします！」
		PRINTFORML 
		CALL CARD_PACK_RELEASE(49)
	ENDIF
	PRINTFORMW そうして次の順番の人にデュエルマシンの상대を譲ってその場から離れた。
	PRINTFORML 
	PRINTFORML 
ENDIF
依頼_完了횟수:209 = 0
TARGET = LOCAL
RETURN 2


