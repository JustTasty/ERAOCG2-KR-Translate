@CARDNAME_79550,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 네가스타트─아누스피어
	CASE "略称"
		RESULTS = -반전시동- AnS
	CASE "カテゴリ"
		RESULTS:0 = 가스타
		RESULTS:1 = 네가스타
		RESULTS:2 = 아누스피어
		RESULTS:3 = E.R.A
ENDSELECT

@CARD_79550(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

