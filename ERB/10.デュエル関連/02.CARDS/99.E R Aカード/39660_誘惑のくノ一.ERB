@CARDNAME_39660,参照先
;ココ39660で지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 유혹의 쿠노이치
	CASE "略称"
		RESULTS = 유혹의 쿠노이치
	CASE "カテゴリ"
		RESULTS:0 = 첩자
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39660(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS


SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 2200
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39660(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  효과 ①②는 각각 1턴에 1번밖에 발동할 수 없다.
CALL CARD_ABLE(1,손패起動)
PRINTL ①：자신 메인 페이즈에 발동할 수 있다.
PRINTL 　패의 이 카드를 묘지로 보낸다.
PRINTL 　그 후, 덱에서 "유혹의 쿠노이치"가 아닌 암컷 / 전사족 몬스터 1장을 패에 넣을 수 있다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②：자신 메인 페이즈에, 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 　상대 필드의 수컷 몬스터 1장의 컨트롤을 얻는다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39660(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39660)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,39660)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,39660)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 雄性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_39660(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 装備先
#DIMS DYNAMIC 装備者


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備先 = RESULT	
装備者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39660,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件固名前 = 유혹의 쿠노이치
		효과条件종족 = 전사족
		효과条件성별 = 牝性
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39660,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件성별 = 雄性
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"強奪")
		IF RESULT == 1
			조교대상 = RESULT:1
			TRYCALLFORM CARDNAME_{조교대상},"名前"
			IF RESULTS != ""
				PRINTFORML 
				PRINTFORMW %RESULTS%는 유혹의 쿠노이치에게 매료당해, 주인을 배신했다!
				PRINTFORML 
			ENDIF
		ENDIF
	ENDIF
ENDIF
