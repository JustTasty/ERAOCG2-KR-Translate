CALL TEXT_DECORATION("ROGUE")


@CARDSUMMON_59711(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要レベル数 = 12
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
	素材条件カテゴリ = 음화
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,59711) 
	RETURN RESULT
ENDIF

@CARDCAN_59711(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59711)
IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,59711)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0

	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲상대덱 = 1
	효과条件종류 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_3(듀얼리스트,59711)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件カテゴリ = 음화
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_59711(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 合計攻撃力

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59711,1)
	IF RESULT == 0
		;♀몬스터를 묘상に/손패
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲상대덱 = 1
		효과条件任意 = 1
		효과条件성별 = 牝性
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード_ID = RESULT:99
		CALL CARD_SELECT(듀얼리스트,RESULT)
		CALL CARD_EQUIP_MONSTER(듀얼리스트,場所,本参照先（CS）,選択カード（CS）,4)
		CALL CARD_EROTICS(選択カード_ID)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORMW  %CALLNAME:조교대상%는 그림 속으로 끌려 들어가 병사와 백성들에게 범해지고 있다！
			CALL EROTIC_MOVE("全身凌辱",조교대상,-1,300)
		ELSE
			TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
			PRINTFORMW  %CALLNAME:조교대상%는 그림 속으로 끌려 들어가 병사와 백성들에게 범해지고 있다！
		ENDIF
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"変化元ID",選択カード_ID,上書きモード)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 음화
	효과条件他所 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"破耐")
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59711,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件カテゴリ = 음화
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
	ENDIF
ENDIF



