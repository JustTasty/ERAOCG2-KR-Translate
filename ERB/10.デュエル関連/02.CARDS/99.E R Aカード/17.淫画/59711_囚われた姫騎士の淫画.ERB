@CARDNAME_59711,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 붙잡힌 여기사의 음화
	CASE "略称"
		RESULTS = 여기사의 음화
	CASE "カテゴリ"
		RESULTS:0 = 음화
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59711(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 12
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 3000
	CASE "튜너"
		RETURN 0
	CASE "성별"
		RETURN 0
ENDSELECT
RETURN 0

@CARD_EXPLANATION_59711(종류)
