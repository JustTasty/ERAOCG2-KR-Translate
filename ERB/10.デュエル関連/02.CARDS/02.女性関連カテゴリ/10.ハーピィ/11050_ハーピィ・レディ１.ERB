@CARDNAME_11050,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 해피 레이디１
	CASE "略称"
		RESULTS = 해피 １
	CASE "カテゴリ"
		RESULTS:0 = 해피
ENDSELECT


@CARD_11050(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1300
	CASE "守備力"
		RETURN 1400
	CASE "성별"
		RETURN 牝性
	CASE "別途ID"
		RETURN 1050
	CASE "連動ID"
		RETURN 1050
ENDSELECT

RETURN 0

@CARD_EXPLANATION_11050(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명은 룰상 "해피 레이디"로 취급한다。
CALLFORM CARD_ABLE(1,攻守変動)
PRINTL ①：이 몬스터가 필드에 존재하는 한、
PRINTL 자신 필드의 바람 속석 몬스터의 공격력은 ３００ 올린다。


@CARDCAN_11050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,11050)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_11050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正



IF 종류 == 攻守変動
	CALL FULLRESET
	효과条件속성 = 바람속성
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,300)
ENDIF
