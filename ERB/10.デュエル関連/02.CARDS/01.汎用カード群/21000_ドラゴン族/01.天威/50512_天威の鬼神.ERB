#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 3

IF EX参照 == 0 || EX参照 == 1
	素材条件종류 = 링크
ENDIF

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,50512) 
	RETURN RESULT
ENDIF


