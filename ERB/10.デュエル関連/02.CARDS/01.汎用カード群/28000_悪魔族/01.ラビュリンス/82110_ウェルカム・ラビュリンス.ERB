@CARDNAME_82110,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 웰컴 라뷰린스
	CASE "略称"
		RESULTS = 웰컴 라뷰린스
	CASE "カテゴリ"
		RESULTS:0 = 라뷰린스
ENDSELECT


@CARD_82110(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_82110(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：덱에서 "라뷰린스」 몬스터 1장을 특수 소환한다。
PRINTL 이 카드 발동 후, 턴 종료시 자신은 엑스트라 덱에서 악마족 몬스터 밖에 특수 소환 할 수 없다。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：이 카드가 묘지에 존재하는 상태에서, 자신의 일반 함정 카드의 효과로 몬스터가 필드에서 벗어났을 경우에 발동할 수 있다。
PRINTL 이 카드를 자신 필드에 세트한다。
;PRINTL 이 효과는 이 카드가 묘지로 보내진 턴에는 발동할 수 없다。


@CARDCAN_82110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,82110)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,82110)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 라뷰린스
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,82110)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 마함数(듀얼리스트) == 5
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 함정全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"除去成功")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_82110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID


IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,82110,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 라뷰린스
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		IF GETVARS(@"%듀얼리스트%_フィールド名") == "라뷰린스 래버린스"
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지",82110)
		ENDIF
		
		EX소환可能종족 = 악마족
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,82110,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,"묘지",場所,4,1)
	ENDIF
ENDIF
