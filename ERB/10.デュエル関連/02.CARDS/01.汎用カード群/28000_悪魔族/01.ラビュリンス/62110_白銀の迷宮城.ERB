@CARDNAME_62110,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 라뷰린스 래버린스
	CASE "略称"
		RESULTS = 라뷰린스래버린스
	CASE "カテゴリ"
		RESULTS:0 = 라뷰린스
ENDSELECT


@CARD_62110(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN フィールド
ENDSELECT

RETURN 0

@CARD_EXPLANATION_62110(종류)
#DIM DYNAMIC 종류

PRINTL 자신의 필드는 「라뷰린스 래버린스」가 된다。
PRINTL 이 카드명의  ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 세트된 "웰컴 라뷰린스" 일반 함정 카드를 자신이 발동했을 경우, 
PRINTL 그 효과에 이하의 효과를 넣을 수 있다。
PRINTL ● 상대 필드의 카드 1장을 파괴한다。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：자신이 「라뷰린스」카드 이외의 일반 함정 카드를 발동했을 경우에 발동할 수 있다。
PRINTL 자신의 패 / 묘지에서 악마족 몬스터 1장을 특수 소환한다。
PRINTL 

@CARDCAN_62110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,62110)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1



ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 通常함정
	효과条件固カテゴリ = 라뷰린스
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT != 1
		RETURN 0
	SIF 발동済_2(듀얼리스트,62110)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲손패 = 1
	효과条件종족 = 악마족
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_62110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성




IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,62110)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,62110,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件종족 = 악마족
		효과条件종류 = モン全般
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "라뷰린스 래버린스")
ENDIF
