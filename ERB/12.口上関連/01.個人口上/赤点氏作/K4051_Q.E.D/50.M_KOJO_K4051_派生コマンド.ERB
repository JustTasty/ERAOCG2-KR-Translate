@M_KOJO_MESSAGE_SCOM_K4051_12
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K4051_12_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K4051_12_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「挟んだまま舐めて欲しい、ですと？❤　全く、仕方ありませんね……っ❤」
		CASE 2
			PRINTFORMW 「んちゅ❤　れろぉ❤　どうですか？　気持ちいいですか？❤」
		CASE 3
			PRINTFORMW 「んっ❤　……っしょ❤　フフ、ビクビクして来ましたねぇ❤　ホラホラぁっ❤」
		CASE 4
			PRINTFORMW 「じゅるるっ❤　イキんですかぁ？❤　フフフ、どうしましょうかねぇ！？❤❤」
		CASE 5 && TALENT:연모
			PRINTFORMW 「れろぉ❤　んちゅっ❤　ホラホラっ❤　私の가슴と口でたーっぷり気持ちよくなってくださいねぇっ❤❤」
	ENDSELECT
ENDIF
