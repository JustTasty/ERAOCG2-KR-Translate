RETURN RESULT

@M_KOJO_MESSAGE_COM_K1953_86_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	호감도修正 = 2
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 2
	IF TEQUIP:TARGET:Ｖ섹스 == MASTER
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「はっ❤あっ❤　キツっ❤気持ちいっ❤　 %CALLNAME:MASTER%っ❤すきっ❤」
			CASE 1
				PRINTFORMW 「なんでっ❤　あんたはっ❤　そんなに余裕そうなのよっ❤　────あああっ❤」
			CASE 2
				PRINTFORML 「いつもよりっ❤　締まりがいいとかっ❤　嬉しいこと言ってくれるじゃない・・・！！！❤」
				PRINTFORMW 少しムカついたのか、 %CALLNAME:TARGET%の締め付けがさらに強くなった・・・
		ENDSELECT
	ELSEIF TEQUIP:딜도
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「（こんなバカみたいなことさせるコイツに腹が立つけど、これで興奮してる자신자신にも腹が立つわ・・・❤）」
			CASE 1
				PRINTFORMW 「さっきからっ❤　軸がブレてるとかっ❤　うるさいのよっ❤　集中っ、できないじゃないっ❤」
			CASE 2
				PRINTFORMW 「ふっ・・・❤ふっ・・・❤あっ・・・❤　き、気持ちいい所に当たってるとか、そんなんじゃないからっ！❤」
			CASE 3
				PRINTFORMW 「もうそろそろっ❤　げんかいっ❤　だからっ❤　休ませてっ❤」
		ENDSELECT
	ELSE
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORML 「ふっ・・・くっ・・・！」
				PRINTFORMW %CALLNAME:TARGET%는顔をしかめて、額から汗をダラダラと流している・・・
			CASE 2
				PRINTFORMW 「このっ・・・！자세っ・・・！キツっ・・・・・・！」
			CASE 3
				PRINTFORMW 「後でっ、目に物見せてあげるからっ・・・！　覚悟しなさいっ・・・❤」
			CASE 4
				PRINTFORML 「これが終わったら・・・！ %CALLNAME:MASTER%の股間のソレ、ぐちゃぐちゃにしてあげるから・・・！
				PRINTFORMW 　楽しみにしてなさいよ・・・！❤」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
