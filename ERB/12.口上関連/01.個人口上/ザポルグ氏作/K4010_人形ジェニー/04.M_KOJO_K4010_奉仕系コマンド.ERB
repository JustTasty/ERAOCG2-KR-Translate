	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;퍼스트키스
		IF EXP:TARGET:Ｍ경험 == 0 && EXP:TARGET:펠라경험 == 1 && CALLNAME:(CFLAG:TARGET:키스상실상대) == CALLNAME:MASTER
			PRINTFORMW 「これが、퍼스트키스？」
			PRINTFORMW 「・・・今から落としたら、無かったことにならない？」
		;それ以外
		ELSE
			PRINTFORMW 「ぶにぶにしてて・・・潰し甲斐がありそうね❤」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 1
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 0
		
	;청소펠라
	IF STAIN:MASTER:2 & 4 && ABL:TARGET:기교 >= 3
		PRINTFORMW 「ふーっ❤　 %CALLNAME:MASTER%の정액はぜーんぶ私の口の中よ❤」

	;通常
	ELSE
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 「フフっ❤　先っぽが弱いのね？❤」
			CASE 2
				PRINTFORMW 「どーしよっかなー❤　ちょっとだけ、えぐっちゃおっかなー❤」
			CASE 3
				PRINTFORMW 「 %CALLNAME:MASTER%って、裏筋をつーって舐められるの好きよね❤」
			CASE 4
				PRINTFORMW 「ちゅっ❤むちゅ❤んー❤　ウフフっ❤私の키스마크で色が変わっちゃったわね❤」
			CASE 5
				PRINTFORMW 「アタシのモノだって証、ちゃーんと付けてあげる❤　はむっ❤」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

