			RETURN 1
		ELSE
			CALL M_KOJO_MESSAGE_COM_K1931_SEXCHANGE_1
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「ん❤ん❤…いいよぉ❤」
		CASE 2
