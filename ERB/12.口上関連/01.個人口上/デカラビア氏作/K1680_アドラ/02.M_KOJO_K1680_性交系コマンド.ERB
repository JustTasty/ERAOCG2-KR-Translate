	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「あん❤…もうせっかちなんだから❤」
		CASE 2
			PRINTFORMW 「そう❤そこ❤もっと❤突いて❤」
		CASE 3
			PRINTFORMW 「あっ❤あっ❤… %CALLNAME:MASTER%上手だよ❤」
		CASE 4
			PRINTFORMW 「 %CALLNAME:MASTER%❤ %CALLNAME:MASTER%❤好きだよ❤」
		CASE 5
			PRINTFORMW 「やぁ❤あぁん❤…しゅごい❤おちんぽ❤もっとぉ❤」
	ENDSELECT
ENDIF
RETURN 1

