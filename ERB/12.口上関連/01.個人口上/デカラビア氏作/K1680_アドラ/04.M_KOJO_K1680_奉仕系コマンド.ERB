RETURN RESULT

@M_KOJO_MESSAGE_COM_K1680_80_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「手で？…うーん、結構難しそうだね」
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「力加減難しいね」
		CASE 2
			PRINTFORMW 「うーんこんなのがいいの？」
		CASE 3
			PRINTFORMW 「おっちょっと慣れてきたかも♪」
		CASE 4
			PRINTFORMW 「しーこ❤しーこ❤気持ちいいかな❤」
		CASE 5
			PRINTFORMW 「ほら❤ほら❤イッちゃっていいよ❤」
	ENDSELECT
ENDIF
RETURN 1

