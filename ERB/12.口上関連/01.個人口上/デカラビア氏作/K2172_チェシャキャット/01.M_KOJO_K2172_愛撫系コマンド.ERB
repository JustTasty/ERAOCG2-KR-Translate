;20,키스한다
;==================================================
@M_KOJO_MESSAGE_COM_K2172_20
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2172_20_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2172_20_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「ちゅっ…마스터しゅき❤」
		PRINTFORMW 
		PRINTFORMW チェシャは키스をするとすぐに蕩けてしまった
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 5
	SIF TALENT:TARGET:애처
		호감도修正 += 1
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 2
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 2
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「ちゅっ…」
		CASE 2
			PRINTFORMW 「ちゅっ❤ちゅぅ❤」
		CASE 3
			PRINTFORMW 「ましゅたーもっとぉ❤」
		CASE 4
			PRINTFORMW 「れろ❤れろ❤」
		CASE 5
			PRINTFORMW 「はむっ❤ちゅぅ❤ちゅぱ❤」
		CASE 6
			PRINTFORMW 「ますたー❤しゅきぃ❤」
		CASE 7
			PRINTFORMW 「にゃぁぁ❤あたまふわふわしゅりゅぅ❤」
		CASE 8
			PRINTFORMW 「れるぅ❤れろぉ❤…もっとぉ❤」
		CASE 9
			PRINTFORMW 「にゃぅ❤ますたー❤それ以上はダメぇ❤」
		CASE 10
			PRINTFORMW 「ん❤ちゅぅ❤ちゅぱ❤じゅるるぅ❤」
	ENDSELECT
ENDIF
RETURN 1


