		PRINTFORML 「その……お背中を流しましょうか？」
		PRINTFORMW アーシュが頬を赤く染めながら入って来た･･･
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「すすすすスイマセンでした！？」
		PRINTFORMW アーシュが慌てながらも戸を閉めて去っていった･･･
		RETURN 1
	ENDIF
	[SKIPEND]
	;-------------------------------------------------
	;TARGETが목욕に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;외출先で遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1に데이트상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため注意
	;注、このあと通常の遭遇구상、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1に데이트상대、ARG:1が0の場合は非데이트
