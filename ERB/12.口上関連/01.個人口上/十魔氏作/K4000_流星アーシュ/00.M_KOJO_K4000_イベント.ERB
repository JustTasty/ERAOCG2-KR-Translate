;==================================================
;10,ダメージ反応！
;{ダメージ量}で数値を言わせることができる
;==================================================
@M_KOJO_DUEL_K4000_10(ダメージ量,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC ダメージ量
#DIMS DYNAMIC カード名
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE ダメージ量
		;ダメージが０の時
		CASE 0
			PRINTFORMW 
		;１～１９９９の時、小ダメージ
		CASE 1 TO 1999
			PRINTFORMW 
		;２０００～３９９９の時、結構なダメージ
		CASE 2000 TO 3999
			PRINTFORMW 
		;４０００～７９９９の時、超大ダメージ
		CASE 4000 TO 7999
			PRINTFORMW 
		;８０００以上、基本即死
		CASEELSE
			PRINTFORMW 
	ENDSELECT
ENDIF


