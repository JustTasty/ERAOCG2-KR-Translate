;340,사타구니를 맡게한다
;==================================================
@M_KOJO_MESSAGE_COM_K2753_340
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2753_340_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2753_340_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	호감도修正 = 1
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 0
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 4
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「すーっ❤はーっ❤凄い臭いですっ❤」
		CASE 2
			PRINTFORMW 「んっ……❤この臭い、何も考えられなくなります……❤」
		CASE 3
			PRINTFORMW 「 %CALLNAME:MASTER%っ❤ %CALLNAME:MASTER%っ❤ちゅっ❤んぷっ❤はぁっ❤」
		CASE 4
			PRINTFORMW 「はぁっ❤はぁっ❤もうっ❤我慢できませんっ❤」
		CASE 5
			PRINTFORMW 「んっ❤おっ❤臭いだけでっ❤すーっ❤はーっ❤」
	ENDSELECT
ENDIF
RETURN 1

