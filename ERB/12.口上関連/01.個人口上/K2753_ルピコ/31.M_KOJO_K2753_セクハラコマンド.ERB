		PRINTFORMW 

	;耳舐め
	ELSE
		호감도修正 = 3
		SIF TALENT:TARGET:애처
			호감도修正 += 0
		SIF TALENT:TARGET:연모 == 2
			호감도修正 += 1
		SIF TALENT:TARGET:연모 >= 1
			호감도修正 += 1
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 
			CASE 2
				PRINTFORMW 
			CASE 3
				PRINTFORMW 
			CASE 4
				PRINTFORMW 
			CASE 5
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
