@M_HNYO_MESSAGE_COM_K5_41
CALL TRAIN_MESSAGE
CALL M_HNYO_MESSAGE_COM_K5_41_1
RETURN RESULT

@M_HNYO_MESSAGE_COM_K5_41_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「マッサージ機、ですか……？　あっいえ、どのように使うかは分かりますが、その……　お手柔らかにお願いします❤」
		PRINTFORMW 「あっ、来ましたぁ❤❤　ブルブルって、子宮に効きますっ❤❤」
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 0
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 0
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「ああっ、お許しください%CSTR:TARGET:MASCALL%っ❤　それされると子宮が疼いて……っ❤❤　あっ、あぁぁぁっ❤❤❤」
		CASE 2
			PRINTFORMW 「%CSTR:TARGET:MASCALL%ぁっ❤　これダメですっ❤　子宮が孕む準備始めてしまいますっ❤❤」
		CASE 3
			PRINTFORMW 「ああっ❤　%CSTR:TARGET:MASCALL%に子宮すらも躾けられてしまいましたぁ❤」
			PRINTFORMW 「精子欲しいですっ❤　음란メイドにおちんぽお恵みしてくださいませぇ❤❤❤」
	ENDSELECT
ENDIF
RETURN 1

