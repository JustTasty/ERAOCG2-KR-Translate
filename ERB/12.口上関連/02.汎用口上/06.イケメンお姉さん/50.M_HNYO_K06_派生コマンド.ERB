@M_HNYO_MESSAGE_SCOM_K6_12
CALL TRAIN_MESSAGE
CALL M_HNYO_MESSAGE_SCOM_K6_12_1
RETURN 1
@M_HNYO_MESSAGE_SCOM_K6_12_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「こね……♪　こねこね❤♪　우후후っ、どうお？　気持ちいいかしら？」
		CASE 2
			PRINTFORMW 「こうやってよだれを、垂らして、れろっ❤　ふふふ、天国に上ったかのような顔ね❤」
		CASE 3
			PRINTFORMW 「じゅるっ……❤ぱんぱんっ……❤　じゅるるっ❤　ぱんぱん……❤」
		CASE 4
			PRINTFORMW 「ふふっ、気持ちいいかしら、そう、それなら……우후후っ、この小さく出ている場所に、近いの키스を、チュッ❤」
		CASE 5
			PRINTFORMW 「んっふふっ、ふっ、ん……❤　完全に、体全身あなたのものになっちゃったわね……❤　れろぉっ、なんでもしてあげる❤」
	ENDSELECT
ENDIF
