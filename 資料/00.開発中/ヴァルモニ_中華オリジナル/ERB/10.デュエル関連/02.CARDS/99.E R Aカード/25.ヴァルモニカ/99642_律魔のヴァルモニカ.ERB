@CARDNAME_99642,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 律魔の바르모니카
	CASE "略称"
		RESULTS = 律魔の聲
	CASE "カテゴリ"
		RESULTS:0 = 바르모니카
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_99642(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99642(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  카드는 １턴에 １장밖에 발동할 수 없다。
CALL CARD_ABLE(1,마함起動)
PRINTL ①：자신 필드上に「바르모니카」몬스터 카드 が存在する경우、이하의 효과에서 １개를 선택해서 발동할 수 있다。
PRINTL 자신 필드上に「바르모니카」Ｓ・Ｌモンスターが存在する경우、両方を選んで適用할 수 있다。
PRINTL ●자신은 ５００ＬＰ回復する。
PRINTL その後、상대필드의 앞면 표시인 마법 / 함정 카드를 全て破壊する。
PRINTL ●자신은 ５００데미지를 受ける。
PRINTL その後、상대필드의 카드１장을 고르고 제외한다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99642(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99642)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,99642)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 바르모니카
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_99642(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者

#DIM DYNAMIC 종류
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	;卡片发动確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99642,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류:0 = 링크
		효과条件종류:1 = 싱크로
		효과条件カテゴリ = 바르모니카
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성 = 1

		IF 듀얼리스트 == "자신"
			PRINTL
			PRINTL こうか
			PRINTL 　聲　を選択
			PRINTL
			PRINTL （ごにょごにょ…！）
			PRINTL （回復しつつ상대の마법 / 함정 카드를 破壊しましょう…！）[0]
			PRINTL
			PRINTL （ひそひそ…♪）
			PRINTL （ダメージは気にせず상대の카드를 제외しろ…♪）[1]
			IF 条件달성 == 1
				PRINTL
				PRINTL （（それとも、両方？））[2]
			ENDIF
			BINPUT
			IF RESULT == 0
				CALL CARDEFFECT_99642_1(듀얼리스트,종류,ゾーン,場所)
			ELSEIF RESULT == 1
				CALL CARDEFFECT_99642_2(듀얼리스트,종류,ゾーン,場所)
			ELSEIF RESULT == 2
				CALL CARDEFFECT_99642_3(듀얼리스트,종류,ゾーン,場所)
			ENDIF
		ELSEIF 듀얼리스트 == "상대"
			PRINTFORML 選ばれた효과 
			IF 条件달성 == 1
				PRINTFORMW ●両方適用する
				CALL CARDEFFECT_99642_3(듀얼리스트,종류,ゾーン,場所)
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW ●５００回復し、상대の앞면 표시인 마법 / 함정 카드를 全て破壊する。
						CALL CARDEFFECT_99642_1(듀얼리스트,종류,ゾーン,場所)
					CASE 1
						PRINTFORMW ●５００데미지를 受け、상대필드의 카드を１장제외한다。
						CALL CARDEFFECT_99642_2(듀얼리스트,종류,ゾーン,場所)
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF


@CARDEFFECT_99642_1(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 조교강도
#DIM DYNAMIC 调教对象

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

;响鸣지시物数量统计
CALL FULLRESET
효과範囲마함 = 1
효과条件カテゴリ = 바르모니카
효과条件CNT = 1
CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
조교강도 = RESULT:9

;调教对象通常是 天魔之声选姬
调教对象 = NO_TO_CHARANUM(2140)

;回复生命通用
CALL FULLRESET
CALL LIFE_CHANGE(듀얼리스트,"増加","효과",500)

;主要效果：破坏对方表侧后场
CALL FULLRESET
효과条件표시 = 表側표시
CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함全体",場所,"묘지")

;天魔之声选姬在场上时进行口交
CALL FULLRESET
효과範囲필드 = 1
효과条件名前:0 = セレトリーチェ・바르모니카
효과条件名前:1 = 바르모니카の神奏－ヴァーラル
CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
IF RESULT == 1 && 듀얼리스트 == "자신"
;&& TALENT:2140:爱慕 >= 1
	
	SETCOLOR C_PINK
	PRINTFORML
	PRINTFORML 상대 필드의 앞면 표시인 마법 / 함정 카드が全て파괴된 。
	PRINTFORMW
	PRINTFORML 「ねえねえ、ご褒美とか欲しいな❤」
	PRINTFORM %CALLNAME:调教对象%는しゃがみこみ、 %CALLNAME:MASTER%の바지から
	IF TALENT:MASTER:형태 >= 7
		PRINTFORML その巨大な陰茎を取り出した。
	ELSEIF TALENT:MASTER:형태 <= 3
		PRINTFORML そのかわいらしい大きさの페니스を取り出した。
	ELSE
		PRINTFORML 陰茎を取り出した。
	ENDIF
	PRINTFORMW
	PRINTFORML %CALLNAME:调教对象%는うっとりとした表情で %CALLNAME:MASTER%の陰茎を見つめている。
	PRINTFORML 「いただきます❤」
	PRINTFORML %CALLNAME:调教对象%는小さい口いっぱいに %CALLNAME:MASTER%の陰茎を頬張った。
	PRINTFORMW
	
	CALL EROTIC_MOVE("口交侍奉",调教对象,MASTER,100 + 25 * 조교강도)
	PRINTFORMW

	SETCOLOR C_PINK
	PRINTFORML 
	PRINTFORML %CALLNAME:调教对象%の積極的な펠라に耐えきれず、 %CALLNAME:MASTER%는口内に정액を放った。
	PRINTFORMW
	PRINTFORML %CALLNAME:调教对象%는口内に溜まっていく정액を飲み干すと、
	PRINTFORML 혀で亀頭全体を舐めまわし、一滴残らず舐め取った。
	PRINTFORMW
	PRINTFORMW 「ごちそうさま❤」
ENDIF
RETURN


