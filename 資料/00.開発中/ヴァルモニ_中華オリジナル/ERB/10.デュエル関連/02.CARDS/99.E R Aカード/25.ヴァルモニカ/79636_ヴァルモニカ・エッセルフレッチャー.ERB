@CARDNAME_79636,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 바르모니카・エッセルフレッチャー
	CASE "略称"
		RESULTS = 惑わす聲
	CASE "カテゴリ"
		RESULTS:0 = 바르모니카
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79636(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79636(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  카드는 １턴에 １장밖에 사용할 수 없다。
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：響鳴카운터を置く事ができる자신의 마법＆함정존의 카드１장に響鳴카운터を１つ둔다。
PRINTL その後、이하의 효과에서 １개를 選んで적용한다。
PRINTL ●자신은 ５００ＬＰ回復する。
PRINTL その後、상대の패를 確認する。
PRINTL 자신 필드에 響鳴카운터が６個以上있는 경우、추가로 상대は패를 １장고르고、덱으로 되돌린다。
PRINTL ●자신은 ５００데미지를 受ける。
PRINTL その後、필드의 表側표시몬스터 １体を고른다。
PRINTL 그 몬스터はターン終了時まで発情し、攻撃力 / 수비력は０になる。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79636(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 종류
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79636)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79636)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前:0 = アンジェロ・바르모니카
	효과条件名前:1 = デモーネ・바르모니카
	효과条件名前:2 = サキューベ・바르모니카
	효과条件名前:3 = 바르모니카の淫神－ラッシーヴィア
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ENDIF



@CARDEFFECT_79636(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 종류
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 설치場所

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;卡片发动確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79636,1)
	IF RESULT == 0
		;通常마법汎用処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CUSTOMDRAWLINE —
		PRINTL 響鳴카운터を乗せる카드를 選択
		CUSTOMDRAWLINE —
		CALL FULLRESET
		효과条件名前:0 = アンジェロ・바르모니카
		효과条件名前:1 = デモーネ・바르모니카
		효과条件名前:2 = サキューベ・바르모니카
		효과条件名前:3 = 바르모니카の淫神－ラッシーヴィア
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"마함",ゾーン,場所,79636)
		SIF RESULT == 9999
			RETURN 0
		설치場所 = RESULT
		CALL SET_FIELD_INFO(듀얼리스트,"마함",설치場所,"個Ｃ",1)
		
		IF 듀얼리스트 == "자신"
			PRINTL
			PRINTL こうか
			PRINTL 　聲　を選択
			PRINTL
			PRINTL （ごにょごにょ…！）
			PRINTL （回復しながら天使の矢を撃って、상대の패를 みちゃいましょう…！）[0]
			PRINTL
			PRINTL （ひそひそ…♪）
			PRINTL （ダメージは気にせず악마の矢を撃って、몬스터를 無力化しろ…！）[1]
			BINPUT
			IF RESULT == 0
				CALL CARDEFFECT_79636_1(듀얼리스트,종류,ゾーン,場所)
			ELSEIF RESULT == 1
				CALL CARDEFFECT_79636_2(듀얼리스트,종류,ゾーン,場所)
			ENDIF
		;对方が发动
		ELSEIF 듀얼리스트 == "상대"
			PRINTFORML 選ばれた효과 
			SELECTCASE RAND:2
				CASE 0
					PRINTW ●５００ＬＰ回復し、상대の패를 確認する。
					CALL CARDEFFECT_79636_1(듀얼리스트,종류,ゾーン,場所)
				CASE 1
					PRINTW ●５００데미지를 受け、필드의 表側표시몬스터 １体を守備표시にして攻守を０으로 한다。
					CALL CARDEFFECT_79636_2(듀얼리스트,종류,ゾーン,場所)
			ENDSELECT
		ENDIF
	ENDIF
ENDIF


@CARDEFFECT_79636_1(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 종류
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교강도
#DIM DYNAMIC 调教对象
#DIM DYNAMIC 调教助手

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

;响鸣지시物数量统计
CALL FULLRESET
효과範囲마함 = 1
효과条件カテゴリ = 바르모니카
효과条件CNT = 1
CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
조교강도 = RESULT:9

;这一次是调教助手 天魔之声选姬
调教助手 = NO_TO_CHARANUM(2140)

;回复生命通用
CALL FULLRESET
CALL LIFE_CHANGE(듀얼리스트,"増加","효과",500)

;天魔之声选姬在场上时
CALL FULLRESET
효과範囲필드 = 1
효과条件名前:0 = セレトリーチェ・바르모니카
효과条件名前:1 = 바르모니카の神奏－ヴァーラル
CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
IF RESULT == 1 && 듀얼리스트 == "자신"
;&& TALENT:2140:爱慕 >= 1
	CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
	IF RESULT != -1  && TALENT:RESULT:성별 == 1
		调教对象 = RESULT

		SETCOLOR C_PINK
		PRINTFORML
		PRINTFORML %CALLNAME:调教助手%는 %CALLNAME:调教对象%に白い矢を放った！
		PRINTFORMW
		PRINTFORML 刺さった天使の矢에 따라、 %CALLNAME:调教对象%の恋心が強制的に燃え上がる！
		PRINTFORML %CALLNAME:调教对象%が %CALLNAME:MASTER%を見る目がどんどん熱っぽくなっていく！
		CALL EVENT_FAVUP(调教对象,조교강도 * 20)
		PRINTFORML ぼーっとしている %CALLNAME:调教对象%는 %CALLNAME:MASTER%に패가 見える위치まで腕を下ろしてしまった！
		PRINTFORMW

		CALL FULLRESET
		효과範囲상대손패 = 1
		효과条件任意 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		
		IF 손패数(対面者) > 0 && 조교강도 >= 6
			SETCOLOR C_PINK
			PRINTFORML
			PRINTFORML %CALLNAME:MASTER%는 %CALLNAME:调教对象%に패를 １장덱으로 되돌린다ように頼んだ。
			PRINTFORMW
			PRINTFORML 明らかに不利になる要求だというのに、
			PRINTFORML %CALLNAME:MASTER%を見るだけで가슴のトキメキが止まらない %CALLNAME:调教对象%는断れない！
			PRINTFORML %CALLNAME:MASTER%の喜ぶ顔が見たいあまりに、 %CALLNAME:调教对象%는패를 １장덱으로 되돌리고 しまった！
			PRINTFORMW

			CALL FULLRESET
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_BASIC(対面者,"손패",ゾーン,場所)
		ENDIF
		IF  조교강도 >= 10
			SETCOLOR C_PINK
			PRINTFORML
			PRINTFORML %CALLNAME:调教对象%の中の恋心は留まることを知らず、
			PRINTFORML 듀얼中だというのに %CALLNAME:MASTER%に抱き着き、胡乱な目をしながら唇を求めた。
			PRINTFORMW

			CALL EROTIC_MOVE("ディープ키스",调教对象,MASTER,100 + 25 * 조교강도)
			PRINTFORMW 
			
			SETCOLOR C_PINK
			IF  调教对象 == 调教助手
				PRINTFORML
				PRINTFORML 「同じ자신だとしても許せない！　 %CALLNAME:MASTER%を放して！」
				PRINTFORML %CALLNAME:调教助手%는 %CALLNAME:调教对象%から天使の矢を引っこ抜き、
				PRINTFORML 분노ながら %CALLNAME:MASTER%を %CALLNAME:调教对象%から引き離した。
				PRINTFORMW	
			ELSE
				PRINTFORML
				PRINTFORML 「もう！　そこまでしろだなんて言ってません！」
				PRINTFORML %CALLNAME:调教助手%는 %CALLNAME:调教对象%から天使の矢を引っこ抜き、
				PRINTFORML 분노ながら %CALLNAME:MASTER%を %CALLNAME:调教对象%から引き離した。
				PRINTFORMW
			ENDIF
		ENDIF
	ELSE
		;主要效果：观看手牌
		CALL FULLRESET
		효과範囲상대손패 = 1
		효과条件任意 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)

		IF 손패数(対面者) > 0 && 조교강도 >= 6
			CALL FULLRESET
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_BASIC(対面者,"손패",ゾーン,場所)
		ENDIF
	ENDIF
ELSE
	PRINTFORMW 패를 確認してください
	;主要效果：观看手牌
	CALL FULLRESET
	효과範囲상대손패 = 1
	효과条件任意 = 1
	CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)

	IF 손패数(対面者) > 0 && 조교강도 >= 6
		CALL FULLRESET
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(対面者,"손패",ゾーン,場所)
	ENDIF
ENDIF

RETURN


@CARDEFFECT_79636_2(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 종류
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 설치場所
#DIM DYNAMIC 조교강도
#DIM DYNAMIC 调教对象
#DIM DYNAMIC 调教助手

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

;响鸣지시物数量统计
CALL FULLRESET
효과範囲마함 = 1
효과条件カテゴリ = 바르모니카
효과条件CNT = 1
CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
조교강도 = RESULT:9

;这一次是调教助手 天魔之声选姬
调教助手 = NO_TO_CHARANUM(2140)

;受到伤害通用
CALL FULLRESET
CALL LIFE_CHANGE(듀얼리스트,"減少","효과",500)

;主要效果，场上一只怪兽 攻守0，发情
CALL FULLRESET
CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"選択")
IF RESULT == 1
	설치場所 = RESULT:1
	CALL CARD_SELECT(듀얼리스트,설치場所)
	설치場所 = RESULT
	CALL CARD_EROTICS(GETVAR(@"%대상者（CS）%_필드:選択カード（CS）"))
	SIF RESULT != -1
		调教对象 = RESULT
	CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"接可",1,)
	CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"発情",1,)
	CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"一攻",- GETVAR(@"%대상者（CS）%_MZ_攻:選択カード（CS）"))
	CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"一守",- GETVAR(@"%대상者（CS）%_MZ_守:選択カード（CS）"))
ENDIF



;天魔之声选姬在场上时
CALL FULLRESET
효과範囲필드 = 1
효과条件名前:0 = セレトリーチェ・바르모니카
효과条件名前:1 = 바르모니카の神奏－ヴァーラル
CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)

IF RESULT == 1 && TALENT:调教对象:성별 == 1 && 듀얼리스트 == "자신"
;&& TALENT:2140:연모 >= 1
	
	PALAM:调教对象:욕정 += 조교강도 * 500
	SETCOLOR C_PINK
	IF 调教对象 == 调教助手 && 설치場所 >2000
		PRINTFORML
		PRINTFORML %CALLNAME:调教助手%는黒い矢を放った！
		PRINTFORMW
		PRINTFORML 黒い矢は空中で大きく弧を描き、 %CALLNAME:调教对象%に矛先を向けた！
		PRINTFORML 「ななな、なんで！？　こっちに来るの！？」
		PRINTFORMW
		PRINTFORML 악마の矢に当たった %CALLNAME:调教对象%는とてつもなく興奮した！
		PRINTFORMW
		PRINTFORML 「なんで、듀얼리스트がモンスターに不利になる효과를 使えるのぉ！」
		PRINTFORML %CALLNAME:调教对象%는もじもじと太ももをこすり合わせている。
		PRINTFORMW
		PRINTFORML 「 %CALLNAME:MASTER%がこんな身体にしたんだから、責任とってすっきりさせてっ❤」
		PRINTFORML %CALLNAME:调教对象%는치마の端を口で咥えている。
		SIF EQUIP:调教对象:6 != 0
			PRINTFORML そして、履いている%CLOTHNAME(6,EQUIP:调教对象:6)%をふくらはぎまで下ろして보여준 。
		PRINTFORMW  
		PRINTFORML 악마の矢が %CALLNAME:调教对象%의 이성を奪っていく。
		PRINTFORML %CALLNAME:调教对象%는何かを期待するような目で %CALLNAME:MASTER%を見つめながら、自らの秘部を見せつけた。
		PRINTFORMW
		PRINTFORML ……
		PRINTFORMW
		PRINTFORML あなたが反応しないのを見て、 %CALLNAME:调教对象%는顔を真っ赤にしておねだりをした。
		PRINTFORML 「からだが、すごくあついんです❤　わ、わたしのおまんこ、舐めてください❤」
		PRINTFORMW
		PRINTFORML ……
		PRINTFORMW
		PRINTFORML それでも動かないあなたを見て、 %CALLNAME:调教对象%의 이성は完全に붕괴した。
		PRINTFORML 「おねがいしますっ、 %CALLNAME:MASTER%さまっ❤❤❤　 %CALLNAME:调教对象%のいやらしいおまんこを虐めてくださいっ❤❤❤」

		CALL EROTIC_MOVE("舐阴",调教对象,MASTER,100 + 25 * 조교강도)

		SETCOLOR C_PINK
		PRINTFORML
		PRINTFORML 「もう…… %CALLNAME:MASTER%ったらやり過ぎなんだから……」
		PRINTFORML ぐったりとした %CALLNAME:调教对象%는あなたの上で文句を言った。
		PRINTFORMW
		PRINTFORML でも気持ちよかったんでしょ？
		PRINTFORMW
		PRINTFORML %CALLNAME:调教对象%는少しの침묵の後、小さく頷いた。
		PRINTFORMW

	ELSEIF 调教对象 != -1
		PRINTFORML
		IF 설치場所 >2000
			PRINTFORML %CALLNAME:调教助手%는 %CALLNAME:调教助手%（投影）に向けて黒い矢を放った！
		ELSE
			PRINTFORML %CALLNAME:调教助手%는 %CALLNAME:调教对象%に向けて黒い矢を放った！
		ENDIF
		PRINTFORMW
		PRINTFORML 악마の矢に当たった %CALLNAME:调教对象%는とてつもなく興奮した！
		PRINTFORML 湧き上がる욕망が津波のように押し寄せ、 %CALLNAME:调教对象%의 이성を破壊していく……
		PRINTFORMW
		PRINTFORML %CALLNAME:调教对象%는ついに我慢できなくなり、자위を始めてしまった！
		PRINTFORML %CALLNAME:调教对象%の秘部から水音が絶えず流れ続ける……
		PRINTFORMW
		
		CALL EROTIC_MOVE("자위",调教对象,MASTER,100 + 25 * 조교강도)

		SETCOLOR C_PINK
		PRINTFORML
		PRINTFORML 절정に達した %CALLNAME:调教对象%는びくびくと痙攣している。
		PRINTFORML しばらくはまともに戦闘を行えないだろう。
		PRINTFORMW
		PRINTFORML %CALLNAME:调教助手%も試してみる？
		PRINTFORML 「……何言ってるのよ❤」
		PRINTFORML %CALLNAME:调教助手%는顔を真っ赤にして恥ずかしそうにしていたが、
		PRINTFORML 目を潤ませて股をもじもじとさせている様子から、彼女の内心は明らかだ。
		PRINTFORMW
	ENDIF
ENDIF
RETURN

