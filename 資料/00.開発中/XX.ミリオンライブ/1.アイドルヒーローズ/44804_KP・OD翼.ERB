@CARDNAME_44804,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = I・HERO キネティックパワー・オーバードライブ 翼
	CASE "略称"
		RESULTS = KP・OD翼
	CASE "カテゴリ"
		RESULTS:0 = HERO
		RESULTS:1 = I・HERO
		RESULTS:2 = THE IDOLM@STER
		; RESULTS:4 = MILLION LIVE!
		; RESULTS:5 = 伊吹翼
		RESULTS:4 = 伊吹翼
ENDSELECT

@CARD_44804(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 3500
	CASE "성별"
		RETURN 牝性
	CASE "특수불가"
		RETURN 1
ENDSELECT
RETURN 0

