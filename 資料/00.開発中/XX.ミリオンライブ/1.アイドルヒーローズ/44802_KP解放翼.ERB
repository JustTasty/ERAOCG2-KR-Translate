		RESULTS:0 = HERO
		RESULTS:1 = I・HERO
		RESULTS:2 = THE IDOLM@STER
		; RESULTS:4 = MILLION LIVE!
		; RESULTS:5 = 伊吹翼
		RESULTS:4 = 伊吹翼
ENDSELECT

@CARD_44802(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 2500
	CASE "성별"
		RETURN 牝性
	CASE "특수불가"
		RETURN 1
