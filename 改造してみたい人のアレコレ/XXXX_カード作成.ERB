@CARDNAME_XXXX,参照先
;여기서 지정한 카드의 이름、약칭을 되돌려줄 예정
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 名前
	CASE "略称"
		RESULTS = 略称
	CASE "カテゴリ"
		RESULTS:0 = 仮実装
ENDSELECT


@CARD_XXXX(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 通常モン
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 속성

	CASE "종족"
		RETURN 族
	CASE "レベル"
		RETURN 0
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	;암컷/수컷이 있음
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

