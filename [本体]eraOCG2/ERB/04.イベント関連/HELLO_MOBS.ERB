@SET_CHARA_RAND(설정캐릭터_ID)
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC 能力値
#DIM DYNAMIC 설정캐릭터_ID
#DIM DYNAMIC ガチャ結果
#DIM DYNAMIC 구상OP
#DIM DYNAMIC 子供用番号
#DIM DYNAMIC MOB_NO
#DIM DYNAMIC 설정値


FOR 能力値,10,137
	SIF TALENTNAME:能力値 == ""
		CONTINUE
	SELECTCASE 能力値
	CASE 10 TO 14, 23, 24, 26, 30, 31, 33, 40, 41, 50, 70, 100 TO 105
		설정値 = RAND:100
		IF INRANGE(설정値,0,30)
			설정値 = -1	
		ELSEIF INRANGE(설정値,31,70)
			설정値 = 0
		ELSEIF INRANGE(설정値,71,100)
			설정値 = 1	
		ENDIF
	;성별嗜好
	CASE 81
		설정値 = RAND:100
		IF INRANGE(설정値,0,50)
			설정値 = RAND:5 - 1
		ELSE
			설정値 = 0
		ENDIF
	;おっぱいとふともも
	CASE 121, 122
		설정値 = RAND:100
		IF INRANGE(설정値,0,50)
			설정値 = 0
		ELSEIF INRANGE(설정値,51,80)
			설정値 = 1
		ELSEIF INRANGE(설정値,81,94)
			설정値 = 2
		ELSEIF INRANGE(설정値,95,99)
			설정値 = 3
		ENDIF
	CASE 27, 29, 34, 36, 57, 73 TO 77, 84, 85, 90 TO 94, 106 TO 108, 120, 123 TO 127, 117, 131, 132
		CONTINUE
	CASEELSE
		설정値 = RAND:100
		IF INRANGE(설정値,0,30)
			설정値 = 1
		ELSE
			설정値 = 0
		ENDIF
	ENDSELECT
	TALENT:설정캐릭터_ID:能力値 = 설정値
NEXT
;연령
설정値 = RAND:4 -2
TALENT:설정캐릭터_ID:연령 = 설정値


TALENT:설정캐릭터_ID:키스미경험 = 1
TALENT:설정캐릭터_ID:처녀 = 1

CFLAG:설정캐릭터_ID:키스상실상대 = -1
; CALL SHOP_BUST_RESET(설정캐릭터_ID)
; CALL SHOP_BUTT_RESET(설정캐릭터_ID)
CALL SET_BODYSIZE(설정캐릭터_ID,0,1)
CALL CLOTHES_SETTING(설정캐릭터_ID)


