@CARDNAME_9990,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 듀얼리스트 %CALLNAME:MASTER%
	CASE "略称"
		IF TALENT:MASTER:성별 == 1
			RESULTS = ＠걸
		ELSEIF TALENT:MASTER:성별 == 2
			RESULTS = ＠맨
		ELSEIF TALENT:MASTER:성별 == 3
			RESULTS = ＠맨걸
		ELSE
	RESULTS = ＠맨
	ENDIF
	CASE "カテゴリ"
		RESULTS:0 = 듀얼리스트
ENDSELECT




@CARD_9990(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		SIF TALENT:MASTER:형태 >= 8
			RETURN 10
		SIF TALENT:MASTER:형태 == 7
			RETURN 8
		SIF TALENT:MASTER:형태 == 6
			RETURN 6
		SIF TALENT:MASTER:형태 == 5
			RETURN 4
		SIF TALENT:MASTER:형태 == 4
			RETURN 3
		SIF TALENT:MASTER:형태 == 3
			RETURN 2
		SIF TALENT:MASTER:형태 == 2
			RETURN 1
		SIF TALENT:MASTER:형태 <= 1
			RETURN 1
	CASE "攻撃力"
		SIF TALENT:MASTER:형태 >= 8
			RETURN 4000
		SIF TALENT:MASTER:형태 == 7
			RETURN 3000
		SIF TALENT:MASTER:형태 == 6
			RETURN 2500
		SIF TALENT:MASTER:형태 == 5
			RETURN 2000
		SIF TALENT:MASTER:형태 == 4
			RETURN 1500
		SIF TALENT:MASTER:형태 == 3
			RETURN 1000
		SIF TALENT:MASTER:형태 == 2
			RETURN 500
		SIF TALENT:MASTER:형태 <= 1
			RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "성별"
		IF TALENT:MASTER:성별 == 1
			RETURN 牝性
		ELSEIF TALENT:MASTER:성별 == 2
			RETURN 雄性
		ELSEIF TALENT:MASTER:성별 == 3
			RETURN ふたなり
		ELSE
			RETURN 雄性
		ENDIF
	CASE "生産불가"
		RETURN 1
ENDSELECT
RETURN 0

