PRINTL 마법 & 함정 존에 존재하는 "AliVe♡Twin 키스킬"１을 엑스트라 덱으로  되돌리고,
PRINTL 묘지에 "Live☆Twin 리일라」１장을 패에 넣고, 묘지의 "Live☆Twin 키스킬」１장을 덱으로 되돌린다。
CALL TEXT_DECORATION("ROGUE")



@CARDSUMMON_59646(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 1

IF EX参照 == 0 || EX参照 == 1
	素材条件名前:0 = Live☆Twin 리일라
	素材条件名前:1 = Live☆Twin 리일라 프로스트
	素材条件名前:2 = Live☆Twin 리일라 스위트
ENDIF


IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,59646) 
	RETURN RESULT
ENDIF



@CARDCAN_59646(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59646)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前 = AliVe♡Twin 리일라
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = Live♥Do
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(RESULTS,"破壊","戦闘破壊")
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = Live☆Twin 키스킬
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = Live☆Twin 리일라
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前 = AliVe♡Twin 키스킬
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_2(듀얼리스트,59646)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 5
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == バトル開始時
	SIF 발동済_3(듀얼리스트,59646)
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59646(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59646,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD(듀얼리스트,"필드","마함",場所,"表側표시")
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = Live♥Do
		효과条件종류 = 함정全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_SS_RESTRICTION(듀얼리스트,"Live☆Twin")
			CALL CARD_EX_RESTRICTION(듀얼리스트,"Live☆Twin")
		ENDIF
	ENDIF
ELSEIF 종류 == 戦闘→묘지 || 종류 == 破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59646,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件名前 = AliVe♡Twin 키스킬
		효과条件行先플래그 = EX덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = Live☆Twin 리일라
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = Live☆Twin 키스킬
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59646,2)
	IF RESULT == 0
		選択カード = 戦闘場所(듀얼리스트,1)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",選択カード,"묘지")
	ENDIF
ELSEIF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59646,3)
	IF RESULT == 0
		CALL FULLRESET
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",1000,1)
	ENDIF
ENDIF


