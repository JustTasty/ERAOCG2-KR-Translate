@CARDNAME_59646,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = AliVe♡Twin 리일라
	CASE "略称"
		RESULTS = AV 리일라
	CASE "カテゴリ"
		RESULTS:0 = Live☆Twin
		RESULTS:1 = AliVe♡Twin
		RESULTS:2 = 리일라
		RESULTS:3 = E.R.A
ENDSELECT


@CARD_59646(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 펜듈럼
	CASE "連動ID"
		RETURN 2078
ENDSELECT


RETURN 0


