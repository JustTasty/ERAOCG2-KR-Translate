@CARDNAME_39702,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 촉수궁의 황새
	CASE "略称"
		RESULTS = 촉수궁 황새
	CASE "カテゴリ"
		RESULTS:0 = 촉수
		RESULTS:1 = E.R.A

ENDSELECT


@CARD_39702(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

