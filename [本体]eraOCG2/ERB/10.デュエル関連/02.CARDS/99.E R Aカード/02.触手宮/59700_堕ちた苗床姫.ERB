@CARDNAME_59700,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 텐타클 바이오 플랜트
	CASE "略称"
		RESULTS = 타락한 묘상공주
	CASE "カテゴリ"
		RESULTS:0 = 촉수
		RESULTS:1 = 묘상
		RESULTS:2 = E.R.A	
ENDSELECT


@CARD_59700(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

