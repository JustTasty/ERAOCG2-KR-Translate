@CARD_EXPLANATION_59700(종류)
#DIM DYNAMIC 종류


CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,EX덱起動)
PRINTL 「텐타클 바이오 플랜트"는 1턴에 1번, 자신 필드의 "묘상」 여성 몬스터를 1장을 소재로 엑시즈 소환할 수 있다。
PRINTL 해당 방법으로 소환 시 사용한 몬스터의 레벨 만큼 엑시즈 소재를 얻는다。
PRINTL 이 카드는 "묘상"으로서 완성품이 되었다。
PRINTL 이 카드명의  ①의 효과는 1턴에 1번 밖에 사용할 수 없다。
CALL CARD_ABLE(2,필드起動)
PRINTL ①：이 카드의 엑시즈 소재 1개를 제거하여 발동할 수 잇다。
PRINTL 덱에서 「촉수」 몬스터 1장을 묘지로 보내고, 보낸 카드의 레벨 만큼 이 카드의 엑시즈 소재를 늘린다。
CALL CARD_ABLE(0,攻守変動)
PRINTL ②：이 카드의 공격력 / 수비력은 이 카드의 엑시즈 소재 ×３００ 만큼 올린다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_59700(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59700)
SIF RESULT == 1
	RETURN 0


IF 종류 == EX덱起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,59700)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	SIF モンスター数(듀얼리스트) < 5
		효과範囲마함 = 1
	효과条件カテゴリ = 묘상
	효과条件성별 = 牝性
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 발동済_1(듀얼리스트,59700)
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	SIF 발동済_2(듀얼리스트,59700)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 촉수
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_59700(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 取込カードID
#DIM DYNAMIC カードID
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59700,1)
	IF RESULT == 0

		CALL FULLRESET
		효과条件カテゴリ = 묘상
		효과条件성별 = 牝性
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			取込カードID = RESULT:1
			CALL FULLRESET
			효과範囲EX덱 = 1
			효과条件名前 = 텐타클 바이오 플랜트
			효과추가종류:0 = 91
			CALLFORM CARD_{取込カードID}("レベル")
			효과추가종류:1 = 40000 + RESULT
			효과추가종류:2 = 1000000 + 取込カードID
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59700,2,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 촉수
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		カードID = RESULT:1
		IF RESULT == 1
			CALLFORM CARD_{カードID}("レベル")
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",RESULT)
		ENDIF
	ENDIF
ELSEIF 종류 == 攻守変動
	攻撃力修正 = (GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") + 1) * 300
	CALL STATCHANGE_BASIC(듀얼리스트,"자신",場所,攻撃力修正,攻撃力修正)
ENDIF
