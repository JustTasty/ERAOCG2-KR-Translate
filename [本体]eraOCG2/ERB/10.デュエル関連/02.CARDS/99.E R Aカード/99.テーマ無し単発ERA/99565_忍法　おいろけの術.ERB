@CARDNAME_99565,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 인법　에로변신술
	CASE "略称"
		RESULTS = 인법　에로변신술
	CASE "カテゴリ"
		RESULTS:0 = 인법
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_99565(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "추가日"
		RETURN 250720
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99565(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL ①：자신 필드의 앞면 표시인 "첩자" 몬스터 １장을 릴리스하고 이 카드를 발동할 수 있다。
PRINTL 그 몬스터와는 카드명이 다른 암컷 몬스터 １장을 패 / 덱에서 특수 소환한다。
PRINTL 그 후、상대 플레이어의 수컷 몬스터 １장을 골라、그 컨트롤을 얻을 수 있다。
PRINTL （이 효과는 대상내성 / 함정내성의 영향을 받지 않는다）


@CARDCAN_99565(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99565)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 첩자
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件성별 = 牝性
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_99565(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード,2
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 選択カード名前
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 조교대상,2
#DIMS DYNAMIC 조교대상名前,2
#DIM DYNAMIC レイプ횟수
#DIM DYNAMIC 絶頂횟수
#DIM DYNAMIC 寝取り플래그

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99565)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 첩자
		효과条件표시 = 表側표시
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			TRYCALLFORM CARDNAME_{RESULT:1},"名前"
			選択カード名前 = %RESULTS%

			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
			효과条件성별 = 牝性
			효과条件固名前 = %選択カード名前%
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				選択カード:0 = カード소환先
				選択カード_ID = RESULT:1
				CALL CARD_EROTICS(選択カード_ID)
				조교대상:0 = RESULT
				IF 조교대상:0 != -1
					조교대상名前:0 = %CALLNAME:(조교대상:0)%
				ELSE
					TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
					조교대상名前:0 = %RESULTS%
				ENDIF

				CALL FULLRESET
				효과条件성별 = 雄性
				효과条件표시 = 表側표시
				효과条件任意 = 1
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
				SIF RESULT == 9999
					RETURN 0
				選択カード:1 = RESULT
				選択カード_ID = RESULT:99
				CALL CARD_EROTICS(選択カード_ID)
				조교대상:1 = RESULT
				IF 조교대상:1 != -1
					조교대상名前:1 = %CALLNAME:(조교대상:1)%
				ELSE
					TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
					조교대상名前:1 = %RESULTS%
				ENDIF

				PRINTFORMW %조교대상名前:0%는 의복을 벗기고、%조교대상名前:1%에게 유혹하는듯한 제스처를 했다！
				IF GETVAR(@"%対面者%_MZ_忠実:(選択カード:1)") >= 1
					PRINTFORMW %조교대상名前:1%는 뛰어오르고 싶은 마음을 꾹 참으며、%조교대상名前:0%를 노려보았다。

				ELSE
					PRINTFORMW %조교대상名前:1%는 솟구치는 욕망을 억누르지 못하고、%조교대상名前:0%에게 뛰어들었다！
					IF 조교대상:1 != -1
						IF TALENT:(조교대상:1):형태 == 0
							レイプ횟수 = 4
						ELSE
							レイプ횟수 = (TALENT:MASTER:형태) - 1
						ENDIF
					ELSE
						レイプ횟수 = 4
					ENDIF

					IF 조교대상:0 != -1
						FOR カウンタ,0,レイプ횟수
							CALL EROTIC_MOVE("前穴凌辱",조교대상:0,조교대상:1,200)
							絶頂횟수 += RESULT:1
						NEXT
						IF 絶頂횟수 >= 10
							寝取り플래그 = 1
						ELSE
							寝取り플래그 = 2
						ENDIF

					ELSE
						寝取り플래그 = 2
						PRINTL
						PRINTFORMW %조교대상名前:0%과 %조교대상名前:1%의 몸이 격렬하게 합쳐진다・・・
						PRINTL
					ENDIF

					IF 寝取り플래그 == 1
						PRINTFORMW %조교대상名前:0%는 %조교대상名前:1%의 격한 능욕에 굴복하고 말았다・・・
						CALL CARD_GET_CONTROL(対面者,選択カード:0)

					ELSEIF 寝取り플래그 == 2
						PRINTFORMW %조교대상名前:1%는 %조교대상名前:0%의 묘기에 매료되어버렸다！
						CALL CARD_GET_CONTROL(듀얼리스트,選択カード:1)
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
