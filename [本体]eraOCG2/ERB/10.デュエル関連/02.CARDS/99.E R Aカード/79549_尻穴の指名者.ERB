@CARDNAME_79549,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엉덩이구멍의 지명자
	CASE "略称"
		RESULTS = 애널의 지명자
	CASE "カテゴリ"
		RESULTS:0 = E.R.A
ENDSELECT


@CARD_79549(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 速攻마법
	CASE "連動ID"
		RETURN 70100
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79549(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ②의 효과는 １턴에 ３번까지 사용할 수 있다。
PRINTL ①：이 카드는 이하의 효과에서 １개를 선택해서 발동할 수 있다。
CALLFORM CARD_ABLE(0,마함起動 ,손패起動 ,フリー誘発)
PRINTL ●상대의 묘지의 암컷 또는 수컷 카드를 １장 선택해서 발동할 수 있다。
PRINTL 그 카드를 제외한다。이 효과로 제외한 같은 이름의 카드의 효과는 이 턴 무효화된다。
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ●자신의 카드의 효과에 체인해서、상대가 패의 암컷・수컷 카드의 효과를 발동할 때 발동할 수 있다。
PRINTL 그 효과를 무효로하고 제외한다。이 효과로 제외한 같은 이름의 카드의 효과는 이 턴 무효화된다。

PRINTL ②：자신 메인 페이즈에 발동할 수 있다。
PRINTL 이 턴、이 카드로 제외한 카드를 묘지로 되돌리고、이 카드를 패로 되돌린다。
PRINTL 이 듀얼중「엉덩이구멍의 지명자」의 대상은、이 효과로 묘지로 되돌린 카드밖에 대상으로 할 수없다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79549(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 紋章カードID = 79549
#DIM DYNAMIC 紋章場所
#DIM DYNAMIC カードID = -1
#DIM DYNAMIC 효과플래그

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79549)
SIF RESULT == 1
	RETURN 0

CALL CARD_GET_EMBLEM_VALUES(듀얼리스트,紋章カードID)
紋章場所 = RESULT

IF 紋章場所 != -1 
	カードID = RESULT:1
	효과플래그 = RESULT:2
ENDIF

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件성별:0 = 牝性
	효과条件성별:1 = 雄性
	SIF GETBIT(효과플래그,4) == 1	;一度も묘지효과を발동してない。一度使ったらそのカードしか대상に取れない。
		효과条件ID = カードID
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF 効後_カウンタ < 2
		RETURN 0
	CALL FULLRESET
	효과条件성별:0 = 牝性
	효과条件성별:1 = 雄性
	SIF GETBIT(효과플래그,4) == 1 ;一度も묘지효과を발동してない。一度使ったらそのカードしか대상に取れない。
		효과条件ID = カードID
	효과範囲손패 = 1
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0

	SIF 効後_実行者:(効後_カウンタ - 2) != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	;이 카드를 使っていなければ使えない
	SIF 紋章場所 == -1
		RETURN 0
	;3回以上使っていたら使えなくする
	SIF GETBIT(효과플래그,1)+GETBIT(효과플래그,2)+GETBIT(효과플래그,3) >= 3
		RETURN 0
	;상대의 제외に、지정の카드가ある
	CALL FULLRESET
	효과範囲상대제외 = 1
	효과条件ID = カードID
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時強制
	;前ターンに이 카드를 발동していたら표시する
	SIF GETBIT(효과플래그,0) == 0
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79549(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン

#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 紋章場所
#DIM DYNAMIC 紋章カードID = 79549
#DIM DYNAMIC 효과플래그

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_GET_EMBLEM_VALUES(듀얼리스트,紋章カードID)
紋章場所 = RESULT
IF 紋章場所 != -1
	選択カードID = RESULT:1
	효과플래그 = RESULT:2
ENDIF

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79549)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件성별:0 = 牝性
		효과条件성별:1 = 雄性
		;一度も묘지효과を발동してない。一度使ったらそのカードしか대상に取れない。
		SIF GETBIT(효과플래그,4) == 1
			효과条件ID = 選択カードID
		CALL CARD_ADD_GY_BASIC(듀얼리스트, "상대묘지",ゾーン,場所)
		選択カードID = RESULT:1
		;次のターンまで효과無効
		CALL CARD_EFFECT_PROHIB(듀얼리스트,選択カードID,2,1)
		
		;제외時の구상
		CALL KETSUANA_KOJO(듀얼리스트,1,選択カードID)
		;발동した플래그を立てる
		SETBIT 효과플래그,0
		CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,효과플래그,2,上書きモード)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79549)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL WARASHI_CHANCE(듀얼리스트,処理中_효과ID,"雑多")
		IF RESULT == 0
			選択カード = 効後_設場所:(処理中番号 - 1)
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"손패지정",選択カード,"제외")
			選択カードID = 効後_효과ID:(処理中番号 - 1)
			CALL CARD_EFFECT_PROHIB(듀얼리스트,選択カードID,2,1)
			
			;제외時の구상
			CALL KETSUANA_KOJO(듀얼리스트,1,選択カードID)
			;발동した플래그を立てる
			SETBIT 효과플래그,0
			CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,효과플래그,2,上書きモード)
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79549)
	IF RESULT == 0
		;이 효과로 제외したカードを묘지に戻す
		CALL FULLRESET
		효과条件ID = 選択カードID
		효과範囲상대제외 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		
		;이 카드를 패에 戻す
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,손패行き)
		
		;플래그を立てる
		;使用횟수
		SIF GETBIT(효과플래그,1) + GETBIT(효과플래그,2) == 2
			SETBIT 효과플래그,3
		SIF GETBIT(효과플래그,1) == 1
			SETBIT 효과플래그,2
		SETBIT 효과플래그,1
		
		IF GETBIT (효과플래그,4) == 0
			TRYCALLFORM CARDNAME_{選択カードID},"略称"
			CUSTOMDRAWLINE ―
			PRINTFORML 「엉덩이구멍의 지명자」는「%RESULTS%」의 엉덩이구멍에 흥미를 보인것같다…
			PRINTFORML 이 듀얼중「엉덩이구멍의 지명자」의 효과로는「%RESULTS%」이외를 지명할 수 없습니다。
			CUSTOMDRAWLINE ―
			PRINTFORMW 
		ELSE
			TRYCALLFORM CARDNAME_{選択カードID},"略称"
			CUSTOMDRAWLINE ―
			PRINTFORML 「엉덩이구멍의 지명자」는 아직"지명"하지 못한것같다…
			CUSTOMDRAWLINE ―
		ENDIF
		
		;묘지효과を使用した플래그
		SETBIT 효과플래그,4
		
		CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,효과플래그,2,上書きモード)
		
	ENDIF
ELSEIF 종류 == ターン開始時強制
	;구상を流し、永紋章の플래그をリセット
	CLEARBIT 효과플래그 , 0;前のターンのカード발동플래그を折る
	CLEARBIT 효과플래그 , 1
	CLEARBIT 효과플래그 , 2
	CLEARBIT 효과플래그 , 3
	CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,효과플래그,2,上書きモード)
	CALL KETSUANA_KOJO(듀얼리스트,3,選択カードID)
ENDIF


;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
;KETSUANA_KOJO
;仕様：
;ケツアナ確定されたカードの구상を표시する。
;구상はそれぞれ妖怪娘がベースになっており、妖怪娘以外のカードの구상を표시する場合、その캐릭터クターの素質で표시구상を切り替える。
;
;また、ついでに永紋章も更新したりする
;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@KETSUANA_KOJO(듀얼리스트,구상종류,選択カードID)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 구상종류	;(1：一回目(短い悲鳴),2：二回目(長い悲鳴)、3：次のターン開始時の手のリアクション)
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 紋章カードID = 79549
#DIM DYNAMIC 紋章場所
#DIM DYNAMIC 효과플래그
#DIM DYNAMIC 累計발동횟수
#DIM DYNAMIC 尻穴開発度
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 조교名称
#DIM DYNAMIC 구상타입	;全6種

;「효과플래그」の内訳
;0_カード발동플래그。ターン開始時の演出(ターンを跨って犯す+ターン開始時強制を動かすかの判断)
;1～3_묘지효과횟수(ターン開始時強制で初期化)
;4_묘지효과플래그(묘지回収효과を使うと、同じ캐릭터しか選択できなくなる)

;구상종류 (1：제외時(1回目・2回目)、2：なし、3：次のターンの指名者の演出)

;紋章を취득、無ければ登録する
CALL CARD_GET_EMBLEM_VALUES(듀얼리스트,紋章カードID)
紋章場所 = RESULT

IF 紋章場所 == -1 
	;PRINTFORMW ◆未登録：登録する
	;未登録の場合
	;永紋章の표시をスキップ
	SKIPDISP 1
	;データの설정(※初期値に-1が入っている+入力値は加減算されるので、上書きで初期化する)
	CALL CARD_ENG_EMBLEM(듀얼리스트,-1,選択カードID,1,上書きモード)	;No１：カードID
	CALL CARD_ENG_EMBLEM(듀얼리스트,-1,0,2,上書きモード)	;No２：플래그관리：0_カード발동플래그、1～3_묘지효과횟수(ターン開始時強制で初期化)、4_묘지효과플래그
	CALL CARD_ENG_EMBLEM(듀얼리스트,-1,0,3,上書きモード)	;No３：累計발동횟수
	CALL CARD_RESERV(듀얼리스트,紋章カードID,永紋章予約)
	CALL CARD_GET_EMBLEM_VALUES(듀얼리스트,紋章カードID)
	紋章場所 = RESULT
	SKIPDISP 0
ENDIF
;CLEARBIT 

효과플래그 = RESULT:2
累計발동횟수 =  RESULT:3

;묘지효과を使っていない場合、대상者の更新、累計발동횟수のリセット
IF GETBIT(효과플래그,4) == 0
	CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,選択カードID,1,上書きモード)	;No１：カードID
	CALL CARD_ENG_EMBLEM(듀얼리스트,-1,0,3,上書きモード)	;No３：累計발동횟수
	累計발동횟수 = 0
ENDIF

CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,효과플래그,2,上書きモード)
累計발동횟수++
CALL CARD_ENG_EMBLEM(듀얼리스트,紋章場所,1,3)	;No３：累計발동횟수＋１

;◆カード名취득
CALL CARD_EROTICS(選択カードID)
조교대상 = RESULT

;エッチな事できる대상なら、そっちから名称を취득する
IF 조교대상 != -1
	조교名称 = %CALLNAME:조교대상%
ELSE
	TRYCALLFORM CARDNAME_{選択カードID},"略称"
	조교名称 = %RESULTS%
ENDIF

;◆尻穴開発度설정
IF 조교대상 == -1
	尻穴開発度 = 累計발동횟수 / 3
ELSE
	尻穴開発度 = 累計발동횟수 / 3 
	SIF EXP:조교대상:Ａ경험 >= EXPLV:3
		尻穴開発度++
ENDIF

;◆구상타입判別(素質、또는 カードIDで判別)
IF 조교대상 == -1
	구상타입 = 1	;さくら
ELSE
	IF INRANGE(選択カードID,1260,1265)
		SELECTCASE 구상타입
		CASE 1260
			구상타입 = 0;幽鬼うさぎ
		CASE 1261
			구상타입 = 1;浮幽さくら
		CASE 1262
			구상타입 = 2;屋敷わらし
		CASE 1263
			구상타입 = 3;灰流うらら
		CASE 1264
			구상타입 = 4;朔夜しぐれ
		CASE 1265
			구상타입 = 5;儚無みずき
		ENDSELECT
	ELSE
		;妖怪娘の素質をベースに、표시구상を振り分け
		
		IF TALENT:조교대상:반응 == 1	;반응（-1=大人しい 1=生意気）
			IF TALENT:조교대상:자기과시욕 == 1	;자기과시욕（1=目立ちたがり）
				구상타입 = 3;灰流うらら
			ELSE
				구상타입 = 0;幽鬼うさぎ
			ENDIF
		ELSEIF TALENT:조교대상:자기애 == -1	;자기애（-1=抑圧)
			구상타입 = 1;浮幽さくら
		ELSEIF TALENT:조교대상:수치심== 1	;수치심（1=恥じらい）
			구상타입 = 2;屋敷わらし
		ELSEIF TALENT:조교대상:성적흥미== -1 || TALENT:조교대상:쾌감반응 == 1	;성적흥미（-1=保守的 ）또는 、쾌감반응（1=쾌감に素直）
			구상타입 = 4;朔夜しぐれ
		ELSEIF TALENT:조교대상:태도== -1 || TALENT:조교대상:배짱 == -1	;태도（-1=素直）또는 배짱（-1=臆病）
