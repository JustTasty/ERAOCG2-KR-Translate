@CARDNAME_39693,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 만지기머신　말랑말랑 핑거
	CASE "略称"
		RESULTS = 말랑 손가락
	CASE "カテゴリ"
		RESULTS:0 = 만지기머신
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_39693(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 500
	CASE "추가日"
		RETURN 250804
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39693(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：このカードがモンスターと戦闘を行う場合、その２体はその戦闘では破壊されない。
CALLFORM CARD_ABLE(10,필드起動,フリー誘発)
PRINTL ②：자신・상대ターンに１度、상대フィールドの無性でないモンスター１体を대상として발동できる。
PRINTL そのモンスターの攻撃力・守備力は５００ダウンし、守備표시になり、ターン終了時まで표시形式を변경できない。
CALLFORM CARD_ABLE(0,戦破壊→묘지)
PRINTL ③：このカードがモンスターゾーンから묘지へ送られた場合、상대フィールドの無性でないモンスター１体を대상として발동できる。
PRINTL このカードをそのモンスターに装備する。
CALLFORM CARD_ABLE(0,攻撃時強制誘発)
PRINTL ④：このカードを装備している상대モンスターの攻撃宣言時に발동する。
PRINTL 装備モンスターを守備표시にする。


@CARDCAN_39693(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 装備先
#DIMS DYNAMIC 装備者

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備先 = RESULT	
装備者 = %RESULTS%

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39693)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 성별在
	효과条件표시 = 表側표시
	효과条件除去種 = 選択
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF ゾーン != "필드"
		RETURN 0
	SIF !GROUPMATCH(묘지送り종류,"戦闘破壊","破壊","リリース")
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 성별在
	효과条件표시 = 表側표시
	효과条件除去種 = 選択
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 装備者 == 듀얼리스트
		RETURN 0
	SIF 装備先 == 5
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) != 装備先
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	IF 戦闘場所(듀얼리스트,1) == 5
		CALL CARD_EROTICS(상대プレイヤー,対面者)
		SIF RESULT == -1
			RETURN 0
	ELSE
		TRYCALLFORM CARD_{戦闘カードID(듀얼리스트,1)},"성별"
		SIF RESULT == 無性
			RETURN 0
	ENDIF
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	RETURN 1

ELSEIF 종류 == リアクション효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF 装備先 == 5
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_39693(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 装備場所
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 装備先
#DIMS DYNAMIC 装備者

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備先 = RESULT	
装備者 = %RESULTS%

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39693,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件성별 = 성별在
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"選択")
		IF RESULT == 1
			選択カード = RESULT:1
			CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",選択カード,1)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORMW 말랑말랑 핑거は%CALLNAME:조교대상%へ飛んでいき、その애널を指で解しまくった！
				CALL EROTIC_MOVE("애널解し",조교대상,-1,1000)
			ELSE
				TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:選択カード")},"名前"
				PRINTFORML 말랑말랑 핑거は%RESULTS%へ飛んでいき、その애널を指で解しまくった！
				PRINTFORMW 最初は저항感から顔を歪めていた%RESULTS%だったが、愛撫が続くに連れ、吐かれる息には熱がこもり始めていた。
			ENDIF
			CALL SET_FIELD_INFO(対面者,"필드",選択カード,"表",2,1)
			CALL SET_FIELD_INFO(対面者,"필드",選択カード,"表固",1,1)
			CALL SET_FIELD_INFO(対面者,"필드",選択カード,"永攻",-500)
			CALL SET_FIELD_INFO(対面者,"필드",選択カード,"永守",-500)
		ENDIF
	ENDIF
ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39693)
	IF RESULT == 0
		CALL FULLRESET
		효과条件名前 = 만지기머신　말랑말랑 핑거
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT != 9999
			選択カード = RESULT

			CALL FULLRESET
			효과範囲상대필드 = 1
			효과条件성별 = 성별在
			효과条件표시 = 表側표시
			CALLFORM CARD_EQUIP_ADV(듀얼리스트,選択カード,"묘지")
			IF RESULT == 1 && LOCAL == 10000102
				装備場所 = RESULT:2 - 6
				CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",装備場所,1)
				IF RESULT != -1
					조교대상 = RESULT
					PRINTFORMW 말랑말랑 핑거は%CALLNAME:조교대상%のおしりに張り付いた！
					CALL EROTIC_MOVE("애널解し",조교대상,-1,10)
				ELSE
					TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:装備場所")},"名前"
					PRINTFORMW 말랑말랑 핑거は%RESULTS%のおしりに張り付いた！
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 攻撃時強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39693)
	IF RESULT == 0
		CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",装備先,1)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML %CALLNAME:조교대상%が動き出した瞬間、張り付いた말랑말랑 핑거が애널を激しく刺激し始めた！
			PRINTFORMW %CALLNAME:조교대상%は애널を襲う감각に耐え切れず、膝を着いてしまった。
			CALL EROTIC_MOVE("애널解し",조교대상,-1,1000)
		ELSE
			TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:装備先")},"名前"
			PRINTFORML %RESULTS%が動き出した瞬間、張り付いた말랑말랑 핑거が애널を激しく刺激し始めた！
			PRINTFORMW %RESULTS%は애널を襲う감각に耐え切れず、膝を着いてしまった。
		ENDIF
		CALL SET_FIELD_INFO(対面者,"필드",装備先,"表",2,1)
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
	IF 戦闘場所(듀얼리스트,1) == 5
		CALL CARD_EROTICS(상대プレイヤー,対面者)
		조교대상 = RESULT
		PRINTFORMW %CALLNAME:조교대상%は애널を指でぐぽぐぽと解された！
		CALL EROTIC_MOVE("애널解し",조교대상,-1,200)
	ELSE
		CALL CARD_EROTICS(戦闘カードID(듀얼리스트,1))
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORMW %CALLNAME:조교대상%は애널を指でぐぽぐぽと解された！
			CALL EROTIC_MOVE("애널解し",조교대상,-1,200)
		ELSE
			TRYCALLFORM CARDNAME_{戦闘カードID(듀얼리스트,1)},"名前"
			PRINTFORMW %CALLNAME:조교대상%は애널を指でぐぽぐぽと解された！
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(対面者,"自身",戦闘場所(듀얼리스트,1),"戦耐")

ELSEIF 종류 == リアクション효과
	CALL CARD_EROTICS(GETVAR(@"%装備者%_필드:装備先"))
	IF RESULT != -1
		조교대상 = RESULT
		PRINTFORMW 말랑말랑 핑거は%CALLNAME:조교대상%の애널をねっとりとした手付きで解している……
		CALL EROTIC_MOVE("애널解し",조교대상,-1,50)
	ENDIF
ENDIF
