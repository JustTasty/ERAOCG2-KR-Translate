@CARDNAME_99625,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 서큐버스 오브 블랙페로몬
	CASE "略称"
		RESULTS = 음마도의흑연영
	CASE "カテゴリ"
	    RESULTS:0 = 매지션 걸
		RESULTS:1 = 서큐버스 걸
		RESULTS:2 = E.R.A
ENDSELECT

@CARD_99625(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RESULT:1 = 악마족
		RETURN 마법사족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 1700
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "生成カード"
		RETURN 宝液ID
	CASE "分割"
		RETURN 효과なし
	CASE "追加日"
		RETURN 250802
ENDSELECT
RETURN 0

