@CARD_EXPLANATION_99625(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 자신 필드에 몬스터가 존재하지 않는 경우、이 카드는 패에서도 발동할 수 있다。
PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,손패起動,마함起動,フリー誘発,발동誘発)
PRINTL ①：이 카드를 몬스터 존에 특수 소환한다。
PRINTL 상대의 성별이 있는 몬스터의 효과에 체인해서 발동한 경우、
PRINTL 추가로 ５００ＬＰ를 지불하는 것으로、그 효과를 무효로 할 수 있다。
PRINTL 이 효과의 발동 후、자신은 다음 턴 종료시 까지、"서큐버스 걸" 이외의 몬스터를 ＥＸ덱에서 특수 소환할 수 없다。
CALL CARD_ABLE(0,지속효과)
PRINTL ②：이 카드가 몬스터 존에 존재하는 한、자신 필드의 다른 앞면 표시의 "서큐버스 걸" 카드는 대상 내성을 얻는다。
CALL CARD_ABLE(0,ターン終了時強制)
PRINTL ③：자신 / 상대 엔드 페이즈에、이 카드가 몬스터 존에 존재하는 경우에 발동한다。
PRINTL 자신은５００ＬＰ을 잃고、묘지에 "현자의 보액"토큰 １장을 생성한다。
PRINTL 그 후、이 카드를 묘지로 보낸다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99625(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 選択カード名
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99625)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,99625)
		RETURN 0
	IF ゾーン == "손패"
		CALL FULLRESET
		효과範囲필드 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3 || GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(듀얼리스트,99625,ゾーン)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,99625)
		RETURN 0
	IF ゾーン == "손패"
		CALL FULLRESET
		효과範囲필드 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3 || GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(듀얼리스트,99625,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件성별 = 성별在
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	SIF GETVAR(@"ライフ_%듀얼리스트%") <= 1000 && GETVAR(@"%듀얼리스트%_コスト無効") < 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時予約
	RETURN 1
ENDIF

@CARDEFFECT_99625(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99625,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
		CALL CARD_RESERV(듀얼리스트,99625,スタン予約)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99625,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		PRINTL ５００ＬＰ를 지불하고 상대가 발동한 효과를 무효로 하시겠습니까？
		CALL YN_CHOICE(듀얼리스트)
		IF RESULT == はい
			CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",500)
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
		ENDIF
		CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
		CALL CARD_RESERV(듀얼리스트,99625,スタン予約)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 서큐버스 걸
	효과条件他所 = 1
	효과条件표시 = 表側표시
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"対耐")
ELSEIF 종류 == ターン終了時強制
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99625)
	IF RESULT == 0
		CALL SUCCUGIRL_SPELL_PHEROMONES(듀얼리스트)
		CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",500)
		CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == ターン開始時予約
	CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
ENDIF