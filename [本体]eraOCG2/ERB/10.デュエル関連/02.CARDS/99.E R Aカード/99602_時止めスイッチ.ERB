RETURN 0

@CARD_EXPLANATION_199602(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：이 카드는 상대 턴에도 패에서 발동할 수 있다。
PRINTL 자신 / 상대 필드의 "시간 정지 스위치"의 효과를 받은 몬스터 １장을 골라、그 받고있는 효과를 해제한다。
PRINTL 해제（딸깍！

CALL TEXT_DECORATION("ROGUE")

@CARDCAN_199602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 操作者


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,199602)
SIF RESULT == 1
	RETURN 0


IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	SIF !GROUPMATCH(ゾーン,"손패","마함")
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件특수状態 = 99602
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
	ENDIF


@CARDEFFECT_199602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 대상者

#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 조교대상
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 효과蓄積
#DIMS DYNAMIC 操作者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(操作者,종류,ゾーン,場所,199602,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件특수状態 = 99602
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT >= 0
			選択カード = RESULT
			CALL CARD_SELECT(듀얼리스트,選択カード)
			PRINTL 시간 정지를 해제하시겠습니까?
			CALL YN_CHOICE(操作者)
			IF RESULT == 0
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"특수状態",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"接可",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"モ耐",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"魔耐",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"함정耐",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦耐",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦無",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表固",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"無効",0,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"不材",0,1)
				효과蓄積 = GETVAR(@"%듀얼리스트%_MZ_조교:場所")
				SIF 효과蓄積 > 10
					CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"発情",2 * 효과蓄積)
				CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",場所)
				IF RESULT != -1
					조교대상 = RESULT
					PRINTFORML %CALLNAME:조교대상%의 시간이 움직이기 시작함과 동시에 ...... 멈춰있던 쾌락이 한꺼번에 밀려들어왔다.
					FOR カウンタ,0,효과蓄積
						SIF カウンタ > 10
							PRINTFORML %CALLNAME:조교대상%은 끝나지 않는 쾌락에 고통의 비명을 지르고 말았다.
						CALL EROTIC_MOVE("全身애무",조교대상)
					NEXT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF