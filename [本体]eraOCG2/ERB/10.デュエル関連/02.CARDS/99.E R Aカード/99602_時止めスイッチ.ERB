@CARDNAME_99602,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 시간 정지 스위치（Ｔ Ｓ Ｐ）
	CASE "略称"
		RESULTS = 시간 정지 스위치
	CASE "カテゴリ"
		RESULTS:0 = 없음
RESULTS:2 = E.R.A
ENDSELECT


@CARD_99602(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99602(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：패를 1장 버리고, 필드 위의 암컷 몬스터 1장을 대상으로 이 카드를 발동할 수 있다.
PRINTL 그 몬스터는 완전 내성을 얻고, 전투로는 파괴되지 않으며, 표시 형식을 변경할 수 없게 되고, 효과는 무효화되며,
PRINTL 특수 소환의 소재로 릴리스할 수 없다.
PRINTL 자신의 패에 "해제 버튼"을 생성한다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99602)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件성별 = 牝性	
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_99602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 대상者

#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 조교대상
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 효과蓄積
#DIMS DYNAMIC 操作者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99602)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"選択")
			IF RESULT == 1
				選択カード = RESULT:1
				CALL CARD_SELECT(듀얼리스트,選択カード)
				TRYCALLFORM CARDNAME_{GETVAR(@"%대상者（CS）%_필드:選択カード（CS）")}, "名前"
				PRINTFORMW %RESULTS%는 스위치 하나를 누른 것만으로 마치 얼어붙은 것처럼 굳어버렸다.
				CALL CARD_GRANT_SPECIAL(대상者（CS）,選択カード（CS）,99602)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"接可",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"モ耐",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"魔耐",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"함정耐",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"戦耐",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"戦無",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"表固",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"無効",2,1)
				CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"不材",2,1)
				CALL CARD_GENERATE_HAND(듀얼리스트,199602)
			ENDIF
		ENDIF
	ENDIF
ENDIF




