@CARDNAME_59720,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 비밀여왕벌 유타라스
	CASE "略称"
		RESULTS = 여왕벌 유타라스
	CASE "カテゴリ"
		RESULTS:0 = 비밀벌
		RESULTS:1 = 비밀
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_59720(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 12
	CASE "攻撃力"
		RETURN 3300
	CASE "守備力"
		RETURN 800
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 펜듈럼
ENDSELECT

RETURN 0

@CARD_EXPLANATION_59720(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(0,손패起動)
CALL TEXT_DECORATION("P_MAGIC")
PRINTL 이 카드명의 ①②의 Ｐ효과는 각각 １턴에 1번밖에 사용할 수 없다。
CALL CARD_ABLE(2,마함起動)
PRINTL ①：자신의 메인 페이즈에 발동할 수 있다。
PRINTL EX덱에서 레벨 ８이하의 "비밀벌" 몬스터 １장을 특수 소환한다。
CALL CARD_ABLE(3,ターン終了時)
PRINTL ②：자신 / 상대의 엔드 페이즈에 발동할 수 있다。
PRINTL 상대에게 ４００데미지를 주고、자신은 라이프를 ４００회복한다。
CALL CARD_ABLE(0,攻守変動)
PRINTL ③：이 카드가 마법＆함정 존에 존재하는 한、자신 필드의 레벨１"비밀벌" 몬스터의 공격력은 ４００ 올린다。
CALL TEXT_DECORATION("P_MAGIC_2")
PRINTL "비밀벌 병장 베스포이데어"＋튜너 이외의 몬스터 １장 이상
PRINTL 이 카드명의  ①의효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,戦破壊→묘지)
PRINTL ①：몬스터 존에 존재하는 이 카드가 파괴된 경우에 발동할 수 있다。
PRINTL EX덱에서 "비밀여왕벌 유타라스"１장을 마법＆함정 존에 둔다。
CALL CARD_ABLE(0,攻撃時強制誘発)
PRINTL ②：이 카드가 몬스터와 전투하는 경우、그 공격을 무효로 하고、상대에게 이 카드의 공격력만큼 데미지를 준다。
CALL TEXT_DECORATION("ROGUE")

@CARDSUMMON_59720(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 12
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
	素材条件名前 = 비밀벌 병장 베스포이데어
	ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,59720)
	RETURN RESULT
ENDIF

@CARDCAN_59720(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59720)
SIF RESULT == 1
	RETURN 0

IF 종류 == 戦破壊→묘지
	SIF 발동済_1(듀얼리스트,59720)
		RETURN 0
	SIF !GROUPMATCH(묘지送り종류,"破壊","戦闘破壊")
		RETURN 0

	SIF 마함数(듀얼리스트) >= 5
		RETURN 0

	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動
	SIF 발동済_2(듀얼리스트,59720)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件カテゴリ = 비밀벌
	CALL 효과条件レベル_ADV(8, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_3(듀얼리스트,59720)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59720(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상 = -1


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59720,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件名前 = 비밀女王蜂ユ-タラス
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
	ENDIF
ELSEIF 종류 == 攻撃時強制誘発
	CALL FULLRESET
	IF RESULT == 0
		IF 戦闘場所(듀얼리스트,1) != 5
			CALL CARD_EROTICS(戦闘カードID(듀얼리스트,1))
			IF RESULT != 0
				조교대상 = RESULT
			ENDIF
			CALL LIFE_CHANGE(듀얼리스트,"減少","효과",GETVAR(@"%듀얼리스트%_MZ_攻:場所"),1)
		ELSE
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
			ENDIF
		ENDIF
		IF 조교대상 >= 0
			PRINTFORML 여왕이 직접 범한 %CALLNAME:조교대상%는 몸도 마음도 꿀처럼 질척질척하게 당했다。
			CALL EROTIC_MOVE("全身凌辱",조교대상,-1,800)
		ENDIF

		CALL FULLRESET
		CALL EVADE_ATTACK(듀얼리스트)
	ENDIF
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件カテゴリ = 비밀벌
	효과条件レベル = 1
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,400)
ELSEIF 종류 == 마함起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59720,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件カテゴリ = 비밀벌
		CALL 효과条件レベル_ADV(8, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59720,3)
	IF RESULT == 0
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML 여왕의 명령으로 벌들은 %CALLNAME:조교대상%의 몸으로 일제히 떼지어 애무를 했다。
			PRINTFORM 너무 많이가서 몸을 경련시키는 %CALLNAME:조교대상%을 거들떠도 보지않고、벌들은 채집한 암컷꿀을 마스터에게 헌상한다。
			CALL EROTIC_MOVE("全身凌辱",조교대상)
		ENDIF
		CALL FULLRESET
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",400,1)
		CALL FULLRESET
		CALL LIFE_CHANGE(듀얼리스트,"増加","효과",400)
		CALL CARD_EFFECT_USE(듀얼리스트,59720,3)
	ENDIF
ENDIF
