@CARD_EXPLANATION_79556(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드는 １턴에 １번밖에 발동할 수 없으며, ①의 효과는 듀얼중 1번밖에 적용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：패 / 덱 / 필드에서 "호루스타인" 이외의 암컷 몬스터를 １장 선택하여、
PRINTL 자신의 덱 / 패 / 필드 / 묘지에 존재하는 모든 같은 이름의 카드를 "【미유】호루스타인"으로 재정의한다。
PRINTL 그 후、덱밖에서 "【미유】호루스타인" １장을 패에 넣는다、
PRINTL 이 효과의 처리 후、이하의 효과에서 선택한다。
PRINTL ●패에서 "【미유】호루스타인" １장을 특수 소환한다。
PRINTL ●덱에서 "호루스타인─밀크 팜" １장을 패에 넣는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

;戦闘終了플래그を拾うため
IF 종류 == 永攻紋章起動 && (자신_듀얼패배플래그 > 0 || 상대_듀얼패배플래그 > 0)
	RETURN 1
ENDIF

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79556)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_D1(듀얼리스트,79556) 
		RETURN 0
	SIF 발동済_1(듀얼리스트,79556) 
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲필드 = 1
	효과条件성별 = 牝性
	효과条件固カテゴリ = 호루스타인
	
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 永紋章起動
	RETURN 1
ELSEIF 종류 == 永攻紋章起動
	RETURN 1
ENDIF

@CARDEFFECT_79556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 所有者
#DIM DYNAMIC 参照場所
#DIM DYNAMIC 암소대상
#DIM DYNAMIC 암소所有
#DIMS DYNAMIC 암소名称
#DIMS DYNAMIC 암소略称
#DIM DYNAMIC 암소속성
#DIM DYNAMIC 암소ミルクml
#DIM DYNAMIC 암소ミルク売却額
#DIM DYNAMIC 条件달성

#DIM DYNAMIC 紋章カードID
#DIM DYNAMIC 乳サイズ보정
#DIM DYNAMIC 紋章場所
#DIM DYNAMIC 암소鳴き声
#DIM DYNAMIC 암소절정声
#DIM DYNAMIC 암소弛緩声
#DIMS DYNAMIC 乳소환
#DIMS DYNAMIC 乳名

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

紋章カードID = 79556
IF 듀얼리스트 == "자신"
	書換先カードID = 紋章カードID + 100000
ELSE
	書換先カードID = 紋章カードID + 200000
ENDIF

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,紋章カードID,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲필드 = 1
		효과条件성별 = 牝性
		효과条件固カテゴリ = 호루스타인
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カードID = RESULT:99
		IF RESULT != 9999
			;(デュエル中１번)
			CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,紋章カードID,20)
			
			;永紋章の표시をスキップ
			SKIPDISP 1
			;データの설정(※初期値に-1が入っている+入力値は加減算されるので、上書きで初期化する)
			CALL CARD_ENG_EMBLEM(듀얼리스트,-1,選択カードID,1,上書きモード);No１：カードID
			CALL CARD_ENG_EMBLEM(듀얼리스트,-1,0,2,上書きモード);No２：乳サイズ보정
			CALL CARD_ENG_EMBLEM(듀얼리스트,-1,0,3,上書きモード);No３：ミルクml
			CALL CARD_RESERV(듀얼리스트,紋章カードID,永紋章予約)
			CALL CARD_RESERV(듀얼리스트,紋章カードID,永攻紋章予約)
			CALL CARD_GET_EMBLEM_VALUES(듀얼리스트,紋章カードID)
			紋章場所 = RESULT
			SKIPDISP 0
			
			;◆ホルスタイン情報の취득(紋章カードIDから各種情報を취득)
			CALL HORUSTEIN_GET_DATA(듀얼리스트,紋章カードID)
			암소名称 = %RESULTS:0%
			암소略称 = %RESULTS:1%
			乳소환 = %RESULTS:3%
			紋章場所 = RESULT:0 
			암소대상 = RESULT:1 
			암소鳴き声 = RESULT:2 
			암소절정声 = RESULT:3 
			암소弛緩声 = RESULT:4 
			암소ミルクml = RESULT:97
			参照カードID = RESULT:99
			
			;◆カードの書き換え(フィールド・손패・덱・묘지)
			CALL HORUSTEIN_SET_RECONSTRUCT(듀얼리스트,選択カードID,書換先カードID)
			
			PRINTFORMW 등록증에 이름이 새겨지니、어디선가 호루스의 수호자가 나타났다！
			PRINTFORML %암소名称%는 저항할 새도없이 세명의 수호자에게 밀려져、철컥、하고 피어스가 붙여져 버렸다…
			PRINTFORML 
			PRINTFORML "…음머！…음머！？"
			PRINTFORMW 
			PRINTFORML %암소名称%는 말을 하려 해도、붙여진 피어스의 영향인지、그 목소리는 울음소리로 바뀌어버린다。
			PRINTFORML 어떻게든 움직이려 꼼짝거리며 저항하려 하지만、피어스가 어렴풋이 빛나니、서서히 저항이 약해져간다…
			PRINTFORMW 희미하게 저항하는 %암소名称%를 무시하고、수호자는 그 튼튼한 손으로 가슴을 정중하게 암소에게 상응하는 "상품"으로 "조교"해간다…
			SETCOLOR C_PINK
			PRINTFORML 
			PRINTFORML "음머-…""으、음머-…❤"
			PRINTFORMW 
			RESETCOLOR
			PRINTFORML …조교가 끝난것 같다。
			PRINTFORML 아까까지와 상태가 바뀌어、목소리를 낼때마다 열을 띤 암소의 울음소리로 바뀌어간다…
			PRINTFORML 
			
			;◆乳サイズ変動
			CALL HORUSTEIN_SET_BUST(듀얼리스트,紋章カードID,選択カードID,+1)
			
			PRINTFORML %암소名称%는 밀어붙이던 수호자가 어느샌가 사라진것을 확인하니、
			PRINTFORMW 가슴이 답답한것인지、옷을 벗기 시작했다…
						
			;◆服装の변경(牛柄ビキニと角)
			CALL HORUSTEIN_SET_CLOTHES(듀얼리스트,選択カードID)
			
			PRINTFORML 
			PRINTFORML %암소名称%는, 그것이 당연하다는듯이 소모양 비키니로 갈아입기 시작했다…
			
			IF 암소대상 != -1 
				;악세사리&모자：つけ角 大(2枠目を변경)
				CALL EROTIC_MOVE("バスト조교",암소대상,-1,300)
			ENDIF
			
			;書き換えた 카드를 패에 넣는다。
			CALL CARD_GENERATE_HAND(듀얼리스트,書換先カードID)
			
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件ID = 79555
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			
			IF RESULT == 1
				;덱에 ある場合
				CALL YN_CHOICE(듀얼리스트,"「호루스타인─밀크 팜」을 패에 넣는다","「" + 암소名称 + "」를 패에서 특수 소환한다")
			ELSE
				;덱에 존재하고ない경우、選択肢は표시しない
				PRINTFORML
				PRINTFORMW "호루스타인─밀크 팜"이 덱에 존재하지 않기 때문에、"%암소名称%"를 패에서 특수 소환한다…가 선택됩니다。
				RESULT = 1
			ENDIF
			
			IF RESULT == 0
				;덱에서 밀크 팜を패에 넣는다
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件ID = 79555
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ELSE
				;패에서 특수소환
				CALL FULLRESET
				FOR カウンタ,0,紙束最大値
					SIF GETVAR(@"%듀얼리스트%_손패:カウンタ") != 書換先カードID
						CONTINUE
					CALL CARD_SP_SUMMON(듀얼리스트,"손패",カウンタ)
					BREAK
				NEXT
				IF RESULT != 0
					PRINTFORMW %乳소환%
					CALL HORUSTEIN_RAND_COW_TXT(1,암소鳴き声)
				ENDIF
			ENDIF
			
			PRINTFORML 
			CALL FULLRESET
			효과範囲마함 = 1
			효과範囲손패 = 1
			효과条件ID = 79555
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				;패 / 필드에 호루스타인ファ-ムが존재하는 場合
				SETCOLOR C_PINK
				PRINTFORML %암소名称%의 아름다운 가슴을 탄력있게 흔들고、아무런 말도 하지 않고 가슴을 이쪽으로 내어왔다…
				PRINTFORML %암소名称%는 짜여질 준비가 되어있는것 같다…
				PRINTFORMW "……읏❤ 머-…❤"
				PRINTFORML 
				RESETCOLOR
			ENDIF
			
			PRINTFORML 
			SETCOLOR C_YELLOW
			PRINTFORML "【미유】호루스타인"은 필드에 존재하는 경우 "%암소名称%"로 취급됩니다。
			RESETCOLOR
			PRINTFORMW
			
		ENDIF
		
	ENDIF
	
	SKIPDISP 1
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	SKIPDISP 0
	
ELSEIF 종류 == 永紋章起動 || 종류 == 永攻紋章起動
	;紋章を使用し、패 / 필드의 암소の情報を書き換え
	CALL HORUSTEIN_FIELD_UPDATE(종류,書換先カードID)
	
	IF (자신_듀얼패배플래그 > 0 || 상대_듀얼패배플래그 > 0)
		;ミルクの精算処理
		CALL HORUSTEIN_SELL_MILK()
	ENDIF
ENDIF


;ここから암소のカード処理-----

@CARDNAME_179556,参照先,参照듀얼리스트 = "자신"
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先
#DIMS DYNAMIC 参照듀얼리스트
#DIMS DYNAMIC 암소名称
#DIMS DYNAMIC 암소略称
#DIMS DYNAMIC 乳名
#DIM DYNAMIC 参照カードID
VARSET RESULT
VARSET RESULTS

;◆ホルスタイン情報の취득(紋章カードIDから各種情報を취득)
CALL HORUSTEIN_GET_DATA(参照듀얼리스트,79556)
암소名称 = %RESULTS:0%
암소略称 = %RESULTS:1%
乳名 = %RESULTS:2%
参照カードID = RESULT:99

SELECTCASE 参照先
	CASE "名前"
			RESULTS = 【%乳名%】%암소名称%
	CASE "略称"
			RESULTS = %암소略称%
	CASE "カテゴリ"
		
		TRYCALLFORM CARDNAME_{参照カードID}, "カテゴリ"
		RESULTS:6 = %RESULTS:3%
		RESULTS:5 = %RESULTS:2%
		RESULTS:4 = %RESULTS:1%
		RESULTS:3 = %RESULTS:0%
		RESULTS:0 = 호루스
		RESULTS:1 = 호루스타인
		RESULTS:2 = E.R.A
			
	CASEELSE
		RESULTS =
ENDSELECT


@CARD_179556(参照先,듀얼리스트 = "자신")
#DIMS DYNAMIC 参照先
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 参照カードID
VARSET RESULT
VARSET RESULTS

IF GROUPMATCH(参照先,"レベル","攻撃力","守備力","튜너","連動ID")
	;◆ホルスタイン情報の취득(紋章カードIDから各種情報を취득)
	CALL HORUSTEIN_GET_DATA(듀얼리스트,79556)
	参照カードID = RESULT:99
ENDIF

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常モン
	CASE "속성"
		;임세티と同じ
		RETURN 어둠속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		TRYCALLFORM CARD_{参照カードID},"レベル"
		RETURN RESULT
	CASE "攻撃力"
		TRYCALLFORM CARD_{参照カードID},"攻撃力"
		RETURN RESULT
	CASE "守備力"
		TRYCALLFORM CARD_{参照カードID},"守備力"
		RETURN RESULT
	CASE "튜너"
		TRYCALLFORM CARD_{参照カードID},"튜너"
		RETURN RESULT
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN デュアル
	CASE "連動ID"
		RETURN 参照カードID
		
ENDSELECT

RETURN 0

@CARD_EXPLANATION_179556(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 통상몬스터로 취급하며、마법＆함정 존에 놓인 카드에 의해 스테이터스가 결정된다。
PRINTL 자신 필드에 "호루스타인─밀크 팜"이 존재하는 경우、이하의 효과를 얻는다。
PRINTL 
PRINTL 이 카드의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
PRINTL ①：메인 페이즈에 발동할 수 있다。이 카드의 젖을 계측한다。
;CALLFORM CARD_ABLE(0,필드起動)
PRINTL ②：배틀 페이즈에 발동할 수 있다。이 카드의 젖을 뽑아 발정시킨다。
PRINTL 그 후、이 턴중 이하의 효과를 적용한다。
PRINTL ●이 카드는 배틀 페이즈중에 ２회 공격할 수 있다。
PRINTL ●이 카드는 한번 전투 / 효과로는 파괴되지 않는다。
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ③：발정한 이 카드의 공격이 중단된 경우、발동할 수 있다。
PRINTL 공격 대상인 몬스터를 "호루스타인"카드로 취급해、그 공격력과 수비력을 절반으로 한다。

@CARDCAN_179556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 書換先カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 듀얼리스트 == "자신"
	書換先カードID = 179556
ELSE
	書換先カードID = 279556
ENDIF

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,書換先カードID)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF GETVARS(@"%듀얼리스트%_フィールド名") != "밀크 팜"
		RETURN 0
	IF メインフェイズ == 1
		SIF 書換先カードID > 200000 ;ただ情報を표시するだけなので、NPCは使用しない(ターン1付けてないから多分連打するし…)
			RETURN 0
		SIF ターンプレイヤー != "자신" ;ただ情報を표시するだけなので、NPCは使用しない(ターン1付けてないから多分連打するし…)
			RETURN 0
	ELSEIF バトルフェイズ == 1
		SIF 발동済_1(듀얼리스트,書換先カードID) 
			RETURN 0
	ENDIF
	
	RETURN 1
ELSEIF 종류 == 攻撃後強制誘発;本当は밀크 팜なきゃダメだけど、ただ致すだけだし良いか…
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_発情:場所") == 0
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_攻撃無効_保") != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF GETVARS(@"%듀얼리스트%_フィールド名") != "밀크 팜"
		RETURN 0
	;発情時に攻撃中断された場合
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_発情:場所") == 0
		RETURN 0	
	SIF GETVAR(@"%듀얼리스트%_攻撃無効") != 1
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == 효과後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	;守備표시の場合
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 1 
		RETURN 0
	;자신이 특수状態：ドリップ(99554)の場合
	SIF GETVAR(@"%듀얼리스트%_MZ_특수状態:場所") != 99554
		RETURN 0
	
	CALL FULLRESET
	효과条件固カテゴリ = 호루스타인
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT == 1
		RETURN 1
	
	CALL FULLRESET
	효과条件固カテゴリ = 호루스타인
	CALL CARD_CHECK_FIN_DATA(対面者,"효과발동")
	SIF RESULT == 1
		RETURN 1
	
	RETURN 0
ENDIF

@CARDEFFECT_179556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 攻撃대상
#DIM DYNAMIC 攻撃대상カードID
#DIMS DYNAMIC 상대名称
#DIMS DYNAMIC 搾手名称
#DIM DYNAMIC 搾手
#DIM DYNAMIC 절정횟수

#DIM DYNAMIC 紋章カードID
#DIM DYNAMIC 紋章場所
#DIMS DYNAMIC 암소名称
#DIM DYNAMIC 암소대상
#DIM DYNAMIC 암소鳴き声
#DIM DYNAMIC 암소절정声
#DIM DYNAMIC 암소弛緩声

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

紋章カードID = 79556
IF GETVAR(@"%듀얼리스트%_MZ_所有:場所") == 1
書換先カードID = 179556
ELSE
	書換先カードID = 279556
ENDIF
;◆ホルスタイン情報の취득(紋章カードIDから各種情報を취득)
CALL HORUSTEIN_GET_DATA(듀얼리스트,紋章カードID)
암소名称 = %RESULTS:0%
紋章場所 = RESULT:0
암소대상 = RESULT:1
암소鳴き声 = RESULT:2 
암소절정声 = RESULT:3 
암소弛緩声 = RESULT:4 
参照カードID = RESULT:99

;実行者によって文章の상대を切り替える
IF 듀얼리스트 == "자신"
	搾手名称 = %CALLNAME:MASTER%
	搾手 = MASTER
ELSE
	;대상상대
	CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
	IF RESULT != -1
		搾手名称 = %CALLNAME:RESULT%
	ELSE
		搾手名称 = "대상者"
	ENDIF
	搾手 = RESULT
ENDIF

;공격 대상의 名前を취득
IF 종류 == 攻撃後強制誘発 || 종류 == 효과後誘発
	공격 대상 = 戦闘場所(対面者)
	;名称の설정
	IF 攻撃대상 == 99
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			상대名称 = %CALLNAME:RESULT%
		ELSE
			상대名称 = "대상상대"
		ENDIF
	ELSE
		攻撃대상カードID = GETVAR(@"%対面者%_MZ_ID:공격 대상")
		TRYCALLFORM CARDNAME_{攻撃대상カードID},"略称"
		상대名称 = %RESULTS%
	ENDIF
ENDIF

IF 종류 == 필드起動 && メインフェイズ == 1
	;ステータス표시させるだけなので、無効にさせない
	효과無効不能 = 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID)
	IF RESULT == 0 && 参照カードID != -1
		
		PRINTFORML 
		PRINTFORMW 호루스의 종자들이 %암소名称%를 둘러싸고있다…
		
		;名前から가슴のサイズの変動まで표시
		CALL HORUSTEIN_PRINT_STATE(듀얼리스트,場所,書換先カードID)
	ENDIF	
	
ELSEIF 종류 == 필드起動 && バトルフェイズ == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID,1)
	
	IF RESULT == 0 && 参照カードID != -1
		;가슴を搾る
		IF GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所") == 1
			;◆バトルフェイズ：乳搾りパターン１
			CALL HORUSTEIN_KOJO_BATTLE_MILK(듀얼리스트,書換先カードID,1)
			
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"発情",175)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",1,上書きモード)
		ELSE
			;◆バトルフェイズ：乳搾りパターン２
			CALL HORUSTEIN_KOJO_BATTLE_MILK(듀얼리스트,書換先カードID,2)
			
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"発情",185)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",0,上書きモード)
		ENDIF
		;二回攻撃、一한번만 파괴되어 ない
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦",1)
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"一破",1)
		
		CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:場所"))
		
		;発情
		SETCOLOR C_PINK
		PRINTFORML 
		PRINTFORML %암소名称%는 젖을 짜낸 영향인지、심하게 발정하고있다…
		PRINTFORML
		RESETCOLOR

		IF ターンプレイヤー != 듀얼리스트
			;상대 턴에 발동한 場合
			PRINTFORML %암소名称%는 젖을 짜여진 여운에 잠겨있다…
			
			;절정後の암소鳴き声
			CALL HORUSTEIN_RAND_COW_TXT(2,암소弛緩声)
		ENDIF

		PRINTFORMW
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
	;発情時に攻撃が中断された場合
	
	;ひとりえっち(ここは一回触ってるので短めに。효과処理もあるしね)
	PRINTFORML …발정한 %암소名称%는 %상대名称%에게 흔들흔들하며 가까이 가지만、시선을 발밑으로 향하니 도중에 다리를 멈춰버렸다。
	PRINTFORML 후우후우 하며 거친 호흡을 하는 %암소名称%는、자신의 젖에서 눈을 떼지 못하고있다…
	PRINTFORMW %암소名称%는 %搾手名称%에게 짜여진 기억을 기억해내는듯이、젖을 천천히 문지르기 시작했다…
	
	;암소の鳴き声を鳴らす
	CALL HORUSTEIN_RAND_COW_TXT(1)
	
	IF 암소대상 != -1
		CALL EROTIC_MOVE("バスト조교",암소대상,-1,500)
	ENDIF
	절정횟수 += RESULT:1
	IF 절정횟수 > 0
		PRINTFORML %암소名称%는 {절정횟수}번 절정한것같다。
		PRINTFORMW 여운이 기분좋았던것인지、%암소名称%의 주변엔 작고 하얀 반점이 흩날리고있다…
	ENDIF
	
	PRINTFORML 
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID)
	
	IF RESULT == 0
		IF 攻撃대상 == 5 
			;直接攻撃失敗時
			;再び手を伸ばすと、상대 필드에 他に戦闘を行える 몬스터가 いない為か、タガが外れたように…
			
			PRINTFORMW %암소名称%는 상대 필드에 전투를 할 수 있는 몬스터가 없는것을 확인하고、더욱깊은 쾌락을 탐구해 손가락을 움직이기 시작했다…
			절정횟수 += 2
			SETCOLOR C_PINK
			
			;암소の鳴き声を鳴らす
			CALL HORUSTEIN_RAND_COW_TXT(절정횟수)
			
			IF 암소대상 != -1
				REPEAT 절정횟수
					CALL EROTIC_MOVE("バスト조교",암소대상,-1,200)
				REND
			ENDIF
			
			RESETCOLOR
			PRINTFORML 울음소리가 끝나니、%암소名称%는 "철퍽" 하는 소리를 내며 하얀 물웅덩이에 쓰러졌다…
			PRINTFORMW 마음껏 기분 좋아진 %암소名称%는、만족한것같은 미소를 띄우고있다…
			;절정後の암소鳴き声
			CALL HORUSTEIN_RAND_COW_TXT(2,암소弛緩声)
			PRINTFORMW 
		ELSE
			;モンスター攻撃時に失敗
			PRINTFORML 발정한 %암소名称%는 %상대名称%를 밀어쓰러뜨려、그대로 말타는 자세가 되었다！
			PRINTFORM %암소名称%는
			;短め鳴き
			CALL HORUSTEIN_RAND_COW_TXT(1,암소鳴き声,10)
			
			IF GETVAR(@"%듀얼리스트%_MZ_服状:場所") >= 2
				;全裸の場合
				PRINTFORML 하고 우니、가슴을 딱붙게 %상대名称%에게 밀어붙였다…
				PRINTFORMW %암소名称%는 속옷을 입고있지 않기 때문에、가슴의 감각이 다이렉트로 전해져온다…
				PRINTFORML 밀어붙여진 가슴은 제대로 젖어있어、끝부분에선 뚝뚝하며 하얀 액체가 나오고있다。
				PRINTFORML %암소名称%는 자신에게서 떨어진 액체를 덧그리며、%상대名称%에게 바르려는듯한 모양을 남겼다…
				PRINTFORMW %상대名称%는 모유에 의해 마킹된것같다…
				
				SETCOLOR C_GRAY
				PRINTFORMW ※전라이기 때문에、공수를 내리는 효과는 적용되지 않습니다
				RESETCOLOR
			ELSE
				PRINTFORML 하고 우니、가슴을 딱붙게 %상대名称%에게 밀어붙였다…
				PRINTFORMW 검은색과 흰색의 아무리봐도 소모양인 속옷의 뒤엔 "호루스"에 의한 계약서가 각인되어있다…
				PRINTFORML 
				PRINTFORMW 밀크 팜의 계약이 이행되어、%상대名称%의 공수는 절반이 되었다！
				PRINTFORMW %상대名称%는 모유에 의해 마킹된것같다…
				
				CALL SET_FIELD_INFO(対面者,"필드",攻撃대상,"永攻",-(GETVAR(@"%対面者%_MZ_攻:공격 대상")) / 2)
				CALL SET_FIELD_INFO(対面者,"필드",攻撃대상,"永守",-(GETVAR(@"%対面者%_MZ_守:공격 대상")) / 2)
			ENDIF
			
			CALL CARD_SET_CATEGORY(対面者,"필드",攻撃대상,"호루스타인")
		ENDIF
		
	ENDIF
ELSEIF 종류 == 효과後強制誘発
	;ドリップ状態(守備표시時、ホルスタイン以外のカードが발동する度に搾られる)
	;ドリップ状態の処理
	CALL HORUSTEIN_DRIP(듀얼리스트,書換先カードID)
ENDIF


