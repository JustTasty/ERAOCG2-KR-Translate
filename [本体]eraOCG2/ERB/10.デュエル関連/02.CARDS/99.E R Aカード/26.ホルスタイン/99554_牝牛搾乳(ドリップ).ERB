@CARDNAME_99554,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 【착유】호루스타인─드립
	CASE "略称"
		RESULTS = 드립【착유】
	CASE "カテゴリ"
		RESULTS:0 = 호루스타인
		RESULTS:1 = E.R.A
ENDSELECT

@CARD_99554(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "추가日"
		RETURN 250325
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99554(종류)
#DIM DYNAMIC 종류
#DIMS DYNAMIC 듀얼리스트

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ②의 효과는 １턴에 １번밖에 발동할 수 없다。
CALLFORM CARD_ABLE(0,마함起動,손패起動,フリー誘発)
PRINTL ①：메인 페이즈 및 배틀 페이즈에 발동할 수 있다。자신의 패 / 묘지의 "호루스타인" 몬스터를 １장 골라 특수 소환한다。
PRINT 이 효과로 특수 소환한 카드는 이하의 효과를 얻는다。
SETCOLOR C_GRAY
PRINTL (※)
RESETCOLOR
PRINTL ●메인 페이즈 및 배틀 페이즈에 발동할 수 있다。１턴에 １번、이 카드의 가슴을 짜내 발정시킨다。
PRINTL ●스탠바이 페이즈에 발동한다。이 카드의 젖을 짜내어、공격력 / 수비력을 ２５０ 올린다。
PRINTL 자신의 마법＆함정 존에 세트된 카드가 존재하지 않을 경우、이 카드는 패에서 버리고 발동할 수 있다。
CALLFORM CARD_ABLE(1,묘지起動)
PRINTL ②：배틀 페이즈에 발동할 수 있다。
PRINTL 묘지의 이 카드를 제외하고、자신 필드의 암컷 몬스터 １장을 선택한다。
PRINTL 그 몬스터에게、①의 효과로 부여되는 효과를 적용한다。

SETCOLOR C_GRAY
PRINTL
PRINTL ※：원래의 카드명이 "호루스타인"몬스터인 경우、이하의 효과도 적용된다
PRINTL ●이 카드가 수비표시인 경우、호루스타인 이외의 카드의 효과가 발동한 경우에 발동한다。
PRINTL 이 카드의 젖을 짜내、１００ＬＰ 회복한다。
RESETCOLOR
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99554(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 대상カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99554)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 
	SIF !(メインフェイズ == 1 || バトルフェイズ == 1 )
		RETURN 0
	IF ゾーン == "손패"
		;セット카드가 あれば使えない
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件표시 = 裏側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT > 0
			RETURN 0
	ELSEIF ゾーン == "마함"
		;세트 한 턴에 は使えない
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	
	;패 / 묘지에 호루스타인が居なければ使えない
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 호루스타인
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	
	SIF 발동済_2(듀얼리스트,99554)
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	
	RETURN 1
ELSEIF 종류 == 필드起動付与
	SIF !(メインフェイズ == 1 || バトルフェイズ == 1 )
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	
	RETURN 1
ELSEIF 종류 == 付与효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_99554(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 参照場所
#DIM DYNAMIC 상대플래그
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 암소대상
#DIMS DYNAMIC 암소名称
#DIMS DYNAMIC カテゴリ名称
#DIM DYNAMIC 절정횟수

#DIM DYNAMIC 所有者
#DIMS DYNAMIC 搾手名称
#DIM DYNAMIC 搾手
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 現乳サイズ
#DIM DYNAMIC T1ミルクml
#DIM DYNAMIC ミルクml
#DIM DYNAMIC 암소절정声


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99554,1)
	IF RESULT == 0
		
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 호루스타인
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	
		IF RESULT != -1
			選択カードID = RESULT:1
			
			;ドリップ状態を付与
			CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"특수状態",99554,2)
			
			CALL HORUSTEIN_GET_DATA(듀얼리스트,選択カードID)
			암소名称 = %RESULTS:0%
			
			SETCOLOR C_PINK
			CUSTOMDRAWLINE —
			PRINTFORML 
			PRINTFORML %암소名称%는 드립장치에 연결되어버렸다…
			PRINTFORML (수비표시인 경우、드립장치에 의해 자동적으로 착유당합니다。)
			PRINTFORML 
			CUSTOMDRAWLINE —
			PRINTFORMW 
			RESETCOLOR
			
			;名前から가슴のサイズの変動まで표시
			CALL HORUSTEIN_PRINT_STATE(듀얼리스트,カード소환先,選択カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:カード소환先"))
			
		ENDIF
		;テスト時はコメント化
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99554,2)
	
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件성별 = 牝性
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		
		IF RESULT != -1
			選択カードID = RESULT:99
			
			;자신 필드의 牝性にドリップ状態を付与
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード（CS）,"특수状態",99554,2)
			
			CALL HORUSTEIN_GET_DATA(듀얼리스트,選択カードID)
			암소名称 = %RESULTS:0%
			
			SETCOLOR C_PINK
			CUSTOMDRAWLINE —
			PRINTFORML 
			PRINTFORML %암소名称%는 드립장치에 연결되어버렸다…
			PRINTFORML 
			CUSTOMDRAWLINE —
			PRINTFORMW 
			RESETCOLOR
			
			;호루스타인몬스터로 はない경우、着替えさせる(着こんでると文字ハミ出しちゃうので…)
			CALL HORUSTEIN_SET_CLOTHES(듀얼리스트,選択カードID)
			
			;名前から가슴のサイズの変動まで표시
			CALL HORUSTEIN_PRINT_STATE(듀얼리스트,選択カード（CS）,参照カードID)
			
			;CALL HORUSTEIN_GET_BUST(選択カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:選択カード（CS）"))
			;現乳サイズ = RESULT:2 
			
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動付与 || 종류 == 付与효과
	
	選択カードID = GETVAR(@"%듀얼리스트%_MZ_ID:場所")
	;호루스타인情報の취득
	CALL HORUSTEIN_GET_DATA(듀얼리스트,選択カードID)
	암소名称 = %RESULTS:0%
	암소대상 = RESULT:1
	암소절정声 = RESULT:3

	CALL HORUSTEIN_GET_BUST(参照カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:場所"))
	現乳サイズ = RESULT:2

	SETCOLOR C_PINK
	CUSTOMDRAWLINE —
	PRINTFORML
	PRINTFORML %암소名称%는 드립장치에 연결되어있다…
	PRINTFORML 
	CUSTOMDRAWLINE —
	RESETCOLOR
	
	IF 종류 == 필드起動付与
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,選択カードID,10)
	
		IF RESULT == 0
			;ドリップ状態
			PRINTFORML %암소名称%의 가슴에 붙여진 장치가 흡인을 시작해、가슴에서 뚝뚝 흐르는것이 밀크서버에 쌓여간다…
			
			;착유する(通常よりも少量)
			CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,現乳サイズ-4)

			;이 효과로 付与された発情は、攻撃中断しない
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"発情",100)
			
			;名前から가슴のサイズの変動まで표시
			CALL HORUSTEIN_PRINT_STATE(듀얼리스트,場所,選択カードID,,,,99554)
			
			IF 암소대상 != -1
			
				SKIPLOG 1
				절정횟수 = 0
				WHILE 절정횟수 == 0 || カウンタ <= 5
					;절정するまで(절정しても5回は必ず搾られる)
					CALL EROTIC_MOVE("バスト조교",암소대상,-1,300)
					EXP:암소대상:분유경험 += 1
					절정횟수 = RESULT:1
					
					;착유する(通常よりも少量)
					CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,現乳サイズ -10)
					
					IF 절정횟수 > 0 && カウンタ >= 5
						CALL CARD_EROTICS_END(암소대상)
						SKIPLOG 0
						;절정後の암소鳴き声
						CALL HORUSTEIN_RAND_COW_TXT(절정횟수,암소절정声);うるさい鳴き声
						PRINTFORML %암소名称%는 몸안에서 체액을 견뎌넘기며、움찔움찔 떨고있다…
						PRINTFORML 제대로 "리프레쉬"한것 같다。이거라면 공격중엔 손이 멈추는일은 없겠지…
						PRINTFORML 
						PRINTFORML [0] - 리프레쉬를 종료한다
						BINPUT
					ELSEIF カウンタ % 5 == 0
						PRINTFORML 
						CALL HORUSTEIN_RAND_COW_TXT(1,암소절정声)
						PRINTFORML 
						;一瞬停止
						TWAIT 1,0
					ENDIF
					カウンタ += 1
				WEND
			ELSE
				SKIPLOG 1
				FOR カウンタ,0,9
					CALL HORUSTEIN_RAND_COW_TXT(1,2,10);うるさい鳴き声
					PRINTFORMW %암소名称%는 "리프레쉬"당하고있다…
					PRINTFORMW 
				NEXT
				PRINTFORML 
				SKIPLOG 0
				;절정後の암소鳴き声
				CALL HORUSTEIN_RAND_COW_TXT(5,3);うるさい鳴き声
				PRINTFORML %암소名称%는 몸안에서 체액을 견뎌넘기며、움찔움찔 떨고있다…
				PRINTFORML 제대로 "리프레쉬"한것 같다。이거라면 공격중엔 손이 멈추는일은 없겠지…
				PRINTFORML 
				PRINTFORML [0] - 리프레쉬를 종료한다
				BINPUT
			ENDIF
		ENDIF
	ELSE
		;付与효과( 스탠바이 페이즈에 발동する)
		
		;ドリップ状態
		PRINTFORML %암소名称%의 가슴에 붙여진 장치가 비비는듯이 가슴을 자극하여、흘러떨어진것이 밀크서버에 쌓여간다…
		
		;착유する(通常よりも少量)
		CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,現乳サイズ -2)

		;공격력 / 수비력アップ
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",250)
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永守",250)
		
		PRINTFORML 차분하게 가슴을 마사지해、암소의 잠재능력을 끌어내고있다…！
		PRINTFORMW %암소名称%의 공수가 250올랐다！
	ENDIF
ENDIF
