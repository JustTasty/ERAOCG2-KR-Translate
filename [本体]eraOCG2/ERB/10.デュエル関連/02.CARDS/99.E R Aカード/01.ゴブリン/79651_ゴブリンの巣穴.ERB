@CARDNAME_79651,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 고블린의 둥지
	CASE "略称"
		RESULTS = 고블린의 둥지
	CASE "カテゴリ"
		RESULTS:0 = 고블린
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79651(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79651(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  ①의 효과는 1턴에 1번밖에 사용할 수 없다。
CALL CARD_ABLE(0,지속효과)
PRINTL  자신 필드는 。"고블린의 둥지"가 된다。
CALL CARD_ABLE(1,마함起動)
PRINTL ①：자신의 메인 페이즈에 발동 할 수 있다。
PRINTL 묘지에 "고블린」 카드 1장을 패에 넣는다。
CALL CARD_ABLE(2,지속효과)
PRINTL ②：자신 필드에 존재하는 "고블린" 는 상대 플레이어에게 직접 공격할 수 있다。

@CARDCAN_79651(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79651)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,79651)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 고블린
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == ライフ変強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ダメージ종류 != "直接戦闘"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79651(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79651,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 고블린
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79651)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == ライフ変強制誘発
	CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
	IF RESULT != -1
		조교대상 = RESULT
		SIF TALENT:조교대상:성별 != 1
			RETURN 0
		TRYCALLFORM CARDNAME_{戦闘カードID(듀얼리스트)}("カテゴリ")
		SIF RESULTS != "고블린"
			RETURN 0
		TRYCALLFORM CARD_{戦闘カードID(듀얼리스트)}("성별")
		SIF RESULT != 雄性
			RETURN 0
		PRINTFORML 
		PRINTFORML 고블린들이 기습하여, 불의의 습격을 받아 무방비가 된 %NAME:조교대상%그대로 범해집니다！
		PRINTFORML 
		CALL EROTIC_MOVE("前穴凌辱",조교대상,-1,GETVAR(@"%듀얼리스트%_MZ_攻:%듀얼리스트%_戦闘カード"))
	ENDIF

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "고블린의 둥지")
	CALL FULLRESET
	효과条件カテゴリ = 고블린
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"直攻")
ENDIF
