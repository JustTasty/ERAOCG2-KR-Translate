@CARDNAME_79652,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RUM－고블린 콜로네이션
	CASE "略称"
		RESULTS = RUM고블린콜로
	CASE "カテゴリ"
		RESULTS:0 = RUM
		RESULTS:1 = 고블린
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_79652(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79652(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：자신/ 상대 필드에 "프린세스   고블린" 이외에 암컷 몬스터 1장을 대상으로 하여 발동할 수 있다。
PRINTL 대상 몬스터를 소재로, "프린세스   고블린" 1장을 엑시즈 소환한다。
PRINTL 이 효과로 엑시즈 소환한"프린세스  고블린"은, 소재로 한 암컷 몬스터와 동명의 카드로 취급한다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79652(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79652)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件성별 = 牝性
	효과条件固名前 = 고블린 프린세스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件名前 = 고블린 프린세스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_79652(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 설정レベル
#DIM DYNAMIC 取込カードID
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79652)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;通常마법汎用処理
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件성별 = 牝性
		효과条件固名前 = 고블린 프린세스
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"選択",79652)
;		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT:1
		SIF 選択カード == 9999
			RETURN 0

		CALL CARD_SELECT(듀얼리스트,選択カード)
		取込カードID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:{選択カード（CS）}")

		CALL FULLRESET
		CALL CARD_DISPOSE(대상者（CS）,参照先（CS）,選択カード（CS）,제외行き)
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件名前 = 고블린 프린세스
		효과추가종류:0 = 40000
		효과추가종류:1 = 1000000 + 取込カードID
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0

		CALL CARD_EROTICS(取込カードID)
		IF RESULT != -1
			PRINTFORMW 고블린에게 범해지고, 마음이부서진 %NAME:RESULT%은 고블린의 임신 주머니가 되는 운명을 받아들였다。
			CALL EROTIC_MOVE("全身凌辱", RESULT, -1, 800)
		ENDIF

	ENDIF
ENDIF
