@CARDNAME_39650,参照先
;ココ39650で지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 고블린 기습부대
	CASE "略称"
		RESULTS = 고블린기습부대
	CASE "カテゴリ"
		RESULTS:0 = 고블린
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39650(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS


SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39650(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,손패起動,フリー誘発)
PRINTL ①：자신 / 상대 턴에, 상대 필드에 여성 몬스터가 존재할 경우에 발동할 수 있다。
PRINTL 이 카드를 패에서 특수 소환한다。
CALL CARD_ABLE(2,戦破壊→묘지)
PRINTL ②：필드의 이카드가 묘지로 보내졋을 경우에 발동할수 있다。
PRINTL 패 / 덱에서 「「고블린 기습부대」 이외의 「「고블린」 몬스터 1장을 특수 소환한다。
PRINTL 그 후, 상대 필드에 여성 몬스터가 존재할 경우, 상대의 여성 몬스터 1장의 공격력 / 수비력을 1000 감소시킨다。
CALL CARD_ABLE(3,攻撃後誘発)
PRINTL ③：상대 필드의 몬스터가 전투에 의해 묘지로 보내졌을 경우에 발동할 수 있다。
PRINTL 묘지의 이 카드를 공격 표시로 특수 소환한다。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39650)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == フリー誘発
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39650,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 牝性 
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39650,"묘지")
	SIF RESULT == 1
		RETURN 0
	SIF GETVAR(@"%対面者%_直前戦闘破壊") <= 0
		RETURN 0

	RETURN 1
ELSEIF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"リリース","破壊","戦闘破壊")
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲손패 = 1
	효과条件カテゴリ = 고블린
	효과条件固名前 = 고블린 기습부대
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 1
	RETURN 1
ENDIF

@CARDEFFECT_39650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 攻撃力상승値
#DIM DYNAMIC 조교대상
#DIM DYNAMIC レイプ횟수



CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39650)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39650)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 고블린 기습부대
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		CALL CARD_EROTICS(GETVAR(@"%対面者%_直前戦闘破壊"))
		IF RESULT != -1
			조교대상 = RESULT
			IF ゾーン == "묘지"
				PRINTFORMW 쓰러져 버린 %CALLNAME:조교대상% 에 어딘지 모르게 나타난 고블린들이몰려왔다……。
			ELSE
				PRINTFORMW 쓰러져 버린 %CALLNAME:조교대상%에 고블린들이 몰려왔다……。
			ENDIF
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件カテゴリ = 고블린
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			レイプ횟수 = RESULT
			FOR カウンタ,0,レイプ횟수
				CALL EROTIC_MOVE("両穴凌辱",조교대상)
			NEXT			
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39650)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 고블린 기습부대
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		CALL CARD_EROTICS(GETVAR(@"%対面者%_直前戦闘破壊"))
		IF RESULT != -1
			조교대상 = RESULT
			IF ゾーン == "묘지"
				PRINTFORMW 쓰러져 버린 %CALLNAME:조교대상%어딘지 모르게 나타난 고블린들이몰려왔다……。……。
			ELSE
				PRINTFORMW 쓰러져 버린 %CALLNAME:조교대상%에 고블린들이 몰려왔다……。
			ENDIF
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件カテゴリ = 고블린
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			レイプ횟수 = RESULT
			FOR カウンタ,0,レイプ횟수
				CALL EROTIC_MOVE("両穴凌辱",조교대상)
			NEXT			
		ENDIF
	ENDIF
ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39650)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲손패 = 1
		효과条件カテゴリ = 고블린
		효과条件固名前 = 고블린 기습부대
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과条件성별 = 牝性
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과条件성별 = 牝性
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
				選択カード = RESULT
				IF 選択カード != 9999
					CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永攻",-1000,0,1)
					CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永守",-1000,0,1)
					CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",選択カード,1)
					IF RESULT != -1
						조교대상 = RESULT
						PRINTFORMW 고블린들은 파괴되기전 발기한 분노를 %CALLNAME:조교대상%억지로 물게 하거나,
						PRINTFORMW 하얗고 끈적끈적한 액체를 뿌려 , %CALLNAME:조교대상%에 정신을 피폐시키면서 마모시킵니다.。
						PRINTFORMW %CALLNAME:조교대상%가 조교의 영향을 받았습니다.。
						CALL EROTIC_MOVE("ぶっかけ",조교대상)
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF



