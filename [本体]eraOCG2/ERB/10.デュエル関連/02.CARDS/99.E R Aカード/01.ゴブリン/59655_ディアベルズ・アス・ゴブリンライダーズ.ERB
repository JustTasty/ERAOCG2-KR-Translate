PRINTL 덱에서 "죄보" 마법 / 함정 카드 １장을 묘지로 보내고、이 카드의 Ｘ소재를 １개 늘린다。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_59655(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59655)
SIF RESULT == 1
	RETURN 0


IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,52101)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,52101)
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件固カテゴリ = 고블린
	효과条件カテゴリ = 디아벨
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,59655)
		RETURN 0
	CALL FULLRESET
	효과条件성별 = 牝性
	CALL CARD_CHECK_START_DATA(듀얼리스트,2)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_3(듀얼리스트,59655)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 죄보
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_59655(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 取込カードID
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59655,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件固カテゴリ = 고블린
		효과条件カテゴリ = 디아벨
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			; IF 選択カード_ID >= 10000　BAD_ASS_EROTEXTに이동
			; 	TRYCALLFORM CARD_{選択カード_ID}("連動ID")
			; 	IF GROUPMATCH(RESULT,1920,1921,1934)
			; 		選択カード_ID = 選択カード_ID
			; 	ELSE
			; 		選択カード_ID = 0
			; 	ENDIF
			; ENDIF
			; IF 選択カード_ID != 0
				CALL FULLRESET
				효과추가종류:0 = 効追Ｘ素
				효과추가종류:1 = 効追変化ID + 選択カード_ID
				CALL CARD_EX_SUMMON(듀얼리스트, "EX덱", 場所)
				CALL BAD_ASS_EROTEXT(選択カード_ID)
			; ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59655,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"リリース")
		SIF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",1)
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59655,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 죄보
		효과条件종류 = 마함全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",1)

		選択カード_ID = GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所")
		CALL BAD_ASS_EROTEXT(選択カード_ID,1)
	ENDIF

ELSEIF 종류 == リアクション효과
	選択カード_ID = GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所")
	CALL BAD_ASS_EROTEXT(選択カード_ID,2)
ENDIF





@BAD_ASS_EROTEXT(カード_ID,설정분기)
#DIM DYNAMIC カード_ID
#DIM DYNAMIC 설정분기
#DIM DYNAMIC 조교대상

IF カード_ID >= 10000
	TRYCALLFORM CARD_{カード_ID}("連動ID")
	IF GROUPMATCH(RESULT,1920,1921,1934)
		カード_ID = RESULT
	ENDIF
ENDIF
CALL CARD_EROTICS(カード_ID)
IF RESULT != -1
	조교대상 = RESULT
	PRINTFORML 
	IF 설정분기 == 0 ;重ねてX소환
		SELECTCASE カード_ID
		CASE 1920
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「なっ！？　てめっっ何おっオオッッ❤！？！？　がはっ！？　こんのっっ고블린どもっ、さわんじゃっ――❤」
			CASE 1
				PRINTFORML 「てめぇっっふざけっ、あぐっうあっ……❤　お゛おっっ❤」
			CASE 2
				PRINTFORML 「はーっ、はーっ……ちっ、あんまりちょうしにのっ、うあぁッ❤！？　くそっ、胸にしがみつくなァあああっ❤！」
			ENDSELECT
		CASE 1921
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「汚らわしい、触るんじゃないよッ！　ちっ、数だけ多い雑魚が――ぴっ！？　やっ、やめなさっひゃああっ❤！？」
			CASE 1
				PRINTFORML 「はーっ、くっ、私を乗り物に？　下衆で下等な生物らしいっ！！　ゴミがっ図に乗るな！　お前達なんて簡単にっおほぉおっ❤！？」
			CASE 2
				PRINTFORML 「宝っ？　はぁて何のことやら、リゼわかんな――ッんひっ❤！？　ちょっ、胸にしがみつくなっ！？　そこはよわっにゃっ、はあっ❤！？」
			ENDSELECT
		CASE 1934
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「―――っ、あの子たちはどこ――ッうっ……っ❤ッふっ……❤」
			CASE 1
				PRINTFORML 「事を荒立てる気は、ありません――はっあっ、ぐっ❤　戯れには付き合いましょう、だから彼女達には――あっひっ❤」
			CASE 2
				PRINTFORML 「――乳房を、そのように扱って、如何様に……ひっ、すっごいっ――ああっ❤　そのような目で、見ないで……」
			ENDSELECT
		ENDSELECT
		CALL EROTIC_MOVE("구속晒し",조교대상,-1,200)
		WAIT
	ELSEIF 설정분기 == 1 ;ターン終了時효과
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML 「てめっ、それ죄보、やめろっ！？　うぁっおっ―――ほぉおお❤！？　どこにっいれてんぢゃ……ひぎっいぎゅうっ❤！？」
		CASE 1921
			PRINTFORML 「殺す殺す殺すっッ、お前たち私の죄보に触ったら、殺ッおひっっひいいいっぴぃいい❤！？」
		CASE 1934
			PRINTFORML 「やめなさい、それは……そのように使うものでは――ああァっぎいいぃい❤！？！？　죄보にこんなっ使い方がっ……❤」
		ENDSELECT
		CALL EROTIC_MOVE("Ａ単発",조교대상,-1,200)
		WAIT
	ELSEIF 설정분기 == 2 ;リアクション효과
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML 「ふーっっふーっ……💢　ふーっっ……💢　ふっーー……❤」
		CASE 1921
			PRINTFORML 「アステーリャ……助けて……」
		CASE 1934
			PRINTFORML 「見ないでっ……こんな情けない姿、見ないで、くださいッ❤」
		ENDSELECT
		CALL EROTIC_MOVE("구속晒し",조교대상,-1,50)
		WAIT
	ELSEIF 설정분기 == 3 ;죄보綱装備
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML 「それで何をする気だ！やめろ、はなせ！」
		CASE 1921
			PRINTFORML 「何よそれ、私をお馬さんにしようって事？……やめなさい、放して！」
		CASE 1934
			PRINTFORML 「……分かりました。저항しませんので、お手柔らかにお願いします……❤」
		ENDSELECT
	ENDIF
ENDIF


