@CARDNAME_59655,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 디아벨즈 어스 고블린라이더즈
	CASE "略称"
		RESULTS = 이고령명백귀나찰
	CASE "カテゴリ"
		RESULTS:0 = 고블린
		RESULTS:1 = 고블린라이더
		RESULTS:2 = 디아벨
		RESULTS:3 = E.R.A
ENDSELECT


@CARD_59655(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN エクシーズ
	CASE "属性"
		RETURN 闇属性
	CASE "種族"
		RETURN 戦士族
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "特殊不可"
		RETURN 1
	CASE "性別"
		RETURN 牝性
	CASE "追加日"
		RETURN 250809
ENDSELECT

RETURN 0

