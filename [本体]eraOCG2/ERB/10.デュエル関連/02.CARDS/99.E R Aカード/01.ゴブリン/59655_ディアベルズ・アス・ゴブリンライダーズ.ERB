@CARD_EXPLANATION_59655(種類)
#DIM DYNAMIC 種類

CALL TEXT_DECORATION("ROGUE")
PRINTL 레벨 ７몬스터×２
CALLFORM CARD_ABLE(1,EXデッキ起動)
PRINTL "디아벨즈 어스 고블린라이더즈"는 １턴에 １번、
PRINTL 자신 / 상대 필드에 존재하는 "고블린" 이외의 "디아벨" 몬스터 위에 겹쳐서 X소환할수도 있다。
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ①：암컷 몬스터가 효과를 발동했을 경우,
PRINTL 상대 필드의 앞면표시 몬스터 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 몬스터를 묘지로 보내고、이 카드의 Ｘ소재를 １개 늘린다。
CALLFORM CARD_ABLE(3,ターン終了時)
PRINTL ②：서로의 엔드 페이즈에 발동할 수 있다。
PRINTL 덱에서 "죄보" 마법 / 함정 카드 １장을 묘지로 보내고、이 카드의 Ｘ소재를 １개 늘린다。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_59655(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,59655)
SIF RESULT == 1
	RETURN 0


IF 種類 == EXデッキ起動
	SIF 発動済_1(決闘者,52101)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	CALL CARD_EX_CHECK(決闘者,52101)
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	効果範囲戦場 = 1
	効果範囲相手戦場 = 1
	効果条件固カテゴリ = 고블린
	効果条件カテゴリ = 디아벨
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 種類 == 発動誘発
	SIF ゾーン != "戦場"
		RETURN 0
	SIF 発動済_2(決闘者,59655)
		RETURN 0
	CALL FULLRESET
	効果条件性別 = 牝性
	CALL CARD_CHECK_START_DATA(決闘者,2)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	効果範囲相手戦場 = 1
	効果条件表示 = 表側表示
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == ターン終了時
	SIF 発動済_3(決闘者,59655)
		RETURN 0
	SIF ゾーン != "戦場"
		RETURN 0
	CALL FULLRESET
	効果範囲デッキ = 1
	効果条件カテゴリ = 죄보
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == リアクション効果
	SIF ゾーン != "戦場"
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_59655(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 取込カードID
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
CALL 対面者判定(決闘者)
対面者 = %RESULTS%



IF 種類 == EXデッキ起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,59655,1)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲戦場 = 1
		効果範囲相手戦場 = 1
		効果条件固カテゴリ = 고블린
		効果条件カテゴリ = 디아벨
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			; IF 選択カード_ID >= 10000　BAD_ASS_EROTEXTに移動
			; 	TRYCALLFORM CARD_{選択カード_ID}("連動ID")
			; 	IF GROUPMATCH(RESULT,1920,1921,1934)
			; 		選択カード_ID = 選択カード_ID
			; 	ELSE
			; 		選択カード_ID = 0
			; 	ENDIF
			; ENDIF
			; IF 選択カード_ID != 0
				CALL FULLRESET
				効果追加種類:0 = 効追Ｘ素
				効果追加種類:1 = 効追変化ID + 選択カード_ID
				CALL CARD_EX_SUMMON(決闘者, "EXデッキ", 場所)
				CALL BAD_ASS_EROTEXT(選択カード_ID)
			; ENDIF
		ENDIF
	ENDIF
ELSEIF 種類 == 発動誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,59655,2)
	IF RESULT == 0
		CALL FULLRESET
		効果条件表示 = 表側表示
		CALL CARD_REMOVAL_BASIC(決闘者,"戦場単体",場所,"リリース")
		SIF RESULT == 1
			CALL SET_FIELD_INFO(決闘者,"戦場",場所,"Ｘ",1)
	ENDIF
ELSEIF 種類 == ターン終了時
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,59655,3)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲デッキ = 1
		効果条件カテゴリ = 죄보
		効果条件種類 = 魔罠全般
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		SIF RESULT == 1
			CALL SET_FIELD_INFO(決闘者,"戦場",場所,"Ｘ",1)

		選択カード_ID = GETVAR(@"%決闘者%_MZ_変化元ID:場所")
		CALL BAD_ASS_EROTEXT(選択カード_ID,1)
	ENDIF

ELSEIF 種類 == リアクション効果
	選択カード_ID = GETVAR(@"%決闘者%_MZ_変化元ID:場所")
	CALL BAD_ASS_EROTEXT(選択カード_ID,2)
ENDIF





@BAD_ASS_EROTEXT(カード_ID,설정분기)
#DIM DYNAMIC カード_ID
#DIM DYNAMIC 설정분기
#DIM DYNAMIC 조교대상

IF カード_ID >= 10000
	TRYCALLFORM CARD_{カード_ID}("連動ID")
	IF GROUPMATCH(RESULT,1920,1921,1934)
		カード_ID = RESULT
	ENDIF
ENDIF
CALL CARD_EROTICS(カード_ID)
IF RESULT != -1
	조교대상 = RESULT
	PRINTFORML 
	IF 설정분기 == 0 ;重ねてX召喚
		SELECTCASE カード_ID
		CASE 1920
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「なっ！？　てめっっ何おっオオッッ❤！？！？　がはっ！？　こんのっっ고블린どもっ、さわんじゃっ――❤」
			CASE 1
				PRINTFORML 「てめぇっっふざけっ、あぐっうあっ……❤　お゛おっっ❤」
			CASE 2
				PRINTFORML 「はーっ、はーっ……ちっ、あんまりちょうしにのっ、うあぁッ❤！？　くそっ、胸にしがみつくなァあああっ❤！」
			ENDSELECT
		CASE 1921
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「汚らわしい、触るんじゃないよッ！　ちっ、数だけ多い雑魚が――ぴっ！？　やっ、やめなさっひゃああっ❤！？」
			CASE 1
				PRINTFORML 「はーっ、くっ、私を乗り物に？　下衆で下等な生物らしいっ！！　ゴミがっ図に乗るな！　お前達なんて簡単にっおほぉおっ❤！？」
			CASE 2
				PRINTFORML 「宝っ？　はぁて何のことやら、リゼわかんな――ッんひっ❤！？　ちょっ、胸にしがみつくなっ！？　そこはよわっにゃっ、はあっ❤！？」
			ENDSELECT
		CASE 1934
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「―――っ、あの子たちはどこ――ッうっ……っ❤ッふっ……❤」
			CASE 1
				PRINTFORML 「事を荒立てる気は、ありません――はっあっ、ぐっ❤　戯れには付き合いましょう、だから彼女達には――あっひっ❤」
			CASE 2
				PRINTFORML 「――乳房を、そのように扱って、如何様に……ひっ、すっごいっ――ああっ❤　そのような目で、見ないで……」
			ENDSELECT
		ENDSELECT
		CALL EROTIC_MOVE("구속晒し",조교대상,-1,200)
		WAIT
	ELSEIF 설정분기 == 1 ;ターン終了時効果
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML 「てめっ、それ죄보、やめろっ！？　うぁっおっ―――ほぉおお❤！？　どこにっいれてんぢゃ……ひぎっいぎゅうっ❤！？」
		CASE 1921
			PRINTFORML 「殺す殺す殺すっッ、お前たち私の죄보に触ったら、殺ッおひっっひいいいっぴぃいい❤！？」
		CASE 1934
			PRINTFORML 「やめなさい、それは……そのように使うものでは――ああァっぎいいぃい❤！？！？　죄보にこんなっ使い方がっ……❤」
		ENDSELECT
		CALL EROTIC_MOVE("Ａ単発",조교대상,-1,200)
		WAIT
	ELSEIF 설정분기 == 2 ;リアクション効果
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML 「ふーっっふーっ……💢　ふーっっ……💢　ふっーー……❤」
		CASE 1921
			PRINTFORML 「アステーリャ……助けて……」
		CASE 1934
			PRINTFORML 「見ないでっ……こんな情けない姿、見ないで、くださいッ❤」
		ENDSELECT
		CALL EROTIC_MOVE("구속晒し",조교대상,-1,50)
		WAIT
	ELSEIF 설정분기 == 3 ;죄보綱装備
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML 「それで何をする気だ！やめろ、はなせ！」
		CASE 1921
			PRINTFORML 「何よそれ、私をお馬さんにしようって事？……やめなさい、放して！」
		CASE 1934
			PRINTFORML 「……分かりました。抵抗しませんので、お手柔らかにお願いします……❤」
		ENDSELECT
	ENDIF
ENDIF


