@CARDNAME_59655,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 디아벨즈 어스 고블린라이더즈
	CASE "略称"
		RESULTS = 이고령명백귀나찰
	CASE "カテゴリ"
		RESULTS:0 = 고블린
		RESULTS:1 = 고블린라이더
		RESULTS:2 = 디아벨
		RESULTS:3 = E.R.A
ENDSELECT


@CARD_59655(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "특수불가"
		RETURN 1
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250809
ENDSELECT

RETURN 0

@CARDSUMMON_59655(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
