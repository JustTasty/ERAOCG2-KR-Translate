CALL TEXT_DECORATION("ROGUE")


@CARDCAN_59655(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59655)
SIF RESULT == 1
	RETURN 0


IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,52101)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,52101)
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件固カテゴリ = 고블린
	효과条件カテゴリ = 디아벨
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,59655)
		RETURN 0
	CALL FULLRESET
	효과条件성별 = 牝性
	CALL CARD_CHECK_START_DATA(듀얼리스트,2)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_3(듀얼리스트,59655)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 죄보
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_59655(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 取込カードID
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59655,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件固カテゴリ = 고블린
		효과条件カテゴリ = 디아벨
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			; IF 選択カード_ID >= 10000　BAD_ASS_EROTEXTに이동
			; 	TRYCALLFORM CARD_{選択カード_ID}("連動ID")
			; 	IF GROUPMATCH(RESULT,1920,1921,1934)
			; 		選択カード_ID = 選択カード_ID
			; 	ELSE
			; 		選択カード_ID = 0
			; 	ENDIF
			; ENDIF
			; IF 選択カード_ID != 0
				CALL FULLRESET
				효과추가종류:0 = 効追Ｘ素
				효과추가종류:1 = 効追変化ID + 選択カード_ID
				CALL CARD_EX_SUMMON(듀얼리스트, "EX덱", 場所)
				CALL BAD_ASS_EROTEXT(選択カード_ID)
			; ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59655,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"リリース")
		SIF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",1)
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59655,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 죄보
		효과条件종류 = 마함全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",1)

		選択カード_ID = GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所")
		CALL BAD_ASS_EROTEXT(選択カード_ID,1)
	ENDIF

ELSEIF 종류 == リアクション효과
	選択カード_ID = GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所")
	CALL BAD_ASS_EROTEXT(選択カード_ID,2)
ENDIF





@BAD_ASS_EROTEXT(カード_ID,설정분기)
#DIM DYNAMIC カード_ID
#DIM DYNAMIC 설정분기
#DIM DYNAMIC 조교대상

IF カード_ID >= 10000
	TRYCALLFORM CARD_{カード_ID}("連動ID")
	IF GROUPMATCH(RESULT,1920,1921,1934)
		カード_ID = RESULT
	ENDIF
ENDIF
CALL CARD_EROTICS(カード_ID)
IF RESULT != -1
	조교대상 = RESULT
	PRINTFORML 
	IF 설정분기 == 0 ;重ねてX소환
		SELECTCASE カード_ID
		CASE 1920
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML "뭣!? 네 이 자식 뭐하는 오옷❤!?!? 컥!? 이 고블린 자식들이, 만지지ㅁ――❤"
			CASE 1
				PRINTFORML "네 이 자식, 장난치지, 아굿 우앗……❤ 오옷❤"
			CASE 2
				PRINTFORML "하아, 하아……쳇, 너무 우쭐대지, 우아앗❤!? 젠장, 가슴에 매달리지 마아아앗❤!"
			ENDSELECT
		CASE 1921
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML "더러워, 만지지 마! 쳇, 숫자만 많은 잡것들이――삑!? 얏, 그만둬, 햐아앗❤!?"
			CASE 1
				PRINTFORML "하아, 큿, 나를 탈것으로? 천박하고 하등한 생물답군!! 쓰레기가 기어오르지 마! 너희들 따위 간단히, 오호옷❤!?"
			CASE 2
				PRINTFORML "보물? 글쎄 무슨 소린지, 리제는 모르겠는――읏흣❤!? 잠깐, 가슴에 매달리지 마!? 거긴 약하단, 햐앗❤!?"
			ENDSELECT
		CASE 1934
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML "―――읏, 그 아이들은 어디에――읏……❤흣……❤"
			CASE 1
				PRINTFORML "일을 크게 만들 생각은, 없습니다――핫앗, 굿❤ 장난에는 어울려드리죠, 그러니 그녀들에게는――앗흣❤"
			CASE 2
				PRINTFORML "――유방을, 그렇게 다루어서, 어떻게……힉, 굉장해――아앗❤ 그런 눈으로, 보지 마세요……"
			ENDSELECT
		ENDSELECT
		CALL EROTIC_MOVE("구속晒し",조교대상,-1,200)
		WAIT
	ELSEIF 설정분기 == 1 ;ターン終了時효과
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML "네놈, 그거 죄보, 그만둬!? 우왓옷―――호오오❤!? 어디에 넣는 거야……히긱이규웃❤!?"
		CASE 1921
			PRINTFORML "죽인다죽인다죽인다, 너희들 내 죄보에 손대면, 죽인ㄷ오힛히이이잇삐이이❤!?"
		CASE 1934
			PRINTFORML "그만두세요, 그건……그렇게 쓰는 게 아니――아아앗기이이잇❤!?!? 죄보에 이런 사용법이……❤"
		ENDSELECT
		CALL EROTIC_MOVE("Ａ単発",조교대상,-1,200)
		WAIT
	ELSEIF 설정분기 == 2 ;リアクション효과
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML "후ー, 후ー……💢 후ー……💢 훗ーー……❤"
		CASE 1921
			PRINTFORML "아스테랴……살려줘……"
		CASE 1934
			PRINTFORML "보지 마……이런 한심한 모습, 보지 말아, 주세요❤"
		ENDSELECT
		CALL EROTIC_MOVE("구속晒し",조교대상,-1,50)
		WAIT
	ELSEIF 설정분기 == 3 ;죄보綱装備
		SELECTCASE カード_ID
		CASE 1920
			PRINTFORML "그걸로 뭘 할 셈이야! 그만둬, 놔!"
		CASE 1921
			PRINTFORML "뭐야 그게, 나를 말로 삼겠다는 거야?……그만둬, 놔줘!"
		CASE 1934
			PRINTFORML "……알겠습니다. 저항하지 않을 테니, 살살 부탁드립니다……❤"
		ENDSELECT
	ENDIF
ENDIF


