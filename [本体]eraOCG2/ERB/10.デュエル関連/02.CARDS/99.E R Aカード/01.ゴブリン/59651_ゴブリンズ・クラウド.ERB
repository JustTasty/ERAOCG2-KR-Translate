@CARDNAME_59651,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 고블린 무리들
	CASE "略称"
		RESULTS = 고블린 무리
	CASE "カテゴリ"
		RESULTS:0 = 고블린
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59651(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 2400
	CASE "성별"
		RETURN 雄性
ENDSELECT


RETURN 0


@CARD_EXPLANATION_59651(종류)
#DIM DYNAMIC 종류

PRINTL "고블린 알파" 포함한 몬스터 1장 이상
PRINTL 이 카드명의  ①③의 효과는 각각 1턴에 1번 밖에 사용할 수 없다。
CALL CARD_ABLE(1,필드起動)
PRINTL ①：자신의 메인 페이즈에 발동할 수 있다。
PRINTL 묘지에서 레벨 4 이하의 "고블린」 몬스터 2장을 특수 소환한다。
CALL CARD_ABLE(0,지속효과)
PRINTL ②：자신 필드에 이 카드 이외에 "고블린 」 몬스터가 존재하는 한、
PRINTL 상대는 이카드를 공격 / 효과 의 대상으로 할 수 없다。
CALL CARD_ABLE(2,ターン終了時)
PRINTL ③：자신 / 상대 엔드 페이즈에 발동 할 수 있다。
PRINTL 상대 필드 / 묘지 에 암컷 몬스터를 1장 선택해, 이 카드에 장착한다。
PRINTL 이 카드의 공격력 / 수비력은 장비한 몬스터의 공격력 / 수비력 만큼 증가하며,
PRINTL 이 카드가 파괴될 경우, 장비한 몬스터를 대신 파괴한다。

@CARDSUMMON_59651(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 3


IF EX参照 == 0 || EX参照 == 1
	素材条件名前 = 고블린 알파
ENDIF


IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,59651) 
	RETURN RESULT
ENDIF



@CARDCAN_59651(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59651)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,59651)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 4
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 고블린
	효과条件소환可 = 1
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件他所 = 1
	효과条件カテゴリ = 고블린
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_2(듀얼리스트,59651)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대묘지 = 1
	효과条件성별 = 牝性
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59651(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先
#DIM DYNAMIC 無効カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59651,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 고블린
		효과条件종류 = レベル持ち
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 고블린
			효과条件종류 = レベル持ち
			CALL 효과条件レベル_ADV(4, "以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"攻避")
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"対耐")
ELSEIF 종류 == リアクション효과
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_마함:カウンタ") == -1
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_装:カウンタ") != 場所
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_リ数:カウンタ") >= 10
			CONTINUE
			
		CALL CARD_EROTICS_FIELD(듀얼리스트,"마함",カウンタ)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML 
			PRINTFORMW 붙잡힌 %CALLNAME:조교대상%은 고블린 무리가 돌아가며 게속해서 범해지고 있다。
			CALL EROTIC_MOVE("全身凌辱",조교대상,-1,300)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",カウンタ,"リ数",1)
		ENDIF
	NEXT
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59651,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		효과条件종류 = モン全般
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			CALL CARD_EQUIP_MONSTER(듀얼리스트,場所,本参照先（CS）,選択カード,(3 + 12))
		ENDIF
	ENDIF
ENDIF



