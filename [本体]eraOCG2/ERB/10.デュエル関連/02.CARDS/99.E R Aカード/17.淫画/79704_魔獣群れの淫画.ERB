@CARDNAME_79704,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 마수 무리의 음화
	CASE "略称"
		RESULTS = 마수 무리 음화
	CASE "カテゴリ"
		RESULTS:0 = 음화
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79704(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "分割"
		RETURN 효과モン
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79704(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：패 / 마법 & 함정 존에서 발동한 이 카드는 ②의 효과를 가진 효과 몬스터가 되어 몬스터 존에 특수 소환한다.
PRINTL (야수족 어둠 레벨8 공/수0)
PRINTL 그 후, 자신 / 상대 묘지의 암컷 몬스터 1장을 이 카드에 장착한다.
PRINTL 이 카드의 공격력은 장착 몬스터의 공격력만큼 오르고, 이 카드는 장착 몬스터와 같은 이름의 카드로 취급한다.
CALL CARD_ABLE(2,ターン終了時)
PRINTL ②：상대 엔드 페이즈에 발동할 수 있다.
PRINTL 자신 / 상대 묘지의 암컷 몬스터 1장을 이 카드에 장착하고, 이 카드의 공격력은 장착 몬스터의 공격력만큼 올린다.
CALL CARD_ABLE(3,지속효과)
PRINTL ③： 카드가 파괴될 경우, 대신 이 카드의 효과로 장착한 몬스터를 묘지로 보낸다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79704(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79704)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79704)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(듀얼리스트,79704,ゾーン)
	SIF RESULT == 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_EQUIP_CHECK(듀얼리스트,場所,ゾーン)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79704(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 合計攻撃力
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79704,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		選択カード_ID = RESULT:99
		IF RESULT != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			CALL CARD_EQUIP_MONSTER(듀얼리스트,カード소환先,本参照先（CS）,選択カード,(1+4))
			CALL CARD_EROTICS(選択カード_ID)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORMW %CALLNAME:조교대상%은 마수가 창궐한 그림 속으로 끌려갔다！
				CALL EROTIC_MOVE("구속晒し",조교대상,-1,300)
			ELSE
				TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
				PRINTFORMW %RESULTS%은 마수가 창궐한 그림 속으로 끌려갔다！
			ENDIF
			CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"変化元ID",選択カード_ID,上書きモード)
		ENDIF
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79704,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		選択カード_ID = RESULT:99
		IF RESULT != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			CALL CARD_EQUIP_MONSTER(듀얼리스트,場所,本参照先（CS）,選択カード,(1+4))
			CALL CARD_EROTICS(選択カード_ID)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORMW %CALLNAME:조교대상%은 마수가 창궐한 그림 속으로 끌려갔다！
				CALL EROTIC_MOVE("구속晒し",조교대상,-1,300)
			ELSE
				TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
				PRINTFORMW %RESULTS%은 마수가 창궐한 그림 속으로 끌려갔다！
			ENDIF
		ENDIF
	ENDIF
ENDIF



