@CARDNAME_39605,参照先
;ココ39605で지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 패러사이트 어드밴스
	CASE "略称"
		RESULTS = 패러사이트 Adv
	CASE "カテゴリ"
		RESULTS:0 = 패러사이트
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39605(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS


SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 300
	CASE "추가日"
		RETURN 250429
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39605(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ②③의 효과는 1턴에 1번만 사용할 수 있다.
CALL CARD_ABLE(0,지속효과)
PRINTL ①이 카드는 융합 / S X L 소환의 소재로 할 수 없다.
CALL CARD_ABLE(1,손패起動,묘지起動)
PRINTL ②：이 카드가 패 / 묘지에 존재할 경우, 자신의 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.
CALL CARD_ABLE(2,필드起動)
PRINTL ③：서로의 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드 1장만을 릴리스하고, 레벨 5 이상의 암컷 몬스터 1장을 어드밴스 소환한다.
PRINTL 그 후, 소환한 몬스터에 이 카드를 장착한다.
PRINTL ④：이 카드를 장착한 몬스터가 전투 / 효과로 파괴될 경우, 대신 이 카드를 파괴한다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39605(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39605)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,39605)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39605)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,39605)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	CALLFORM 효과条件レベル_ADV(5,"以上")
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == リアクション효과
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_39605(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 装備先


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == 묘지起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39605,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, ゾーン, 場所)
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39605,2)
	IF RESULT == 0
		CALL FULLRESET
		CALLFORM 효과条件レベル_ADV(5,"以上")
		효과条件성별 = 牝性
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT != 9999
			選択カード = RESULT
			選択カード_ID = RESULT:99
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			CALL CARD_SUMMON_SHORI(듀얼리스트,選択カード,"攻撃표시",1)
			CALL FULLRESET
			효과条件名前 = 패러사이트 어드밴스
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
			選択カードB = RESULT
			IF 選択カードB != 9999
				CALL CARD_EQUIP_MONSTER(듀얼리스트,カード소환先,"묘지",選択カードB,(4))
				CALL CARD_EROTICS(選択カード_ID)
				IF RESULT != -1
					조교대상 = RESULT
						SETCOLOR C_PINK
					PRINTFORML
					PRINTFORMW 패러사이트 어드밴스는 %CALLNAME:조교대상%의 몸에 기생했다！
					PRINTFORML
						RESETCOLOR
					CALL EROTIC_MOVE("亀甲縛り",조교대상,-1,200)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == リアクション효과
	装備先 = GETVAR(@"%듀얼리스트%_魔_装:場所")
	IF GETVAR(@"%듀얼리스트%_MZ_リ数:装備先") < 10
		CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",装備先)
		IF RESULT != -1
			조교대상 = RESULT
				SETCOLOR C_PINK
			PRINTFORML
			PRINTFORML 패러사이트 어드밴스가 %CALLNAME:조교대상%의 몸을 기어다니고 있다
			PRINTFORML
				RESETCOLOR
			CALL EROTIC_MOVE("全身애무",조교대상,-1,100)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",装備先,"リ数",1)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"不材")
ENDIF
