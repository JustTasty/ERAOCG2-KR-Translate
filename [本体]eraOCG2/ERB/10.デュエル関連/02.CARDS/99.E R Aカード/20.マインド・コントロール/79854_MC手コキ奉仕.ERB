@CARDNAME_79854,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 마인드컨트롤－수음봉사
	CASE "略称"
		RESULTS = MC 수음봉사
	CASE "カテゴリ"
		RESULTS:0 = MC
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79854(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79854(종류)
#DIM DYNAMIC 종류
CALL TEXT_DECORATION("ROGUE")
PRINTL ①：상대 플레이어의 자지에 손으로 봉사한다.
PRINTL 
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79854(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79854)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_79854(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 視聴料
#DIM DYNAMIC 최면강도

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79854)
	IF RESULT == 0
		CALL 対面者판정(듀얼리스트)
		対面者 = %RESULTS%
		CALL CARD_EROTICS(상대プレイヤー,対面者)
		IF RESULT != -1
			조교대상 = RESULT
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件名前 = 마인드컨트롤－수음봉사
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			최면강도 = ((ABL:(조교대상):봉사정신) + RESULT + 1) * 100
			PRINTFORML 
			SETCOLOR C_PINK 
			IF  TALENT:조교대상:성별 != 2
				PRINTFORMW %NAME:조교대상%는 컨트롤러에 조종되는 대로, MC 카드를 사용해 버린다......
				PRINTFORMW 카드 효과에 따라, %NAME:조교대상%는 %CALLNAME:MASTER%에게로 가며, 자신의 손으로 %CALLNAME:MASTER%의 자지를 훑기 시작했다......
				CALL EROTIC_MOVE("최면수음",조교대상,MASTER,200)
				IF ABL:(조교대상):봉사정신 == 3
					PRINTFORMW ……%NAME:조교대상%는 손으로 부터 전해지는 %CALLNAME:MASTER%의 자지의 감촉에 불평하면서도 교묘하게 훑고 있다......
				ELSEIF ABL:(조교대상):봉사정신 == 4
					PRINTFORMW ……%NAME:조교대상%는 손으로 부터 전해지는 %CALLNAME:MASTER%의 자지의 감촉에, 기분이 이상해♥ 라고 불평하면서도 비밀스런 장소로 부터 꿀을 흘리고 있다......
				ELSEIF ABL:(조교대상):봉사정신 >= 5
					PRINTFORMW ……%NAME:조교대상%는 손으로 부터 전해지는 %CALLNAME:MASTER%의 자지의 감촉만으로 몹시 흥분하고 있으며 놓치지 않게 자지를 꽈악 잡고 있다......
					IF TALENT:조교대상:음란 >= 1
						PRINTFORMW %CALLNAME:MASTER%의  자지의 감촉에 인내할 수 없게 되었는지, %NAME:조교대상%는 %CALLNAME:MASTER%의 자지를 목 안 까지 물었다!
						PRINTFORMW  효과는 없을 것 같지만, 그래도 말해보고있는 %CALLNAME:MASTER%의 소리는, 염원하던  자지 앞에서『암캐』가 되어있는 %NAME:조교대상%에게는 닿지 않는 것 같다......
						CALL EROTIC_MOVE("최면自発スロート",조교대상,MASTER,최면강도)
					ELSE
							PRINTFORMW %CALLNAME:MASTER%의 자지의 감촉에 인내할 수 없게 되었는지, %NAME:조교대상%는 %CALLNAME:MASTER%의 앞에서 멋대로 사타구니를 만지작 거리기 시작해 버렸다.
							PRINTFORMW 효과는 없을 것 같지만, 그래도 말해보고 있는 %CALLNAME:MASTER%의 소리는, 눈 앞의 자지%TEXTR("에 범해지는 망상으로/를 딸감으로/가 어느 정도의 쾌락을 줄지에 대해")% 머리에 가득 찬 상태에서 손끝으로 사소한 쾌락에 열중하고 있는  %NAME:조교대상%에게 닿지 않는 것 같다......
							CALL EROTIC_MOVE("최면自発자위",조교대상,MASTER,최면강도)
					ENDIF
				ELSE
					PRINTFORMW …… %CALLNAME:MASTER%의 사타구니를 훑고 끝낸 %NAME:조교대상%는, 듀얼 재개할 수 있도록 일어서서 원래 장소로 돌아가고 있었다......
				ENDIF
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件名前 = 마인드컨트롤－수음봉사
				효과条件CNT = 1
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT >= 3
					PRINTFORMW 
					PRINTFORMW 몇 번이나 %CALLNAME:MASTER%의 성기에 봉사한 것으로 %NAME:조교대상%는 %CALLNAME:MASTER%에게 봉사하는 것에 중독이 되어버렸다......
					PRINTFORMW %NAME:조교대상%의『봉사정신』이 1 올랐다!		
					PRINTFORMW %NAME:조교대상%의『손가락 기술』이 1 올랐다!	
					ABL:조교대상:봉사정신 += 1
					ABL:조교대상:손가락 += 1	
				ENDIF
				CALL FULLRESET
				효과範囲상대마함 = 1
				효과条件名前 = AliVe♡Twin 채널
				효과条件CNT = 1
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT >= 1
					視聴料 = 최면강도
					SETCOLOR C_RED 
					PRINTFORMW 
					PRINTFORMW ●AliVe♡Twin
					SETCOLOR C_PINK 
					PRINTFORMW 
					PRINTFORMW %NAME:조교대상%はライブカメラにさきほどの痴態が撮影されていた……
					PRINTFORM 『%NAME:조교대상%は%TEXTR("ご主人様の/듀얼中に")%
					PRINTFORM %TEXTR("수음ご奉仕をする/ちんぽに夢中の/えっちでひみつの")%
					PRINTFORM %TEXTR("淫乱メイド♥/奉仕教育♥")%』がAliVe♡Twinチャンネルで公開された！
					PRINTFORMW 
					CALL EROTIC_MOVE("노출単発",조교대상,MASTER,100)
					PRINTFORMW %CALLNAME:MASTER%の口座にお金が振り込まれた。
					CALL MONEY_GET(視聴料)
					PRINTFORMW 
				ENDIF
			ENDIF
			RESETCOLOR
		ENDIF
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
