@CARDSUMMON_39909(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件素材ID起動 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 광서의 기계
ELSEIF EX参照 >= 10
    素材条件성별 = 牝性
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,39909) 
	RETURN RESULT
ENDIF


@CARDCAN_39909(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39909)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
    효과範囲필드 = 1
	효과条件종족 = 기계족
	효과条件성별 = 牝性
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 1
		RETURN 0
	CALL FULLRESET
    효과範囲필드 = 1
	효과条件カテゴリ = 광서의 기계
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == EX소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,39909)
		RETURN 0
	CALL FULLRESET
	효과範囲상대덱 = 1
	효과範囲상대EX덱 = 1
    효과範囲상대묘지 = 1
    효과範囲상대손패 = 1
	효과条件特不無視 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(対面者,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,39909)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	FOR カウンタ,1,13
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류 = モン全般
		효과条件성별 = 牝性
		효과条件レベル = カウンタ
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			CONTINUE
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件レベル = カウンタ + 2
	    효과条件カテゴリ = 광서의 기계
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성 = 1
	NEXT
	SIF 条件달성 != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF 발동済_3(듀얼리스트,39909)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 0
		RETURN 0

	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 광서의 기계
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 3
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF	GETVAR(@"%듀얼리스트%_MZ_汎数:場所") > 0
		RETURN 1
	SIF	GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所") >= 0
		RETURN 1
	RETURN 1
ENDIF

@CARDEFFECT_39909(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 捕縛대상
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名前
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 제외カードID
#DIM DYNAMIC 破壊カードID
#DIMS DYNAMIC 제외カード名
#DIM DYNAMIC 残り횟수
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先
#DIM DYNAMIC 攻撃力판정
#DIM DYNAMIC 攻撃誘導者
#DIM DYNAMIC 条件달성,13
#DIM DYNAMIC 選択カードレベル
#DIM DYNAMIC 取込カードID
#DIM DYNAMIC 설정場所

VARSET 攻撃力판정, -1
VARSET 설정場所, -1

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39909)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 광서의 기계
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件성별 = 牝性
			효과条件종족 = 기계족
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == EX소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39909,1)
	IF RESULT == 0
		CALL FULLRESET
	    효과範囲상대덱 = 1
        효과範囲상대EX덱 = 1
        효과範囲상대묘지 = 1
        효과範囲상대손패 = 1
	    효과条件종류 = モン全般
        효과条件特不無視 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"",0,1)
		IF RESULT == 1
            CALL FULLRESET
		    효과条件カテゴリ = 광서의 기계
            효과条件종류 = モン全般
		    효과範囲손패 = 1
		    CALL CARD_SP_SUMMON_ADV(듀얼리스트,場所)
			FOR カウンタ,0,5
				SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
					CONTINUE
				SIF GETVAR(@"%対面者%_MZ_表:カウンタ") == 3
					CONTINUE
			IF 攻撃力판정 < GETVAR(@"%対面者%_MZ_攻:カウンタ")
				攻撃力판정 = GETVAR(@"%対面者%_MZ_攻:カウンタ")
				설정場所 = カウンタ
			ENDIF
		NEXT
			IF 설정場所 >= 0
				CALL FULLRESET
				효과条件以攻撃力 = 攻撃力판정
				PRINTFORMW 억지로 소환된 동료를 감싸듯 상대 필드의 몬스터가 앞으로 나온다…….
				PRINTFORML 상대 필드의 공격력이 가장 높은 몬스터 1체에 『공격 유도』가 부여됩니다,
				PRINTFORMW 대상을 선택해주세요。
				CALL CARD_CHOOSE_BASIC(対面者,"필드",ゾーン,場所)
				攻撃誘導者 = RESULT
				CALL SET_FIELD_INFO(対面者,"필드",攻撃誘導者,"攻誘",1,2)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39909,2)
	IF RESULT == 0
		FOR カウンタ,1,13
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件성별 = 牝性
			효과条件レベル = カウンタ
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				CONTINUE
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件レベル = カウンタ + 2
		    효과条件カテゴリ = 광서의 기계
            효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件달성:カウンタ = 1
		NEXT

		CALL FULLRESET
		효과範囲필드 = 1
		효과条件レベル:0 = 0
		FOR カウンタ,1,13
			효과条件レベル:カウンタ = カウンタ
		NEXT
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		捕縛대상 = RESULT:99
		IF RESULT != 9999
			PRINTFORML 카드ID={RESULT:1}
			選択カードレベル = GETVAR(@"%듀얼리스트%_MZ_星:RESULT")
			CALL CARD_DISPOSE(듀얼리스트,"필드",RESULT)
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件レベル = 選択カードレベル + 2
		    효과条件カテゴリ = 광서의 기계
            효과条件종류 = モン全般
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			CALL CARD_EROTICS(捕縛대상)
			IF 捕縛대상 != 1
				CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"変化元ID",捕縛대상,上書きモード)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39909,5)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		CALL CARD_ADD_GY_BASIC(듀얼리스트, "묘지", ゾーン, 場所)
		IF RESULT != 0
			효과条件소환可 = 1
			효과条件カテゴリ = 광서의 기계
    		효과範囲덱 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
	
ELSEIF 종류 == 지속효과

	IF GETVAR(@"%듀얼리스트%_MZ_汎数:場所") > 0
		;汎数読み込み
		取込カードID = GETVAR(@"%듀얼리스트%_MZ_汎数:場所")
		;記録した素材の성별をチェック
		TRYCALLFORM CARD_{取込カードID}, "성별"
		;♀かふたなりなら続行
		選択カード = NO_TO_CHARANUM(取込カードID)
		IF 選択カード >= 1 && オプション_성별
			RESULT = TALENT:選択カード:성별
		ENDIF
		IF GETBIT(RESULT,0)
			;変化元IDに汎数を打ち込み
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"変化元ID",取込カードID,上書きモード)
			;フレーバー、캐릭터リスト読み込み
			CALL CARD_EROTICS(取込カードID)
			IF RESULT != -1
				조교대상 = RESULT
					SELECTCASE RAND:3
						CASE 0
						PRINTFORMW 타지 음구 기사로 개조되기 전 준비로서, %CALLNAME:조교대상%는 온몸을 범하기 위한 기계에 달라붙게 된다.
						PRINTFORMW 성기를 깎아 부수기 위한 드릴 바이브나 실리콘 전기톱, 자궁을 두드려 부수기 위한 수많은 형태의 딜도, 머리를 감싸듯이 설치된 세뇌 장치,
