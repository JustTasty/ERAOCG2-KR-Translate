;―――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@MADORGASM_HYPNOSIS_OPERATION_TXT(繰り返し횟수,광서최면パターン,パターン範囲 = 20)
#DIM DYNAMIC 繰り返し횟수
#DIM DYNAMIC 광서최면パターン
#DIM DYNAMIC パターン範囲
#DIM DYNAMIC 文字色保持
#DIMS DYNAMIC 광서최면声

SIF 繰り返し횟수 == 0
	繰り返し횟수 = 1

REPEAT 繰り返し횟수
	IF 광서최면パターン == 0
		;通常の喘ぎ方
		SELECTCASE RAND:パターン範囲
			CASE 0
				광서최면声 = "……아……❤❤❤" %광서최면声%
			CASE 1
				광서최면声 = "…갑니다……❤…아❤갑니다…❤" %광서최면声%
			CASE 2
				광서최면声 = "…기분 좋아요…❤……보지…기분 좋아요…❤" %광서최면声%
			CASE 3
				광서최면声 = "%TEXTR("최면 마조 오나홀/무자각 최면 세뇌된 보지/최면 받은 친구 케이스/SEX용 최면 암구멍")%……갑니다…❤" %광서최면声%
			CASE 4
				광서최면声 = "아아아❤아❤아❤아아아❤" %광서최면声%
			CASE 5
				광서최면声 = "아❤갑니다❤온몸이 떨리면서 갑니다❤" %광서최면声%
			CASE 6
				광서최면声 = "네❤절정이에요❤가요❤아❤아❤" %광서최면声%
			CASE 7
				광서최면声 = "아❤아아❤아❤" %광서최면声%
			CASE 8
				광서최면声 = "아아❤❤❤아아❤아❤" %광서최면声%
			CASE 9
				광서최면声 = "아아아❤아아아아아❤" %광서최면声%
			CASE 10
				광서최면声 = "아아아❤아아❤아❤" %광서최면声%
			CASE 11
				광서최면声 = "아❤아아아아❤……아❤" %광서최면声%
			CASE 12
				광서최면声 = "아❤아아아아…❤" %광서최면声%
			CASE 13
				광서최면声 = "응❤기분, 기분 좋아요❤네, 갈 것 같아요" %광서최면声%
			CASE 14
				광서최면声 = "가❤갔어요❤가요, 또 가❤갑니다❤" %광서최면声%
			CASE 15
				광서최면声 = "……❤" %광서최면声%
			CASE 16
				광서최면声 = "암컷❤암구멍❤암구멍 만져요❤" %광서최면声%
			CASE 17
				광서최면声 = "아❤아아아…❤" %광서최면声%
			CASE 18
				광서최면声 = "아❤아❤❤❤" %광서최면声%
			CASE 19
				광서최면声 = "응❤응❤" %광서최면声%
		ENDSELECT
	ENDIF
REND

;GETCOLORで現在値を취득して、설정後に戻す
文字色保持 = GETCOLOR()

SETCOLOR C_P_PURPLE
;通常(カッコつき)
PRINTFORML %광서최면声%

;文字色を元に戻す
SETCOLOR 文字色保持

