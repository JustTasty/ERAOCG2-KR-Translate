@CARDCAN_39902(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39902)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0
	SIF モンスター数(対面者) >= 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 발동済_3(듀얼리스트,39902)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") >= 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件성별 = 牝性
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF	GETVAR(@"%듀얼리스트%_MZ_汎数:場所") > 0
		RETURN 1
	SIF	GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所") >= 0
		RETURN 1
	RETURN 1
ENDIF


@CARDEFFECT_39902(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 選択カード名前
#DIM DYNAMIC 攻撃횟수

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39902,1)
    IF RESULT == 0
		TRYCALLFORM CARDNAME_{直前戦闘破壊ID(듀얼리스트,1)}("名前")
		選択カード名前 = %RESULTS%
		CALL FULLRESET
		효과範囲상대묘지 = 1
		효과条件名前 = %選択カード名前%
		효과추가종류 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"",0,1)
		CALL FULLRESET
		CALL CARD_EROTICS(直前戦闘破壊ID(듀얼리스트,1))
		조교대상 = RESULT
		IF 조교대상 != -1
			IF MARK:조교대상:쾌락각인 + MARK:조교대상:고통각인 + MARK:조교대상:굴복각인 - MARK:조교대상:반발각인 < 8
				SELECTCASE RAND:3
					CASE 0 
						PRINTFORML 거대톱완병은 『아하아……❤』라며 새로운 사냥감을 차분히 바라보고, 실리콘 체인소로 %CALLNAME:조교대상%의 보지 상태를 빙글빙글 확인하고 있다.
						PRINTFORML 한편 쓰러져버린 %CALLNAME:조교대상%는 다른 광서의 기계에 의해 암컷 구멍을 드러내는 체위가 되어,
						PRINTFORMW 살주름투성이의 실리콘 체인소가 조임이 좋을 것 같은 미개발의 암컷 보지에 파고들어 버린다.
						PRINTFORMW %CALLNAME:조교대상%의 제지하는 목소리도 무시하고 거대톱완병은 실리콘 체인소를 기동시킨다……
					CASE 1
						PRINTFORML 쓰러져버린 %CALLNAME:조교대상%는 구속되어, 양다리를 머리 뒤로 넘기는 만구리카에시 체위가 되어버렸다.
						PRINTFORML 보지를 무방비하게 드러내는 그 체위가 된 %CALLNAME:조교대상%는, 눈앞에 내밀어진 실리콘 체인소를 보고 겁에 질린 표정이 된다.
						PRINTFORMW 회전하는 실리콘 부분이 보지에 눌려 무엇을 당할지 짐작했지만, %CALLNAME:조교대상%는 아무것도 할 수 없다……
					CASE 2
						PRINTFORML 쓰러져 붙잡혀버린 %CALLNAME:조교대상%는, 억지로 목과 양손을 잇는 단두대 같은 구속을 당해버린다.
						PRINTFORML 상반신을 억지로 숙이고 하반신을 내미는 듯한 체위 때문에 %CALLNAME:조교대상%의 양쪽 구멍은 드러난다.
						PRINTFORMW 무슨 일을 당할지 이해하지 못하는 %CALLNAME:조교대상%는 겁에 질린 채, 암컷 구멍에 무언가 눌리는 감촉에 무서워 소리를 내버린다……
					ENDSELECT

				FOR カウンタ,0,4
				SELECTCASE RAND:2
					CASE 0 
						CALL MADORGASM_CHAINSAW_TXT(1,0)
						CALL MADORGASM_CHAINSAW_TXT(1,2)
						CALL EROTIC_MOVE("前穴凌辱",조교대상,-1,100)
					CASE 1
						CALL MADORGASM_CHAINSAW_TXT(1,1)
						CALL MADORGASM_CHAINSAW_TXT(1,2)
						CALL EROTIC_MOVE("Ｃ単発",조교대상,-1,100)
				ENDSELECT
				NEXT

				SELECTCASE RAND:3
					CASE 0 
						PRINTFORML 실리콘 체인소에 의해 성기가 으깨진 %CALLNAME:조교대상%는, 넋 나간 표정으로 바닥에 뒹굴고 있었다.
						PRINTFORML 벌렁하고 벌어진 보지, 부어올라 딱딱하게 선 클리토리스, 애액투성이의 하반신.
						PRINTFORMW 쩍 벌린 다리로 분수를 뿜으며 「응기오……오…❤」라고 신음하며 %CALLNAME:조교대상%는 다시 필드로 끌려 나와버린다……
					CASE 1
						PRINTFORML %CALLNAME:조교대상%의 보지는 실리콘 체인소에 의해 보짓살이 벌렁하고 벌어진 오나홀 보지가 되어버렸다.
						PRINTFORML 장시간 동안 쉬지 않고 으깨진 암컷 구멍은, 분수를 계속 뿜으며 무언가 원하는 듯이 뻐끔뻐끔 개폐를 반복하고 있다.
						PRINTFORMW %CALLNAME:조교대상%는 연속 절정의 여운이 가시지 않는 육체를 이끌려 필드에 내던져져 버린다……
					CASE 2
						PRINTFORML %CALLNAME:조교대상%는 「옷……❤오기오오…오…❤」라고 평소의 태도에서는 생각할 수 없는 목소리를 내며 바닥에 뒹굴고 있었다.
						PRINTFORML 하반신은 흘러내린 애액으로 젖어, 활짝 벌어진 보지에서는 끝없이 분수를 뿜어내고 있다,
						PRINTFORMW 거대톱완병에게 다리를 잡혀, 난잡하게 필드에 내던져져도, %CALLNAME:조교대상%는 비참한 모습 그대로 계속 있다……
					ENDSELECT

			ELSEIF MARK:조교대상:쾌락각인 + MARK:조교대상:고통각인 + MARK:조교대상:굴복각인 - MARK:조교대상:반발각인 >= 8
				SELECTCASE RAND:3
					CASE 0 
						PRINTFORML %CALLNAME:조교대상%는 일부러 잡힌 것처럼 약한 저항을 하고 그대로 덮쳐질 뿐만 아니라,
						PRINTFORML 스스로 양다리를 머리 뒤로 돌려 양팔로 껴안듯이 암컷 구멍을 드러내고, 필사적으로 거대톱완병에게 목숨을 구걸하고 있었다.
						PRINTFORMW %CALLNAME:조교대상%의 표정은 이제부터 행해질 행위를 짐작하고 발정해버린 암컷의 표정 그 자체였다……
					CASE 1
						PRINTFORML 쓰러져버린 %CALLNAME:조교대상%는, 스스로 옷을 벗어던지고 목숨을 구걸하기 위해 전라 도게자를 하고 있었다.
						PRINTFORML 단지 %CALLNAME:조교대상%의 표정은 얼굴을 붉히고 입꼬리를 올리는 듯한 발정해버린 암컷의 표정이며,
						PRINTFORMW 이제부터 행해질 실리콘 체인소에 의한 능욕을 기대해 마지않는 마조 노예 그 자체였다.
					CASE 2
						PRINTFORML %CALLNAME:조교대상%는 목과 양손을 잇는 단두대 같은 구속을 받아버린다.
						PRINTFORML ……하지만, %CALLNAME:조교대상%는 스스로 허리를 흔들며 아양을 떨듯이 애액투성이의 암컷 구멍을 과시하고 있었다.
						PRINTFORMW 그리고, 내밀어진 실리콘 체인소에 자신의 암컷 구멍을 얹고, 기대와 공포에 숨을 헐떡이고 있다……
					ENDSELECT
				;シリコンチェーンソーによる性器破壊１回目
				FOR カウンタ,0,4
				SELECTCASE RAND:2
					CASE 0 
						CALL MADORGASM_CHAINSAW_TXT(1,0)
						CALL MADORGASM_CHAINSAW_TXT(1,4)
						CALL EROTIC_MOVE("Ｖ単発",조교대상,-1,100)
					CASE 1
						CALL MADORGASM_CHAINSAW_TXT(1,1)
						CALL MADORGASM_CHAINSAW_TXT(1,5)
						CALL EROTIC_MOVE("Ｃ単発",조교대상,-1,100)
				ENDSELECT
				NEXT
					SELECTCASE RAND:3
					CASE 0 
						PRINTFORML 실리콘 체인소에 의한 보지 처형을 마친 %CALLNAME:조교대상%의 표정은,
						PRINTFORML 『이 암컷 구멍을 망가뜨려 주셔서 감사합니다……❤』라고 말하는 듯한 열락의 표정을 짓고 있었다.
						PRINTFORMW 가랑이를 무방비하게 열고 하반신을 애액투성이로 만들면서, %CALLNAME:조교대상%는 필드로 돌려보내진다……
					CASE 1
						PRINTFORML 『응오……오오오오옷…❤』이라며 질퍽질퍽 애액을 흩뿌리며 보지 처형의 여운에 잠겨있는 %CALLNAME:조교대상%
						PRINTFORML 바닥에 뒹굴며, 덜덜 전신을 튀어오르게 하며 멈추지 않는 절정을 억제하지도 못하고,
						PRINTFORMW %CALLNAME:조교대상%는 상반신을 바닥에 기게 한 채 스스로 양쪽 구멍을 내밀듯이 하반신을 위로 올려 다음 능욕을 요구해버린다……
					CASE 2
						PRINTFORML %CALLNAME:조교대상%는 븃❤뷰쟈쟈❤ 소리가 날 정도로 애액을 보지에서 흘러넘치게 하고 있다.
						PRINTFORML 활짝 벌어진 보지를 손가락으로 휘젓고, 부어오른 클리토리스를 집으며 신음하는 %CALLNAME:조교대상%,
						PRINTFORMW 『오❤보지처형고마워엇❤감사합니다❤』라고 사례를 말하며 다시 절정을 맞이해버린다……
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 攻守変動
	攻撃力修正 = GETVAR(@"%듀얼리스트%_MZ_薬Ｃ:場所") * 1000
    攻撃횟수 = GETVAR(@"%듀얼리스트%_MZ_薬Ｃ:場所") + 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",攻撃횟수)
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)
    ELSEIF 종류 == 攻撃後強制誘発
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦",-99,1)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"追戦",-99,1)

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39902,3)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件他所 = 1
            효과条件行先플래그 = 破壊行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"薬Ｃ",1)
			ENDIF
		ENDIF

ELSEIF 종류 == ターン終了時強制
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 0
	CALL CARD_EROTICS(GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所"))
		IF RESULT != -1
		조교대상 = RESULT
			SELECTCASE RAND:6
				CASE 0
					PRINTFORMW 기계 거대 톱팔 병사의 생체 부품으로 편입된 %CALLNAME:조교대상%가, 외장이 움직이기 위한 동력을 바치기 위해 드릴 바이브로 범해지기 시작한다…….
					CALL MADORGASM_MACHINESEXTOY_TXT(1,0)
					CALL MADORGASM_MACHINESEXTOY_TXT(1,1)
					CALL MADORGASM_DRILLVIBERAPE_TXT(1,4)
					CALL EROTIC_MOVE("Ｖ単発",조교대상,-1,200)
