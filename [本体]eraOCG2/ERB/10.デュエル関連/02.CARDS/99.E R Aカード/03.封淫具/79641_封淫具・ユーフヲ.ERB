@CARDNAME_79641,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 봉음구 유포
	CASE "略称"
		RESULTS = 봉음 가슴빨판
	CASE "カテゴリ"
		RESULTS:0 = 봉음구
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79641(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 装備마법
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	;CASE "分割"
	;	RETURN 효과なし
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79641(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：이 카드는 발동 후、상대 플레이어에게 장착된다。
CALL CARD_ABLE(2,攻守変動)
PRINTL ②：자신 몬스터 존에 "봉음구"효과몬스터가 존재 할 경우、
PRINTL 상대 필드의 몬스터의 공격력 수비력은、
PRINTL 자신 필드의"봉음구"장착마법카드 １종류마다 ３００씩 내려간다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79641(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79641)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動 || 종류 == リアクション효과
	SIF !GROUPMATCH(ゾーン,"마함","필드")
		RETURN 0
	IF  종류 == 攻守変動
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 봉음구
		효과条件종류 = 효과モン
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ENDIF


@CARDEFFECT_79641(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 조교대상 = -1
#DIM DYNAMIC 조교강도

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79641)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			조교대상 = RESULT
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과条件カテゴリ = 봉음구
			효과条件종류 = 装備마법
			효과条件固名前起動 = 1
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			조교강도 = RESULT
			IF 조교대상 >= 0
				SETCOLOR C_PINK
				PRINTFORML 
				PRINTFORML %CALLNAME:조교대상%의 유두에 봉음구 유포가 장착되었다。
				RESETCOLOR
				CALL EROTIC_MOVE("Ｂ単発",조교대상,-1,50 * 조교강도)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 봉음구
	효과条件종류 = 装備마법
	효과条件固名前起動 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	조교강도 = RESULT * 300
	CALL FULLRESET
	CALL STATCHANGE_BASIC(듀얼리스트,"상대필드",場所,-(조교강도),-(조교강도),1657)
ELSEIF 종류 == リアクション효과
	CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
	IF RESULT != -1
		조교대상 = RESULT

		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 봉음구
		효과条件종류 = 装備마법
		효과条件固名前起動 = 1

		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		조교강도 = RESULT

		IF 조교대상 >= 0
			SETCOLOR C_PINK
			PRINTFORML 
			PRINTFORML %CALLNAME:조교대상%은 유두를 핥아먹는듯이 간지럽혀졌다。
			RESETCOLOR
			CALL EROTIC_MOVE("Ｂ単発",조교대상,-1,50 * 조교강도)
		ENDIF
	ENDIF
ENDIF
