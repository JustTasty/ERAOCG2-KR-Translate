@CARDNAME_79490,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 계약の각음
	CASE "略称"
		RESULTS = 계약の각음
	CASE "カテゴリ"
		RESULTS:0 = 각음
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_79490(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250626
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79490(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALLFORM CARD_ABLE(0,마함起動,손패起動)
PRINTL ①：このデュエル中、以下の효과をそれぞれ適用する。
PRINTL ●자신は손패のモンスターの효과を발동できない。
PRINTL ●ターンの開始時に발동する。自身に発情카운터を１つ置く。
PRINTL 　자신のフィールドにモンスターが存在しない場合、
PRINTL 　자신の손패、덱、묘지から「각음の夢魔」１体を특수소환する。
PRINTL 　「각음の夢魔」が자신フィールドに存在する場合、代わりにその攻撃力、守備力を５００アップする。
CALLFORM CARD_ABLE(0,묘지起動)
PRINTL ②：묘지のこのカードを제외し、손패から「계약の각음」１枚を捨てて발동できる。
PRINTL カードを１枚ドローする。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79490(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79490)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_D1(듀얼리스트,79490)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 時限紋章起動
	SIF スタンバイフェイズ != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件名前 = 계약の각음
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79490(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79490)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL SET_PLAYER_INFO(듀얼리스트,"逆魔香효과",2)
		CALL CARD_RESERV(듀얼리스트,79490,時限紋章予約)
		CALL CARD_EFFECT_D1USE(듀얼리스트,79490)
		
		CALL 対面者판정(듀얼리스트)
		対面者 = %RESULTS%
		CALL CARD_EROTICS(상대プレイヤー,対面者)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML 
			SETCOLOR C_PINK 
			IF  TALENT:조교대상:성별 != 2
				PRINTFORMW %NAME:조교대상%は夢魔と계약を結び、助力と引き換えに下腹部に淡く光る淫紋を刻まれました。
				PRINTFORMW 刻まれた淫紋が輝くと下腹部を中心に%NAME:조교대상%の体に疼きが広がっていく……
				CALL EROTIC_MOVE("淫紋の疼き",조교대상,-1,300)
				PRINTFORMW
			ENDIF
			RESETCOLOR
		ENDIF
	ENDIF
ELSEIF 종류 == 時限紋章起動
	CALL SET_PLAYER_INFO(듀얼리스트,"薬Ｃ",1,0,1)
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件名前 = 각음の夢魔
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"永攻",500)
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"永守",500)
	ELSE
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件名前 = 각음の夢魔
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
	
	CALL 対面者판정(듀얼리스트)
		対面者 = %RESULTS%
		CALL CARD_EROTICS(상대プレイヤー,対面者)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML 
			SETCOLOR C_PINK 
			IF  TALENT:조교대상:성별 != 2
				PRINTFORMW %NAME:조교대상%に刻まれた淫紋が輝き、力を与えると同時に体の疼きが強くなっていく……
				カウンタ = GETVAR(@"%듀얼리스트%_薬Ｃ")
				CALL EROTIC_MOVE("淫紋の疼き",조교대상,-1,300)
				PRINTFORMW
				IF ABL:(조교대상):욕망 + カウンタ >= 10
				;욕망+経過ターン数が10以上で추가조교
					PRINTFORM 夢魔の淫紋に侵され続け、体の疼きが限界に達した%NAME:조교대상%は듀얼中であることも忘れ、
					효과範囲필드 = 1
					효과条件名前 = 각음の夢魔
					CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
					IF RESULT == 1
						SETCOLOR C_PINK 
						PRINTFORML 夢魔へと快楽を懇願してしまった……
						PRINTFORML 夢魔は快楽に呑まれた%NAME:조교대상%を見て満足そうに微笑むとゆっくりと歩み寄り、
						SELECTCASE RAND:5
							;エロの종류。全身愛撫、ふたV、ふたA、ふたフェラ、ふたパイズリ
							;ふたなりが苦手な人は↑のをSELECTCASE RAND:0にすればレズ愛撫オンリーになります。
							;추가조교の조교효과は조교대상があなただと快楽刻印や好感度補正がないためやや高めに설정。
							CASE 0
								PRINTFORML %NAME:조교대상%を抱き寄せると、全身を愛撫しながら口づけを行ってきた……
								PRINTFORMW 夢魔の手管と貪るような口付けに、%NAME:조교대상%は声も出せないまま甘く蕩かされていく……
								CALL EROTIC_MOVE("全身レズ愛撫",조교대상,-1,700)
							CASE 1
								PRINTFORML 並の男なら自信を喪失してしまうような硬く反り返った肉棒を見せつけてきた……
								PRINTFORM 快楽の予感に胎を疼かせる%NAME:조교대상%を夢魔は
								SELECTCASE RAND:3
									CASE 0
										PRINTFORML 仰向けに押し倒すと、正常位で犯してきた……！
									CASE 1
										PRINTFORML 四つん這いにさせると、後背位で犯してきた……！
									CASE 2
										PRINTFORML 抱き上げると、騎乗位で犯してきた……！
								ENDSELECT
								PRINTFORMW 子宮まで押し潰し犯されるような暴力的な快感に%NAME:조교대상%は悲鳴のような喘ぎ声をあげることしかできない……
								CALL EROTIC_MOVE("前穴凌辱",조교대상,-1,700)
							CASE 2
								PRINTFORML 並の男なら自信を喪失してしまうような硬く反り返った肉棒を見せつけてきた……
								PRINTFORM 快楽の予感に胎を疼かせる%NAME:조교대상%を夢魔は
								SELECTCASE RAND:3
									CASE 0
										PRINTFORML 仰向けに押し倒すと、アナルを正常位で犯してきた……！
									CASE 1
										PRINTFORML 四つん這いにさせると、アナルを後背位で犯してきた……！
									CASE 2
										PRINTFORML 抱き上げると、アナルを騎乗位で犯してきた……！
								ENDSELECT
								PRINTFORMW 後ろの穴を犯される、背徳的で暴力的な快感に、%NAME:조교대상%は悲鳴のような喘ぎ声をあげることしかできない……
								CALL EROTIC_MOVE("後穴凌辱",조교대상,-1,700)
							CASE 3
								PRINTFORML 並の男なら自信を喪失してしまうような硬く反り返った肉棒を見せつけてきた……
								PRINTFORM 思わずごくりと唾を呑み込む%NAME:조교대상%へ夢魔は肉棒を突きつけ、
								SELECTCASE RAND:2
									CASE 0
										PRINTFORML 口で奉仕をするように要求してきた……！
										PRINTFORML 目の前の「雄」に惹きつけられるように、%NAME:조교대상%は要求に従い奉仕を行ってしまう……
									CASE 1
										PRINTFORML 頭を掴むと、その肉棒で荒々しく口を犯してきた……！
								ENDSELECT
								PRINTFORMW 強い「雄」に支配され、奉仕するという状況に%NAME:조교대상%は酸欠でくらくらとしながらも昂っていく……
								CALL EROTIC_MOVE("フェラチオ",조교대상,-1,700)
							CASE 4
								PRINTFORML 並の男なら自信を喪失してしまうような硬く反り返った肉棒を見せつけてきた……
								PRINTFORM 思わず胸を高鳴らせる%NAME:조교대상%へ夢魔は肉棒を突きつけると、
								SELECTCASE RAND:2
									CASE 0
										PRINTFORML 胸で挟み、奉仕をするように要求してきた……！
										PRINTFORML 目前に迫った「雄」に惹きつけられるように、%NAME:조교대상%は要求に従い奉仕を行ってしまう……
									CASE 1
										PRINTFORML 両手で胸を掴み、まるで性交を行うかのように腰を振りたててきた……！
								ENDSELECT
								PRINTFORMW 強い「雄」に支配され、女の象徴で奉仕するという状況に%NAME:조교대상%は昂っていく……
								CALL EROTIC_MOVE("パイズリ",조교대상,-1,700)
						ENDSELECT
					ELSE
						SETCOLOR C_PINK 
						PRINTFORMW 自慰行為を始めてしまった……
						CALL EROTIC_MOVE("自慰",조교대상,-1,500)
					ENDIF
					PRINTFORMW
				ENDIF
			ENDIF
			RESETCOLOR
		ENDIF
	
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79490)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件名前 = 계약の각음
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARDDRAW_BASIC(듀얼리스트,1)
		ENDIF
	ENDIF
ENDIF
