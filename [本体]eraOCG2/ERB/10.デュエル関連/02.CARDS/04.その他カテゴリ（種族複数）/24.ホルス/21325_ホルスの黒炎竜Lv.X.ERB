@CARDNAME_21325,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 호루스의 흑염룡ＬＶ.X
	CASE "略称"
		RESULTS = 흑염룡Lv.X
	CASE "カテゴリ"
		RESULTS:0 = Lv
		RESULTS:1 = 호루스
ENDSELECT


@CARD_21325(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
	CASE "生成カード"
		RESULT:3 = 421325
		RESULT:2 = 321325
		RESULT:1 = 221325
		RETURN 121325
ENDSELECT

RETURN 0

@CARD_EXPLANATION_21325(종류)
#DIM DYNAMIC 종류
CALL CARD_ABLE(0,ターン終了時)
PRINTL ①：이 카드가 전투를 행한 엔드 페이즈에 발동할 수 있다。이 카드는 성장한다。
CALL CARD_ABLE(0,소환時)
PRINTL ②：이 카드가 소환에 성공했을 경우에 발동할 수 있다。
PRINTL 덱에서 "왕의 관"카드나 "카노푸스의 수호자" １장을 패에 넣는다。
PRINTL 이 효과는 １턴에 １번밖에 사용할 수 없다。


@CARDCAN_21325(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,21325)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_21325(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ

IF 종류 == 지속효과
	CALL CARD_RECONSTRUCT(듀얼리스트,"필드",場所,121325)
	
	IF 발동済_1(듀얼리스트,21325) == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 왕의 관
		효과条件スーパー名前 = 카노푸스의 수호자
		효과条件任意 = 1
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT == 1
			CALL CARD_EFFECT_USE(듀얼리스트,21325,1)
	ENDIF
ENDIF


