@CARDNAME_21324,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 호루스의 가호－케벤세누프
	CASE "略称"
		RESULTS = 케벤세누프
	CASE "カテゴリ"
		RESULTS:0 = 호루스
ENDSELECT

@CARD_21324(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_21324(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 방법에 의한 특수 소환은 １턴에 １번밖에 할 수 없으며、
PRINTL ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,묘지起動)
PRINTL ①：자신 필드에 "왕의 관"카드가 존재하는 경우、이 카드는 묘지에서 특수 소환할 수 있다。
CALL CARD_ABLE(2,효과後誘発)
PRINTL ②：이 카드가 몬스터 존에 존재하는 상태에서、
PRINTL 자신 필드의 다른 카드가 상대의 효과로 필드에서 벗어났을 경우에 발동할 수 있다。
PRINTL 이 턴、자신 필드의 "호루스" 몬스터는 대상내성과 공격회피를 얻는다。


@CARDCAN_21324(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,21324)
SIF RESULT == 1
	RETURN 0

IF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,21324)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,21324,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 왕의 관
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,21324)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF 処理中_듀얼리스트 == 듀얼리스트
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊以外")
	SIF RESULT == 1
		RETURN 1
	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과破壊")
	SIF RESULT == 1
		RETURN 1
	RETURN 0

ELSEIF 종류 == 短紋章起動
	RETURN 1
ENDIF

@CARDEFFECT_21324(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,21324,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"묘지",場所)
	ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,21324,2)
	IF RESULT == 0
		CALL CARD_RESERV(듀얼리스트,21324,短紋章予約)
	ENDIF

ELSEIF 종류 == 短紋章起動
	CALL FULLRESET
	효과条件カテゴリ = 호루스
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"攻避")
ENDIF

