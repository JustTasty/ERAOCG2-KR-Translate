@CARDNAME_58045,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.45 멸망의 예언자 크램블 로고스
	CASE "略称"
		RESULTS = #45 멸망의예언자
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
ENDSELECT


@CARD_58045(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 언데드족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 0
	CASE "추가日"
		RETURN 250712
ENDSELECT

RETURN 0

