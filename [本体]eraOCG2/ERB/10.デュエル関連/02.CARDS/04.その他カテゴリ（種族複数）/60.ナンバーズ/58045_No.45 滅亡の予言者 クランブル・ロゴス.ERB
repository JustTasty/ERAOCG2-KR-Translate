@CARDCAN_58045(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,58045)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	IF 듀얼리스트 == "자신"
		효과範囲필드 = 1
		효과範囲마함 = 1
	ENDIF
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 表側표시
	효과条件他所 = 1
	효과条件除去種 = 無効
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_汎数:場所") < 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_58045(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58045,10,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		효과条件他所 = 1
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両全場単体",場所,"選択")
		IF RESULT == 1
			CALL CARD_SELECT(듀얼리스트,RESULT)
			選択カード = RESULT
			選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:選択カード")
			CALL SET_FIELD_INFO(듀얼리스트,ゾーン,場所,"汎数",選択カード_ID)
			CALL SET_FIELD_INFO(대상者（CS）,参照先（CS）,選択カード,"無効",2)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	選択カード_ID = GETVAR(@"%듀얼리스트%_MZ_汎数:場所")
	IF 選択カード_ID != -1
		CALL CARD_EFFECT_PROHIB(듀얼리스트,選択カード_ID,2)
		TRYCALLFORM CARDNAME_{選択カード_ID}("名前")
	ENDIF
ENDIF