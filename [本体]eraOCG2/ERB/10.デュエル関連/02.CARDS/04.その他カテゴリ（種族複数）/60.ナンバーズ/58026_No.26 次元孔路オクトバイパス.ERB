	CASE "名前"
		RESULTS = No.26 차원공로 옥토바이패스
	CASE "略称"
		RESULTS = #26 차원공로
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
ENDSELECT


@CARD_58026(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 800
	CASE "守備力"
		RETURN 2400
	CASE "추가日"
		RETURN 250712
ENDSELECT

RETURN 0

