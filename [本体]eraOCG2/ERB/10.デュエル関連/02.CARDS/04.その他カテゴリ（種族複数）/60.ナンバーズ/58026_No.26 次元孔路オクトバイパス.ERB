@CARDCAN_58026(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,58026)
SIF RESULT == 1
	RETURN 0

IF 종류 == バトル開始時
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,58026)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF !MATCH(ダメージ종류,"直接戦闘")
		RETURN 0
	RETURN 1

ELSEIF 종류 == 短紋章起動
	RETURN 1
ENDIF

@CARDEFFECT_58026(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58026,1,X消費1)
	IF RESULT == 0
		CALL CARD_RESERV(듀얼리스트,58026,短紋章予約)
		CALL SET_PLAYER_INFO(듀얼리스트,"単体攻撃",1)
		CALL SET_PLAYER_INFO(듀얼리스트,"単体攻撃",1,1)
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58026)
	IF RESULT == 0
		IF ターンプレイヤー == 듀얼리스트
			CALL CARD_GET_CONTROL(対面者,戦闘場所(듀얼리스트))
		ELSE
			CALL CARD_GET_CONTROL(듀얼리스트,戦闘場所(듀얼리스트,1))
		ENDIF
	ENDIF

ELSEIF 종류 == 短紋章起動
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"直攻")
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"直攻")
ENDIF