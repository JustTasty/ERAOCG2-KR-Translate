#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.35 라베노스 타란툴라
	CASE "略称"
		RESULTS = #35 라베노스
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
ENDSELECT


@CARD_58035(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "추가日"
		RETURN 250712
ENDSELECT

RETURN 0

