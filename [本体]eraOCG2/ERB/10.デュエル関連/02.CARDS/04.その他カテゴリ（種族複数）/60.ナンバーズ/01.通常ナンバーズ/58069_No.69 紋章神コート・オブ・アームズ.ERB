@CARDNAME_58069,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.69 갓 메달리온 코트 오브 암즈
	CASE "略称"
		RESULTS = #69 Co암즈
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
		RESULTS:1 = 문장수
ENDSELECT


@CARD_58069(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 엑시즈
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 1400
	CASE "추가日"
		RETURN 250713
ENDSELECT
RETURN 0


@CARDSUMMON_58069(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 3
素材条件レベル = 4

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,58069) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_58069(종류)
#DIM DYNAMIC 종류

PRINTL 레벨４몬스터×３
PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(0,EX소환時,특수소환時)
PRINTL ①：이 카드가 특수 소환한 경우에 발동할 수 있다。
PRINTL 상대 필드의 모든 X몬스터의 효과는 무효화된다。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ②：필드의 다른 몬스터 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 카드의 『필드기동효과』（필드에 존재하는 경우、기동 가능한 효과）를 발동하고、
PRINTL 이 카드는 그 몬스터의 원래 카드명과 동일한 카드명을 얻는다。


@CARDCAN_58069(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 必要カード

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,58069)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時 || 종류 == 특수소환時
    SIF 발동済_1(듀얼리스트,58069)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1		


ELSEIF 종류 == 필드起動
    SIF 발동済_1(듀얼리스트,58069)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1		

ENDIF

@CARDEFFECT_58069(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 選択カード名前

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58069)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 엑시즈
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"無効")
	ENDIF

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58069,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件표시 = 表側표시
		효과条件他所 = 1
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"選択")
		選択カード = RESULT:1
		IF RESULT == 1
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = 選択カード（CS）
			選択カード_ID = RESULT:99
			TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
			選択カード名前 = %RESULTS%

			CALLFORM CARDEFFECT_{GETVAR(@"%대상者（CS）%_%参照先（CS）%:選択カード（CS）")}(듀얼리스트,필드起動,"효과処理")
			
			CALL FULLRESET
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"変化元ID",選択カード_ID,上書きモード)
		ENDIF
	ENDIF
ENDIF

