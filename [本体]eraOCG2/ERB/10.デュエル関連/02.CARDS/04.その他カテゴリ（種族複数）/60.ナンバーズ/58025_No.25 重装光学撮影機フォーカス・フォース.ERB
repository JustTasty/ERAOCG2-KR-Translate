#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.25 포스 포커스
	CASE "略称"
		RESULTS = #25 포스 포커스
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
ENDSELECT


