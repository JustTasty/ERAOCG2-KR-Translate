@CARDNAME_58089,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.89 電脳獣ディアブロシス
	CASE "略称"
		RESULTS = #89 ディアブロ
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
ENDSELECT


@CARD_58089(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 1200
	CASE "추가日"
		RETURN 250712
ENDSELECT

RETURN 0

