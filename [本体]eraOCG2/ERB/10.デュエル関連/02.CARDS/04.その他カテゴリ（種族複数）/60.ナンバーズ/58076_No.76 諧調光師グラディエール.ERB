ENDSELECT


@CARD_58076(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2700
	CASE "守備力"
		RETURN 2100
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250712
