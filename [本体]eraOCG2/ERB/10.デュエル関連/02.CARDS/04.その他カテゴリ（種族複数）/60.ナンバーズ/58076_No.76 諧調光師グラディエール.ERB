@CARDCAN_58076(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,58076)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_3(듀얼리스트,58076)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_58076(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 자신속성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58076)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT != 9999
			TRYCALLFORM CARD_{RESULT:99},"속성"
			CALL SET_FIELD_INFO(듀얼리스트,ゾーン,場所,"永属",RESULT,1)
		ENDIF
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58076,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대묘지 = 1
		효과条件종류 = モン全般
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			TRYCALLFORM CARD_{RESULT:1},"속성"
			CALL SET_FIELD_INFO(듀얼리스트,ゾーン,場所,"永属",RESULT,1)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	자신속성 = GETVAR(@"%듀얼리스트%_MZ_属:場所")

	SIF GETVAR(@"%対面者%_MZ_属:戦闘場所(듀얼리스트,1)") == 자신속성
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	
	TRYCALLFORM CARD_{処理中_효과ID},"속성"
	SIF MATCH(RESULT,자신속성)
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
ENDIF
