@CARDNAME_58049,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.49 비조 포츈츈
	CASE "略称"
		RESULTS = #49 츈츈
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
ENDSELECT


@CARD_58049(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 400
	CASE "守備力"
		RETURN 900
	CASE "추가日"
		RETURN 250712
ENDSELECT

RETURN 0

