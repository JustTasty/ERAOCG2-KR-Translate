@CARDNAME_12191,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = K9-666호“Jacks”
	CASE "略称"
		RESULTS = K9“Jacks”
	CASE "カテゴリ"
		RESULTS:0 = K9
ENDSELECT


@CARD_12191(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN エクシーズ
	CASE "属性"
		RETURN 闇属性
	CASE "種族"
		RETURN 悪魔族
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 2500
	CASE "性別"
		RETURN 牝性
	CASE "追加日"
		RETURN 250726
	CASE "連動ID"
		RESULT:1 = 2192
		RETURN 2191
ENDSELECT
RETURN 0


