@CARDCAN_12191(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,12191)
SIF RESULT == 1
	RETURN 0

IF 種類 == EX召喚時
	SIF 発動済_1(決闘者,12191)
		RETURN 0
	SIF GETVAR(@"%決闘者%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	効果範囲相手戦場 = 1
	効果範囲相手魔罠 = 1
	効果条件除去種 = 墓地
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 効果後誘発
	SIF 発動済_1(決闘者,12191)
		RETURN 0
	SIF GETVAR(@"%決闘者%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	効果範囲手札 = 1
	効果範囲墓地 = 1
	効果条件種類 = モン全般
	CALL CARD_CHECK_FIN_DATA(決闘者,"効果発動",2)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	効果範囲相手戦場 = 1
	効果範囲相手魔罠 = 1
	効果条件除去種 = 墓地
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 永続効果
	SIF ゾーン != "墓地"
		RETURN 0
	RETURN 1
ELSEIF 種類 == 攻撃時強制誘発
	SIF ゾーン != "戦場"
		RETURN 0
	TRYCALLFORM CARDNAME_{戦闘カードID(決闘者)}("カテゴリ")
	IF !MATCH(RESULTS,"K9")
		RETURN 0
	ENDIF
	TRYCALLFORM CARDNAME_{戦闘カードID(決闘者)}("種類")
	IF !MATCH(RESULT,エクシーズ)
		RETURN 0
	ENDIF
	RETURN 1
ENDIF


@CARDEFFECT_12191(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード


IF 種類 == EX召喚時 || 種類 == 効果後誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,12191,1,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(決闘者,"全場単体",場所,"墓地")
	ENDIF
ELSEIF 種類 == 攻撃時強制誘発
	CALL FULLRESET
	CALL SET_PLAYER_INFO(決闘者,"ダメージ倍化",3,1)
ENDIF



