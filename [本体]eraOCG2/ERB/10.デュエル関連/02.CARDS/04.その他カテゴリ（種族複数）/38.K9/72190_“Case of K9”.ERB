@CARDNAME_72190,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = “Case of K9”
	CASE "略称"
		RESULTS = “Case of K9”
	CASE "カテゴリ"
		RESULTS:0 = K9
ENDSELECT


@CARD_72190(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "추가日"
		RETURN 250322
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72190(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  카드는 １턴에 １장 밖에 발동할 수 없으며、이 카드명의 ③의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,손패起動)
PRINTL ①：이 카드의 발동시의 효과 처리로서、덱에서 "K9" 몬스터 １장을 패에 넣을 수 있다。
CALL CARD_ABLE(0,攻守変動)
PRINTL ②：상대가 패 / 묘지의 몬스터의 효과를 발동한 턴,
PRINTL 자신 필드의 「「K9」 몬스터의 공격력은 ９００ 올린다。
CALL CARD_ABLE(3,戦破壊→묘지)
PRINTL ③：마법/함정 존의 이 카드가 효과로 파괴된 경우에 발동할 수 있다.
PRINTL 자신의 덱 / 묘지에서 "K9" 속공 마법 카드 1장을 자신 필드에 세트할 수 있다.


@CARDCAN_72190(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72190)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,72190)
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF GET_PLAYER_INFO(듀얼리스트,"K9발동数",1) <= 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"リリース","破壊","戦闘破壊")
		RETURN 0
	SIF 발동済_3(듀얼리스트,72190)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = K9
	효과条件종류 = 速攻마법
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_72190(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード


IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72190,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과条件カテゴリ = K9
		효과条件종류 = モン全般
		효과条件任意 = 1
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件カテゴリ = K9
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,900)
ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72190,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = K9
		효과条件종류 = 速攻마법
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"裏側표시")
	ENDIF
ENDIF
