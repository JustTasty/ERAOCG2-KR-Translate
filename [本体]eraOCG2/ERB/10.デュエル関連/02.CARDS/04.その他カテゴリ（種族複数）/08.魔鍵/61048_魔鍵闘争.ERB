@CARDNAME_61048,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 마건투쟁
	CASE "略称"
		RESULTS = 마건투쟁
	CASE "カテゴリ"
		RESULTS:0 = 마건
ENDSELECT


@CARD_61048(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_61048(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  카드는 １턴에 １장 밖에 발동할 수 없다。
CALLFORM CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①：자신 묘지의 일반 몬스터 또는 「마건」몬스터 １장과 「마건－マフテア」 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 카드를 패에 넣는다。
PRINTL 그 후、자신 필드의 일반 몬스터及び「마건" 몬스터는 次のフェイズまで完全내성을 얻는다。


@CARDCAN_61048(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,61048)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,61048)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 通常モン
	효과条件スーパーカテゴリ = 마건
	효과条件スーパー종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 마건－マフテア
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_61048(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 選択カード名前

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,61048,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 通常モン
		효과条件スーパーカテゴリ = 마건
		효과条件スーパー종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 마건－マフテア
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과条件종류 = 通常モン
		효과条件スーパーカテゴリ = 마건
		효과条件スーパー종류 = モン全般
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"モ耐",5)
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"魔耐",5)
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"함정耐",5)
	ENDIF
ENDIF

