@CARDNAME_32153,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 바이올런스 위치
	CASE "略称"
		RESULTS = Violence Witch
	CASE "カテゴリ"
		RESULTS:0 = 로즈
ENDSELECT


@CARD_32153(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 효과モン
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1100
	CASE "守備力"
		RETURN 1200
	;牝性/雄性があるよ
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250726
ENDSELECT
RETURN 0

@CARD_EXPLANATION_32153(종류)
#DIM DYNAMIC 종류


PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：「로즈」モンスター、または식물족Sモンスターが자신フィールドに存在する場合に발동できる。
PRINTL このカードを손패から특수소환する。
CALLFORM CARD_ABLE(2,필드→묘지,손패→묘지,덱→묘지,破壊→묘지,戦闘→묘지)
PRINTL ②：このカードが묘지へ送られた場合、フィールドにSモンスターが存在していれば、손패を１枚捨てて발동できる。
PRINTL 덱から守備力１５００以下の식물족モンスター１体を특수소환する。
PRINTL この효과の발동後、ターン終了時まで자신はSモンスターしかEX덱から특수소환できない。


@CARDCAN_32153(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,32153)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,32153)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,32153,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	효과条件종족 = 식물족
	효과条件スーパーカテゴリ = 로즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1	

ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_2(듀얼리스트,32153)
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0	
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0	
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 식물족
	효과条件守備力 = 1500
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF


@CARDEFFECT_32153(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,32153,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, ゾーン, 場所)
	ENDIF

ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
    CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,32153,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
	        효과条件종족 = 식물족
			효과条件守備力 = 1500
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

			EX소환可能종류 = 싱크로
			CALL CARD_EX_RESTRICTION(듀얼리스트)
		ENDIF
	ENDIF
ENDIF
			

