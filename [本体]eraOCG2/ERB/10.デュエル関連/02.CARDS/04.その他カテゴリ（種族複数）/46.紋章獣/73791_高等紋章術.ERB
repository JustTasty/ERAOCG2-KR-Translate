#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 하이 메달리온 아츠
	CASE "略称"
		RESULTS = 고등문장술
	CASE "カテゴリ"
		RESULTS:0 = 메달리온
ENDSELECT


@CARD_73791(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250713
ENDSELECT
RETURN 0

@CARD_EXPLANATION_73791(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：자신의 묘지의 「문장수」몬스터 ２장을 대상으로 하고 발동할 수 있다。
PRINTL 그 몬스터를 특수 소환한다。그 후、フィールドの몬스터를 素材とし、ランク４X몬스터 １장을 X소환する。


@CARDCAN_73791(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73791)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT >= 4
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 문장수
	효과条件종류 = EX以外全般
	효과条件소환可 = 1	
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73791(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73791,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		FOR カウンタ,0,2
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 문장수
			효과条件종류 = EX以外全般
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		NEXT
		CALL FULLRESET
		효과条件종류 = 엑시즈
		효과条件レベル = 4
		효과条件소환可 = 1
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
		    選択カードID = RESULT:99
				TRYCALLFORM CARDSUMMON_{選択カードID}(듀얼리스트,選択カード)
				CALL XYZSUMMON(듀얼리스트, 選択カード)
			ENDIF
		ENDIF
	ENDIF
		    

