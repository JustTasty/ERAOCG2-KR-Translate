@CARD_EXPLANATION_73791(種類)
#DIM DYNAMIC 種類

CALLFORM CARD_ABLE(1,魔罠起動,手札起動)
PRINTL ①：自分の墓地の「문장수」モンスター２体を対象として発動できる。
PRINTL そのモンスターを特殊召喚する。その後、フィールドのモンスターを素材とし、ランク４Xモンスター１体をX召喚する。


@CARDCAN_73791(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,73791)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 || 種類 == 手札起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	CALL FULLRESET
	効果範囲戦場 = 1
	効果条件CNT = 1
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT >= 4
		RETURN 0
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件カテゴリ = 문장수
	効果条件種類 = EX以外全般
	効果条件召喚可 = 1	
	効果条件CNT = 1
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73791(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

CALL 対面者判定(決闘者)
対面者 = %RESULTS%


IF 種類 == 魔罠起動 || 種類 == 手札起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,73791,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		FOR カウンタ,0,2
			CALL FULLRESET
			効果範囲墓地 = 1
			効果条件カテゴリ = 문장수
			効果条件種類 = EX以外全般
			CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
		NEXT
		CALL FULLRESET
		効果条件種類 = エクシーズ
		効果条件レベル = 4
		効果条件召喚可 = 1
		CALL CARD_CHOOSE_BASIC(決闘者,"EXデッキ",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
		    選択カードID = RESULT:99
				TRYCALLFORM CARDSUMMON_{選択カードID}(決闘者,選択カード)
				CALL XYZSUMMON(決闘者, 選択カード)
			ENDIF
		ENDIF
	ENDIF
		    

