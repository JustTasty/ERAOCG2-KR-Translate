@CARDNAME_38373,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 문장수 두 머리의 이글
	CASE "略称"
		RESULTS = TH 이글
	CASE "カテゴリ"
		RESULTS:0 = 문장수
ENDSELECT


@CARD_38373(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 효과モン
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1200
	CASE "守備力"
		RETURN 1400
	CASE "추가日"
		RETURN 250713
ENDSELECT
RETURN 0

@CARD_EXPLANATION_38373(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①②の효과は１ターンに１度しか使用できない。
CALL CARD_ABLE(1,손패起動)
PRINTL ①：자신フィールドに「문장수」モンスターが存在する場合に발동できる。
PRINTL 손패のこのモンスターを특수소환する。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：묘지のこのカードをゲームから제외し、발동できる。
PRINTL 자신フィールド上のX素材の無いXモンスター１体のX素材を２増やす。


@CARDCAN_38373(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,38373)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,38373)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,38373,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 문장수
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 발동済_2(듀얼리스트,38373)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0	
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 엑시즈
	효과条件Ｘ素所持 = 888
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF	

@CARDEFFECT_38373(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動 
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38373,1)
	IF RESULT == 0
        CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38373,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류 = 엑시즈
		효과条件Ｘ素所持 = 888
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"Ｘ",2)
		ENDIF
	ENDIF
ENDIF







