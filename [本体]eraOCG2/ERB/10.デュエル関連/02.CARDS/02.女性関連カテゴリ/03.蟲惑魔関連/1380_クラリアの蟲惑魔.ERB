@CARDNAME_1380,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 클라리아의 충혹마
	CASE "略称"
		RESULTS = 클라리아
	CASE "カテゴリ"
		RESULTS:0 = 충혹마
ENDSELECT


@CARD_1380(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_1380(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종족:0 = 곤충족
素材条件종족:1 = 식물족
;素材条件R종족起動 = 1

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,1380) 
	RETURN RESULT
ENDIF

