@CARDNAME_1376,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 란카의 충혹마
	CASE "略称"
		RESULTS = 란카
	CASE "カテゴリ"
		RESULTS:0 = 충혹마
ENDSELECT


@CARD_1376(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1300
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1376(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,소환時)
PRINTL ①：이 카드를 일반 소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 「충혹마」 몬스터 1장을 패에 넣는다.
CALLFORM CARD_ABLE(1,필드起動,フリー誘発)
PRINTL ②：자신 / 상대의 턴에, 자신 필드의 마법 / 함정 카드 1장을 대상으로 하고 발동할 수 있다.
PRINTL 그 카드를 주인의 패로 되돌린다. 그 후, 패에서 함정 카드 1장을 세트할 수 있다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ③：이 카드는 함정 내성을 가진다.


@CARDCAN_1376(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1376)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 충혹마
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,1376)
		RETURN 0
	SIF 마함数(듀얼리스트) == 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1376(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード


IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1376)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般
		효과条件カテゴリ = 충혹마
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1376,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件行先플래그 = 손패行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"마함",ゾーン,場所)	
		IF RESULT == 1
			CALL FULLRESET
			효과条件종류 = 함정全般
			효과条件任意 = 1
			CALL CARD_SET_BASIC(듀얼리스트,"손패",ゾーン,場所,"裏側표시")
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"함정耐")
ENDIF

