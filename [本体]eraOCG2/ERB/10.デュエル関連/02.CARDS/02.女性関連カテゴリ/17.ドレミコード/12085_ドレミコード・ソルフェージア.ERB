@CARDNAME_12085,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 도레미코드 솔페지아
	CASE "略称"
		RESULTS = 솔페지아
	CASE "カテゴリ"
		RESULTS:0 = 도레미코드
ENDSELECT


@CARD_12085(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 400
	CASE "守備力"
		RETURN 400
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 펜듈럼
	CASE "추가日"
		RETURN 250817
ENDSELECT
RETURN 0

@CARD_EXPLANATION_12085(종류)
#DIM DYNAMIC 종류


CALL TEXT_DECORATION("P_MAGIC")
PRINTL 이 카드명의 ①②의 P효과는 １턴에 １번밖에 사용할 수 없으며、
PRINTL 자신의 마법＆함정 존에 이 카드명이 존재하지 않는 경우에 패에서 지속 마법으로 발동할 수 있다.
CALLFORM CARD_ABLE(1,마함起動)
PRINTL ①：자신 메인 페이즈에 발동할 수 있다.
PRINTL 패 / EX덱에서 레벨 ５ 이상의 "도레미코드" 몬스터 １장을 특수 소환할 수 있다.
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②：상대가 필드에서 발동한 몬스터의 효과의 처리 시에、
PRINTL 자신 필드에 "도레미코드" L 몬스터가 존재하는 경우、그 발동한 효과를 무효로 할 수 있다.
PRINTL 그 후、이 카드를 파괴한다.

CALL TEXT_DECORATION("P_MAGIC_2")
PRINTL 이 카드명의 ①②의 몬스터 효과는 각각 １턴에 １번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(3,손패起動)
PRINTL ①：자신 필드의 몬스터가 존재하지 않거나 "도레미코드"몬스터만인 경우에 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환할 수 있다.
CALLFORM CARD_ABLE(4,필드起動)
PRINTL ②：자신 메인 페이즈에 발동할 수 있다（이 카드를 릴리스 하여 발동하는 것도 가능하다）。
PRINTL 패에서 "도레미코드 솔페지아" 이외의 "도레미코드"몬스터 １장을 특수 소환할 수 있다.
PRINTL 이 카드를 릴리스하여 발동한 경우、특수 소환하는 몬스터를 자신의 EX덱（앞면） / 묘지에서 선택할 수 있다。

@CARDSUMMON_AA_12085


@CARDCAN_12085(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 효과使用불가,3

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,12085)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		효과使用불가:0 = 1
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前 = 도레미코드 솔페지아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		효과使用불가:0 = 1

	SIF 발동済_3(듀얼리스트,12085)
		효과使用불가:1 = 1
	CALL CARD_SS_CHECK(듀얼리스트,12085,"손패")
	SIF RESULT == 1
		효과使用불가:1 = 1
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件固カテゴリ = 도레미코드
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 0
		효과使用불가:1 = 1

	SIF 효과使用불가:0 == 1 && 효과使用불가:1 == 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 마함起動
	SIF 발동済_1(듀얼리스트,12085)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲EX덱 = 1
	효과条件カテゴリ = 도레미코드
	효과条件종류 = EX以外全般
	CALL 효과条件レベル_ADV(5, "以上")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,12085)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 도레미코드
	효과条件종류 = 링크
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_4(듀얼리스트,12085)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과範囲EX덱 = 1
	효과条件カテゴリ = 도레미코드
	효과条件固名前 = 도레미코드 솔페지아
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_12085(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 条件A
#DIM DYNAMIC 条件B
#DIM DYNAMIC 효과使用불가,3

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 손패起動
	SIF 마함数(듀얼리스트) >= 5
		효과使用불가:0 = 1
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前 = 도레미코드 솔페지아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		효과使用불가:0 = 1

	SIF 발동済_2(듀얼리스트,12085)
		효과使用불가:1 = 1
	CALL CARD_SS_CHECK(듀얼리스트,12085,"손패")
	SIF RESULT == 1
		효과使用불가:1 = 1
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件カテゴリ = 도레미코드
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		효과使用불가:1 = 1

	IF 효과使用불가:0 == 0 && 효과使用불가:1 == 0
		PRINTL 지속 마법으로 도레미코드 솔페지아를 발동하시겠습니까？
		CALL YN_CHOICE(듀얼리스트)
	ELSEIF 효과使用불가:1 == 1
		RESULT = はい
	ELSEIF 효과使用불가:0 == 1
		RESULT = いいえ
	ENDIF

	IF RESULT == はい
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,12085)
		IF RESULT == 0
			CALL FULLRESET
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		ENDIF
	ELSEIF RESULT == いいえ
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,12085,3)
		IF RESULT == 0
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,12085,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲EX덱 = 1
		효과条件カテゴリ = 도레미코드
		효과条件종류 = EX以外全般
		CALL 효과条件レベル_ADV(5, "以上")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,12085,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,破壊行き)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,12085,4)
	IF RESULT == 0
		IF モンスター数(듀얼리스트) >= 5
			RESULT = はい
		ELSE
			CALL YN_CHOICE(듀얼리스트,"이 카드를 릴리스한다","이 카드를 릴리스하지 않는다")
		ENDIF
		IF RESULT == はい
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲묘지 = 1
			효과範囲EX덱 = 1
			효과条件カテゴリ = 도레미코드
			효과条件固名前 = 도레미코드 솔페지아
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ELSE
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = 도레미코드
			효과条件固名前 = 도레미코드 솔페지아
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF


