@CARDNAME_71622,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 창람 함대
	CASE "略称"
		RESULTS = 창람 함대
	CASE "カテゴリ"
		RESULTS:0 = 아쿠아포스
		RESULTS:1 = 창람
		RESULTS:2 = 바다
		RESULTS:4 = 뱅가드
ENDSELECT


@CARD_71622(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 지속마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_71622(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 "바다"가 된다。
PRINTL 이 카드 이름의 ①③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動)
PRINTL ①: 자신의 마법 / 함정 존의 "아쿠아포스" P 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드를 파괴하고, 자신 덱에서 "아쿠아포스" P 몬스터 1장을 손에 넣는다.
CALLFORM CARD_ABLE(2,バトル開始時)
PRINTL ②: 자신 배틀 페이즈 개시시에 발동할 수 있다.
PRINTL 자신 필드의 엑스트라 몬스터 이외의 물 속성 몬스터 1장을 고르고,
PRINTL 그 몬스터의 공격 가능 횟수를 1회 증가시킨다。
CALLFORM CARD_ABLE(0,攻撃後誘発)
PRINTL ③: 자신 배틀 페이즈 중에 자신 바다족 몬스터는 전투로 파괴되지 않으며,
PRINTL 자신의 바다족 몬스터의 전투로 발생하는 자신에게의 전투 데미지는 0이 된다。

@CARDCAN_71622(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71622)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,71622)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件カテゴリ = 아쿠아포스
	효과条件分割 = 펜듈럼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 아쿠아포스
	효과条件分割 = 펜듈럼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	RETURN 1
ELSEIF 종류 == バトル開始時
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_種:%듀얼리스트%_戦闘カード") != 바다생물족
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_71622(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カード名前
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71622,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件カテゴリ = 아쿠아포스
		효과条件分割 = 펜듈럼
		효과条件行先플래그 = 破壊行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件カテゴリ = 아쿠아포스
			효과条件分割 = 펜듈럼
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71622)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71622,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲종류 = EX以外全般
		효과条件속성 = 물속성
		효과条件표시 = 表側표시
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",RESULT,"戦",1)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "海")
	SIF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과条件종족 = 바다생물족
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦耐")
ELSEIF 종류 == 攻撃時強制誘発
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3)
ENDIF