@CARDNAME_52290,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 셀레브로즈 럭셔리 매지션즈
	CASE "略称"
		RESULTS = CR럭셔리
	CASE "カテゴリ"
		RESULTS:0 = 셀레브로즈
ENDSELECT


@CARD_52290(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 0
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족 
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3200
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		SELECTCASE RAND:2
			CASE 0
				RESULT:1 = 2321
				RETURN 2320
			CASE 1
				RESULT:1 = 2320
				RETURN 2321
		ENDSELECT
	CASE "추가日"
		RETURN 250720
ENDSELECT
RETURN 0

@CARD_EXPLANATION_52290(종류)
#DIM DYNAMIC 종류

PRINTL 「셀레브로즈・マジシャン」＋「셀레브로즈・ウィッチ」
PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：このカードを융합소환した場合に발동できる。
PRINTL 묘지の「셀레브로즈 럭셔리 매지션즈」以外の「셀레브로즈」モンスター１体を대상とし특수소환する。
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②：상대がカードの효과を발동した場合、フィールドの装備마법カード１枚を묘지に送って발동できる。
PRINTL その발동を無効にし破壊する。
CALLFORM CARD_ABLE(3,지속효과)
PRINTL ③：このカードは装備마법カードを装備している場合、貫通攻撃を得て１ターンに２回攻撃できる。

@CARDSUMMON_52290(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件名前 = 셀레브로즈・マジシャン
ELSEIF EX参照 >= 10
	素材条件名前 = 셀레브로즈・ウィッチ
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,52290) 
	RETURN RESULT
ENDIF

@CARDCAN_52290(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,52290)
		RETURN 0
	SIF モンスター数(듀얼리스트) == 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 셀레브로즈
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,52290)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件종류 = 装備마법
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_EQUIP_CHECK(듀얼리스트,場所,ゾーン)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52290(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52290,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 셀레브로즈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
	
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52290,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 装備마법
		효과条件표시 = 10
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"마함",ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		RETURN 1
	ENDIF
	RETURN 0
	
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",1)
ENDIF