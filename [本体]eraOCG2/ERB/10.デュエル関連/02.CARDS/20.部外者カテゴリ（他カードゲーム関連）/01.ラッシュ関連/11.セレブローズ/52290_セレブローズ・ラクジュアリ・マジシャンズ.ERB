@CARDNAME_52290,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 셀레브로즈 럭셔리 매지션즈
	CASE "略称"
		RESULTS = CR럭셔리
	CASE "カテゴリ"
		RESULTS:0 = 셀레브로즈
ENDSELECT


@CARD_52290(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 0
	CASE "種類"
		RETURN 融合
	CASE "属性"
		RETURN 光属性
	CASE "種族"
		RETURN 魔法使い族 
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3200
	CASE "守備力"
		RETURN 1900
	CASE "性別"
		RETURN 牝性
	CASE "連動ID"
		SELECTCASE RAND:2
			CASE 0
				RESULT:1 = 2321
				RETURN 2320
			CASE 1
				RESULT:1 = 2320
				RETURN 2321
		ENDSELECT
	CASE "追加日"
		RETURN 250720
ENDSELECT
RETURN 0

@CARD_EXPLANATION_52290(種類)
#DIM DYNAMIC 種類

PRINTL 「셀레브로즈・マジシャン」＋「셀레브로즈・ウィッチ」
PRINTL このカード名の①②の効果はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,EX召喚時)
PRINTL ①：このカードを融合召喚した場合に発動できる。
PRINTL 墓地の「셀레브로즈 럭셔리 매지션즈」以外の「셀레브로즈」モンスター１体を対象とし特殊召喚する。
CALLFORM CARD_ABLE(2,発動誘発)
PRINTL ②：相手がカードの効果を発動した場合、フィールドの装備魔法カード１枚を墓地に送って発動できる。
PRINTL その発動を無効にし破壊する。
CALLFORM CARD_ABLE(3,永続効果)
PRINTL ③：このカードは装備魔法カードを装備している場合、貫通攻撃を得て１ターンに２回攻撃できる。

@CARDSUMMON_52290(決闘者,召喚カード,EX参照)
#DIMS DYNAMIC 決闘者
#DIM DYNAMIC 召喚カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件名前 = 셀레브로즈・マジシャン
ELSEIF EX参照 >= 10
	素材条件名前 = 셀레브로즈・ウィッチ
ENDIF

IF EX参照 == 1
	CALL FUSCAN(決闘者,召喚カード,52290) 
	RETURN RESULT
ENDIF

@CARDCAN_52290(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

IF 種類 == EX召喚時
	SIF 発動済_1(決闘者,52290)
		RETURN 0
	SIF モンスター数(決闘者) == 5
		RETURN 0
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件カテゴリ = 셀레브로즈
	効果条件召喚可 = 1
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 種類 == 発動誘発
	SIF 発動済_2(決闘者,52290)
		RETURN 0
	SIF ゾーン != "戦場"
		RETURN 0
	CALL FULLRESET
	効果範囲魔罠 = 1
	効果条件種類 = 装備魔法
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(決闘者,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 種類 == 永続効果
	SIF ゾーン != "戦場"
		RETURN 0
	CALL FULLRESET
	CALL CARD_EQUIP_CHECK(決闘者,場所,ゾーン)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52290(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件達成

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

IF 種類 == EX召喚時
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,52290,1)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲墓地 = 1
		効果条件カテゴリ = 셀레브로즈
		CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
	ENDIF
	
ELSEIF 種類 == 発動誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,52290,2)
	IF RESULT == 0
		CALL FULLRESET
		効果条件種類 = 装備魔法
		効果条件表示 = 10
		CALL CARD_ADD_GY_BASIC(決闘者,"魔罠",ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(決闘者,"発動無効",場所,"墓地")
		RETURN 1
	ENDIF
	RETURN 0
	
ELSEIF 種類 == 永続効果
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"貫通")
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"追戦",1)
ENDIF