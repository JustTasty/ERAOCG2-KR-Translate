@CARDNAME_52290,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 셀레브로즈 럭셔리 매지션즈
	CASE "略称"
		RESULTS = CR럭셔리
	CASE "カテゴリ"
		RESULTS:0 = 셀레브로즈
ENDSELECT


@CARD_52290(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 0
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족 
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3200
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		SELECTCASE RAND:2
			CASE 0
				RESULT:1 = 2321
				RETURN 2320
			CASE 1
				RESULT:1 = 2320
				RETURN 2321
		ENDSELECT
	CASE "추가日"
		RETURN 250720
ENDSELECT
RETURN 0

@CARD_EXPLANATION_52290(종류)
#DIM DYNAMIC 종류

PRINTL 「셀레브로즈・マジシャン」＋「셀레브로즈・ウィッチ」
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：이 카드를 융합소환した경우에 발동할 수 있다。
PRINTL 묘지의 「셀레브로즈 럭셔리 매지션즈」이외의 「셀레브로즈」몬스터 １장을 대상とし특수 소환한다。
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②：상대がカードの효과를 발동한 경우、필드의 装備마법 카드 １장을 묘지에 送って발동할 수 있다。
PRINTL その발동を無効にし파괴한다。
CALLFORM CARD_ABLE(3,지속효과)
PRINTL ③：이 카드는 装備마법카드를 装備している경우、貫通攻撃を得て１ターンに２回攻撃できる。

@CARDSUMMON_52290(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件名前 = 셀레브로즈・マジシャン
ELSEIF EX参照 >= 10
	素材条件名前 = 셀레브로즈・ウィッチ
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,52290) 
	RETURN RESULT
ENDIF

@CARDCAN_52290(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,52290)
		RETURN 0
	SIF モンスター数(듀얼리스트) == 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 셀레브로즈
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,52290)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件종류 = 装備마법
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_EQUIP_CHECK(듀얼리스트,場所,ゾーン)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52290(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52290,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 셀레브로즈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
	
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52290,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 装備마법
		효과条件표시 = 10
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"마함",ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		RETURN 1
	ENDIF
	RETURN 0
	
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",1)
ENDIF