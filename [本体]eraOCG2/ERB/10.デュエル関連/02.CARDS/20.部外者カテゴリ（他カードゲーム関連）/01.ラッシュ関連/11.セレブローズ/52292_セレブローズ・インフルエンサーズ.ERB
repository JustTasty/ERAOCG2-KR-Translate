@CARDNAME_52292,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 셀레브로즈 인플루언서즈
	CASE "略称"
		RESULTS = CR인플루
	CASE "カテゴリ"
		RESULTS:0 = 셀레브로즈
ENDSELECT


@CARD_52292(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 0
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 1600
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		SELECTCASE RAND:2
			CASE 0
				RESULT:1 = 2324
				RETURN 2326
			CASE 1
				RESULT:1 = 2326
				RETURN 2324
		ENDSELECT
	CASE "추가日"
		RETURN 250720
ENDSELECT
RETURN 0

@CARD_EXPLANATION_52292(종류)
#DIM DYNAMIC 종류

PRINTL 「셀레브로즈・エンチャンター」＋「셀레브로즈・ウィズ」
PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：このカードが융합소환した場合に발동できる。
PRINTL 덱から「셀레브로즈」カード１枚を묘지に送る。
CALLFORM CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ②：자신・상대ターンに발동できる。상대の묘지のモンスター１体を選んで덱に戻す。
PRINTL このカードの攻撃力はターン終了時まで[この효과で戻したモンスターのコスト]×２００アップする。
CALLFORM CARD_ABLE(3,지속효과)
PRINTL ③：このカードは装備마법カードを装備している場合、효과では破壊されず、１ターンに２回攻撃できる。


@CARDSUMMON_52292(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件名前 = 셀레브로즈・エンチャンター
ELSEIF EX参照 >= 10
	素材条件名前 = 셀레브로즈・ウィズ
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,52292) 
	RETURN RESULT
ENDIF

@CARDCAN_52292(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,52292)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 셀레브로즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,52292)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_EQUIP_CHECK(듀얼리스트,場所,ゾーン)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52292(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択カードレベル
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52292,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 셀레브로즈
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
	
ELSEIF 종류 ==필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52292,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般
		효과範囲상대묘지 = 1
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"덱")
		IF RESULT == 1
			選択カード_ID = RESULT:2
			TRYCALLFORM CARD_{選択カード_ID},"レベル"
			選択カードレベル = RESULT
			攻撃力修正 = 200 * 選択カードレベル
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"一攻",攻撃力修正)
		ENDIF
	ENDIF
	
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",1)
	
ENDIF
