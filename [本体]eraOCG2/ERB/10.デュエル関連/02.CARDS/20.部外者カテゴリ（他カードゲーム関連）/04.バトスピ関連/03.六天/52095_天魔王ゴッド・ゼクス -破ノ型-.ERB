@CARDNAME_52095,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 천마왕 갓 젝스 -파괴형-
	CASE "略称"
		RESULTS = 파괴형
	CASE "カテゴリ"
		RESULTS = 0 = 천마왕
		RESULTS = 4 = 배틀 스피리츠
ENDSELECT


@CARD_52095(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 0
	CASE "종류"
		RETURN 초차원
	CASE "속성"
		RESULT:1 = 화염속성
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3500
	CASE "守備力"
		RETURN 2500
	CASE "連動ID"
		RETURN 4126
ENDSELECT

RETURN 0


@CARD_EXPLANATION_52095(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("INVASION")
PRINTL 이 카드의 속성은 불꽃으로도 취급한다.
PRINTL 이 카드명의 ①②③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,攻撃時誘発)
PRINTL 『P침략』
PRINTL 자신의 초차원이 아닌 「천마왕」 몬스터가 공격할 경우에 발동할 수 있다.
PRINTL 자신의 EX 덱에 있는 이 카드를 그 위에 겹쳐도 좋다. (공격은 계속된다)
CALLFORM CARD_ABLE(2,특수소환時)
PRINTL ②: 이 카드가 특수 소환에 성공했을 경우, 손패를 1장 EX 덱으로 보내는 것으로, 발동할 수 있다.
PRINTL 상대 필드의 카드 1장을 파괴한다.
PRINTL (이 효과는 대상 내성・몬스터 내성의 영향을 받지 않는다)
CALLFORM CARD_ABLE(3,필드起動,フリー誘発)
PRINTL ③: 상대 필드에 존재하는 몬스터의 속성을 모두 선언한다.
PRINTL 선언할 때마다, 선언한 속성과 동일한 속성을 가진 상대 필드의 몬스터 1장을 파괴한다.
PRINTL 『육천연쇄: 조건《속성 4색》』
PRINTL 자신 필드상의 몬스터의 속성이 4종류 이상일 경우, 이 효과는 상대 턴에서도 발동할 수 있다.
CALL TEXT_DECORATION("INVASION")

@CARDSUMMON_52095(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

RETURN 0


@CARDCAN_52095(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52095)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻撃時誘発
	SIF ゾーン != "EX덱"
		RETURN 0
	SIF 발동済_1(듀얼리스트,52095)
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) == 5
		RETURN 0
	TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_필드:戦闘場所(듀얼리스트)")},"종류"
	SIF RESULT == 초차원
		RETURN 0
	TRYCALLFORM CARDNAME_{戦闘カードID(듀얼리스트)},"カテゴリ"
	SIF !MATCH(RESULTS, "천마왕")
		RETURN 0
	RETURN 1
ELSEIF 종류 == 특수소환時
	SIF 발동済_2(듀얼리스트,52095)
		RETURN 0
	SIF 손패数(듀얼리스트) < 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_3(듀얼리스트,52095)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
	CALL FIELD_INFO_COUNT(듀얼리스트,"属")
	IF RESULT < 4
		SIF メインフェイズ != 1
			RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
		SIF 종류 == フリー誘発
			RETURN 0
	ENDIF
ENDIF

@CARDEFFECT_52095(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カードID


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52095,1)
	IF RESULT == 0
		CALL INVASION(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52095,2)
	IF RESULT == 0		
		CALL FULLRESET
		효과条件任意 = 1
		효과条件行先플래그 = EX덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52095,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件속성 = 바람속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			SETCOLOR C_GREEN
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL
PRINTFORMW 바람!
			PRINTL
			CUSTOMDRAWLINE ―	
			PRINTFORML 
			CALL FULLRESET
			효과条件속성 = 바람속성
			효과条件표시 = 表側표시
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"묘지")
		ENDIF

		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件속성 = 화염속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			SETCOLOR C_RED
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL
PRINTFORMW 불꽃!
			PRINTL
			CUSTOMDRAWLINE ―	
			PRINTFORML 
			CALL FULLRESET
			효과条件속성 = 화염속성
			효과条件표시 = 表側표시
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"묘지")
		ENDIF

		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件속성 = 땅속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			SETCOLOR 0xb8860b
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL
PRINTFORMW 땅!
			PRINTL
