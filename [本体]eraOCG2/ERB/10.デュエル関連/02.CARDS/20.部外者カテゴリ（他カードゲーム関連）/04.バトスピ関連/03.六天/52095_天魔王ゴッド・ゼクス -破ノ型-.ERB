			PRINTFORML 
			CALL FULLRESET
			효과条件속성 = 땅속성
			효과条件표시 = 表側표시
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"묘지")
		ENDIF

		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件속성 = 물속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			SETCOLOR C_BLUE
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL
PRINTFORMW 물!
			PRINTL
			CUSTOMDRAWLINE ―	
			PRINTFORML 
			CALL FULLRESET
			효과条件속성 = 물속성
			효과条件표시 = 表側표시
