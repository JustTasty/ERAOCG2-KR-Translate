		IF RESULT == 1
			SETCOLOR C_PURPLE
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL
PRINTFORMW 어둠!
			PRINTL
			CUSTOMDRAWLINE ―	
			PRINTFORML 
			CALL FULLRESET
			효과条件속성 = 어둠속성
			효과条件표시 = 表側표시
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"묘지")
		ENDIF
	ENDIF
ENDIF