@CARDNAME_4126,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4126)

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 젝시아 텐마
	CASE "略称"
RESULTS = 젝시아
	CASE "カテゴリ"
		RESULTS:0 = 시희
		IF TALENT:カード主:연모
			RESULTS:1 = 천마왕
		ENDIF
		IF TALENT:カード主:애처
			RESULTS:2 = 계약
		ENDIF
		RESULTS:4 = 배틀 스피리츠
ENDSELECT


@CARD_4126(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RESULT:1 = 바람속성
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_4126(종류)
#DIM DYNAMIC 종류
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4126)

IF TALENT:カード主:애처
PRINTL 이 카드의 속성은 바람으로도 취급한다.
PRINTL 이 카드명의 ①③의 효과는 각각 1턴에 1번만 사용할 수 없다.
	CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：자신 필드의 코어 카운터를 1개 제거하고 발동할 수 있다.
PRINTL 이 카드를 손패에서 특수 소환한다.
	CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②：자신의 묘지의 임의의 몬스터와 같은 속성을 가진 자신의 몬스터는 파괴 내성을 얻는다.
PRINTL 자신의 마법/함정 존의 「창계신」에 놓인 코어 카운터의 수가 10개 이상일 경우,
PRINTL 자신 턴에, 자신 필드의 몬스터는 완전 내성을 얻는다.
	CALLFORM CARD_ABLE(3,필드起動)
PRINTL ③：자신의 마법/함정 존의 「창계신」에 코어 카운터를 2개 놓는 것으로 발동할 수 있다.
PRINTL 상대 필드의 몬스터 1장을 선택할 수 있다.
PRINTL 그 몬스터의 공격력을 자신 필드의 몬스터의 속성의 종류 수 ×1000 포인트 다운시킨다.
PRINTL 이 효과로 공격력 또는 수비력이 0이 되었을 경우, 추가로 그 몬스터를 묘지로 보낸다.
ELSE
PRINTL 이 카드의 속성은 바람으로도 취급한다.
	CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：자신의 묘지의 임의의 몬스터와 같은 속성을 가진 자신의 몬스터는 파괴 내성을 얻는다.
ENDIF

@CARDCAN_4126(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4126)

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4126)
SIF RESULT == 1
	RETURN 0


IF 종류 == 손패起動
	IF TALENT:カード主:애처
		SIF 발동済_1(듀얼리스트,4126)
			RETURN 0
		SIF メインフェイズ != 1
			RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
		CALL CARD_SS_CHECK(듀얼리스트,4126,"손패")
		SIF RESULT == 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 배틀 스피리츠
		효과条件표시 = 表側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT:9 < 1
			RETURN 0
	ELSE
		RETURN 0	
	ENDIF
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	IF TALENT:カード主:애처
		SIF メインフェイズ != 1
			RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
		SIF 발동済_3(듀얼리스트,4126)
			RETURN 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件표시 = 表側표시
효과条件カテゴリ = 창조신
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		RETURN 1
	ELSE
		RETURN 0	
	ENDIF
ENDIF

@CARDEFFECT_4126(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 軽減数
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4126)


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4126,1)
	IF RESULT == 0
		PRINTL コア카운터を取り除くカードを選択
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 배틀 스피리츠
		효과条件표시 = 表側표시
		효과条件個Ｃ所持 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
PRINTL 코어가 제거되었습니다
			CALL CARD_SELECT(듀얼리스트,RESULT)
			CALL SET_FIELD_INFO(대상者（CS）,参照先（CS）,選択カード（CS）,"個Ｃ",-1)

			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 바람속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 바람속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 화염속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 땅속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 물속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 빛속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 빛속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 어둠속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 어둠속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 신속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件속성 = 신속성
		효과条件표시 = 表側표시
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF

	IF TALENT:カード主:애처
		CALL FULLRESET
		효과範囲마함 = 1
효과条件カテゴリ = 창조신
		효과条件표시 = 表側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT:9 < 10
			RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0

		CALL FULLRESET
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"モ耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"魔耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"함정耐")
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4126,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
효과条件カテゴリ = 창조신
		효과条件표시 = 表側표시
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			CALL SET_FIELD_INFO(듀얼리스트,参照先（CS）,選択カード（CS）,"個Ｃ",2)
		ENDIF

		CALL FULLRESET
		CALL FIELD_INFO_COUNT(듀얼리스트,"属")
		攻撃力修正 = RESULT * 1000
		CALL FULLRESET
		효과条件표시 = 表側표시
		효과추가종류 = 11
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"永攻DOWN",攻撃力修正)
	ENDIF
ENDIF

