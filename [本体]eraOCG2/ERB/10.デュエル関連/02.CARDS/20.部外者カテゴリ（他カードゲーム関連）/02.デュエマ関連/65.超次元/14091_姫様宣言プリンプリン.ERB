@CARDNAME_14091,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 프린세스 러쉬 푸링푸링
	CASE "略称"
		RESULTS = 공주님선언푸링
	CASE "カテゴリ"
		RESULTS:0 = 커맨드
		RESULTS:1 = 헌터
		RESULTS:2 = 에일리언
		RESULTS:4 = DUEL MASTERS
ENDSELECT


@CARD_14091(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RESULT:2 = 물속성
		RESULT:1 = 화염속성
		RETURN 바람속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250716
ENDSELECT
RETURN 0


@CARD_EXPLANATION_14091(종류)
#DIM DYNAMIC 종류
PRINTL このカード名の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：자신フィールドに「사이킥」モンスターが存在する場合に발동できる。
PRINTL 손패を１枚捨て、このカードを손패から특수소환する。
CALLFORM CARD_ABLE(0,특수소환時,攻撃後誘発)
PRINTL ②：このカードが특수소환した場合または、戦闘で상대モンスターを破壊した場合に발동できる。
PRINTL EX덱からレベル８以下の「사이킥」초차원モンスター１体を특수소환する。

@CARDCAN_14091(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,14091)
SIF RESULT == 1
	RETURN 0
IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,14091)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,14091,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 손패数(듀얼리스트) == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 사이킥
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 특수소환時 || 종류 == 攻撃後誘発
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	IF 종류 == 攻撃後誘発
		SIF ゾーン != "필드"
			RETURN 0
		SIF 戦闘場所(듀얼리스트) != 場所
			RETURN 0
		SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 초차원
	효과条件カテゴリ = 사이킥
	CALLFORM 효과条件レベル_ADV(8, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_14091(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード_ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,14091,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件他所 = 1
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)			
		ENDIF
	ENDIF
ELSEIF 종류 == 특수소환時 || 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,14091)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 초차원
		효과条件カテゴリ = 사이킥
		CALLFORM 효과条件レベル_ADV(8, "以下")
		CALL DIMSUMMON(듀얼리스트,ゾーン,場所,8)
	ENDIF
ENDIF
