		RETURN 0

	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦無")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"無差耐")
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
효과条件名前 = 신제 루나틱 갓
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,GETVAR(@"%듀얼리스트%_MZ_攻:場所"))
ENDIF


