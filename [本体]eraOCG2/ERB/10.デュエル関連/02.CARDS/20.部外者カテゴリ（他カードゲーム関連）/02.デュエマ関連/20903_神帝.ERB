	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 신제 루나틱 갓
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦無")
