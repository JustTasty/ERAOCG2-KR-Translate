@CARDCAN_120903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,120903)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_3(듀얼리스트,120903)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_120903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 神帝
	효과条件他所 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"攻避")

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件名前 = 神帝ルナティック・ゴッド
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件名前 = 神帝ヴィシュ
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件名前 = 神帝アージュ
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件名前 = 神帝ムーラ
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"魔耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",98)
	RETURN 1
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ:0 = オリジン
	효과条件カテゴリ:1 = ゴッド
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	攻撃力修正 = RESULT * 200
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,120903,3)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		효과추가종류 = 11
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"永攻DOWN",2000)
	ENDIF
ENDIF



