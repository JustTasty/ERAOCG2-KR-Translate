@CARDCAN_20903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件他所 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ:0 = 오리진
	효과条件カテゴリ:1 = 갓
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 3
		RETURN 0
	RETURN 1
ELSEIF 종류 == チェーン可否
	RETURN 1
ENDIF

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,20903)
SIF RESULT == 1
	RETURN 0


@CARDEFFECT_20903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20903)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,消滅行き)

		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = モン全般
		효과条件カテゴリ:0 = 오리진
		효과条件カテゴリ:1 = 갓
		효과条件他所 = 1
		효과条件行先플래그 = 消滅行き
		효과条件複数 = 3
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			FOR カウンタ,0,5
				SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
					CONTINUE
				CALL CARD_DISPOSE(듀얼리스트,"필드",カウンタ)
			NEXT	
			
			CALL FULLRESET
			CALL CARD_GENERATE(듀얼리스트,120903,"攻撃표시")				
			CALL CARD_GENERATE(듀얼리스트,220903,"守備표시")	
			CALL CARD_GENERATE(듀얼리스트,320903,"守備표시")		
			CALL CARD_GENERATE(듀얼리스트,420903,"守備표시")	
		ENDIF
	ENDIF
ENDIF



