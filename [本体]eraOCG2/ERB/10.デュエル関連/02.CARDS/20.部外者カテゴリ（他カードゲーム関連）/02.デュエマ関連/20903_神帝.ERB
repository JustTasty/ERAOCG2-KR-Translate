	CALL FULLRESET
효과条件カテゴリ = 신제
	효과条件他所 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"攻避")

	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 신제 루나틱 갓
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 신제 비슈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 신제 아쥬
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 신제 무라
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"魔耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",98)
	RETURN 1
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ:0 = 오리진
	효과条件カテゴリ:1 = 갓
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	攻撃力修正 = RESULT * 200
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)
