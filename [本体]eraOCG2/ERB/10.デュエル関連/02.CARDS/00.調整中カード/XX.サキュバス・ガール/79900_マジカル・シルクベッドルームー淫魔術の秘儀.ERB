@CARDNAME_79900,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;マジカルシルクハット(名前だけ)
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 매지컬 실드 베드룸－음마술의 비의
	CASE "略称"
		RESULTS = 음마도의 계약장
	CASE "カテゴリ"
	    RESULTS:0 = 매지션 걸
		RESULTS:1 = 서큐버스 걸
		RESULTS:2 = E.R.A
ENDSELECT

@CARD_79900(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN フィールド
ENDSELECT
RETURN 0

@CARDSUMMON_79900(決闘者,召喚カード,EX参照)
#DIMS DYNAMIC 決闘者
#DIM DYNAMIC 召喚カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
RETURN 0

@CARD_EXPLANATION_79900(種類)
#DIM DYNAMIC 種類

CALL TEXT_DECORATION("ROGUE")
PRINTL 自分フィールドは「淫魔導の契りの間」となる。
PRINTL このカード名の①の効果は１ターンに１度しか使用できない。
CALL CARD_ABLE(1,手札起動,魔罠起動)
PRINTL ①：このカードの発動時の効果処理として、
PRINTL デッキから「매지션 걸」モンスター１体またはこのカード名以外の「서큐버스 걸」魔法・罠カード１枚を手札に加えることができる。
PRINTL この効果を発動するターン、自分は「서큐버스 걸」モンスターしかＥＸデッキから特殊召喚できない。
CALL CARD_ABLE(0,手札起動,魔罠起動)
PRINTL ②：自分メインフェイズに発動できる。
PRINTL 融合モンスターカードによって決められた１体目の融合素材モンスターだけを自分の手札・フィールドから墓地に送り、
PRINTL 自分フィールドに同名のカードが存在しない「서큐버스 걸」融合モンスター１体をＥＸデッキから特殊召喚する。
PRINTL その後、墓地に「賢者の宝液」トークンを１枚生成する。
PRINTL この効果では同じカード名のモンスターは１ターンに１度しか特殊召喚できず、
PRINTL この効果を発動するターン、自分は「서큐버스 걸」モンスターしかＥＸデッキから特殊召喚できない。
CALL CARD_ABLE(0,身代効果)
PRINTL ③：このカードが効果によって破壊される場合、代わりに墓地の「매지션 걸」魔法カード２枚を除外できる。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79900(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カード名
#DIM DYNAMIC 条件達成

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,79900)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 && GETVAR(@"%決闘者%_魔_表:場所") == 1
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	CALL FULLRESET
	効果条件固カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_FIN_DATA(決闘者,"EX履歴")
	SIF RESULT != 0
		RETURN 0
	;後続のEXデッキチェック内の召喚可 = 1とちょっと被ってるけど
	;その後続のEXデッキチェックの処理が省けるためここで確認しておく
	SIF モンスター数(決闘者) >= 5
		RETURN 0
	FOR カウンタ, 0, 現EX山札数(決闘者)
		選択カードID = GETVAR(@"%決闘者%_EXデッキ:カウンタ")
		SIF 選択カードID < 1
			CONTINUE
		CALL FULLRESET
		効果範囲戦場 = 1
		効果範囲魔罠 = 1
		効果条件ID = 選択カードID
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			CONTINUE
		CALL CARD_EX_CHECK(決闘者,選択カードID)
		SIF RESULT == 1
			CONTINUE
		TRYCALLFORM CARDNAME_{選択カードID}("カテゴリ")
		SIF !MATCH(RESULTS,"서큐버스 걸")
			CONTINUE
		;この効果による召喚履歴は発動効果１で記録
		SIF 発動済_1(決闘者,選択カードID)
			CONTINUE
		CALL RESETCONDITION_MAT
		TRYCALLFORM CARDSUMMON_{選択カードID}(決闘者,選択カードID,0)
		選択カード名 = %素材条件名前%
		SIF 選択カード名 == ""
			CONTINUE
		CALL FULLRESET
		効果範囲手札 = 1
		効果範囲戦場 = 1
		効果条件名前 = %選択カード名%
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		IF RESULT == 1
			条件達成++
			BREAK
		ENDIF
	NEXT
	SIF 条件達成 < 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 魔罠起動 || 種類 == 手札起動
	SIF 発動済_1(決闘者,79900)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	CALL FULLRESET
	効果条件固カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_FIN_DATA(決闘者,"EX履歴")
	SIF RESULT != 0
		RETURN 0
	RETURN 1
ELSEIF 種類 == 永続効果
	SIF ゾーン != "魔罠"
		RETURN 0
	SIF GETVAR(@"%決闘者%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ELSEIF 種類 == 身代効果
	SIF ゾーン != "魔罠"
		RETURN 0
	SIF GETVAR(@"%決闘者%_魔_表:場所") >= 2
		RETURN 0
	SIF 破壊種類 != "効果"
		RETURN 0
	SIF 破壊対象ゾーン != "魔罠"
		RETURN 0
	SIF 破壊対象場所 != 場所
		RETURN 0
	SIF GETVAR(@"%決闘者%_除外NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件カテゴリ = 서큐버스 걸
	効果条件種類 = 魔法全般
	効果条件CNT = 1
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79900(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カード名
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択効果
#DIM DYNAMIC 選択カード候補,VARSIZE("効果条件ID")

CALL 対面者判定(決闘者)
対面者 = %RESULTS%


IF 種類 == 魔罠起動 && GETVAR(@"%決闘者%_魔_表:場所") == 1
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79900)
	IF RESULT == 0
		FOR カウンタ, 0, 現EX山札数(決闘者)
			選択カードID = GETVAR(@"%決闘者%_EXデッキ:カウンタ")
			SIF 選択カードID < 1
				CONTINUE
			SIF MATCH(選択カード候補,選択カードID)
				CONTINUE
			CALL FULLRESET
			効果範囲戦場 = 1
			効果範囲魔罠 = 1
			効果条件ID = 選択カードID
			CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
			SIF RESULT == 1
				CONTINUE
			CALL CARD_EX_CHECK(決闘者,選択カードID)
			SIF RESULT == 1
				CONTINUE
			TRYCALLFORM CARDNAME_{選択カードID}("カテゴリ")
			SIF !MATCH(RESULTS,"서큐버스 걸")
				CONTINUE
			;この効果による召喚履歴は発動効果１で記録
			SIF 発動済_1(決闘者,選択カードID)
				CONTINUE
			CALL RESETCONDITION_MAT
			TRYCALLFORM CARDSUMMON_{選択カードID}(決闘者,選択カードID,0)
			選択カード名 = %素材条件名前%
			SIF 選択カード名 == ""
				CONTINUE
			CALL FULLRESET
			効果範囲手札 = 1
			効果範囲戦場 = 1
			効果条件名前 = %選択カード名%
			CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
			IF RESULT == 1
				選択カード候補:カウンタB = 選択カードID
				カウンタB++
				SIF カウンタB >= VARSIZE("効果条件ID")
					BREAK
			ENDIF
		NEXT
		CALL FULLRESET
		効果条件種類 = 融合
		効果条件カテゴリ = 서큐버스 걸
		FOR カウンタ, 0, カウンタB
			効果条件ID:カウンタ = 選択カード候補:カウンタ
		NEXT
		CALL CARD_CHOOSE_BASIC(決闘者,"EXデッキ",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			選択カードID = RESULT:99
			CALL RESETCONDITION_MAT
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%決闘者%_EXデッキ:{選択カード}")}(決闘者,選択カード,0)
			選択カード名 = %素材条件名前%
			CALL FULLRESET
			効果範囲手札 = 1
			効果範囲戦場 = 1
			効果条件名前 = %選択カード名%
			CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
			IF RESULT == 1
				CALL CARD_EFFECT_USE(決闘者,選択カードID,1)
				CALL CARD_EX_SUMMON(決闘者,"EXデッキ",選択カード)
				IF RESULT == 1
					CALL CARD_GENERATE_GY(決闘者,宝液ID)
				ENDIF
			ENDIF
		ENDIF
		CALL CARD_EX_RESTRICTION(決闘者,"서큐버스 걸")
	ENDIF
ELSEIF 種類 == 魔罠起動 || 種類 == 手札起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79900,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所,魔罠行き)
		CALL FULLRESET
		効果範囲デッキ = 1
		効果条件スーパーカテゴリ = 매지션 걸
		効果条件スーパー種類 = モン全般
		効果条件カテゴリ = 서큐버스 걸
		効果条件種類 = 魔罠全般
		効果条件固名前 = 매지컬 실드 베드룸－음마술의 비의
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			効果範囲デッキ = 1
			効果条件スーパーカテゴリ = 매지션 걸
			効果条件スーパー種類 = モン全般
			効果条件カテゴリ = 서큐버스 걸
			効果条件種類 = 魔罠全般
			効果条件固名前 = 매지컬 실드 베드룸－음마술의 비의
			CALL CARD_ADD_ADV(決闘者,ゾーン,場所)
		ENDIF
		CALL CARD_EX_RESTRICTION(決闘者,"서큐버스 걸")
	ENDIF
ELSEIF 種類 == 身代効果
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79900)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲墓地 = 1
		効果条件カテゴリ = 서큐버스 걸
		効果条件種類 = 魔法全般
		効果条件行先フラグ = 除外行き
		効果条件複数 = 2
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		IF RESULT == 1
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0	
ELSEIF 種類 == 永続効果
	CALL SET_FIELD_MAGIC_INFO(決闘者, "淫魔導の契りの間")
ENDIF
