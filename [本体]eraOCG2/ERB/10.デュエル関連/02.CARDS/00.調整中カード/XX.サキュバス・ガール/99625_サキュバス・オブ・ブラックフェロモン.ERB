@CARD_EXPLANATION_99625(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 자신フィールドにモンスターが存在しない場合、このカードは손패からも발동できる。
PRINTL このカード名の①の효과は１ターンに１度しか使用できない。
CALL CARD_ABLE(1,손패起動,마함起動,フリー誘発,발동誘発)
PRINTL ①：このカードをモンスターゾーンに특수소환する。
PRINTL 상대の성별を有するモンスターの효과にチェーンして발동した場合、
PRINTL さらに５００ＬＰを払うことで、その효과を無効にできる。
PRINTL この효과の발동後、자신は次のターン終了時まで、「서큐버스 걸」以外のモンスターをＥＸ덱から특수소환できない。
CALL CARD_ABLE(0,지속효과)
PRINTL ②：このカードがモンスターゾーンに存在する限り、자신フィールドの他の表側표시の「서큐버스 걸」カードは대상耐性を得る。
CALL CARD_ABLE(0,ターン終了時強制)
PRINTL ③：자신・상대エンドフェイズに、このカードがモンスターゾーンに存在する場合に발동する。
PRINTL 자신は５００ＬＰを失い、묘지に「현자의 보액」トークン１枚を生成する。
PRINTL その後、このカードを묘지に送る。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99625(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 選択カード名
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99625)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,99625)
		RETURN 0
	IF ゾーン == "손패"
		CALL FULLRESET
		효과範囲필드 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3 || GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(듀얼리스트,99625,ゾーン)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,99625)
		RETURN 0
	IF ゾーン == "손패"
		CALL FULLRESET
		효과範囲필드 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3 || GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(듀얼리스트,99625,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件성별 = 성별在
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	SIF GETVAR(@"ライフ_%듀얼리스트%") <= 1000 && GETVAR(@"%듀얼리스트%_コスト無効") < 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時予約
	RETURN 1
ENDIF

@CARDEFFECT_99625(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99625,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
		CALL CARD_RESERV(듀얼리스트,99625,スタン予約)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99625,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		PRINTL ５００ＬＰを払って상대が발동した효과を無効にしますか？
		CALL YN_CHOICE(듀얼리스트)
		IF RESULT == はい
			CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",500)
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
		ENDIF
		CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
		CALL CARD_RESERV(듀얼리스트,99625,スタン予約)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 서큐버스 걸
	효과条件他所 = 1
	효과条件표시 = 表側표시
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"対耐")
ELSEIF 종류 == ターン終了時強制
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99625)
	IF RESULT == 0
		CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",500)
		CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == ターン開始時予約
	CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
ENDIF