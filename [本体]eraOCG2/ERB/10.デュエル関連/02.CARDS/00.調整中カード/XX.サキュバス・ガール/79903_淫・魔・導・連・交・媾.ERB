PRINTL 代わりに자신フィールドの「서큐버스 걸」モンスターの数まで以下の효과を適用できる。
PRINTL ●자신の묘지の「서큐버스 걸」마법カードを１枚제외することで、상대フィールドのモンスター１枚を대상として효과を無効にする。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79903)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,79903)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,79903)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 매지션 걸
	효과条件スーパー名前 = 매지컬 실드 베드룸－음마술의 비의
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		条件달성++
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 서큐버스 걸
		효과条件종류 = 마법全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과条件除去種 = 無効
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件달성++
		ENDIF
	ENDIF
	SIF 条件달성 < 1
		RETURN 0
    RETURN 1
ENDIF

@CARDEFFECT_79903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択マジシャン
#DIMS DYNAMIC 選択マジシャン名
#DIM DYNAMIC 보정値

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79903,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 서큐버스 걸
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カードID = RESULT:99
		IF RESULT != 9999
			CALL LIFE_CHANGE(듀얼리스트,"増加","효과",1000)
			選択マジシャン = RESULT
			CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
			CALL CARD_LOADUP
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択マジシャン,"一攻",500)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79903,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 매지션 걸
		효과条件スーパー名前 = 매지컬 실드 베드룸－음마술의 비의
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件Ａ++
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 서큐버스 걸
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 서큐버스 걸
			효과条件종류 = 마법全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲상대필드 = 1
				효과条件除去種 = 無効
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				SIF RESULT == 1
					条件Ｂ++
			ENDIF
		ENDIF
		IF 条件Ａ != 0 && 条件Ｂ != 0
			CALL YN_CHOICE(듀얼리스트,"덱・묘지から「매지션 걸」モンスター１体または「매지컬 실드 베드룸－음마술의 비의」１枚を손패に加える。","『묘지「서큐버스 걸」마법カード１枚제외、상대場のモンスター１体대상효과無効』を자신場の「서큐버스 걸」モンスターの数まで繰り返す。")
			選択효과 = RESULT
		ELSEIF 条件Ｂ == 1 && 条件Ａ == 0
			選択효과 = いいえ
		ELSEIF 条件Ａ == 1 && 条件Ｂ == 0
			選択효과 = はい
		ENDIF
		IF 選択효과 == はい
			CALL FULLRESET
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件スーパー名前 = 매지컬 실드 베드룸－음마술의 비의
			효과条件カテゴリ = 매지션 걸
			효과条件종류 = モン全般
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ELSEIF 選択효과 == いいえ
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件カテゴリ = 서큐버스 걸
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			カウンタ = RESULT
			REPEAT カウンタ
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件カテゴリ = 서큐버스 걸
				효과条件종류 = 마법全般
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					효과範囲상대필드 = 1
					효과条件除去種 = 無効
					CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
					SIF RESULT != 1
						BREAK
				ELSE
					BREAK
				ENDIF
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件カテゴリ = 서큐버스 걸
				효과条件종류 = 마법全般
				효과条件任意 = 1
				효과条件行先플래그 = 1
				CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					효과範囲상대필드 = 1
					CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"지속無効")
				ENDIF
			REND
		ENDIF
	ENDIF
ENDIF