@CARDNAME_79903,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;ブラックツインバースト
SELECTCASE 参照先
	CASE "名前"
		IF GETBIT(TALENT:MASTER:성별,1)
			RESULTS = 서큐버스 매직 키스삽입
		ELSEIF GETBIT(TALENT:MASTER:성별,0)
			RESULTS = 서큐버스 매직 키스가위
		ELSE
			RESULTS = 서큐버스 매직 키스삽입
		ENDIF
	CASE "略称"
		RESULTS = 음마도연교구
	CASE "カテゴリ"
	    RESULTS:0 = 매지션 걸
		RESULTS:1 = 서큐버스 걸
		RESULTS:2 = E.R.A
ENDSELECT

@CARD_79903(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
	CASE "生成カード"
		RETURN 宝液ID
ENDSELECT
RETURN 0

