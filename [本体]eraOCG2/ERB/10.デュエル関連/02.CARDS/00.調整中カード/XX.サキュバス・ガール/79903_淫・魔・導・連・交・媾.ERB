@CARDCAN_79903(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 条件達成

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,79903)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 || 種類 == 手札起動 || 種類 == フリー誘発
	SIF 発動済_1(決闘者,79903)
		RETURN 0
	IF ゾーン == "手札"
		SIF ターンプレイヤー != 決闘者
			RETURN 0
	ELSEIF ゾーン == "魔罠"
		SIF GETVAR(@"%決闘者%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	効果範囲戦場 = 1
	効果条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 墓地起動
	SIF 発動済_2(決闘者,79903)
		RETURN 0
	SIF GETVAR(@"%決闘者%_除外NG置換") >= 1
		RETURN 0
	SIF ゾーン != "墓地"
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	効果範囲デッキ = 1
	効果範囲墓地 = 1
	効果条件カテゴリ = 매지션 걸
	効果条件スーパー名前 = 매지컬 실드 베드룸－음마술의 비의
	効果条件種類 = モン全般
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT == 1
		条件達成++
	CALL FULLRESET
	効果範囲戦場 = 1
	効果条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		効果範囲墓地 = 1
		効果条件カテゴリ = 서큐버스 걸
		効果条件種類 = 魔法全般
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			効果範囲相手戦場 = 1
			効果条件除去種 = 無効
			CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
			SIF RESULT == 1
				条件達成++
		ENDIF
	ENDIF
	SIF 条件達成 < 1
		RETURN 0
    RETURN 1
ENDIF

@CARDEFFECT_79903(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択効果
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択マジシャン
#DIMS DYNAMIC 選択マジシャン名
#DIM DYNAMIC 補正値

CALL 対面者判定(決闘者)
対面者 = %RESULTS%


IF 種類 == 魔罠起動 || 種類 == 手札起動 || 種類 == フリー誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79903,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件カテゴリ = 서큐버스 걸
		CALL CARD_CHOOSE_ADV(決闘者,ゾーン,場所)
		選択カードID = RESULT:99
		IF RESULT != 9999
			CALL LIFE_CHANGE(決闘者,"増加","効果",1000)
			選択マジシャン = RESULT
			CALL CARD_GENERATE_GY(決闘者,宝液ID)
			CALL CARD_LOADUP
			CALL SET_FIELD_INFO(決闘者,"戦場",選択マジシャン,"一攻",500)
		ENDIF
	ENDIF
ELSEIF 種類 == 墓地起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79903,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果範囲デッキ = 1
		効果範囲墓地 = 1
		効果条件カテゴリ = 매지션 걸
		効果条件スーパー名前 = 매지컬 실드 베드룸－음마술의 비의
		効果条件種類 = モン全般
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			条件Ａ++
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件カテゴリ = 서큐버스 걸
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			効果範囲墓地 = 1
			効果条件カテゴリ = 서큐버스 걸
			効果条件種類 = 魔法全般
			CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				効果範囲相手戦場 = 1
				効果条件除去種 = 無効
				CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
				SIF RESULT == 1
					条件Ｂ++
			ENDIF
		ENDIF
		IF 条件Ａ != 0 && 条件Ｂ != 0
			CALL YN_CHOICE(決闘者,"デッキ・墓地から「매지션 걸」モンスター１体または「매지컬 실드 베드룸－음마술의 비의」１枚を手札に加える。","『墓地「서큐버스 걸」魔法カード１枚除外、相手場のモンスター１体対象効果無効』を自分場の「서큐버스 걸」モンスターの数まで繰り返す。")
			選択効果 = RESULT
		ELSEIF 条件Ｂ == 1 && 条件Ａ == 0
			選択効果 = いいえ
		ELSEIF 条件Ａ == 1 && 条件Ｂ == 0
			選択効果 = はい
		ENDIF
		IF 選択効果 == はい
			CALL FULLRESET
			効果範囲デッキ = 1
			効果範囲墓地 = 1
			効果条件スーパー名前 = 매지컬 실드 베드룸－음마술의 비의
			効果条件カテゴリ = 매지션 걸
			効果条件種類 = モン全般
			CALL CARD_ADD_ADV(決闘者,ゾーン,場所)
		ELSEIF 選択効果 == いいえ
			CALL FULLRESET
			効果範囲戦場 = 1
			効果条件カテゴリ = 서큐버스 걸
			効果条件CNT = 1
			CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
			カウンタ = RESULT
			REPEAT カウンタ
				CALL FULLRESET
				効果範囲墓地 = 1
				効果条件カテゴリ = 서큐버스 걸
				効果条件種類 = 魔法全般
				CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					効果範囲相手戦場 = 1
					効果条件除去種 = 無効
					CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
					SIF RESULT != 1
						BREAK
				ELSE
					BREAK
				ENDIF
				CALL FULLRESET
				効果範囲墓地 = 1
				効果条件カテゴリ = 서큐버스 걸
				効果条件種類 = 魔法全般
				効果条件任意 = 1
				効果条件行先フラグ = 1
				CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					効果範囲相手戦場 = 1
					CALL CARD_REMOVAL_ADV(決闘者,"単体除去",場所,"永続無効")
				ENDIF
			REND
		ENDIF
	ENDIF
ENDIF