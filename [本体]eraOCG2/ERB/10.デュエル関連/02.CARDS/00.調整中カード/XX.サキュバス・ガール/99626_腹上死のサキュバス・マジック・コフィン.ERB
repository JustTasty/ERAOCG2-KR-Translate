@CARDCAN_99626(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 選択カード名

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99626)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,99626)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 短紋章予約
	RETURN 1
ELSEIF 종류 == ターン終了時予約
	RETURN 1
ENDIF

@CARDEFFECT_99626(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99626,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 서큐버스 걸
		효과条件行先플래그 = 一時제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_RESERV(듀얼리스트,99626,短紋章予約)
			CALL CARD_RESERV(듀얼리스트,99626,ターン終了時予約)
		ENDIF
	ENDIF
ELSEIF 短紋章予約
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"直不")
ELSEIF ターン終了時予約
	CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",1000)
	CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
	CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
	CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
ENDIF