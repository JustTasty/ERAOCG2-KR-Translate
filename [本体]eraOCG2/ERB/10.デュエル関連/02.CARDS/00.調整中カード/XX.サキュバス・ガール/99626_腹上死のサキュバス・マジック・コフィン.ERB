@CARDCAN_99626(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 選択カード名

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,99626)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 || 種類 == フリー誘発
	SIF 発動済_1(決闘者,99626)
		RETURN 0
	SIF ゾーン != "魔罠"
		RETURN 0
	SIF GETVAR(@"%決闘者%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%決闘者%_除外NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	効果範囲戦場 = 1
	効果条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 短紋章予約
	RETURN 1
ELSEIF 種類 == ターン終了時予約
	RETURN 1
ENDIF

@CARDEFFECT_99626(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

IF 種類 == 魔罠起動 || 種類 == フリー誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,99626,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件カテゴリ = 서큐버스 걸
		効果条件行先フラグ = 一時除外行き
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_RESERV(決闘者,99626,短紋章予約)
			CALL CARD_RESERV(決闘者,99626,ターン終了時予約)
		ENDIF
	ENDIF
ELSEIF 短紋章予約
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(決闘者,"相手戦場",場所,"直不")
ELSEIF ターン終了時予約
	CALL LIFE_CHANGE(決闘者,"減少","コスト",1000)
	CALL CARD_GENERATE_GY(決闘者,宝液ID)
	CALL CARD_GENERATE_GY(決闘者,宝液ID)
	CALL CARD_GENERATE_GY(決闘者,宝液ID)
ENDIF