@CARDNAME_79904,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;マジック・エキスパンド
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 서큐버스 매직 익스팬드
	CASE "略称"
		RESULTS = 음마도강정
	CASE "カテゴリ"
	    RESULTS:0 = 매지션 걸
		RESULTS:1 = 서큐버스 걸
		RESULTS:2 = E.R.A
ENDSELECT

@CARD_79904(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
ENDSELECT
RETURN 0

@CARDSUMMON_79904(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
RETURN 0

@CARD_EXPLANATION_79904(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の①②の효과は１ターンにいずれか１つしか使用できない。
CALL CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①：자신フィールドの「서큐버스 걸」モンスターの数によって以下の효과を適用する。
PRINTL ●１体以上：묘지の「서큐버스 걸」마법カードの数×１００ＬＰを回復する。
PRINTL ●２体以上：자신フィールドの「서큐버스 걸」モンスターの数×３００ＬＰを回復する。
PRINTL ●３体以上：ターン終了時まで、자신はカードの효과によってＬＰを失わなくなり、払う必要もなくなる。
CALL CARD_ABLE(2,효과後誘発)
PRINTL ②：자신の「서큐버스 걸」モンスターが효과を발동した場合に발동できる。
PRINTL 묘지のこのカードを자신フィールドにセットする。
PRINTL この효과でセットしたこのカードは、フィールドから離れた場合に제외される。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79904(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79904)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,79904)
		RETURN 0
	SIF 발동済_2(듀얼리스트,79904)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 서큐버스 걸
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,79904)
		RETURN 0
	SIF 발동済_2(듀얼리스트,79904)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = 서큐버스 걸
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT != 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
    RETURN 1
ENDIF

@CARDEFFECT_79904(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79904,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 서큐버스 걸
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		カウンタ = RESULT
		IF カウンタ
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 서큐버스 걸
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			CALL LIFE_CHANGE(듀얼리스트,"増加","효과",RESULT * 100)
			IF カウンタ > 1
				CALL LIFE_CHANGE(듀얼리스트,"増加","효과",カウンタ * 300)
				IF カウンタ > 2
					PRINTFORML %듀얼리스트%はこのターンＬＰを失わず、払う必要もなくなりました。
					CALL SET_PLAYER_INFO(듀얼리스트,"コスト無効",1)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79904,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 서큐버스 매직 익스팬드
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF