@CARDNAME_59900,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 심메트리아 마뉴아
	CASE "略称"
		RESULTS = Sym마뉴아
	CASE "カテゴリ"
		RESULTS:0 = 심메트리아
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59900(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 100
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250721
ENDSELECT

RETURN 0

@CARD_EXPLANATION_59900(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 (1)(2)(3)의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,손패起動)
PRINTL ①：패의 이 카드를 묘지로 보내고 발동할 수 있다。
PRINTL 덱에서 "심메트리아 마뉴아" 이외의 "심메트리아" 몬스터 １장을 패에 넣는다。
CALL CARD_ABLE(2,묘지起動)
PRINTL ②：이 카드가 묘지에 존재하고、자신 필드에 "타천칭" 카드가 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 특수 소환한다。
CALLFORM CARD_ABLE(3,필드起動,フリー誘発)
PRINTL ③：자신 / 상대 턴에、자신 필드에 "타천칭" 카드가 존재하는 경우、
PRINTL 필드의 이 카드를 묘지로 보내고 발동할 수 있다。
PRINTL 상대는 이하의 효과에서 １개를 고른다。그 후、그 효과를 적용한다。
PRINTL ●상대 필드의 몬스터를 전부 제외한다。
PRINTL ●상대에게 수음으로 봉사시킨다。그 후 200데미지를 주고、"타천칭" 카드에 개별 카운터를 １개 놓는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_59900(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59900)
SIF RESULT == 1
	RETURN 0

;サーチ효과
IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,59900)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 심메트리아
	효과条件종류 = モン全般
	효과条件固名前 = 심메트리아 마뉴아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1

;묘지에서 のss효과
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,59900)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,59900,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

;フリー誘発효과
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_3(듀얼리스트,59900)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59900(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 選択後メッセージ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 카운터数
#DIM DYNAMIC ダメージ量
#DIM DYNAMIC 효과分岐
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59900)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	;패에서 자신を捨ててサーチする효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59900,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			;うららチェック
			CALL URARA_CHANCE(듀얼리스트,,"덱")
			IF RESULT == 1
				CALL SHUFFLE(듀얼리스트)
				CALL SHUFFLE(対面者)
				RETURN 0
			ENDIF
			;효과処理
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 심메트리아
			효과条件종류 = モン全般
			효과条件固名前 = 심메트리아 마뉴아
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	;묘지에서 자신を특수소환する효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59900,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"묘지",場所)
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	;ダメージか固有のボーナス효과かを상대に選択させる효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59900,3)
	IF RESULT == 0
		;자신を묘지에 送る
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;どの효과가 選択されるか決定
		CALL FULLRESET
		CALL CALC_PROBABILITY_SYMMETRIA(듀얼리스트,종류,ゾーン,場所)
		효과分岐 = RESULT

		IF 효과分岐 == 1 || 효과分岐 == 2
			;RESULTが1なら確率によってボーナス효과가 選択された
			;RESULTが2ならダメージ的にボーナス효과를 選択するしかなかった状況
			;각각 に違うメッセージを用意する予定があったが、今のところは結果は同じ
			;몬스터를 すべて제외
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"제외")
		ELSEIF 효과分岐 == 3
			;RESULTが3なら使途が居るので両方の효과를 適用させる
			;몬스터가 個別で持つボーナス효과の処理
			;몬스터를 すべて제외
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"제외")
			;奉仕処理
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML %CALLNAME:조교대상%는 수음으로 봉사를 강요받고있다……
				RESETCOLOR
				CALL EROTIC_MOVE("催眠手コキ",조교대상,MASTER,500)
			ENDIF
			;ダメージを与え카운터を置く효과
			CALL FULLRESET
			ダメージ量 = 200
			CALL CARD_DAMAGE_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,ダメージ量)
		ELSE
			;それ以外ならダメージを与える효과가 選択されている。
			;奉仕処理
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML %CALLNAME:조교대상%는 수음으로 봉사를 강요받고있다……
				RESETCOLOR
				CALL EROTIC_MOVE("催眠手コキ",조교대상,MASTER,500)
			ENDIF
			;ダメージを与え카운터を置く효과の処理
			CALL FULLRESET
			ダメージ量 = 200
			CALL CARD_DAMAGE_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,ダメージ量)
		ENDIF
	ENDIF
ENDIF



