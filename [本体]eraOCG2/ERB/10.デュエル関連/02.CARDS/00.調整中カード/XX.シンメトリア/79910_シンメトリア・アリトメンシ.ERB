@CARDNAME_79910,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 심메트리아 아리토멘시
	CASE "略称"
		RESULTS = 아리토멘시
	CASE "カテゴリ"
		RESULTS:0 = 심메트리아
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79910(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 通常魔法
	CASE "追加日"
		RETURN 250721
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79910(種類)
#DIM DYNAMIC 種類

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の(1)(2)の効果はそれぞれ１ターンに１度しか使用できない。
PRINTL 自分フィールドに「타천칭」カードが存在する場合、
PRINTL このカードの発動に対して魔法・罠・モンスターの効果は発動できない。
CALLFORM CARD_ABLE(1,魔罠起動,手札起動)
PRINTL ①：デッキから「심메트리아」モンスター１枚を手札に加える。
CALLFORM CARD_ABLE(2,墓地起動)
PRINTL ②：墓地からこのカードを除外して発動できる。
PRINTL 自分の墓地から「심메트리아」カードを任意の数だけデッキに戻す。
PRINTL その後、戻した数まで相手フィールドのカードを選んで破壊できる。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79910(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 調教対象


CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,79910)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 || 種類 == 手札起動
	;①のサーチ効果
	;ターン制限あり
	SIF 発動済_1(決闘者,79910)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	;条件を指定して一致するか確認
	CALL FULLRESET
	効果範囲デッキ = 1
	効果条件カテゴリ = 심메트리아
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 種類 == 墓地起動
	;②の墓地効果
	;ターン制限あり。除外できないなら発動不可
	SIF 発動済_2(決闘者,79910)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	SIF GETVAR(@"%決闘者%_除外NG置換") >= 1
		RETURN 0
	;条件を指定して一致するか確認
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件カテゴリ = 심메트리아
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	効果範囲相手戦場 = 1
	効果範囲相手魔罠 = 1
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 種類 == チェーン可否
	;②は条件次第でチェーン不可
	SIF !GROUPMATCH(チェーン可否種類, 墓地起動)
		RETURN 0
	;条件を指定して一致するか確認
	CALL FULLRESET
	効果範囲戦場 = 1
	効果範囲魔罠 = 1
	効果条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79910(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 破壊枚数


CALL 対面者判定(決闘者)
対面者 = %RESULTS%

IF 種類 == 魔罠起動 || 種類 == 手札起動
	;①の効果（サーチ）
	;カード発動確認
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79910,1)
	IF RESULT == 0
		;通常魔法汎用発動後墓地送り処理
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果条件カテゴリ = 심메트리아
		効果条件種類 = モン全般
		CALL URARA_CHANCE(決闘者,,"デッキ")
		IF RESULT == 1
			CALL SHUFFLE(決闘者)
			CALL SHUFFLE(対面者)
			RETURN 0
		ENDIF
		CALL CARD_ADD_BASIC(決闘者,"デッキ",ゾーン,場所)
	ENDIF
ELSEIF 種類 == 墓地起動
	;②の効果（除去効果）
	;カード発動確認
	CALL CARD_DISPOSE(決闘者,ゾーン,場所)
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,79910,2)
	IF RESULT == 0
		;墓地のカテゴリカードの数を数える
		CALL FULLRESET
		効果範囲墓地 = 1
		効果条件カテゴリ = 심메트리아
		効果条件CNT = 1
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		破壊枚数 = RESULT

		;その数だけデッキに戻す
		効果条件カテゴリ = 심메트리아
		効果範囲墓地 = 1
		効果条件行先フラグ = デッキ行き
		効果条件複数 = 100 - 破壊枚数
		CALL CARD_ADD_GY_BASIC(決闘者,"墓地",ゾーン,場所)
		IF RESULT == 1
			;戻した数だけ破壊する
			CALL FULLRESET
			効果条件任意 = 1
			効果条件複数 = 100 - 破壊枚数
			CALL CARD_REMOVAL_BASIC(決闘者,"全場単体",場所,"墓地")
		ENDIF
	ENDIF
ENDIF