@CARDNAME_79910,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 심메트리아 아리토멘시
	CASE "略称"
		RESULTS = 아리토멘시
	CASE "カテゴリ"
		RESULTS:0 = 심메트리아
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79910(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250721
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79910(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の(1)(2)の효과はそれぞれ１ターンに１度しか使用できない。
PRINTL 자신フィールドに「타천칭」カードが存在する場合、
PRINTL このカードの발동に対して마법・함정・モンスターの효과は발동できない。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：덱から「심메트리아」モンスター１枚を손패に加える。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：묘지からこのカードを제외して발동できる。
PRINTL 자신の묘지から「심메트리아」カードを任意の数だけ덱に戻す。
PRINTL その後、戻した数まで상대フィールドのカードを選んで破壊できる。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79910(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79910)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	;①のサーチ효과
	;ターン制限あり
	SIF 발동済_1(듀얼리스트,79910)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	;条件を지정して一致するか確認
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 심메트리아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	;②の묘지효과
	;ターン制限あり。제외できないなら발동불가
	SIF 발동済_2(듀얼리스트,79910)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	;条件を지정して一致するか確認
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 심메트리아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == チェーン可否
	;②は条件次第でチェーン불가
	SIF !GROUPMATCH(チェーン可否종류, 묘지起動)
		RETURN 0
	;条件を지정して一致するか確認
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79910(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 破壊枚数


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;①の효과（サーチ）
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79910,1)
	IF RESULT == 0
		;通常마법汎用발동後묘지送り処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ = 심메트리아
		효과条件종류 = モン全般
		CALL URARA_CHANCE(듀얼리스트,,"덱")
		IF RESULT == 1
			CALL SHUFFLE(듀얼리스트)
			CALL SHUFFLE(対面者)
			RETURN 0
		ENDIF
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	;②の효과（除去효과）
	;カード발동確認
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79910,2)
	IF RESULT == 0
		;묘지のカテゴリカードの数を数える
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 심메트리아
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		破壊枚数 = RESULT

		;その数だけ덱に戻す
		효과条件カテゴリ = 심메트리아
		효과範囲묘지 = 1
		효과条件行先플래그 = 덱行き
		효과条件複数 = 100 - 破壊枚数
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			;戻した数だけ破壊する
			CALL FULLRESET
			효과条件任意 = 1
			효과条件複数 = 100 - 破壊枚数
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지")
		ENDIF
	ENDIF
ENDIF