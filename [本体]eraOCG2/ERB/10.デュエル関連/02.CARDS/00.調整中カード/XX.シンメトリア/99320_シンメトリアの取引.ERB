@CARDCAN_99320(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99320)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,99320)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	;ここまで和睦コピペ
	;ここで条件지정
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件CNT = 1
	효과条件固カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_99320(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99320)
	IF RESULT == 0
		;손패또는 필드의 카드를 묘지へ
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲손패 = 1
		효과条件固カテゴリ = 타천칭
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			;和睦효과
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",1)
			CALL CARD_EFFECT_USE(듀얼리스트,99320,1)

			;和睦효과가 適応されているかどうかをシステム上で判別しているっぽい？
			;という事で和睦にも使用済み플래그を立てる
			CALL CARD_EFFECT_USE(듀얼리스트,91090,1)
		ENDIF
	ENDIF
ENDIF