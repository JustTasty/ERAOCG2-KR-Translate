#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 심메트리아의 구속
	CASE "略称"
		RESULTS = Sym구속
	CASE "カテゴリ"
		RESULTS:0 = 심메트리아
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_99321(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "추가日"
		RETURN 250721
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99321(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALLFORM CARD_ABLE(0,마함起動 ,フリー誘発)
PRINTL ①：자신フィールドに存在する「타천칭」以外のカード１枚を묘지へ送ってこのカードを발동できる。
PRINTL 상대フィールド・묘지のモンスター１体を「심메트리아・スレイヴ」として、
PRINTL 지속마법カード扱いで자신の마법＆함정ゾーンに表側표시で置く。
PRINTL ②：묘지のこのカードを제외して발동できる。자신の덱の上からカードを３枚묘지へ送る。
PRINTL その後、「타천칭」カードに個別카운터を４つ乗せる。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_99321(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99321)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	;ここで条件지정
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件CNT = 1
	효과条件固カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대묘지 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF ゾーン != "묘지"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_99321(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カード추가先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99321)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;지정したカードを묘지に送る
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件固カテゴリ = 타천칭
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			;상대のカードを마법함정ゾーンに置く효과の処理
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			;カードを置く位置の情報を취득
			カード추가先 = 現마함(듀얼리스트)
			;상대のカードを置く
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과範囲상대묘지 = 1
			효과条件표시 = 10
			효과条件종류 = モン全般
			효과条件除去種 = 強奪
			CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
			;名前を「Symスレイヴ」に변경（処理上はこの表記）
			CALL CARD_SET_RENAME(듀얼리스트,"마함",カード추가先,"Symスレイヴ")
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99321)
	IF RESULT == 0
		;カードを묘지に送る
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL CARD_DECK_DISPOSE(듀얼리스트,"덱",3)
		;카운터を乗せる
		CALL CARD_COUNTER_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,4)
	ENDIF
ENDIF