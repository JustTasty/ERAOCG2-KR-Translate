@CARDNAME_59901,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 심메트리아 블로우
	CASE "略称"
		RESULTS = Sym블로우
	CASE "カテゴリ"
		RESULTS:0 = 심메트리아
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59901(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 200
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250721
ENDSELECT

RETURN 0

@CARD_EXPLANATION_59901(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の(1)(2)(3)の효과はそれぞれ１ターンに１度しか使用できない。
CALL CARD_ABLE(1,손패起動)
PRINTL ①：손패のこのカードを묘지へ送って발동できる。
PRINTL EX덱から「타천칭」포트리스１枚を자신の마법＆함정ゾーンに表側표시で置く。
CALL CARD_ABLE(2,묘지起動,フリー誘発)
PRINTL ②：자신・상대ターンにこのカードが묘지に存在し、자신フィールドに「타천칭」カードが存在する場合に발동できる。
PRINTL このカードを특수소환する。
CALLFORM CARD_ABLE(3,필드起動,フリー誘発)
PRINTL ③：자신・상대ターンに、자신フィールドに「타천칭」カードが存在する場合、
PRINTL フィールドのこのカードを묘지へ送って발동できる。
PRINTL 상대は以下の효과から１つを選ぶ。その後、その효과を適用する。
PRINTL ●상대フィールドの마법・함정カードを全て제외する。
PRINTL ●상대にフェラで奉仕をさせる。その後200ダメージを与え、「타천칭」カードに個別카운터を１つ乗せる。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_59901(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59901)
SIF RESULT == 1
	RETURN 0

;포트리스효과
IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,59901)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 초차원
	효과条件カテゴリ = 포트리스
	효과条件カテゴリ = 타천칭
	CALL 효과条件レベル_ADV(11, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

;묘지からのss효과
ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,59901)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,59901,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

;フリー誘発효과
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_3(듀얼리스트,59901)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲필드 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59901(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 選択後メッセージ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 카운터数
#DIM DYNAMIC ダメージ量
#DIM DYNAMIC 효과分岐
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59901)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	;손패から자신を捨て、포트리스を置く효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59901,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件종류 = 초차원
		효과条件カテゴリ = 포트리스
		효과条件カテゴリ = 타천칭
		CALL 효과条件レベル_ADV(11, "以下")
		CALL DIMSUMMON(듀얼리스트,ゾーン,場所,4)
	ENDIF

ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発
	;묘지から자신を특수소환する효과（このカードのみフリーチェーン）
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59901,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"묘지",場所)
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	;ダメージか固有のボーナス효과かを상대に選択させる효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59901,3)
	IF RESULT == 0
		;자신を묘지に送る
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;どの효과が選択されるか決定
		CALL FULLRESET
		CALL CALC_PROBABILITY_SYMMETRIA(듀얼리스트,종류,ゾーン,場所)
		효과分岐 = RESULT

		IF 효과分岐 == 1 || 효과分岐 == 2
			;RESULTが1なら確率によってボーナス효과が選択された
			;RESULTが2ならダメージ的にボーナス효과を選択するしかなかった状況
			;それぞれに違うメッセージを用意する予定があったが、今のところは結果は同じ
			;마법함정をすべて제외
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함全体",場所,"제외")
		ELSEIF 효과分岐 == 3
			;RESULTが3なら使途が居るので両方の효과を適用させる
			;モンスターが個別で持つボーナス효과の処理
			;마법함정をすべて제외
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함全体",場所,"제외")
			;奉仕処理
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML %CALLNAME:조교대상%はフェラでの奉仕を強要されている……
				RESETCOLOR
				CALL EROTIC_MOVE("フェラチオ",조교대상,MASTER,500)
			ENDIF
			;ダメージを与え카운터を置く효과
			CALL FULLRESET
			ダメージ量 = 200
			CALL CARD_DAMAGE_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,ダメージ量)
		ELSE
			;それ以外ならダメージを与える효과が選択されている。
			;奉仕処理
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML %CALLNAME:조교대상%はフェラでの奉仕を強要されている……
				RESETCOLOR
				CALL EROTIC_MOVE("フェラチオ",조교대상,MASTER,500)
			ENDIF
			;ダメージを与え카운터を置く효과の処理
			ダメージ量 = 200
			CALL CARD_DAMAGE_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,ダメージ量)
		ENDIF
	ENDIF
ENDIF