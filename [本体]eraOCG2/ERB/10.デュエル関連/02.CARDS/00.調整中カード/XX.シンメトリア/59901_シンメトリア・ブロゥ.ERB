@CARDNAME_59901,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 심메트리아 블로우
	CASE "略称"
		RESULTS = Sym블로우
	CASE "カテゴリ"
		RESULTS:0 = 심메트리아
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59901(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 200
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250721
ENDSELECT

RETURN 0

@CARD_EXPLANATION_59901(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 (1)(2)(3)의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,손패起動)
PRINTL ①：패의 이 카드를 묘지로 보내고 발동할 수 있다。
PRINTL EX덱에서 "타천칭" 포트리스１장을 자신의 마법＆함정 존에 앞면 표시로 둔다。
CALL CARD_ABLE(2,묘지起動,フリー誘発)
PRINTL ②：자신 / 상대 턴에 이 카드가 묘지에 존재하고、자신 필드에 "타천칭" 카드가 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 특수 소환한다。
CALLFORM CARD_ABLE(3,필드起動,フリー誘発)
PRINTL ③：자신 / 상대 턴에、자신 필드에 "타천칭" 카드가 존재하는 경우、
PRINTL 필드의 이 카드를 묘지로 보내고 발동할 수 있다。
PRINTL 상대는 이하의 효과에서 １개를 고른다。그 후、그 효과를 적용한다。
PRINTL ●상대 필드의 마법 / 함정카드를 전부 제외한다。
PRINTL ●상대에게 펠라로 봉사를 시킨다。그 후200 데미지를 주고、"타천칭" 카드에 개별 카운터를 １개 놓는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_59901(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59901)
SIF RESULT == 1
	RETURN 0

;포트리스효과
IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,59901)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 초차원
	효과条件カテゴリ = 포트리스
	효과条件カテゴリ = 타천칭
	CALL 효과条件レベル_ADV(11, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

;묘지에서 のss효과
ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,59901)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,59901,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

;フリー誘発효과
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_3(듀얼리스트,59901)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲필드 = 1
	효과条件カテゴリ = 타천칭
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59901(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 選択後メッセージ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 카운터数
#DIM DYNAMIC ダメージ量
#DIM DYNAMIC 효과分岐
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59901)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	;패에서 자신を捨て、포트리스を置く효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59901,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件종류 = 초차원
		효과条件カテゴリ = 포트리스
		효과条件カテゴリ = 타천칭
		CALL 효과条件レベル_ADV(11, "以下")
		CALL DIMSUMMON(듀얼리스트,ゾーン,場所,4)
	ENDIF

ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発
	;묘지에서 자신を특수소환する효과（이 카드의 みフリーチェーン）
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59901,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"묘지",場所)
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	;ダメージか固有のボーナス효과かを상대に選択させる효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59901,3)
	IF RESULT == 0
		;자신を묘지에 送る
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;どの효과가 選択されるか決定
		CALL FULLRESET
		CALL CALC_PROBABILITY_SYMMETRIA(듀얼리스트,종류,ゾーン,場所)
		효과分岐 = RESULT

		IF 효과分岐 == 1 || 효과分岐 == 2
			;RESULTが1なら確率によってボーナス효과가 選択された
			;RESULTが2ならダメージ的にボーナス효과를 選択するしかなかった状況
			;각각 に違うメッセージを用意する予定があったが、今のところは結果は同じ
			;마법함정をすべて제외
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함全体",場所,"제외")
		ELSEIF 효과分岐 == 3
			;RESULTが3なら使途が居るので両方の효과를 適用させる
			;몬스터가 個別で持つボーナス효과の処理
			;마법함정をすべて제외
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함全体",場所,"제외")
			;奉仕処理
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML %CALLNAME:조교대상%는 펠라로 봉사를 강요받고있다……
				RESETCOLOR
				CALL EROTIC_MOVE("펠라치오",조교대상,MASTER,500)
			ENDIF
			;ダメージを与え카운터を置く효과
			CALL FULLRESET
			ダメージ量 = 200
			CALL CARD_DAMAGE_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,ダメージ量)
		ELSE
			;それ以外ならダメージを与える효과가 選択されている。
			;奉仕処理
			CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML %CALLNAME:조교대상%는 펠라로 봉사를 강요받고있다……
				RESETCOLOR
				CALL EROTIC_MOVE("펠라치오",조교대상,MASTER,500)
			ENDIF
			;ダメージを与え카운터を置く효과の処理
			ダメージ量 = 200
			CALL CARD_DAMAGE_SYMMETRIA(듀얼리스트,종류,ゾーン,場所,ダメージ量)
		ENDIF
	ENDIF
ENDIF