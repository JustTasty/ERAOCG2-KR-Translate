@CARDCAN_99665(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 효과条件플래그 = 0

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99665)
SIF RESULT == 1
RETURN 0

IF 종류 == 攻守変動
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_2(듀얼리스트,99665)
		RETURN 0
    SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
    효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,99665)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_3(듀얼리스트,99665)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 광서의 기계
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_4(듀얼리스트,99665)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 마함起動 
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_99665(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カードID
#DIM DYNAMIC 효과条件플래그 = 0

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99665,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
        효과範囲상대필드 = 1
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"選択")
		IF RESULT == 1
			選択カード = RESULT:1
			CALL CARD_SELECT(듀얼리스트,選択カード)
			CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"薬Ｃ",1) 
		ENDIF
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99665,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 광서의 기계
        효과条件行先플래그 = 破壊行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
			RETURN 1
		ENDIF
	ENDIF

ELSEIF 종류 == 攻守変動
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "기계감금암농상")
	CALL FULLRESET
	효과条件薬Ｃ所持 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"表",1,99665)
    CALL FULLRESET
    효과条件薬Ｃ所持 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"戦狂",1,99665)

	ELSEIF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99665)
	IF RESULT == 0
	ENDIF

ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99665,2)
    	IF RESULT == 0
        	CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
			조교대상 = RESULT
            IF 조교대상 != -1 && TALENT:조교대상:성별 == 1
				PRINTFORMW 場に充満する淫らな成分を含めている蒸気が牝弄箱に溜まり、
				PRINTFORMW %CALLNAME:조교대상%の패에 一つのカードを形成する……
				SELECTCASE RAND:9
					CASE 0
						CALL CARD_GENERATE_HAND(対面者,79840)
					CASE 1
						CALL CARD_GENERATE_HAND(対面者,79841)
					CASE 2
						CALL CARD_GENERATE_HAND(対面者,79842)
					CASE 3
						CALL CARD_GENERATE_HAND(対面者,79843)
					CASE 4
						CALL CARD_GENERATE_HAND(対面者,79844)
					CASE 5
						CALL CARD_GENERATE_HAND(対面者,79845)
					CASE 6
						CALL CARD_GENERATE_HAND(対面者,79846)
					CASE 7
						CALL CARD_GENERATE_HAND(対面者,79847)
					CASE 8
						CALL CARD_GENERATE_HAND(対面者,79848)
				ENDSELECT
            ENDIF

        ENDIF

ENDIF




