@CARDSUMMON_39907(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 3
素材条件素材ID起動 = 2


IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 광서의 기계
	素材条件レベル = 8
ELSEIF EX参照 >= 10
    素材条件성별 = 牝性
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,39907) 
	RETURN RESULT
ENDIF


@CARDCAN_39907(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39907)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
    효과範囲필드 = 1
	효과条件종족 = 기계족
	효과条件성별 = 牝性
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 2
		RETURN 0
	CALL FULLRESET
    효과範囲필드 = 1
	효과条件カテゴリ = 광서의 기계
	효과条件レベル = 8
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과 || 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0
    CALL FULLRESET
    효과範囲묘지 = 1
    효과条件カテゴリ = 광서의 기계
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF 발동済_4(듀얼리스트,39907)
		RETURN 0
    SIF ゾーン != "필드"
	 	RETURN 0
    CALL FULLRESET
    효과条件종류 = モン全般
    CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
    	효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
    RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 3
		RETURN 0
    RETURN 1

ENDIF

@CARDEFFECT_39907(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 제외カードID
#DIM DYNAMIC 破壊カードID
#DIMS DYNAMIC 제외カード名
#DIM DYNAMIC 残り횟수

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39907)
		IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 광서의 기계
		효과条件レベル = 8
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
        CALL FULLRESET
		효과範囲필드 = 1
		효과条件성별 = 牝性
		효과条件종족 = 기계족
		효과条件複数 = 2
        CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
			ENDIF
		ENDIF

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 기계족
    효과条件성별 = 牝性
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	攻撃力修正 = RESULT * 300
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)	
	SIF GETVAR(@"%듀얼리스트%_MZ_攻:場所") >= 5000
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	SIF ターンプレイヤー == 듀얼리스트
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦阻")

ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39907)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_EROTICS(直前戦闘破壊ID(듀얼리스트,1))
			IF RESULT != -1
			조교대상 = RESULT
				IF MARK:조교대상:쾌락각인 + MARK:조교대상:고통각인 + MARK:조교대상:굴복각인 - MARK:조교대상:반발각인 < 8
					SELECTCASE RAND:3
						CASE 0
						PRINTFORML %CALLNAME:조교대상%은 호화 봉사 기인에게 억지로 %CALLNAME:MASTER%님 앞으로 끌려온다, %TEXTR("온몸에 백탁액을 뒤집어쓴 채로/정액을 주입당해 부풀어 오른 배에 멍이 잔뜩 남은 채로/무릎을 흔들며 뺨이 붉어질 정도로 발정당한 채로/기계성의 음독 섞인 증기를 너무 많이 마셔 육체가 과민해진 상태로")%.
						PRINTFORMW "주인님, 이 암컷을 데려왔습니다. %TEXTR("다소 반항적이긴 하지만 광서의 기계 증기를 마셔/저항하긴 했지만 다른 기계에게 범해져/성의 함정에 걸려 기계간을 당해 자지님을 받아들일 준비를 하고/성의 증기를 과도하게 마셔 육체가 너무 발정해 움직일 수 없게 되어")% 제대로 된 저항을 할 수 없는 상태에 빠져있네요."
						PRINTFORMW 호화 봉사 기인이 %CALLNAME:조교대상%의 %TEXTR("음부를 손가락으로 훑는 것만으로/클리토리스를 손가락 끝으로 집어 비비는 것만으로/유두를 손가락으로 집어, 꽉 조이듯이 힘을 주는 것만으로/입안에 손가락을 삽입해 목구멍 안쪽을 자극하는 것만으로")%,
						PRINTFORMW %CALLNAME:조교대상%의 몸은 %TEXTR("전류가 흐른 듯 튀어 오르며 절정하고 만다/음부에서 애액을 뿜어내 수컷 자지에게 어필하고 만다/움찔❤움찔❤하며 몸을 튀어 올려 타인이 봐도 알 수 있을 정도로 절정하고 만다/숨을 헐떡이며 저항도 못 한 채 그 쾌락을 받아 이키려 하고 만다")%.
						PRINTFORML "주인님, 이 암컷은 아직 꺾이지 않고 조교의 흔적도 없는, 꺾이지 않은 암컷입니다"
						PRINTFORMW "%TEXTR("아직 자지님께 아양 떨고 그 정액을 받아들이는 것이 암컷에게는 기쁨이라는 것을 이해하지 못하는 가련한 암컷이기도 합니다/가련한 암컷이네요…, 주인님의 자지님께 괴롭힘당하면 반항할 의지도 꺾일 텐데……/천박하게도 이렇게 주인님의 총애를 받고 싶어 하는 마조 구멍일지도 모르겠네요/어떤 의미로는 부럽기도 하네요, 자지님께 범해져 암컷으로서의 본래의 모습을 마음속 깊이 이해할 수 있는 상태라고도 할 수 있고요")%."
						PRINTFORMW 호화 봉사 기인은 그렇게 말하며 데려온 암컷을 %CALLNAME:MASTER%님의 발밑에 무릎 꿇리듯이 %TEXTR("억지로 목덜미를 잡고 끌어당겨 바닥에 짓누르듯이/무릎을 꿇리고 세 손가락을 바닥에 대도록 강요하며/손에 든 무기로 위협하며 바닥에 정좌시키고/채찍을 휘둘러, 머리를 바닥에 문지르도록 강요하며")%,
						PRINTFORMW %CALLNAME:조교대상%은 억지로 명령받는 대로 %CALLNAME:MASTER%님의 발밑에서 너덜너덜한 옷에 능욕의 흔적을 남긴 채 무릎 꿇고, 고개를 숙인다.
						CASE 1
						PRINTFORML 호화 봉사 기인은 %CALLNAME:MASTER%님 앞에 %CALLNAME:조교대상%을 어깨에 메고 나타난다, %TEXTR("성의 함정에 걸려 억지로 절정당한 곳을 잡은 것일까/다른 광서의 기계들에게 얻어맞아 쓰러져 있던 곳을 데려온 것일지도 모른다/성에 가득 찬 음란한 독소를 섞은 안개를 너무 많이 마신 곳을 습격당한 것일지도 모른다/호화 봉사 기인에게 쓰러져, 그대로 끌려온 것이겠지")%
						PRINTFORMW %CALLNAME:조교대상%은 %TEXTR("호화 봉사 기인에게 젖은 은밀한 곳을 더듬어지면서/저항할 기력도 없이 옷을 호화 봉사 기인이 벗겨 드러난 분홍색 엉덩이를 손자국이 남을 정도로 강하게 때려 겨우 정신을 차리게 하고/호화 봉사 기인에게 옷을 벗겨지고, 제지하려는 손은 간단히 뿌리쳐지고/는 벗어나려 몸을 비틀지만 호화 봉사 기인은 그 정도 힘을 내지 않고도 간단히 제압당하고")%, %CALLNAME:조교대상%은 알몸이 되어버린다
						PRINTFORMW 저항도 못 하고 나체를 드러낸 채, %CALLNAME:조교대상%은 %TEXTR("팔 아래와 양 다리를 안듯이 호화 봉사 기인이 팔을 통과시킨 상태로 안겨/호화 봉사 기인에게 옆구리에 안기듯이 들려 은밀한 곳을 향하게 한 채/호화 봉사 기인에 의해 그 주인의 발밑까지 옮겨져/호화 봉사 기인이 그 양 구멍을 손가락으로 벌려 상태를 확인하고")%, %CALLNAME:MASTER%님께 마음대로 사용하기 쉽도록 %CALLNAME:조교대상%을 든 채 다가간다.
