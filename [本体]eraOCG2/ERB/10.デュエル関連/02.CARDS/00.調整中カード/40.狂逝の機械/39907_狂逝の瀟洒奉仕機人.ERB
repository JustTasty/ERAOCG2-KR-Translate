@CARDSUMMON_39907(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 3


IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 광서의 기계
	素材条件レベル = 8
ELSEIF EX参照 >= 10
	素材条件종족 = 기계족
    素材条件성별 = 牝性
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,39907) 
	RETURN RESULT
ENDIF


@CARDCAN_39907(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39907)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
    효과範囲필드 = 1
	효과条件종족 = 기계족
	효과条件성별 = 牝性
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 2
		RETURN 0
	CALL FULLRESET
    효과範囲필드 = 1
	효과条件カテゴリ = 광서의 기계
	효과条件レベル = 8
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과 || 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0
    CALL FULLRESET
    효과範囲묘지 = 1
    효과条件カテゴリ = 광서의 기계
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF 발동済_4(듀얼리스트,39907)
		RETURN 0
    SIF ゾーン != "필드"
	 	RETURN 0
    CALL FULLRESET
    효과条件종류 = モン全般
    CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
    	효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
    RETURN 1

ENDIF

@CARDEFFECT_39907(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 제외カードID
#DIM DYNAMIC 破壊カードID
#DIMS DYNAMIC 제외カード名
#DIM DYNAMIC 残り횟수

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39907)
		IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 광서의 기계
		효과条件レベル = 8
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
        CALL FULLRESET
		효과範囲필드 = 1
		효과条件성별 = 牝性
		효과条件종족 = 기계족
		효과条件複数 = 2
        CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
			ENDIF
		ENDIF

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 기계족
    효과条件성별 = 牝性
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	攻撃力修正 = RESULT * 300
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)	
	SIF GETVAR(@"%듀얼리스트%_MZ_攻:場所") >= 5000
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	SIF ターンプレイヤー == 듀얼리스트
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦阻")

ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39907)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_EROTICS(直前戦闘破壊ID(듀얼리스트,1))
			IF RESULT != -1
			조교대상 = RESULT
				IF MARK:조교대상:쾌락각인 + MARK:조교대상:고통각인 + MARK:조교대상:굴복각인 - MARK:조교대상:반발각인 < 8
					SELECTCASE RAND:3
						CASE 0
						PRINTFORML %CALLNAME:조교대상%は瀟洒奉仕機人に無理やり%CALLNAME:MASTER%の前に連れ込まれる、%TEXTR("全身に白濁液を浴びせられてたままの状態で/精液を流し込まれ膨らんだ腹部に痣を多量に残したまま/膝を揺らしながら頬が紅潮するほどに発情させられたまま/機械城の淫毒混じりの蒸気を吸い過ぎ、肉体を過敏にさせられた状態にされて")%。
						PRINTFORMW 「ご主人様、こちらの雌をお連れしました。%TEXTR("やや反抗的ではありますが광서의 기계の蒸気を吸い/抵抗こそしていましたが他の機械に犯され/城の함정にかかり機械姦を受けおちん様を受け入れる準備をして/城の蒸気を過剰に吸い肉体が発情しすぎてうごけなくなり")%真っ当な抵抗が出来ない状態には陥ってますね。」
						PRINTFORMW 瀟洒奉仕機人が%CALLNAME:조교대상%の%TEXTR("雌陰を指でなぞるだけで/クリトリスを指先で摘み擦るだけで/乳首を指で摘み、ぐっと挟むように力を籠めるだけで/口の中に指を삽입し喉奥を刺激するだけで")%、
						PRINTFORMW %CALLNAME:조교대상%の体は%TEXTR("電流を流されたように跳ねあがり절정をしてしまう/牝穴から愛液を吹き出し雄ちんぽへのアピールをしてしまう/びくっ❤びくっ❤と体を跳ね上げ他者が見ても分かる程に절정してしまう/息を荒げ抵抗も出来ないままその쾌락を受けイきそうになってしまう")%。
						PRINTFORML 「ご主人様、この雌はまだ折れる事無く조교の痕跡もない、折れずにいる牝です」
						PRINTFORMW 「%TEXTR("未だおちんぽ様に媚びその精液を注いでいただく事が牝にとっての喜びであると理解できない哀れな牝でもあります/哀れな牝ですね…、ご主人様のおちんぽ様に責められれば反抗する意思も折れてしまうというのに……/浅ましくもこのようにしてご主人様の寵愛を受けたいマゾ穴なのかもしれませんね/ある意味羨ましくもありますね、おちんぽ様に犯され牝としての本来の在り方を心の底から理解できる状態とも言えますし")%。」
