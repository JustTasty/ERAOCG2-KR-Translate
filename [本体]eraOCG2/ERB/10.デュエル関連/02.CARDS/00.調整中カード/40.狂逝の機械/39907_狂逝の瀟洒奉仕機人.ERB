@CARDNAME_39907,参照先
;여기서 지정 카드의 이름, 약칭을 반환할 예정
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이쿠기어 라크쉬리 메이드노이드
	CASE "略称"
		RESULTS = 광서의소쇄봉사기
	CASE "カテゴリ"
		RESULTS:0 = 광서의 기계
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39907(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3500
	CASE "守備力"
		RETURN 3400
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

