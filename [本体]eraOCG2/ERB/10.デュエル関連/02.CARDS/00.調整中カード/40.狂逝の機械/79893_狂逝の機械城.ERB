@CARDCAN_79893(듀얼리스트,종류,ゾーン,場所)

#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 효과条件플래그 = 0


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79893)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,79893)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	;5以上で발동 = 5未満なら발동させない(RETURN 0)
	;あとこれはフィールド마법 = 마함前提なのでMZ_個Ｃじゃなくて魔_個Ｃを参照する
	SIF GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") < 8 
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == 身代효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF 破壊대상ゾーン != "필드"
		RETURN 0
	SIF !GROUPMATCH(破壊종류,"효과","戦闘")
		RETURN 0
	TRYCALLFORM CARDNAME_{破壊대상ID}("カテゴリ")
	SIF !MATCH(RESULTS,"광서의 기계")
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") < 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
    SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = 광서의 기계
	CALL 효과条件レベル_ADV(8,"以上")
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79893(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 付与個数
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カードID
#DIM DYNAMIC 攻撃力上昇
#DIM DYNAMIC 選択結果
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードレベル

#DIM DYNAMIC 配置先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79893,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			TRYCALLFORM CARD_{RESULT:1},"レベル"
			選択カードレベル = RESULT
			CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",選択カードレベル)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
    효과範囲필드 = 1
    효과条件종류 = モン全般
	효과条件カテゴリ = 광서의 기계
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻")

ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79742)
	IF RESULT == 0
		CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",-1)
		PRINTFORMW 이쿠기어 프리즌 캐슬의 효과로 카운터를 １개 제거하는 것으로 파괴를 막습니다。
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79893,4)
	IF RESULT == 0
	
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
			CONTINUE
		CALL SET_FIELD_INFO(듀얼리스트,"필드",カウンタ,"薬Ｃ",1)
	NEXT
	FOR カウンタ,0,5
		SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
			CONTINUE
		CALL SET_FIELD_INFO(対面者,"필드",カウンタ,"薬Ｃ",1)
	NEXT
	CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",1)
		CALL FULLRESET
		효과条件성별 = 성별在
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드全体",場所,"発情",2)
		CALL FULLRESET
        효과範囲필드 = 1
    	효과条件종류 = モン全般
		효과条件カテゴリ = 광서의 기계
        CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"発情",100)
	ENDIF

ELSEIF 종류 == 攻撃時強制誘発
	IF 戦闘場所(듀얼리스트,1) != 5
		CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",戦闘場所(듀얼리스트,1),1)
		IF RESULT != -1
			조교대상 = RESULT
		ENDIF
	ELSE
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			조교대상 = RESULT
		ENDIF
	ENDIF

	IF 조교대상 > -1
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML %CALLNAME:조교대상%의 두 다리가 기계의 촉수에 휘감겨 움직일 수 없게 된다.
			CALL MADORGASM_MACHINE_VIOLATE_TXT(1,0)
			CALL MADORGASM_RAND_ROB_TXT(1,1)
		CASE 1
			PRINTFORML %CALLNAME:조교대상%에게 조준을 맞추고, 강제로 쾌락을 주입해 절정시키는 광선이 발사된다. %CALLNAME:조교대상%는
			PRINTFORML …그것을 받은 %CALLNAME:조교대상%는,
			CALL MADORGASM_MACHINE_VIOLATE_TXT(1,1)
			CALL MADORGASM_RAND_ROB_TXT(1,4)
		CASE 2
			PRINTFORML %CALLNAME:조교대상%는 갑자기 몸의 움직임을 멈춘다.
			PRINTFORML 그리고 어느새 멍한 눈이 되어버린 %CALLNAME:조교대상%는,
			CALL MADORGASM_MACHINE_VIOLATE_TXT(1,2)
			CALL MADORGASM_HYPNOSIS_OPERATION_TXT(1,0)
		ENDSELECT
		PRINTFORML ……
		PRINTFORML …………
		PRINTFORML ………………그 후, %CALLNAME:조교대상%는 겨우 함정에서 벗어날 수 있었다.
		PRINTFORML 광서의 기계들이 돌아다니고, 음란한 함정이 계속 움직이는 기계성,
		PRINTFORMW 가득 찬 공기마저도 미독이 섞여 있어, %CALLNAME:조교대상%는 그것을 무의식적으로 들이마신다…
		CALL EROTIC_MOVE("媚毒汚染",조교대상,-1,300)

	ENDIF

ENDIF




