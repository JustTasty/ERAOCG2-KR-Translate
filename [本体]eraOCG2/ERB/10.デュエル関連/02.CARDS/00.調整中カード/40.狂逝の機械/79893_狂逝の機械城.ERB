@CARDNAME_79893,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이쿠기어 프리즌 캐슬
	CASE "略称"
		RESULTS = 광서기계간성
	CASE "カテゴリ"
		RESULTS:0 = 광서의 기계
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79893(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79893(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL
PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：이 카드의 발동시의 효과 처리로서、자신 필드か자신・상대의 묘지의 牝性モンスター１장까지 選び제외한다。
PRINTL その後제외した몬스터의 レベルと同じ数の個別카운터を이 카드에 置く。
CALLFORM CARD_ABLE(2,身代효과)
PRINTL ②：자신 필드의 「광서의 기계」モンスターが戦闘・효과で破壊される場合、
PRINTL 대신에 이 카드의 個別카운터を１つ取り除くことができる。
CALLFORM CARD_ABLE(3,지속효과)
PRINTL ③：個別카운터が８個以上乗った이 카드가 마법＆함정ゾーンに存在する限り、
PRINTL 자신의 「광서의 기계」モンスターが戦闘を行う場合、상대는 ダメージステップ終了時まで상대는 カード의 효과를 발동できない。
CALLFORM CARD_ABLE(4,효과後誘発)
PRINTL ④：자신의 レベル８以上の「광서의 기계」몬스터가 소환、특수소환された時、
PRINTL フィールド上の全ての성별を持つ몬스터를 発情させ、미약카운터を１つ置き、이 카드에 個別카운터를 １개 놓는다。
PRINTL 이 효과로 発情した「광서의 기계」モンスターは必ず攻撃できる。
PRINTL
SETCOLOR C_RED
PRINTL ＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊ーー경고ーー＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊
CALLFORM CARD_ABLE(0,攻撃時誘発)
PRINTL ⑤：■の카드가場に存在する時、상대モンスターが攻撃した後に발동する。
PRINTL 機械城の様々な施設や광서의 기계、更には함정が상대 몬스터를 責め犯す。
PRINTL ＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊ーー주의ーー＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊＊
PRINTL
RESETCOLOR
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79893(듀얼리스트,종류,ゾーン,場所)

#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 효과条件플래그 = 0


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79893)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,79893)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	;5以上で발동 = 5未満なら발동させない(RETURN 0)
	;あとこれはフィールド마법 = 마함前提なのでMZ_個Ｃじゃなくて魔_個Ｃを参照する
	SIF GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") < 8 
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == 身代효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF 破壊대상ゾーン != "필드"
		RETURN 0
	SIF !GROUPMATCH(破壊종류,"효과","戦闘")
		RETURN 0
	TRYCALLFORM CARDNAME_{破壊대상ID}("カテゴリ")
	SIF !MATCH(RESULTS,"광서의 기계")
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") < 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
    SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = 광서의 기계
	CALL 효과条件レベル_ADV(8,"以上")
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79893(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 付与個数
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カードID
#DIM DYNAMIC 攻撃力上昇
#DIM DYNAMIC 選択結果
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードレベル

#DIM DYNAMIC 配置先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79893,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			TRYCALLFORM CARD_{RESULT:1},"レベル"
			選択カードレベル = RESULT
			CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",選択カードレベル)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
    효과範囲필드 = 1
    효과条件종류 = モン全般
	효과条件カテゴリ = 광서의 기계
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻")

ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79742)
	IF RESULT == 0
		CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",-1)
		PRINTFORMW 이쿠기어 프리즌 캐슬の효과で카운터を１つ取り除くことで破壊を防ぎました。
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79893,4)
	IF RESULT == 0
	
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
			CONTINUE
		CALL SET_FIELD_INFO(듀얼리스트,"필드",カウンタ,"薬Ｃ",1)
	NEXT
	FOR カウンタ,0,5
		SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
			CONTINUE
		CALL SET_FIELD_INFO(対面者,"필드",カウンタ,"薬Ｃ",1)
	NEXT
	CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",1)
		CALL FULLRESET
		효과条件성별 = 성별在
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드全体",場所,"発情",2)
		CALL FULLRESET
        효과範囲필드 = 1
    	효과条件종류 = モン全般
		효과条件カテゴリ = 광서의 기계
        CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"発情",100)
	ENDIF

ELSEIF 종류 == 攻撃時強制誘発
	IF 戦闘場所(듀얼리스트,1) != 5
		CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",戦闘場所(듀얼리스트,1),1)
		IF RESULT != -1
			조교대상 = RESULT
		ENDIF
	ELSE
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			조교대상 = RESULT
		ENDIF
	ENDIF

	IF 조교대상 > -1
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML %CALLNAME:조교대상%の両足が機械の触手で絡めとられ、身動きが取れなくなり、
			CALL MADORGASM_MACHINE_VIOLATE_TXT(1,0)
			CALL MADORGASM_RAND_ROB_TXT(1,1)
		CASE 1
			PRINTFORML %CALLNAME:조교대상%に照準を向け、強制的に쾌락を叩き込み절정させる光線が放たれる。%CALLNAME:조교대상%は
			PRINTFORML …それを受けた%CALLNAME:조교대상%は、
			CALL MADORGASM_MACHINE_VIOLATE_TXT(1,1)
			CALL MADORGASM_RAND_ROB_TXT(1,4)
		CASE 2
			PRINTFORML %CALLNAME:조교대상%は急に体の動きを止めてしまう。
			PRINTFORML そして、いつの間にか虚ろな瞳となっていた%CALLNAME:조교대상%は、
			CALL MADORGASM_MACHINE_VIOLATE_TXT(1,2)
			CALL MADORGASM_HYPNOSIS_OPERATION_TXT(1,0)
		ENDSELECT
		PRINTFORML ……
		PRINTFORML …………
		PRINTFORML ………………그 후、%CALLNAME:조교대상%はなんとか함정から逃れる事ができた。
		PRINTFORML 광서의 기계たちが動き回り、淫らな함정が動き続ける機械城、
		PRINTFORMW 充満する空気にすら媚毒が混ざっており、%CALLNAME:조교대상%はそれを無自覚に吸ってしまう…
		CALL EROTIC_MOVE("媚毒汚染",조교대상,-1,300)
	ENDIF
ENDIF




