@CARDCAN_79892(듀얼리스트,종류,ゾーン,場所)

#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 효과条件플래그 = 0


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79892)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,79892)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

    CALL FULLRESET
    효과範囲손패 = 1
    효과範囲묘지 = 1
	효과条件カテゴリ = 광서의 기계
	효과条件종류 = モン全般
	효과条件CNT = 2
    CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT < 2
		CALL FULLRESET
    	효과範囲덱 = 1
    	효과条件종류 = モン全般
    	효과条件カテゴリ = 광서의 기계
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		CALL FULLRESET
    	효과範囲묘지 = 1
		효과条件カテゴリ = 광서의 기계
		CALL 효과条件レベル_ADV(8, "以上")
    	효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
		IF 종류 == 마함起動
			SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
				RETURN 0
		ENDIF
		SIF メインフェイズ != 1
		RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79892(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カードID
#DIM DYNAMIC 攻撃力上昇
#DIM DYNAMIC 選択結果
#DIM DYNAMIC 효과条件플래그 = 0
#DIM DYNAMIC 捕縛대상
#DIM DYNAMIC 捕縛候補,紙束最大値
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 절정횟수

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1

CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79892,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종류 = モン全般
		효과条件カテゴリ = 광서의 기계
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			효과条件플래그 |= 1

		CALL FULLRESET
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79892)
		SIF RESULT == 0
			효과条件플래그 |= 2

		IF 효과条件플래그 & 1 && 효과条件플래그 & 2
			IF 듀얼리스트 == "상대"
				選択結果 = はい
			ELSE
				CUSTOMDRAWLINE -
				PRINTL 광서의 기계관리端末のどちらの효과を適用しますか？
				CUSTOMDRAWLINE -
				CALL YN_CHOICE(듀얼리스트,"덱から「광서의 기계」モンスターを１枚묘지に送り、자신フィールド上の기계족の牝性モンスターを１体の攻撃力を１０００アップさせる","자신の손패・묘지から「광서의 기계」カードを２枚제외し、덱・손패・묘지からレベル８以上の「광서의 기계」モンスターを１体특수소환する。")
				選択結果 = RESULT
			ENDIF
		ELSEIF 효과条件플래그 & 2
			選択結果 = いいえ
		ELSE
			選択結果 = はい
		ENDIF

	    IF 選択結果 == はい
			CALL FULLRESET
			CUSTOMDRAWLINE -
			PRINTL 【덱から「광서의 기계」モンスターを１枚묘지に送る事を選択しました。】
			CUSTOMDRAWLINE -
			효과範囲덱 = 1
			효과条件종류 = モン全般
			효과条件カテゴリ = 광서의 기계
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CUSTOMDRAWLINE -
				PRINTL 【자신フィールド上の牝性モンスター１体の攻撃力を１０００アップし、発情させ、「광서의 기계」としても扱うことが出来ます、】
				PRINTL 【효과を付与するモンスターを選択してください。】
				CUSTOMDRAWLINE -
				효과条件종족 = 기계족
				효과条件성별 = 牝性
				효과条件표시 = 表側표시
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
				選択カード = RESULT
				IF 選択カード != 9999
					CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永攻",1000)
					CALL STATCHANGE_EFFECT(듀얼리스트,"필드",選択カード,"発情",1)
				ENDIF
				CALL FULLRESET
					FOR カウンタ,0,現山札数(対面者)
					捕縛대상 = GETVAR(@"%対面者%_덱:カウンタ") ;0→カウンタ
					SIF 捕縛대상 == -1 ;念のため-1の場合は次へ
						CONTINUE
					CALLFORM CARD_{捕縛대상},"성별"
					SIF RESULT == 無性 || RESULT == 雄性
						CONTINUE
					CALL CARD_EROTICS(捕縛대상)
					SIF RESULT < 1 ;CSV持ち以外は제외
						CONTINUE
					捕縛候補:カウンタB = 捕縛대상 ;牝性を候補に格納
					カウンタB++ ;候補数加算
				NEXT
				SIF カウンタB < 1 ;カウンタが0以下=候補0
					RETURN
				捕縛대상 = 捕縛候補:(RAND:カウンタB) ;カウンタBまでの乱数で捕縛候補の中からランダムに捕縛대상を決定
				CALL CARD_EROTICS(捕縛대상) ;必ずCARD_EROTICSを挟む
				조교대상 = RESULT
				IF 조교대상 > 0 ;조교대상条件はあなたを想定しないなら>0で
					SELECTCASE RAND:6
						CASE 0
							PRINTFORML
							PRINTFORML 新しい광서의 기계の増産のために、捕らえられていた%CALLNAME:조교대상%はその牝穴を使いつぶされている、
							PRINTFORML 쾌락を伝えるための信号、その体液、고통、恐怖、쾌감の全てが광서의 기계を動かすための糧にされていく、
							PRINTFORMW %CALLNAME:조교대상%は、新しい牝肉を得るための糧を광서의 기계に捧げるためだけに使いつぶされていく……
						CASE 1
							PRINTFORML
							PRINTFORML 광서의 기계に力を与えるべく既に捕らえられていた%CALLNAME:조교대상%に対しての責めが加速していく。
							PRINTFORML ごつんごつんっ、と子宮を潰し続けていた生殖器の模造品のピストン運動がより重く速いものとなる。
							PRINTFORMW 四肢を失っている%CALLNAME:조교대상%はその胴をのけぞらせ、혀を出しながらイくことしかできない……
						CASE 2
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%を含めた牝穴への凌辱が急に激しく、その肉体の消耗を問わない激しさとなっていた。
							PRINTFORML 광서의 기계に対して一時的な強化の糧を得るべく、牝穴たちの性的な蹂躙を強化させられたのだ。
							PRINTFORMW %CALLNAME:조교대상%は壊されてしまいそうな恐怖と고통から必死に助けを求めながら절정を繰り返す……

						CASE 3
							PRINTFORML
							PRINTFORML 手足を失い、鉄の棒によって組み上げられたフレームの中に押し込められ、機械のアームで運搬される%CALLNAME:조교대상%、
							PRINTFORML アームが彼女を入れようとする鉄の箱には隙間が無いほどに触手が詰め込まれていた。
							PRINTFORMW %CALLNAME:조교대상%は必死に首を振りながら助けを求めるが、フレームごと入れられ、密閉され並べられてしまう……

						CASE 4
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%は媚薬を体内に流し込まれながら口からは呼吸をさせられながら催淫ガスを吸わされ、
							PRINTFORML 肉体を完全に雄ちんぽを咥えるのに適した状態にさせられたまま触手の詰まった箱に詰め込まれる。
							PRINTFORMW 쾌락を受け、発狂し、イき狂うためだけに使われる牝穴として%CALLNAME:조교대상%はその役目を全うする……。

						CASE 5
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%は必死に助けを乞うものの、その声は虚しく機械城にこだますだけであった。
							PRINTFORML そうしていてもしっとりと熱を帯びた息を吐き、全身から溢れる汗とともに両穴から溢れる愛液が垂れ落ちている。
							PRINTFORMW 機械城を動かす糧となるべく生き永らえさせられながら犯される牝肉として、触手の箱に%CALLNAME:조교대상%、は入れられていく……。
					ENDSELECT
					FOR カウンタ,0,4
						CALL EROTIC_MOVE("全身凌辱",조교대상,MASTER,500)
						PRINTFORML 
						CALL MADORGASM_RAND_ROB_TXT(1,0)
						CALL MADORGASM_RAND_ROB_TXT(1,4)
						PRINTFORML
					NEXT
					CALL MADORGASM_RAND_ROB_TXT(1,4);うるさい鳴き声
					PRINTFORML %CALLNAME:조교대상%は体中から体液を耐え流し、涙を溢れさせながら必死に救いを求めている。
					PRINTFORMW この광서의 기계はいつでも動けるようだ、듀얼の盤面に置いても효과を発揮するだろう
				ENDIF
			ENDIF
		ELSE
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲묘지 = 1
			효과条件종류 = モン全般
			효과条件カテゴリ = 광서의 기계
			효과条件行先플래그 = 제외行き
			효과条件複数 = 2
			CUSTOMDRAWLINE -
			PRINTL レベル８以上の광서의 기계を소환するには손패・묘지に存在する「광서의 기계」モンスターを２体제외しなければなりません、
			CUSTOMDRAWLINE -
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲손패 = 1
				효과範囲묘지 = 1
				효과範囲덱 = 1
				효과条件カテゴリ = 광서의 기계
				CALL 효과条件レベル_ADV(8, "以上")
				효과条件소환可 = 1
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					FOR カウンタ,0,現山札数(対面者)
						捕縛대상 = GETVAR(@"%対面者%_덱:カウンタ") ;0→カウンタ
						SIF 捕縛대상 == -1 ;念のため-1の場合は次へ
							CONTINUE
						CALLFORM CARD_{捕縛대상},"성별"
						SIF RESULT == 無性 || RESULT == 雄性
							CONTINUE
						CALL CARD_EROTICS(捕縛대상)
						SIF RESULT < 1 ;CSV持ち以外は제외
							CONTINUE
						捕縛候補:カウンタB = 捕縛대상 ;牝性を候補に格納
						カウンタB++ ;候補数加算
					NEXT
					SIF カウンタB < 1 ;カウンタが0以下=候補0
						RETURN
					捕縛대상 = 捕縛候補:(RAND:カウンタB) ;カウンタBまでの乱数で捕縛候補の中からランダムに捕縛대상を決定
					CALL CARD_EROTICS(捕縛대상) ;必ずCARD_EROTICSを挟む
					조교대상 = RESULT
					IF 조교대상 > 0 ;조교대상条件はあなたを想定しないなら>0で
						SELECTCASE RAND:4
							CASE 0
								PRINTFORML
								PRINTFORML 強大な機械兵を動かすための糧を得るべく、牝穴たちは加工され尽くされた肉体を犯され続けている、
								PRINTFORML 捕らえられている%CALLNAME:조교대상%も気づけばその１つとされていた。
								PRINTFORMW 機械城の施設の１つである密閉され媚薬が詰め込まれたカプセルの中で、%CALLNAME:조교대상%は凌辱され続けている……
							CASE 1
								PRINTFORML
								PRINTFORML 大型の광서의 기계を動かすためのエネルギーを得るべく、%CALLNAME:조교대상%を含めた牝肉は犯され続けていた
								PRINTFORML 절정と発狂を繰り返し、精神が壊れそうになれば無理矢理に補強され正気を保たされ続ける。
								PRINTFORMW %CALLNAME:조교대상%は、終わる事のない性的蹂躙に절정と喘ぎ声を返し、광서의 기계に捧げている……
							CASE 2
								PRINTFORML
								PRINTFORML %CALLNAME:조교대상%は大型の광서의 기계を作動させるためのエネルギー源を生み出すために、
								PRINTFORML 四肢を切断され、機械城の施設の中で延々と犯されるための牝穴として組み込まれてしまっていた。
								PRINTFORMW %CALLNAME:조교대상%を含めた無数の牝穴たちの喘ぎ声が機械城に響き渡る……

							CASE 3
								PRINTFORML
								PRINTFORML 既に捕縛されていた牝の１つが機械兵として手元に写される
								PRINTFORML 手足を切断され、責め苦を与えられ続ける機械に頼らないと動くこともできない、そんな牝肉。
								PRINTFORMW %CALLNAME:조교대상%、機械に組み込まれた牝の腹部にその中が刻まれていた。
						ENDSELECT
						FOR カウンタ,0,4
							PRINTFORML 
							CALL MADORGASM_RAND_ROB_TXT(1,0)
							CALL MADORGASM_RAND_ROB_TXT(1,4)
							PRINTFORML
							CALL EROTIC_MOVE("全身凌辱",조교대상,MASTER,500)
						NEXT
						CALL MADORGASM_RAND_ROB_TXT(1,4);うるさい鳴き声
						PRINTFORML %CALLNAME:조교대상%は体中から体液を耐え流し、涙を溢れさせながら必死に救いを求めている。
						PRINTFORMW この광서의 기계はいつでも動けるようだ、듀얼の盤面に置いても효과を発揮するだろう
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79892)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ENDIF





