@CARDCAN_39901(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 調教対象
#DIM DYNAMIC 選択カード名前

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,39901)
SIF RESULT == 1
	RETURN 0

IF 種類 == 攻撃後誘発
	SIF ゾーン != "戦場"
		RETURN 0
	SIF GETVAR(@"%決闘者%_戦闘カード") != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(決闘者,1) == 0
		RETURN 0
	SIF モンスター数(対面者) >= 5
		RETURN 0
	RETURN 1

ELSEIF 種類 == 永続効果
	SIF ゾーン != "戦場"
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0

	RETURN 1

ELSEIF 種類 == 戦場起動
	SIF 発動済_3(決闘者,39901)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	RETURN 1
ENDIF


