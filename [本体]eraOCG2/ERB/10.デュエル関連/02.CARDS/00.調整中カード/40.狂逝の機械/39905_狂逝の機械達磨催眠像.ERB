@CARDCAN_39905(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39905)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,39905)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39905,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
    효과範囲마함 = 1
	효과条件カテゴリ = 광서의 기계
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 戦闘→묘지
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	SIF 戦闘カードID(듀얼리스트,1) == -1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_39905(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 減少値

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39905,1)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류:0 = 24
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"攻誘")

ELSEIF 종류 == 攻撃時強制誘発
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3)

ELSEIF 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39905,3)
    IF RESULT == 0
		조교대상 = 戦闘場所(듀얼리스트,1)
			CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",조교대상)
				IF 조교대상 != -1
					SELECTCASE RAND:5
						CASE 0
						PRINTFORML
						PRINTFORML 達磨にされた牝をそのまま案山子に括り付けたかのように置いているだけのような達磨최면像に%CALLNAME:조교대상%は目を奪われる。
						PRINTFORMW すると、%CALLNAME:조교대상%の肉体の蜜穴の膣肉、その子宮口、胃や喉に至るまで「びくんっ！」と跳ねあがり절정する。
						PRINTFORML 急な現象に対して%CALLNAME:조교대상%は呆然と자신が倒した達磨최면像に目を向け続ける。
						PRINTFORMW 呆然と立ち続け、下半身の足先に至るまで愛液でぐっしょりと濡れてしまうまで%CALLNAME:조교대상%は최면像から目を離せなかった……
						CASE 1
						PRINTFORML
						PRINTFORML %CALLNAME:조교대상%は天井に下げられ首を吊るかのようにされながら、下からはディルドで突き上げられていた達磨の牝の目を見てしまっていた。
						PRINTFORMW すると、%CALLNAME:조교대상%はぼうっ…と脳内に靄がかかったかのようになり、肉体が無意識に自身の過敏な部位に触れ弄り始めてしまう。
						PRINTFORML 目線を合わせ、喘ぎ声の音の振動を伝って脳に命令を与える達磨최면像、
						PRINTFORMW %CALLNAME:조교대상%はその術中にハまり、ガニ股で目の前の牝を見ながら자위をし続けてしまう……
						CASE 2
						PRINTFORML
						PRINTFORML %CALLNAME:조교대상%は達磨최면像を何事も無く倒す事ができた。
						PRINTFORMW 転げ落ちても尚喘ぎ、”ご主人様”と仰ぐ存在へ媚びる言葉を並べながら転がる牝穴を見て恐怖を覚えていく。
						PRINTFORML 「捕まるわけにはいかない」と思いながら%CALLNAME:조교대상%はぐちゅぐちゅと自身の蜜穴をほじくり弄っていた、
						PRINTFORMW 光を失い自身が최면にかかったことも気づかず、とろけている牝穴を達磨최면像に삽입されていたディルドで慰めている…
						CASE 3
						PRINTFORML
						PRINTFORML 転げ倒れる達磨최면像が下の口で咥え続け愛液まみれになっていたディルドで%CALLNAME:조교대상%は何を思ったか自身を慰め始めた。 
						PRINTFORMW 気づけば熱と愛液で蒸れてしまっていた衣服を脱ぎ、そのディルドでぐちゅぐちゅと牝として雄ちんぽに媚びようとする穴をほじくる。
						PRINTFORML 虚ろな瞳のまま手に持ったディルドを根元までねじ込んでは奥をこつんこつんと突き続け肉体を절정させる%CALLNAME:조교대상%、
						PRINTFORMW ”자신が최면にかかっている”という自覚なく、自らの肉体を弄び続けた…
						CASE 4
						PRINTFORML 
						PRINTFORML 達磨최면像が転げ落ち倒れる、「ぁ……あ”…❤」と喘ぎ、愛液を溢れ吹き出させながら倒れても尚イき続けている。
						PRINTFORMW %CALLNAME:조교대상%はその光景に恐怖を覚えながら、その場を去ろうとした……が、
						PRINTFORML なにを思ったか自身の過敏な要所を자신自身で弄り始めてしまう。
						PRINTFORMW 目の前にある達磨최면像からの최면の影響で、無様な姿を晒している達磨娘の姿に자신を投影しながら%CALLNAME:조교대상%は자위をし続ける……
					ENDSELECT
				ENDIF
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",조교대상,"지속無効")
	    CALL STATCHANGE_EFFECT(対面者,"필드",조교대상,"発情",2)
	ENDIF



ENDIF




