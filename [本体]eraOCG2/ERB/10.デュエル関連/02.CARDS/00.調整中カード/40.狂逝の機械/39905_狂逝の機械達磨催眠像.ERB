@CARDNAME_39905,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이쿠기어 림즈 커트 샌드백
	CASE "略称"
		RESULTS = 광서의달마상
	CASE "カテゴリ"
		RESULTS:0 = 광서의 기계
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39905(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 1300
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

