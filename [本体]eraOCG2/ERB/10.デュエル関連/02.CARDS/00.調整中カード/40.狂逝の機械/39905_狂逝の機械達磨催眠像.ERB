@CARDCAN_39905(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39905)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,39905)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39905,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
    효과範囲마함 = 1
	효과条件カテゴリ = 광서의 기계
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 戦闘→묘지
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	SIF 戦闘カードID(듀얼리스트,1) == -1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_39905(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 減少値

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39905,1)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류:0 = 24
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"攻誘")

ELSEIF 종류 == 攻撃時強制誘発
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3)

ELSEIF 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39905,3)
    IF RESULT == 0
		조교대상 = 戦闘場所(듀얼리스트,1)
			CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",조교대상)
				IF 조교대상 != -1
					SELECTCASE RAND:5
						CASE 0
						PRINTFORML
						PRINTFORML 달마로 만들어진 암컷을 허수아비처럼 묶어둔 것 같은 달마최면상에 %CALLNAME:조교대상%은 시선을 빼앗긴다.
						PRINTFORMW 그러자 %CALLNAME:조교대상%의 육체의 질육, 자궁구, 위와 목까지 "움찔!"하며 튀어오르며 절정한다.
						PRINTFORML 갑작스러운 현상에 %CALLNAME:조교대상%은 멍하니 자신이 쓰러뜨린 달마최면상을 계속 바라본다.
						PRINTFORMW 멍하니 서있으며, 하반신 발끝까지 애액으로 흠뻑 젖을 때까지 %CALLNAME:조교대상%은 최면상에서 시선을 뗄 수 없었다……
						CASE 1
						PRINTFORML
						PRINTFORML %CALLNAME:조교대상%은 천장에 매달려 목이 졸린 듯 하면서, 아래에서는 딜도로 찔려 올라가는 달마 암컷의 눈을 보고 있었다.
						PRINTFORMW 그러자 %CALLNAME:조교대상%은 머릿속에 안개가 낀 듯 멍해지고, 육체가 무의식적으로 자신의 예민한 부위를 만지기 시작한다.
						PRINTFORML 시선을 맞추고, 신음소리의 진동을 통해 뇌에 명령을 내리는 달마최면상,
						PRINTFORMW %CALLNAME:조교대상%은 그 술수에 빠져 다리를 벌리고 눈앞의 암컷을 보며 자위를 계속한다……
						CASE 2
						PRINTFORML
						PRINTFORML %CALLNAME:조교대상%은 달마최면상을 아무 일 없이 쓰러뜨릴 수 있었다.
						PRINTFORMW 굴러떨어져도 신음하며, "주인님"이라 부르는 존재에게 아첨하는 말을 늘어놓으며 굴러다니는 암컷 구멍을 보고 두려움을 느낀다.
						PRINTFORML "잡히면 안 된다"고 생각하며 %CALLNAME:조교대상%은 질을 파고 만지작거렸다,
						PRINTFORMW 빛을 잃고 자신이 최면에 걸린 것도 모른 채, 녹아내린 암컷 구멍을 달마최면상에 삽입된 딜도로 위로하고 있다…
						CASE 3
						PRINTFORML
						PRINTFORML 굴러떨어진 달마최면상이 아래 입으로 물고 있던 애액 범벅 딜도로 %CALLNAME:조교대상%은 자신을 위로하기 시작했다.
						PRINTFORMW 정신을 차려보니 열기와 애액에 젖은 옷을 벗고, 그 딜도로 암컷으로서 수컷 자지에 아첨하려는 구멍을 파고 있다.
						PRINTFORML 멍한 눈으로 손에 든 딜도를 뿌리까지 밀어넣고 안쪽을 계속 찔러 육체를 절정시키는 %CALLNAME:조교대상%,
						PRINTFORMW "자신이 최면에 걸려 있다"는 자각 없이, 자신의 육체를 계속 만지작거렸다…
						CASE 4
						PRINTFORML 
						PRINTFORML 달마최면상이 굴러떨어져 쓰러지며, "아……아”…❤"라 신음하고, 애액을 흘리며 쓰러져도 계속 절정하고 있다.
						PRINTFORMW %CALLNAME:조교대상%은 그 광경에 두려움을 느끼며 그 자리를 떠나려 했다…… 하지만,
						PRINTFORML 무슨 생각인지 자신의 예민한 부위를 스스로 만지기 시작한다.
						PRINTFORMW 눈앞에 있는 달마최면상에서의 최면 영향으로, 무방비한 모습을 드러내는 달마 소녀의 모습에 자신을 투영하며 %CALLNAME:조교대상%은 자위를 계속한다……
					ENDSELECT
				ENDIF
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",조교대상,"지속無効")
	    CALL STATCHANGE_EFFECT(対面者,"필드",조교대상,"発情",2)
	ENDIF



ENDIF




