@CARDNAME_24865,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 偽りの竜 “Я”
	CASE "略称"
		RESULTS = 竜Я
	CASE "カテゴリ"
		RESULTS:0 = Я
ENDSELECT

@CARD_24865(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "種類"
		RETURN 通常モン
	CASE "属性"
		RETURN 炎属性
	CASE "種族"
		RETURN ドラゴン族
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2500
	CASE "性別"
		RETURN 無性
ENDSELECT
RETURN 0

