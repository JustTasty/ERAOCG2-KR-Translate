SELECTCASE 参照先
	CASE "名前"
		RESULTS = 暴虐なる獣 “Я”
	CASE "略称"
		RESULTS = 獣Я
	CASE "カテゴリ"
		RESULTS:0 = Я
ENDSELECT

@CARD_24863(参照先)

#DIMS DYNAMIC 参照先
