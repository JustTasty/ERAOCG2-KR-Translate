@CARDNAME_33302,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = CAN－I：D
	CASE "略称"
		RESULTS = CAN－I：D
	CASE "カテゴリ"
		RESULTS:0 = CAN：D
ENDSELECT


@CARD_33302(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33302(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 1번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 사이킥족 융합 몬스터 또는 일반 몬스터가 존재할 경우 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다。
CALLFORM CARD_ABLE(2,특수소환時)
PRINTL ②: 이 카드가 특수 소환에 성공했을 경우, 패에서 사이킥족 일반 몬스터 1장을 버리고 발동할 수 있다.
PRINTL 패에서 레벨 7 사이킥족 몬스터 1장을 특수 소환한다.
PRINTL 자신의 필드 / 묘지에 「CAN：D」가 존재할 경우, 추가로 자신 필드의 사이킥족 몬스터의 공격력은 700 포인트 올린다.

@CARDCAN_33302(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33302)
SIF RESULT == 1
	RETURN 0


IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,33302)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33302,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이킥족
	효과条件종류:0 = 융합
	효과条件종류:1 = 通常モン
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 특수소환時
	SIF 발동済_2(듀얼리스트,33302)
		RETURN 0
	SIF 손패数(듀얼리스트) < 2
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종족 = 사이킥족
	효과条件종류 = 通常モン
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종족 = 사이킥족
	효과条件レベル = 7
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33302(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33302,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33302,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종족 = 사이킥족 
		효과条件종류 = 通常モン
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件종족 = 사이킥족
			효과条件レベル = 7
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲묘지 = 1

			효과条件名前 = CAN：D
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과条件종족 = 사이킥족
				CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"永攻",700)
			ENDIF
		ENDIF
	ENDIF
ENDIF


