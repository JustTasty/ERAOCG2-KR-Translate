@CARDNAME_33301,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = CAN－Melo：D
	CASE "略称"
		RESULTS = CAN－Melo：D
	CASE "カテゴリ"
		RESULTS:0 = CAN：D
ENDSELECT


@CARD_33301(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33301(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 1번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：자신 필드에 사이킥족の융합モンスター、또는 일반 몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 패에서 특수 소환한다。
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②：자신 필드의 사이킥족 몬스터 １장을 패로 되돌리고、상대 필드의 앞면 표시인 카드 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 카드를 상대의 패로 되돌린다。
PRINTL 패에 戻した자신 필드의 카드가 「CAN：D"인 경우、대신에 상대의 덱으로 되돌린다。

@CARDCAN_33301(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33301)
SIF RESULT == 1
	RETURN 0


IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,33301)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33301,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이킥족
	효과条件종류:0 = 융합
	효과条件종류:1 = 通常モン
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,33301)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33301(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33301,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33301,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 사이킥족 
		효과条件行先플래그 = 손패行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARDNAME_{RESULT:1}("名前")
			IF RESULTS == "CAN：D"
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"덱")
			ELSE
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"손패")
			ENDIF
		ENDIF
	ENDIF
ENDIF


