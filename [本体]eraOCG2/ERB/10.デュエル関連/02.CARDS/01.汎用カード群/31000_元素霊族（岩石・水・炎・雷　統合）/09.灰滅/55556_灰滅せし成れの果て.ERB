@CARDSUMMON_AA_55556


@CARDSUMMON_55556(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件종족 = 원소정령족

IF EX参照 == 0 || EX参照 == 1
ELSEIF EX参照 >= 10
ENDIF

IF EX参照 == 1
	;PRINTL サモンまでは言ってるよ
	CALL FUSCAN(듀얼리스트,소환カード,55556) 
	RETURN RESULT
ENDIF


@CARDCAN_55556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,55556)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,55556)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = フィールド
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時誘発
	SIF 발동済_2(듀얼리스트,55556)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_55556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,55556,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = フィールド
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,55556,2)
	IF RESULT == 0
		選択カード = 戦闘場所(듀얼리스트,1)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",選択カード,"손패")
		SIF RESULT == 1 && ターンプレイヤー == 듀얼리스트
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦",1)
	ENDIF
ENDIF


