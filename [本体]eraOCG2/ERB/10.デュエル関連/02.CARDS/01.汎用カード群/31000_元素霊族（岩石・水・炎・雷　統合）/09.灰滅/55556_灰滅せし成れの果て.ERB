@CARDNAME_55556,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 회멸せし成れの果て
	CASE "略称"
		RESULTS = 회멸せし成の果て
	CASE "カテゴリ"
		RESULTS:0 = 회멸
ENDSELECT


@CARD_55556(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RESULT:1 = 화염속성
		RETURN 어둠속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2900
	CASE "守備力"
		RETURN 2400
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250630
ENDSELECT
RETURN 0

