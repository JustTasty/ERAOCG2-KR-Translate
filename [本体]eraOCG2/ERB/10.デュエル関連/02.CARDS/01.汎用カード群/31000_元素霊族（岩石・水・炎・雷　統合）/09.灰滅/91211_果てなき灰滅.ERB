@CARDNAME_91211,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 끝없는 회멸
	CASE "略称"
RESULTS = 끝없는 회멸
	CASE "カテゴリ"
RESULTS = 0 = 회멸
ENDSELECT


@CARD_91211(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "추가日"
		RETURN 250630
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91211(종류)
#DIM DYNAMIC 종류
#DIMS DYNAMIC 듀얼리스트

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動)
PRINTL ①: 자신・상대의 메인 페이즈에 발동할 수 있다.
PRINTL 자신・상대 필드의 몬스터를 융합 소재로 하여, 원소정령족의 융합 몬스터 1체를 융합 소환한다.
PRINTL 그 때, 첫 번째 융합 소재는 '멸망룡 베이도스'로 해야 한다.
PRINTL 이 효과로 특수 소환한 몬스터의 공격력은 1500 업한다.
;この효과で素材にされたモンスターの数を取れないので、適当に中間値に。
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 자신의 묘지에서 원소정령족・어둠속성 몬스터 1체와 이 카드를 제외하고 발동할 수 있다.
PRINTL 상대 필드의 모든 몬스터는 턴 종료시까지 원소정령족이 된다.


@CARDCAN_91211(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91211)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1 || 종류 == フリー誘発 && ゾーン == "마함" ;&& GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,91211)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL RESETAREA_MAT
	素材範囲필드 = 1
	素材範囲상대필드 = 1
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 융합
	효과条件종족 = 원소정령족
	強制必要素材名前 = 멸망룡 베이도스
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動; || 종류 == フリー誘発 && ゾーン == "마함" && GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発 && ゾーン == "묘지"
	SIF 발동済_2(듀얼리스트,91211)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 원소정령족
	효과条件속성 = 어둠속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_91211(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 行先플래그
#DIM DYNAMIC ライフ회복量



CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1 || 종류 == フリー誘発 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91211,1)
	IF RESULT == 0
		CALL RESETAREA_MAT
		素材範囲필드 = 1
		素材範囲상대필드 = 1
		CALL FULLRESET
		효과条件종류 = 융합
		효과条件종족 = 원소정령족
		強制必要素材名前 = 멸망룡 베이도스
		효과条件소환可 = 2
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
			CALL FUSSUMMON(듀얼리스트,選択カード)
			IF RESULT == 1
				CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"永攻",1500)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == フリー誘発 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91211)
	IF RESULT == 0
		TRYCALLFORM CARDCAN_91211(듀얼리스트,마함起動,"마함",場所)
		IF RESULT == 1
			PRINTFORML 
			PRINTFORML 今、효과を使用しますか？
			PRINTFORML 
			CALL YN_CHOICE(듀얼리스트)
			IF RESULT == 0
				TRYCALLFORM CARDEFFECT_91211(듀얼리스트,마함起動,"마함",場所)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発 && ゾーン == "묘지"
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91211,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 원소정령족
		효과条件속성 = 어둠속성
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL STATCHANGE_OTHER(듀얼리스트,"상대필드",場所,"一種",원소정령족,91211)
		ENDIF
	ENDIF
ENDIF
