@CARDNAME_93115,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라미드 펄스
	CASE "略称"
		RESULTS = 고대유적의 각성
	CASE "カテゴリ"
		RESULTS:0 = 트라미드
ENDSELECT


@CARD_93115(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "추가日"
		RETURN 250706
ENDSELECT

RETURN 0

@CARD_EXPLANATION_93115(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(10,마함起動,フリー誘発)
PRINTL ①：１ターンに１度、자신の묘지から、
PRINTL 땅속성・원소정령족モンスターまたはフィールド마법カードを合計２枚제외し、
PRINTL 以下の효과から１つを選択して발동できる。
PRINTL ●상대フィールドの表側표시カード１枚を破壊する。
PRINTL ●자신の묘지の땅속성・원소정령족モンスター１体を守備표시で특수소환する。


@CARDCAN_93115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 발동불가,2
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93115)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1 || 종류 == フリー誘発 && ゾーン == "마함"
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件スーパー종류 = フィールド
	효과条件속성 = 땅속성
	효과条件종족 = 원소정령족
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0

	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 表側표시
	효과条件除去種 = 묘지
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 = 1
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 땅속성
	효과条件종족 = 원소정령족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:1 = 1
	
	SIF 발동불가:0 == 1 && 발동불가:1 == 1
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_93115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 발동불가,2
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1 || 종류 == フリー誘発 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93115,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件スーパー종류 = フィールド
		효과条件속성 = 땅속성
		효과条件종족 = 원소정령족
		효과条件行先플래그 = 제외行き
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
			효과条件표시 = 表側표시
			효과条件除去種 = 묘지
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				발동불가:0 = 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件속성 = 땅속성
			효과条件종족 = 원소정령족
			효과条件소환可 = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				발동불가:1 = 1
			
			IF 발동불가:0 != 1 && 발동불가:1 != 1
				CALL YN_CHOICE(듀얼리스트,"●상대の表側カードを破壊する。","●묘지の땅속성・원소정령족を특수소환する。")
			ELSEIF 발동불가:0 != 1
				RESULT = 0
			ELSEIF 발동불가:1 != 1
				RESULT = 1
			ELSEIF 발동불가:0 == 1 && 발동불가:1 == 1
				PRINTW 효과の대상が存在しないため処理を終了します
				RETURN 0
			ENDIF

			IF RESULT == 0
				CALL FULLRESET
				효과条件표시 = 表側표시
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지")
			ELSEIF RESULT == 1
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件속성 = 땅속성
				효과条件종족 = 원소정령족
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 마함起動 || 종류 == フリー誘発 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93115)
	IF RESULT == 0
		TRYCALLFORM CARDCAN_93115(듀얼리스트,마함起動,"마함",場所)
		IF RESULT == 1
			PRINTFORML 
			PRINTFORML 今、효과を使用しますか？
			PRINTFORML 
			CALL YN_CHOICE(듀얼리스트)
			IF RESULT == 0
				TRYCALLFORM CARDEFFECT_93115(듀얼리스트,마함起動,"마함",場所)
			ENDIF
		ENDIF
	ENDIF
ENDIF
