PRINTL 이 카드가 몬스터 존에 있는 경우、
PRINTL 이 카드의 공격력은 자신의 필드 / 묘지의 "트라미드"몬스터 카드의 수×５００올린다。
CALLFORM CARD_ABLE(0,バトル開始時)
PRINTL ②：배틀 페이즈 개시시에 발동할 수 있다。
PRINTL 마법＆함정존의 이 카드를 특수 소환한다。
PRINTL 몬스터 존의 이 카드는、배틀 페이즈 종료시에 마법＆함정존으로 돌아간다。
CALLFORM CARD_ABLE(3,마함起動,필드起動,フリー誘発)
PRINTL ③：서로의 배틀 페이즈에、패를 １장 버리고 발동할 수 있다。
PRINTL 상대 필드의 카드 １장을 파괴한다。

@CARDCAN_173115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,173115)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1 || 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_3(듀얼리스트,173115)
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	SIF 손패数(듀얼리스트) < 1
		RETURN 0
	IF ゾーン == "필드"
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == バトル開始時
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,173115,ゾーン)
	SIF RESULT == 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == バトル終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과 || 종류 == 攻守変動
	SIF ゾーン != "마함" && ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_173115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1 || 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,173115,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
			CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"묘지")
		ENDIF
	ENDIF

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,173115)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,173115)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == バトル終了時強制
	IF ターンプレイヤー == 듀얼리스트
		;クルーザー
		CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,73115)
	ELSE
		;포트리스
		CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,273115)
	ENDIF
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件カテゴリ = 트라미드
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,500,500)

	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 트라미드
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	攻撃力修正 = RESULT * 500
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "트라미드")
	CALL FULLRESET
	효과条件カテゴリ = 트라미드
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻")
ENDIF

