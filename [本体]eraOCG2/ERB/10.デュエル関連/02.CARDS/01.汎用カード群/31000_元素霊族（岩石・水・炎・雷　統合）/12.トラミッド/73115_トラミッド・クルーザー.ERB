		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件カテゴリ = 트라미드
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,273115)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
;クルーザー
ELSEIF 종류 == ターン開始時強制
	CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,73115)
;キング
ELSEIF 종류 == バトル開始時強制
	CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,173115)

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "트라미드")

	CALL FULLRESET
	효과条件カテゴリ = 트라미드
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 트라미드
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		CALL STATCHANGE_EFFECT(듀얼리스트,"魔自身",場所,"破耐")
ENDIF
