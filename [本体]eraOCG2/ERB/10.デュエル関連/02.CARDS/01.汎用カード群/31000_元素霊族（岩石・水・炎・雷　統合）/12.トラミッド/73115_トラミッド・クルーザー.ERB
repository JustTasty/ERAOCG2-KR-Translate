#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라미드 크루저
	CASE "略称"
		RESULTS = 트라미드 Ｃ
	CASE "カテゴリ"
		RESULTS:0 = 트라미드
ENDSELECT


@CARD_73115(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
	CASE "生成カード"
		RETURN 173115
	CASE "추가日"
		RETURN 250706
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73115(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 「트라미드」가 된다。
PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ①：자신 필드에 "트라미드"몬스터가 소환된 경우에 발동할 수 있다。
PRINTL 자신은 ５００ＬＰ회복하고、１장 드로우한다。
CALLFORM CARD_ABLE(2,마함起動)
PRINTL ②：자신 메인 페이즈에 발동할 수 있다。
PRINTL 덱에서 「트라미드」몬스터 １장을 패에 넣고、패를 １장 버린다。
PRINTL ③：이 카드는 턴 플레이어 및 페이즈에 따라서 이하의 카드로 변형한다。
PRINTL ●자신 턴　　："트라미드 크루저"
PRINTL ●배틀 페이즈："트라미드 킹 골렘"
PRINTL ●상대 턴　　："트라미드 포트리스"

@CARDCAN_73115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73115)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,73115)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 트라미드
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0

	CALL FULLRESET
	효과条件カテゴリ = 트라미드
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"通常소환")
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン開始時強制
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == バトル開始時強制
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73115,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 트라미드
		효과条件종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73115)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73115)
	IF RESULT == 0
		CALL LIFE_CHANGE(듀얼리스트,"増加","효과",500)
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
;포트리스
ELSEIF 종류 == ターン開始時強制
	CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,273115)
;キング
ELSEIF 종류 == バトル開始時強制
	CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,173115)

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "트라미드")
ENDIF

