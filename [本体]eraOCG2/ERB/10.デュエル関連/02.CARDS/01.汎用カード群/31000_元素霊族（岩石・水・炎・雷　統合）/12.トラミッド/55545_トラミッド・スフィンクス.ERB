@CARDNAME_55545,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라미드 스핑크스
	CASE "略称"
		RESULTS = Ｔ 스핑크스
	CASE "カテゴリ"
		RESULTS:0 = 트라미드
ENDSELECT


@CARD_55545(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2500
	CASE "추가日"
		RETURN 250706
ENDSELECT
RETURN 0


@CARDSUMMON_55545(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 3
素材条件カテゴリ = 트라미드

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,55545) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_55545(종류)
#DIM DYNAMIC 종류

PRINTL レベル３「트라미드」モンスター×２
PRINTL このカード名の①の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①：このカードのＸ素材を１つ取り除いて발동できる。
PRINTL 덱から「트라미드」カード１枚を손패に加える。
CALLFORM CARD_ABLE(0,攻守変動,지속효과)
PRINTL ②：このカードの攻撃力・守備力は자신の묘지の「트라미드」カードの종류×５００アップし、
PRINTL 상대は「트라미드 스핑크스」以外のモンスターを攻撃대상にできない。

@CARDCAN_55545(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 발동불가,2

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,55545)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,55545)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
    CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 트라미드
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_55545(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 발동불가,2
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,55545,1,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 트라미드
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 트라미드
	효과条件固名前起動 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,RESULT*500,RESULT*500)

ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件固名前 = 트라미드 스핑크스
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"攻避")
ENDIF