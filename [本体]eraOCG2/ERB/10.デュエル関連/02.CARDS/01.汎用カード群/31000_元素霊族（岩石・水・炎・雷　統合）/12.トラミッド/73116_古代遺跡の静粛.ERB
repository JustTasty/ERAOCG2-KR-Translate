@CARDNAME_73116,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라미드 로딩
	CASE "略称"
		RESULTS = 고대유적의 정순
	CASE "カテゴリ"
		RESULTS:0 = 트라미드
ENDSELECT


@CARD_73116(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "추가日"
		RETURN 250706
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73116(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①：フィールド마법カードの효과が발동した時に발동できる。
PRINTL 덱からこのカード名以外の「트라미드」마법・함정カード１枚を손패に加える。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：자신フィールドの땅속성・원소정령족モンスターが戦闘または상대の효과で破壊された場合に발동できる。
PRINTL 덱から「트라미드」モンスター１体を특수소환する。


@CARDCAN_73116(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73116)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,73116)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = フィールド
	CALL CARD_CHECK_START_DATA(듀얼리스트,2)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 트라미드
	효과条件종류 = 마함全般
	효과条件固名前 = 트라미드 로딩
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,73116)
		RETURN 0
	SIF 処理中_듀얼리스트 == 듀얼리스트
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과条件속성 = 땅속성
	효과条件종족 = 원소정령족
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般",2)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 트라미드
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
			RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_73116(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73116,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 트라미드
		효과条件종류 = 마함全般
		효과条件固名前 = 트라미드 로딩
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73116,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 트라미드
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF


ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73116)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ENDIF

