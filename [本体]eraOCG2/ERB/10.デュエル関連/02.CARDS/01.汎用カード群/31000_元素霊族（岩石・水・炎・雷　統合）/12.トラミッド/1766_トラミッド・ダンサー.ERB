@CARDNAME_1766,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라미드 댄서
	CASE "略称"
		RESULTS = Ｔ 댄서
	CASE "カテゴリ"
		RESULTS:0 = 트라미드
ENDSELECT


@CARD_1766(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 600
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250706
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1766(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,소환時,특수소환時)
PRINTL ①：이 카드가 소환 / 특수 소환した場合に발동할 수 있다。
PRINTL 덱에서 땅속성・원소정령족몬스터 １장을 묘지へ送り、덱에서 「트라미드」함정카드 １장을 패에 넣는다。
CALLFORM CARD_ABLE(10,필드起動,フリー誘発)
PRINTL ②：お互いのターンに１度、자신의 묘지의 「트라미드」カードか원소정령족モンスター１枚を대상으로 하고 발동할 수 있다。
PRINTL そのカードを덱に戻し、자신 필드의 땅속성・원소정령족몬스터의 공격력 / 수비력은 ５００올린다。


@CARDCAN_1766(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1766)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,1766)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 땅속성
	효과条件종족 = 원소정령족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 트라미드
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件スーパーカテゴリ = 트라미드
	효과条件종족 = 원소정령족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1766(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1766,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件속성 = 땅속성
		효과条件종족 = 원소정령족
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 트라미드
			효과条件종류 = 함정全般
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1766,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件スーパーカテゴリ = 트라미드
		효과条件종족 = 원소정령족
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件종족 = 원소정령족
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"永攻",500)
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"永守",500)
		ENDIF
	ENDIF
ENDIF
