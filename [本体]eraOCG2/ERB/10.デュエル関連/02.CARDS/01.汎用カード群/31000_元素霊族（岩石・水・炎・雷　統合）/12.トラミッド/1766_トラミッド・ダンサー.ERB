@CARDNAME_1766,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라미드 댄서
	CASE "略称"
		RESULTS = Ｔ 댄서
	CASE "カテゴリ"
		RESULTS:0 = 트라미드
ENDSELECT


@CARD_1766(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 効果モン
	CASE "属性"
		RETURN 地属性
	CASE "種族"
		RETURN 元素霊族
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 600
	CASE "守備力"
		RETURN 1900
	CASE "性別"
		RETURN 牝性
	CASE "追加日"
		RETURN 250706
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1766(種類)
#DIM DYNAMIC 種類

PRINTL このカード名の①の効果は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,召喚時,特殊召喚時)
PRINTL ①：このカードが召喚・特殊召喚した場合に発動できる。
PRINTL デッキから地属性・元素霊族モンスター１体を墓地へ送り、デッキから「트라미드」罠カード１枚を手札に加える。
CALLFORM CARD_ABLE(10,戦場起動,フリー誘発)
PRINTL ②：お互いのターンに１度、自分の墓地の「트라미드」カードか元素霊族モンスター１枚を対象として発動できる。
PRINTL そのカードをデッキに戻し、自分フィールドの地属性・元素霊族モンスターの攻撃力・守備力は５００アップする。


@CARDCAN_1766(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,1766)
SIF RESULT == 1
	RETURN 0

IF 種類 == 召喚時 || 種類 == 特殊召喚時
	SIF 発動済_1(決闘者,1766)
		RETURN 0
	CALL FULLRESET
	効果範囲デッキ = 1
	効果条件属性 = 地属性
	効果条件種族 = 元素霊族
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	効果範囲デッキ = 1
	効果条件カテゴリ = 트라미드
	効果条件種類 = 罠全般
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 種類 == 戦場起動 || 種類 == フリー誘発
	SIF ゾーン != "戦場"
		RETURN 0
	SIF 効果発動済(決闘者,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件スーパーカテゴリ = 트라미드
	効果条件種族 = 元素霊族
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1766(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 種類 == 召喚時 || 種類 == 特殊召喚時
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,1766,1)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲デッキ = 1
		効果条件属性 = 地属性
		効果条件種族 = 元素霊族
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			効果範囲デッキ = 1
			効果条件カテゴリ = 트라미드
			効果条件種類 = 罠全般
			CALL CARD_ADD_ADV(決闘者,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 種類 == 戦場起動 || 種類 == フリー誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,1766,10)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲墓地 = 1
		効果条件スーパーカテゴリ = 트라미드
		効果条件種族 = 元素霊族
		効果条件行先フラグ = デッキ行き
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			効果条件種族 = 元素霊族
			CALL STATCHANGE_EFFECT(決闘者,"戦場",場所,"永攻",500)
			CALL STATCHANGE_EFFECT(決闘者,"戦場",場所,"永守",500)
		ENDIF
	ENDIF
ENDIF
