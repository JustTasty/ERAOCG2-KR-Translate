@CARDNAME_94233,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 기황창세
	CASE "略称"
		RESULTS = 기황창세
	CASE "カテゴリ"
		RESULTS:0 = 기황
ENDSELECT


@CARD_94233(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "추가日"
		RETURN 250627
ENDSELECT
RETURN 0

@CARD_EXPLANATION_94233(종류)

#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動 ,フリー誘発)
PRINTL ①："기황신" 이외의 바람속성 / 땅속성 / 어둠속성의 기계족 몬스터를 １장씩、패에서 버리거나、묘지에서 제외하고 발동할 수 있다。
PRINTL 자신의 패 / 덱 / 묘지에서 "기황신" 몬스터 １장을 특수 소환한다。
PRINTL 이 효과를 발동하기 위해서 버리거나 제외한 몬스터가 "스카이 코어"、"그랜드 코어"、"와이즈 코어"、
PRINTL "기황제" 몬스터중 어느것일 경우에만、듀얼 중 이하의 효과를 적용한다。
PRINTL ●자신의 "기황신 마시니클∞"은 완전 내성을 얻는다。
PRINTL 

@CARDCAN_94233(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,94233)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") == 0
		효과範囲묘지 = 1
	효과条件종족 = 기계족
	효과条件속성 = 땅속성
	효과条件固カテゴリ = 기황신
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") == 0
		효과範囲묘지 = 1
	효과条件종족 = 기계족
	효과条件속성 = 바람속성
	효과条件固カテゴリ = 기황신
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") == 0
		효과範囲묘지 = 1
	효과条件종족 = 기계족
	효과条件속성 = 어둠속성
	효과条件固カテゴリ = 기황신
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件종류 = モン全般
	효과条件소환可 = 1
	효과条件カテゴリ = 기황신
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 永紋章起動
	RETURN 1
ENDIF


@CARDEFFECT_94233(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 바람속성플래그
#DIM DYNAMIC 땅속성플래그
#DIM DYNAMIC 어둠속성플래그
#DIM DYNAMIC 選択カード_ID

IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,94233)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		효과範囲손패 = 1
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") == 0
			효과範囲묘지 = 1
		효과条件종족 = 기계족
		효과条件속성 = 바람속성
		효과条件固カテゴリ = 기황신
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT != 1
			RETURN
		選択カード_ID = RESULT:1

		TRYCALLFORM CARDNAME_{選択カード_ID}("カテゴリ")
		SIF MATCH(RESULTS,"기황제")
			바람속성플래그 = 1

		TRYCALLFORM CARDNAME_{選択カード_ID}("名前")　
		SIF RESULTS == "스카이 코어"
			바람속성플래그 = 1

		효과範囲손패 = 1
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") == 0
			효과範囲묘지 = 1
		효과条件종족 = 기계족
		효과条件속성 = 땅속성
		효과条件固カテゴリ = 기황신
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT != 1
			RETURN
		選択カード_ID = RESULT:1

		TRYCALLFORM CARDNAME_{選択カード_ID}("カテゴリ")
		SIF MATCH(RESULTS,"기황제")
			땅속성플래그 = 1

		TRYCALLFORM CARDNAME_{選択カード_ID}("名前")
		SIF RESULTS == "그랜드 코어"
			땅속성플래그 = 1

		효과範囲손패 = 1
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") == 0
			효과範囲묘지 = 1
		효과条件종족 = 기계족
		효과条件속성 = 어둠속성
		효과条件固カテゴリ = 기황신
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT != 1
			RETURN
		選択カード_ID = RESULT:1

		TRYCALLFORM CARDNAME_{選択カード_ID}("カテゴリ")
		SIF MATCH(RESULTS,"기황제")
			어둠속성플래그 = 1

		TRYCALLFORM CARDNAME_{選択カード_ID}("名前")
		SIF RESULTS == "와이즈 코어"
			어둠속성플래그 = 1

		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 기황신
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		SIF 바람속성플래그 == 1 && 땅속성플래그 == 1 && 어둠속성플래그 == 1
			CALL CARD_RESERV(듀얼리스트,94233,永紋章予約)
	ENDIF

ELSEIF 종류 == 永紋章起動
	CALL FULLRESET
	효과条件名前 = 기황신 마시니클∞
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"モ耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"魔耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"함정耐")
ENDIF
