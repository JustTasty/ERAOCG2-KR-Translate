@CARDNAME_94234,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 카오스 블래스트
	CASE "略称"
		RESULTS = 카오스 블래스트
	CASE "カテゴリ"
		RESULTS:0 = 기황
ENDSELECT


@CARD_94234(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "추가日"
		RETURN 250602
ENDSELECT
