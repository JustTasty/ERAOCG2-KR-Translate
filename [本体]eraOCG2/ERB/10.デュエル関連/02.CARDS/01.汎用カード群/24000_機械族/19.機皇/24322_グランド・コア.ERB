対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124237)
	IF RESULT == 0
		SIF 종류 == 필드起動
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲손패 = 1
		효과条件속성:0 = 바람속성
		효과条件속성:1 = 땅속성
		효과条件속성:2 = 어둠속성
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			IF 選択カード_ID == 24234
				CALL CARD_GENERATE(듀얼리스트,324237)

			ELSE
				TRYCALLFORM CARD_{選択カード_ID},"속성"
				IF RESULT == 바람속성
					CALL CARD_GENERATE(듀얼리스트,124232)
				ELSEIF RESULT == 땅속성
					SIF 종류 == 필드起動
						CALL CARD_GENERATE(듀얼리스트,224237)
					SIF 종류 == 묘지起動
						CALL CARD_GENERATE(듀얼리스트,124237)
				ELSEIF RESULT == 어둠속성
					CALL CARD_GENERATE(듀얼리스트,124322)
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124237)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲마함 = 1
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0

ELSEIF 종류 == 맥시멈条件
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦無")

	CALL FULLRESET
	효과条件종류 = 싱크로
	효과範囲필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT != 1
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"攻誘")
	ELSE
		CALL FULLRESET
		효과条件종류 = 싱크로
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"攻誘")
	ENDIF
ENDIF


