@CARDCAN_124235(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,124235)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件除去種 = 無効
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,124235)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 맥시멈条件
	CALL CARD_CHECK_MAXX(듀얼리스트,ゾーン,場所,"기황제 파츠")
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == バトル開始時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"破壊","戦闘破壊")
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件名前 = 기황제 그란엘∞
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,124235)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 싱크로
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF


@CARDEFFECT_124235(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻守変動
	攻撃力修正 = GETVAR(@"ライフ_%듀얼리스트%") / 2
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正,攻撃力修正)

ELSEIF 종류 == バトル開始時強制
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_필드:{カウンタ}") == -1
			CONTINUE
		TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:{カウンタ}")}, "カテゴリ"
		IF !MATCH(RESULTS, "A")
			SIF !MATCH(RESULTS, "G")
			CONTINUE
		ENDIF 
		攻撃力修正 = GETVAR(@"%듀얼리스트%_MZ_攻:カウンタ")
		CALL SET_FIELD_INFO(듀얼리스트,"필드",カウンタ,"一攻",-攻撃力修正)
	NEXT


ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 와이젤
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一破")

	CALL FULLRESET
	효과条件カテゴリ = 스키엘
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一破")

	CALL FULLRESET
	효과条件カテゴリ = 그란엘
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一破")


ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124235,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"無効")
	ENDIF

ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124235)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件名前 = 기황제 그란엘∞
		효과条件소환可 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)	
	ENDIF


ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124235,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件종류 = 싱크로
		효과条件표시 = 表側표시
		CALL CARD_EQUIP_MONSTER_ADV(듀얼리스트,ゾーン,場所,1,124235)
	ENDIF

ENDIF

