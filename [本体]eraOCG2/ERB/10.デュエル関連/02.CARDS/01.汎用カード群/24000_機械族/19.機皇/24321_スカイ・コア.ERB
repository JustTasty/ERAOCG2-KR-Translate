	CALL FULLRESET
	효과条件カテゴリ = 와이젤
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"無差耐")

	CALL FULLRESET
	효과条件カテゴリ = 스키엘
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"無差耐")

	CALL FULLRESET
	효과条件カテゴリ = 그란엘
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"無差耐")


ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124230,10)
	IF RESULT == 0
		CALL EVADE_ATTACK(듀얼리스트)
	ENDIF

ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124230)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件名前 = 기황제 스키엘∞
		효과条件소환可 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)	
	ENDIF


ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124230,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件종류 = 싱크로
		효과条件표시 = 表側표시
		CALL CARD_EQUIP_MONSTER_ADV(듀얼리스트,ゾーン,場所,1,124230)
	ENDIF

ENDIF

