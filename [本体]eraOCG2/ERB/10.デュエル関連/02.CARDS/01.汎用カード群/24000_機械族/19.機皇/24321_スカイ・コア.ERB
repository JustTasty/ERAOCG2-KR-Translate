@CARDSUMMON_24321(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件종족 = 기계족

IF EX参照 == 1
	CALL MAXCAN(듀얼리스트,소환カード,24321) 
	RETURN RESULT

ENDIF

@CARDCAN_24321(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24321)
SIF RESULT == 1
	RETURN 0

IF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"破壊")
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_24321(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 候補カードID,紙束最大値


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24321)
	IF RESULT == 0
		CALL CARD_GENERATE_HAND(듀얼리스트,224230)
		CALL CARD_GENERATE_HAND(듀얼리스트,224230)

		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件名前 = 스카이 코어
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			VARSET 候補カードID,-1
			FOR カウンタ, 0, 紙束最大値
				選択カードID = GETVAR(@"%듀얼리스트%_손패:カウンタ")
				SIF 選択カードID == -1
					BREAK
				TRYCALLFORM CARD_{選択カードID}("맥시멈")
				SIF RESULT < 1
					CONTINUE
				TRYCALLFORM CARDSUMMON_{選択カードID}(듀얼리스트,カウンタ,1)
				IF RESULT >= 1
					候補カードID:カウンタB = 選択カードID
					カウンタB++
					SIF カウンタB >= VARSIZE("효과条件ID")
						BREAK
				ENDIF
			NEXT
			IF カウンタB > 0
				PRINTL 패에서 맥시멈 소환할 몬스터를 고를 수 있습니다。
				CALL FULLRESET
				효과範囲손패 = 1
				효과条件任意 = 1
				FOR カウンタ, 0, VARSIZE("효과条件ID")
					SIF 候補カードID == -1
						BREAK
					효과条件ID:カウンタ = 候補カードID:カウンタ
				NEXT
				CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT != 9999
					CALL CARD_SELECT(듀얼리스트,RESULT)
					CALL MAXSUMMON(듀얼리스트,選択カード（CS）,"손패")
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
ENDIF

