	SIF 破壊대상ゾーン != "필드"
		RETURN 0

	SIF !GROUPMATCH(破壊종류,"효과","戦闘")
		RETURN 0

	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0

	TRYCALLFORM CARDNAME_{破壊대상ID},"カテゴリ"
	IF !MATCH(RESULTS,"스키엘")
		IF !MATCH(RESULTS,"그란엘")
			IF !MATCH(RESULTS,"와이젤")
				RETURN 0
			ENDIF
		ENDIF
	ENDIF

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 스키엘
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 맥시멈条件
	SIF ゾーン != "필드"
		RETURN 0

	CALL CARD_CHECK_MAXX(듀얼리스트,ゾーン,場所,"기황제 코어")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_224232(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,224232)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 스키엘
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,224232,10)
	IF RESULT == 0
		CALL EVADE_ATTACK(듀얼리스트)
	ENDIF

ELSEIF 종류 == 맥시멈条件
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦無")
ENDIF

