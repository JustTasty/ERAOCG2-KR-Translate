@CARDNAME_50510,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 천위의 권승
	CASE "略称"
		RESULTS = 천위의 권승
	CASE "カテゴリ"
		RESULTS:0 = 천위
ENDSELECT


@CARD_50510(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1000
	CASE "分割"
		RETURN 효과なし
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARDSUMMON_50510(듀얼리스트,소환カード,EX参照)
