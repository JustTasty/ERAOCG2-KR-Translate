@CARDNAME_91272,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 무한광 아인 소프 오르
	CASE "略称"
RESULTS = 아인 소프 오
	CASE "カテゴリ"
RESULTS:0 = 시계신
ENDSELECT


@CARD_91272(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "レア度"
		RETURN ULR
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91272(종류)
#DIM DYNAMIC 종류

PRINTL 자신의 마법 & 함정 존에 앞면 표시된 「무한계 아인 소프」 1장을 묘지로 보내고 이 카드를 발동할 수 있다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 이 카드는 효과로 파괴되지 않는다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②: 자신 필드의 「시계신」 몬스터는 효과의 대상이 되지 않으며, 스탠바이 페이즈에 덱으로 돌아가는 효과는 발동하지 않는다.
CALLFORM CARD_ABLE(10,마함起動)
PRINTL ③: 1턴에 1번, 자신 필드에 몬스터가 존재하지 않는 경우에 발동할 수 있다.
PRINTL 덱/묘지에서 「궁극시계신 세피론」 1체를 손패에 넣는다.


@CARDCAN_91272(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件達成
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91272)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
효과条件名前 = 궁극시계신 세피론
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
효과条件名前 = 무한계 아인 소프
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_91272(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 속성数


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91272,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
효과条件名前 = 궁극시계신 세피론
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91272)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
효과条件名前 = 무한계 아인 소프
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"魔自身",場所,"破耐")
	CALL FULLRESET
효과条件カテゴリ = 시계신
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
ENDIF
