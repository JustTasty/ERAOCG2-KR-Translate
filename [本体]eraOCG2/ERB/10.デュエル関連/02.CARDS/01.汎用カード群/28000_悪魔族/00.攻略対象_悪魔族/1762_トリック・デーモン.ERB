@CARDNAME_1762,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트릭 데몬
	CASE "略称"
		RESULTS = 트릭 Ｄ
	CASE "カテゴリ"
		RESULTS:0 = 데몬
ENDSELECT


@CARD_1762(参照先)
;名前専用
#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1762(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,필드→묘지 ,손패→묘지 ,덱→묘지 ,破壊→묘지 ,戦闘→묘지)
PRINTL ①このカードが묘지へ送られた場合に발동できる。
PRINTL 덱から「트릭 데몬」以外の「데몬」カード１枚を손패に加える。
PRINTL 

@CARDCAN_1762(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1762)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_1(듀얼리스트,1762)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件固名前 = 트릭 데몬
	효과条件カテゴリ = 데몬
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_1762(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM サーチカード 




IF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1762,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件固名前 = 트릭 데몬
		효과条件カテゴリ = 데몬
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
