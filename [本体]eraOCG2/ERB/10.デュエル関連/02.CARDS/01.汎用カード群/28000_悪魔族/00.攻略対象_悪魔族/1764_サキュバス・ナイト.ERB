#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 악령의 여전사
	CASE "略称"
		RESULTS = 악령의 여전사
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT

@CARD_1764(参照先)
