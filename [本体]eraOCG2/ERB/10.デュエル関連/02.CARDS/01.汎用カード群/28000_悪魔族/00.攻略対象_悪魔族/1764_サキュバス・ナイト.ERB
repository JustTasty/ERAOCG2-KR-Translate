#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 通常モン
	CASE "属性"
		RETURN 闇属性
	CASE "種族"
		RETURN 戦士族
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1650
	CASE "守備力"
		RETURN 1300
	CASE "性別"
		RETURN 牝性
ENDSELECT
RETURN 0

