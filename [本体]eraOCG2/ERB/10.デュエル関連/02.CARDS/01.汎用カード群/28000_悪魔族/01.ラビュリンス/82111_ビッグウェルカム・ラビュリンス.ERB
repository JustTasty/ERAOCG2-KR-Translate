@CARDNAME_82111,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 빅웰컴 라뷰린스
	CASE "略称"
		RESULTS = 빅웰컴 라뷰린스
	CASE "カテゴリ"
		RESULTS:0 = 라뷰린스
ENDSELECT


@CARD_82111(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_82111(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  ①②의 효과는 1턴에 1번, 어느 쪽이든 1개밖에 사용할 수 없다.。
CALLFORM CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：자신의 패 / 덱 / 묘지에서 "라뷰린스」 1장을 특수 소환한다。
PRINTL 그 후, 자신 필드의 몬스터 1장을 원래 소유자의 패로 되돌린다。
CALLFORM CARD_ABLE(1,묘지起動,フリー誘発)
PRINTL ②: 묘지의 이 카드를 제외하고, 자신 필드의 악마족 몬스터 1장을 대상으로 하여 발동할 수 있다。
PRINTL 그 카드를 패로 되돌린다。
PRINTL 자신 필드에 레벨 8이상의 악마족 몬스터가 존재할 경우, 대신 상대 필드의 카드 1장을 대상으로 한다。


@CARDCAN_82111(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,82111)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,82111)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 라뷰린스
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF 발동済_1(듀얼리스트,82111)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_82111(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 対面者


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,82111,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 라뷰린스
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT != 9999
			選択カード = RESULT
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",選択カード,"손패",0,1)
		ENDIF

		IF GETVARS(@"%듀얼리스트%_フィールド名") == "라뷰린스 래버린스"
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지")
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,82111,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		CALL 효과条件レベル_ADV(8, "以上")
		효과条件종족 = 악마족
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"손패")
		ELSE 
			CALL FULLRESET
			효과条件종족 = 악마족
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			IF RESULT != 9999
				選択カード = RESULT
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",選択カード,"손패",0,1)
			ENDIF
		ENDIF
	ENDIF
ENDIF