@CARDNAME_2110,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 백은 성의 라뷰린스
	CASE "略称"
		RESULTS = 라뷰린스
	CASE "カテゴリ"
		RESULTS:0 = 라뷰린스
ENDSELECT


@CARD_2110(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2900
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

