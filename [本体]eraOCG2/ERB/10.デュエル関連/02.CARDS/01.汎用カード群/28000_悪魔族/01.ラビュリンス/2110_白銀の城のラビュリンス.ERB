@CARDCAN_2110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2110)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,2110)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) == 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 通常함정
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0

	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,2110)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 함정全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"除去成功")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_2110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2110,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 通常함정
		효과추가종류 = 500
		CALL CARD_SET_BASIC(듀얼리스트,"묘지",ゾーン,場所,"裏側표시")
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2110,2)
	IF RESULT == 0
		IF モンスター数(対面者) == 0 && 마함数(対面者) == 0
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"손패破壊",場所,"묘지",2110)
		ELSE
			PRINTL 필드의 카드를 파괴 하시겠습니까？
			CALL YN_CHOICE(듀얼리스트)
			IF RESULT == 0
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지",2110)
			ELSE
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"손패破壊",場所,"묘지",2110)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_PLAYER_INFO(듀얼리스트,"通함정阻害",3)
ENDIF



