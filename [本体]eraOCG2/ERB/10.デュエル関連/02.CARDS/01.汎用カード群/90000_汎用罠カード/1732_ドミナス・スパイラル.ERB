@CARDNAME_1732,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 도미나스 스파이럴
	CASE "略称"
		RESULTS = 명왕의 나선
	CASE "カテゴリ"
		RESULTS:0 = 도미나스
ENDSELECT


@CARD_1732(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250726
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1732(종류)
#DIM DYNAMIC 종류

PRINTL このカード名のカードは１ターンに１枚しか발동できない。
PRINTL 상대が손패・묘지のモンスターの효과を발동したターン、このカードの발동は손패からもできる。
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①：상대フィールドのモンスター１体を손패・EX덱に戻す。
PRINTL 자신の묘지に함정カードが存在しない場合、さらに상대は自身の묘지からモンスター１体を특수소환できる。
PRINTL このカードを손패から발동した場合、발동後、
CALLFORM CARD_ABLE(20,永紋章起動)
PRINTL このデュエル中に자신は光・어둠속성モンスターの효과を발동できない。


@CARDCAN_1732(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1732)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,1732)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"손패","마함")
		RETURN 0
	IF ゾーン == "손패"
		SIF GET_PLAYER_INFO(듀얼리스트,"K9발동数",1) <= 0
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件除去種 = 손패
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_1732(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 묘지送カード,5
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1732,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"손패")
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 함정全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT != 1
			CALL FULLRESET
			효과範囲묘지 = 1
			CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
		ENDIF
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		IF ゾーン == "손패"
			CALL SET_PLAYER_INFO(듀얼리스트,"빛속성효과無効",2)
			CALL SET_PLAYER_INFO(듀얼리스트,"어둠속성효과無効",2)
			CALL CARD_EFFECT_D1USE(듀얼리스트,1732)
		ENDIF
	ENDIF
ENDIF
