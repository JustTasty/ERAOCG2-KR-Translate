@CARDNAME_1732,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 도미나스 스파이럴
	CASE "略称"
		RESULTS = 명왕의 나선
	CASE "カテゴリ"
		RESULTS:0 = 도미나스
ENDSELECT


@CARD_1732(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 通常罠
	CASE "性別"
		RETURN 牝性
	CASE "追加日"
		RETURN 250726
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1732(種類)
#DIM DYNAMIC 種類

PRINTL このカード名のカードは１ターンに１枚しか発動できない。
PRINTL 相手が手札・墓地のモンスターの効果を発動したターン、このカードの発動は手札からもできる。
CALLFORM CARD_ABLE(1,発動誘発)
PRINTL ①：相手フィールドのモンスター１体を手札・EXデッキに戻す。
PRINTL 自分の墓地に罠カードが存在しない場合、さらに相手は自身の墓地からモンスター１体を特殊召喚できる。
PRINTL このカードを手札から発動した場合、発動後、
CALLFORM CARD_ABLE(20,永紋章起動)
PRINTL このデュエル中に自分は光・闇属性モンスターの効果を発動できない。


@CARDCAN_1732(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 効果制限達成
#DIM DYNAMIC カウンタ

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,1732)
SIF RESULT == 1
	RETURN 0

IF 種類 == 手札起動 || 種類 == 魔罠起動 || 種類 == フリー誘発
	SIF 発動済_1(決闘者,1732)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"手札","魔罠")
		RETURN 0
	IF ゾーン == "手札"
		SIF GET_PLAYER_INFO(決闘者,"K9発動数",1) <= 0
			RETURN 0
	ELSEIF ゾーン == "魔罠"
		SIF GETVAR(@"%決闘者%_魔_表:場所") != 2
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	効果範囲相手戦場 = 1
	効果条件除去種 = 手札
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_1732(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 墓地送カード,5
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

IF 種類 == 手札起動 || 種類 == 魔罠起動 || 種類 == フリー誘発
	;カード発動確認
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,1732,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(決闘者,"戦場単体",場所,"手札")
		CALL FULLRESET
		効果範囲墓地 = 1
		効果条件種類 = 罠全般
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		IF RESULT != 1
			CALL FULLRESET
			効果範囲墓地 = 1
			CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
		ENDIF
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		IF ゾーン == "手札"
			CALL SET_PLAYER_INFO(決闘者,"光属性効果無効",2)
			CALL SET_PLAYER_INFO(決闘者,"闇属性効果無効",2)
			CALL CARD_EFFECT_D1USE(決闘者,1732)
		ENDIF
	ENDIF
ENDIF
