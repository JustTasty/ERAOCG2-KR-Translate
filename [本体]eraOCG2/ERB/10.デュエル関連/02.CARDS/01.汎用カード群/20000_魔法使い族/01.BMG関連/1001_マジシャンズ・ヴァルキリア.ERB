@CARDNAME_1001,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 매지션즈 발키리아
	CASE "略称"
		RESULTS = M발키리아
	CASE "カテゴリ"
		RESULTS:0 = 매지션즈 발키리아
ENDSELECT


@CARD_1001(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

