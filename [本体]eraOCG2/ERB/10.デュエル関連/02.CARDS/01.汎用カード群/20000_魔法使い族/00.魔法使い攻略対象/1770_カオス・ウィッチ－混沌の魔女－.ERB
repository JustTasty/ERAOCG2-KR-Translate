@CARDCAN_1770(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,1770)
SIF RESULT == 1
	RETURN 0

IF 種類 == 戦場起動
	SIF 発動済_1(決闘者,1770)
		RETURN 0
	SIF 発動済_2(決闘者,1770)
		RETURN 0
	CALL FULLRESET
	効果条件属性:0 = 地属性
	効果条件属性:1 = 水属性
	効果条件属性:2 = 炎属性
	効果条件属性:3 = 風属性
	効果条件種類:0 = 융합
	効果条件種類:1 = シンクロ
	効果条件種類:2 = エクシーズ
	効果条件種類:3 = リンク
	効果条件種類:4 = 超次元
	CALL CARD_CHECK_FIN_DATA(決闘者,"EX履歴")
	SIF RESULT != 0
		RETURN 0
	CALL CARD_SS_CHECK(決闘者,101770,"デッキ外")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 除外時
	SIF 発動済_1(決闘者,1770)
		RETURN 0
	SIF 発動済_2(決闘者,1770)
		RETURN 0
	CALL FULLRESET
	効果条件属性:0 = 地属性
	効果条件属性:1 = 水属性
	効果条件属性:2 = 炎属性
	効果条件属性:3 = 風属性
	効果条件種類:0 = 융합
	効果条件種類:1 = シンクロ
	効果条件種類:2 = エクシーズ
	効果条件種類:3 = リンク
	効果条件種類:4 = 超次元
	CALL CARD_CHECK_FIN_DATA(決闘者,"EX履歴")
	SIF RESULT != 0
		RETURN 0
	CALL CARD_SS_CHECK(決闘者,201770,"デッキ外")
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(決闘者) >= 4
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1770(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


IF 種類 == 戦場起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,1770,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_GENERATE(決闘者,101770)
		CALL FULLRESET
		CALL CARD_GENERATE(決闘者,101770)
	ENDIF
ELSEIF 種類 == 除外時
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,1770,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_GENERATE(決闘者,201770)
		CALL FULLRESET
		CALL CARD_GENERATE(決闘者,201770)
	ENDIF
ENDIF




