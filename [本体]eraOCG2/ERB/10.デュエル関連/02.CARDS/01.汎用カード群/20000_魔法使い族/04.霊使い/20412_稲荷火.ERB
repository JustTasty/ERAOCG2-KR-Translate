@CARDNAME_20412,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이나리불
	CASE "略称"
		RESULTS = 이나리불
	CASE "カテゴリ"
		RESULTS:0 = 없음
		RESULTS:1 = 사역마
ENDSELECT


@CARD_20412(参照先)
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 200
	CASE "生成カード"
		RETURN 120412
ENDSELECT
RETURN 0


@CARD_EXPLANATION_20412(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번만 사용할 수 있다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 마법사족 몬스터가 존재할 경우 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다.
CALLFORM CARD_ABLE(2,破壊→묘지,戦闘→묘지)
PRINTL ②: 필드의 앞면 표시의 이 카드가 파괴되어 묘지로 보내졌을 경우 발동할 수 있다.
PRINTL 엔드 페이즈에 이 카드를 묘지에서 특수 소환한다.
CALLFORM CARD_ABLE(3,戦場起動)
PRINTL ③：自分フィールドから、このカードと魔法使い族モンスター１体を墓地に送って発動できる。
PRINTL 自分フィールドに「憑依覚醒－大稲荷火」を特殊召喚する。
PRINTL その後、相手フィールドの元々の攻撃力が一番高いモンスター１体の、その数値分のダメージを相手に与える事ができる。


@CARDCAN_20412(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,20412)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,20412)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,20412,"손패")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 마법사족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_2(듀얼리스트,20412)
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時予約
	CALL CARD_SS_CHECK(듀얼리스트,20412,"묘지")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 이나리불
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0	
RETURN 1
ELSEIF 種類 == 戦場起動
	SIF 発動済_3(決闘者,20412)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	CALL CARD_SS_CHECK(決闘者,120412,"",1)
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	効果範囲戦場 = 1
	効果条件他所 = 1
	効果条件種族 = 魔法使い族
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_20412(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC カウンタ
#DIM DYNAMIC ダメージ量

IF 종류 == 손패起動 
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20412,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 種類 == 破壊→墓地 || 種類 == 戦闘→墓地
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,20412,2)
	IF RESULT == 0
		CALL CARD_RESERV(決闘者,20412)
	ENDIF
ELSEIF 種類 == ターン終了時予約
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件名前 = 稲荷火
	CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
ELSEIF 종류 == 戦場起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20412,3)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件種族 = 魔法使い族
		CALL CARD_ADD_GY_ADV(決闘者,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_GENERATE(決闘者,120412)

			FOR カウンタ, 0, 5
				SIF GETVAR(@"%決闘者%_戦場:カウンタ") <= -1
					CONTINUE
				CALLFORM CARD_{GETVAR(@"%決闘者%_戦場:カウンタ")}, "攻撃力"
				ダメージ量 = MAX(ダメージ量, RESULT)
			NEXT
			IF ダメージ量 > 0
				PRINTL 相手フィールドの元々の攻撃力が一番高いモンスター１体の、その数値分のダメージを相手に与えますか？
				CALL YN_CHOICE(決闘者)
				IF RESULT == はい
					CALL LIFE_CHANGE(決闘者,"減少","効果",ダメージ量,1)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF


