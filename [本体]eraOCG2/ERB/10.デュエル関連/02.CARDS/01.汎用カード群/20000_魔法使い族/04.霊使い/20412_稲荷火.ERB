VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 빙의각성－대이나리불
	CASE "略称"
		RESULTS = 대이나리불
	CASE "カテゴリ"
		RESULTS:0 = 없음
		RESULTS:1 = 사역마
ENDSELECT


@CARD_120412(参照先)
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 効果モン
	CASE "トークン"
		RETURN 1
	CASE "属性"
		RETURN 炎属性
	CASE "種族"
		RETURN 元素霊族
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 200
