@CARDNAME_71081,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 세계수
	CASE "略称"
		RESULTS = 세계수
	CASE "カテゴリ"
		RESULTS:0 = 없음
	CASE "추가日"
		RETURN 250512
ENDSELECT


@CARD_71081(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_71081(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,효과後強制誘発)
PRINTL ①：식물족/ 야수족 / 곤충족 몬스터가 전투 / 효과로 파괴되어 묘지에 보내졌을 경우에 발동한다。
PRINTL 이 카드에 카운터를 1개 놓는다。
CALLFORM CARD_ABLE(0,마함起動)
PRINTL ②：이 카드의 카운터를 이하의 수만큼 제거해, 그 효과를 발동할 수 있다。
PRINTL ●１개：자신 필드의 식물족/ 야수족 / 곤충족 몬스터를 1장 대상으로 하고、
PRINTL 그 몬스터의 공격력은 턴 종료시까지 ４００ 올린다。
PRINTL ●２개：자신의 덱에서 식물족/ 야수족 / 곤충족 중 1장을 선택해 패로 가져온다。
PRINTL ●３개：자신의 묘지에 식물족/ 야수족 / 곤충족 중 1장을 선택해 묘지에서 특수 소환한다。
PRINTL 


@CARDCAN_71081(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71081)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") < 2
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	RETURN 1
ELSEIF 종류 == 효과後強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0

	CALL FULLRESET
	효과条件종족:0 = 식물족
	효과条件종족:1 = 야수족
	효과条件종족:2 = 곤충족
	효과条件종류 = モン全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_71081(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択肢
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71081)
	IF RESULT == 0
		PRINTFORML 카운터를 몇 장 제거하시겠습니까?？
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족:0 = 식물족
		효과条件종족:1 = 야수족
		효과条件종족:2 = 곤충족
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1 && GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") >= 1
			SIF 듀얼리스트 == "자신"
				PRINTFORML [1] - 식물족 / 야수족 / 곤충족의 공격력 ４００업。
		ENDIF

		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종족:0 = 식물족
		효과条件종족:1 = 야수족
		효과条件종족:2 = 곤충족
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1 && GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") >= 2
			IF 듀얼리스트 == "자신"
				PRINTFORML [2] - 식물족 / 야수족 / 곤충족 몬스터 서치。
			ELSEIF 듀얼리스트 == "상대"
				選択肢 = 2
			ENDIF
		ENDIF

		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족:0 = 식물족
		효과条件종족:1 = 야수족
		효과条件종족:2 = 곤충족
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1 && GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") >= 3
			IF 듀얼리스트 == "자신"
				PRINTFORML [3] - 묘지에서 식물족 / 야수족 / 곤충족 를 특수소환。
			ENDIF
		ENDIF
		IF 듀얼리스트 == "자신"
			BINPUT
			選択肢 = RESULT
		ENDIF

		IF 選択肢 == 1
			CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",-1)
			CALL FULLRESET
			효과条件종족:0 = 식물족
			효과条件종족:1 = 야수족
			효과条件종족:2 = 곤충족
			효과条件종류 = モン全般
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			選択カード = RESULT
			IF 選択カード != 9999
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"一攻",400)
			ENDIF
		ELSEIF 選択肢 == 2
			CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",-2)
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종족:0 = 식물족
			효과条件종족:1 = 야수족
			효과条件종족:2 = 곤충족
			효과条件종류 = モン全般	
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ELSEIF 選択肢 == 3
			CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",-3)
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족:0 = 식물족
			효과条件종족:1 = 야수족
			효과条件종족:2 = 곤충족
			효과条件종류 = モン全般
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71081)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 효과後強制誘発
	PRINTFORML 세계수『{場所}』의 카운터가１올랐습니다。
	CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",1)
ENDIF