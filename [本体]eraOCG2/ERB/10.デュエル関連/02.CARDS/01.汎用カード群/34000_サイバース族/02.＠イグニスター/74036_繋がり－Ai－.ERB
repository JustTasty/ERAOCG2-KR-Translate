@CARDNAME_74036,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 츠나가리－Ai－
	CASE "略称"
		RESULTS = 츠나가리－Ai－
	CASE "カテゴリ"
		RESULTS:0 = Ai
ENDSELECT


@CARD_74036(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_74036(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  카드는 １턴에 １장 밖에 발동할 수 없다。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：패의 사이버스족 몬스터 １장을 상대에게 보여주고 발동할 수 있다。
PRINTL 보여준 몬스터의 속성에 따라 이하의 효과를 적용한다.
PRINTL 이 효과의 발동 후、턴 종료시까지 자신은 사이버스족 몬스터밖에 특수 소환할 수 없다。
PRINTL ●어둠：보여준 몬스터를 특수 소환하고、
PRINTL 어둠 속성 이외의 레벨 4 이하의 사이버스족 몬스터 1장을 덱에서 패에 넣는다.
PRINTL ●어둠 속성 이외: 보여준 몬스터를 덱으로 되돌리고,
PRINTL 되돌린 몬스터와는 속성이 다른「＠이그니스터" 몬스터 1장을 덱에서 패에 넣는다.



@CARDCAN_74036(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성,5

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,74036)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,74036)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종족 = 사이버스족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK

	RETURN 1
ENDIF

@CARDEFFECT_74036(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択カード속성


IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,74036,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종족 = 사이버스족
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			選択カード = RESULT
			選択カード_ID = RESULT:99
			TRYCALLFORM CARD_{選択カード_ID}("속성")
			選択カード속성 = RESULT
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			IF 選択カード속성 == 어둠속성
				CALL FULLRESET
				CALL CARD_SP_SUMMON(듀얼리스트,"손패",選択カード)
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件종류 = モン全般
				효과条件종족 = 사이버스족
				CALLFORM 효과条件속성_ADV(選択カード속성,"以外")
				CALLFORM 효과条件レベル_ADV(4,"以下")
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					SS소환可能종족 = 사이버스족
					CALL CARD_SS_RESTRICTION(듀얼리스트)
				ENDIF
			ELSE
				CALL CARD_DISPOSE(듀얼리스트,"손패",選択カード,덱行き)
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件종류 = モン全般
				효과条件カテゴリ = ＠이그니스터
				CALLFORM 효과条件속성_ADV(選択カード속성,"以外")
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					SS소환可能종족 = 사이버스족
					CALL CARD_SS_RESTRICTION(듀얼리스트)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

