@CARDNAME_62075,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 시크릿 패스프레이즈
	CASE "略称"
		RESULTS = 패스프레이즈
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_62075(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_62075(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  카드는 1턴에 1장밖에 발동할 수 없다.。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：덱에서 "Live☆Twin」 마법 / 함정 카드 또는,
PRINTL 「Evil★Twin」 마법 / 함정 카드를 1장 패에 넣는다。
;PRINTL 자신 필드에 "키스킬" 몬스터 및 「리일라" 몬스터가 존재할 경우,
;PRINTL 대신에 "Evil★Twin" 몬스터를 1장을 패에 넣을 수도 있다。


@CARDCAN_62075(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,62075)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,62075)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ:0 = Live☆Twin
	효과条件カテゴリ:1 = Evil★Twin
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_62075(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM 選択カード



IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,62075,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ:0 = Live☆Twin
		효과条件カテゴリ:1 = Evil★Twin
		효과条件종류 = 마함全般
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
