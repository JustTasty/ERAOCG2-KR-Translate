@CARDNAME_62077,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = Live☆Twin 엔트런스
	CASE "略称"
		RESULTS = L☆T 엔트런스
	CASE "カテゴリ"
		RESULTS:0 = Live☆Twin
ENDSELECT


@CARD_62077(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_62077(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  카드는 1턴에 1장밖에 발동할 수 없다.。
CALLFORM CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①：패를 1장 버리고 발동할 수 있다。
PRINTL 덱에서 "키스킬" 몬스터 또는 「리일라" 몬스터 1장을 특수 소환한다。
PRINTL 이 카드 발동 후, 턴 종료시까지 「Evil★Twin" 몬스터 밖에 엑스트라덱에서 특수 소환할 수 없다。


@CARDCAN_62077(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,62077)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0		
		SIF 손패数(듀얼리스트) == 1
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF 손패数(듀얼리스트) == 0
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,62077)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = Live☆Twin
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_62077(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM 選択カード



IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,62077,1)
	IF RESULT == 0
		;通常마법汎用処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)		
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = Live☆Twin
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			CALL CARD_EX_RESTRICTION(듀얼리스트,"Evil★Twin")
		ENDIF
	ENDIF
ENDIF
