@CARDNAME_56511,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 메탈포제・미스리엘
	CASE "略称"
RESULTS = MF 미스리엘
	CASE "カテゴリ"
RESULTS = 0 = 메탈포제
ENDSELECT


@CARD_56511(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250630
ENDSELECT

RETURN 0
@CARDSUMMON_56511(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2


IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 메탈포제
ELSEIF EX参照 >= 10
	素材条件分割 = 펜듈럼
ENDIF
IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,56511) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_56511(종류)
#DIM DYNAMIC 종류

PRINTL 「메탈포제」 몬스터 ＋ P 몬스터
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(10,필드起動,フリー誘発)
PRINTL ①：자신의 묘지의 「메탈포제」 카드 2장을 덱으로 되돌리고 발동할 수 있다.
PRINTL 상대 필드의 카드 1장을 주인의 덱으로 되돌린다.
CALLFORM CARD_ABLE(2,戦破壊→묘지)
PRINTL ②：이 카드가 필드에서 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 자신의 EX 덱의 EX 이외의 「메탈포제」 P 몬스터,
PRINTL 또는 자신의 묘지의 「메탈포제」 P 몬스터 1장을 고르고 특수 소환한다.



@CARDCAN_56511(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,56511)
SIF RESULT == 1
	RETURN 0



IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,56511)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 메탈포제
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 戦破壊→묘지
	SIF 발동済_2(듀얼리스트,56511)
		RETURN 0
	SIF !GROUPMATCH(묘지送り종류,"リリース","破壊","戦闘破壊")
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 메탈포제
	효과条件종류 = EX以外全般
	효과条件分割 = 펜듈럼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_56511(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID 


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56511,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 메탈포제
		효과条件複数 = 2
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"덱")
		ENDIF
	ENDIF
ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56511,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 메탈포제
		효과条件종류 = EX以外全般
		효과条件分割 = 펜듈럼
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
