@CARDNAME_92352,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 고블린라이더 대핀치
	CASE "略称"
		RESULTS = 백귀나찰대재해
	CASE "カテゴリ"
		RESULTS:0 = 고블린
		RESULTS:1 = 고블린라이더
ENDSELECT


@CARD_92352(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92352(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,フリー誘発)
PRINTL ①： 자신 필드 또는 자신 묘지의 '고블린' 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 대상 카드의 위치에 따라 다음과 같은 효과를 적용한다.
PRINTL ●필드 : 대상 '고블린' 몬스터 1장과 상대 필드의 몬스터 1장을 제외한다.
PRINTL ●묘지 : 대상 '고블린' 몬스터 1장과 상대 묘지의 몬스터 1장을 제외한다.


@CARDCAN_92352(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92352)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	IF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	ELSEIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
		RETURN 1
	ELSEIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1

		SIF 발동済_1(듀얼리스트,92352)
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 고블린
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				RETURN 0
		ENDIF

		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 고블린
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대묘지 = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				RETURN 0
		ENDIF
	ENDIF
	RETURN 1
ELSEIF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_92352(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성A
#DIM DYNAMIC 条件달성B
#DIMS DYNAMIC 無効대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == フリー誘発
	IF GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92352)
		IF RESULT == 0
		ENDIF
	ELSEIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1

		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92352,1)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件カテゴリ = 고블린
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲상대필드 = 1
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				SIF RESULT == 1
					条件달성A = 1
			ENDIF

			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 고블린
			효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲상대묘지 = 1
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				SIF RESULT == 1
					条件달성B = 1
			ENDIF
			SIF 条件달성A == 0 && 条件달성B == 0
				RETURN 0

			CALL FULLRESET
			SIF 条件달성A == 1
				효과範囲필드 = 1
			SIF 条件달성B == 1
				효과範囲묘지 = 1
			효과条件カテゴリ = 고블린
			효과条件종류 = モン全般
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
			選択カード = RESULT
			IF 選択カード != 9999
				CALL CARD_SELECT(듀얼리스트,選択カード)
				選択カード = RESULT
				CALL CARD_DISPOSE(듀얼리스트,参照先（CS）,選択カード)
				IF 参照先（CS） == "필드"
					CALL FULLRESET
					CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"제외")
				ELSEIF 参照先（CS） == "묘지"
					CALL FULLRESET
					효과範囲상대묘지 = 1
					효과条件行先플래그 = 제외行き
					CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
