@CARDNAME_72184,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 둠즈 레이더
	CASE "略称"
		RESULTS = 둠즈 레이더
	CASE "カテゴリ"
		RESULTS:0 = 둠즈
ENDSELECT

@CARD_72184(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "추가日"
		RETURN 250809
ENDSELECT
RETURN 0

@CARD_EXPLANATION_72184(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①②の효과は１ターンに１度、いずれか１つしか使用できない。
CALLFORM CARD_ABLE(1,마함起動)
PRINTL ①：자신メインフェイズに발동できる。
PRINTL 자신の손패・フィールド（表側표시）の他の「둠즈」カード１枚を破壊する。
PRINTL その後、덱から「둠즈」モンスター１体を選び、손패に加えるか특수소환する。
PRINTL この효과の발동後、ターン終了時まで자신はＸモンスターしかＥＸ덱から특수소환できない。
CALLFORM CARD_ABLE(2,破壊→묘지)
PRINTL ②：このカードが효과で破壊された場合、フィールドの表側표시モンスター１体を대상として발동できる。
PRINTL そのモンスターを破壊する。


@CARDCAN_72184(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72184)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,72184)
		RETURN 0
	SIF 발동済_2(듀얼리스트,72184)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	효과条件他所 = 1
	효과条件カテゴリ = 둠즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 둠즈
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 破壊→묘지
	SIF 발동済_1(듀얼리스트,72184)
		RETURN 0
	SIF 발동済_2(듀얼리스트,72184)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_72184(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 속성数


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72184,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件他所 = 1
		효과条件표시 = 表側표시
		효과条件行先플래그 = 破壊行き
		효과条件カテゴリ = 둠즈
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1		
			효과条件종류 = モン全般
			효과条件カテゴリ = 둠즈
			IF モンスター数(듀얼리스트) != 5
				IF 듀얼리스트 == "자신"
					PRINTL 특수소환しますか？
					CALL YN_CHOICE(듀얼리스트)
				ENDIF
				IF RESULT == はい || 듀얼리스트 == "상대"
					CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
				ELSE
					CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)	
				ENDIF
			ELSE
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)	
			ENDIF
		ENDIF
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72184)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72184,2)
	IF RESULT == 0
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"묘지")
	ENDIF
ENDIF
