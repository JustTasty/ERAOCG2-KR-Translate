@CARDNAME_52183,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 둠즈 브레이크 디아크트로스
	CASE "略称"
		RESULTS = 둠즈 브레이크
	CASE "カテゴリ"
		RESULTS:0 = 둠즈
ENDSELECT

@CARD_52183(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 0
    CASE "성별"
        RETURN 雄性
    CASE "連動ID"
        RETURN 1960
	CASE "추가日"
		RETURN 250809
ENDSELECT

RETURN 0
@CARDSUMMON_52183(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
