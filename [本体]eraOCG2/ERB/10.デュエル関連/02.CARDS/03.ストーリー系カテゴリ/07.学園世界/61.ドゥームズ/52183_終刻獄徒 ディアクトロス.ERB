;추가효과用

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,52183) 
	RETURN RESULT
ENDIF



@CARD_EXPLANATION_52183(종류)
#DIM DYNAMIC 종류

PRINTL 레벨４ 몬스터×２
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,EX소환時,특수소환時)
PRINTL ①：이 카드가 특수 소환된 경우에 발동할 수 있다。
PRINTL 자신의 패 / 필드（앞면표시）의 "둠즈" 카드 １장을 파괴한다。
PRINTL 그 후、필드의 몬스터 １장을 파괴할 수 있다。
CALLFORM CARD_ABLE(2,破壊→묘지)
PRINTL ②：자신 묘지에 "둠즈" 몬스터 또는 "무구한 자 메디우스"가 존재하고、
PRINTL 이 카드가 효과로 파괴된 경우에 발동할 수 있다。
PRINTL 덱에서 장착 마법 카드 １장을 선택하여, 패에 넣거나 묘지로 보낸다。


@CARDCAN_52183(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52183)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,52183)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 둠즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 破壊→묘지
	SIF 발동済_2(듀얼리스트,52183)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 둠즈
	효과条件スーパー名前 = 무구한 자 메디우스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = 装備마법
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52183(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択結果

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52183,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件표시 = 表側표시
		효과条件カテゴリ = 둠즈
		효과条件行先플래그 = 破壊行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			IF 듀얼리스트 == "자신"
				PRINTL 필드의 몬스터를 파괴하시겠습니까？
				CALL YN_CHOICE(듀얼리스트)
				選択結果 = RESULT
			ELSE
				CALL FULLRESET
				효과範囲상대필드 = 1
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				SIF RESULT != 1
					選択結果 = 1
			ENDIF
			IF 選択結果 == 0
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"묘지")
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52183,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종류 = 装備마법
		IF 듀얼리스트 == "자신"
			PRINTL 장착 마법을 패에 넣으시겠습니까？
			CALL YN_CHOICE(듀얼리스트)
			選択結果 = RESULT
		ENDIF
		;상대が使う時は絶対サーチ
		IF 選択結果 == 0
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ELSE
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF