@CARDNAME_52185,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 괴감옥신 유피테르
	CASE "略称"
		RESULTS = 괴감옥신 유피테르
	CASE "カテゴリ"
		RESULTS:0 = 감옥신
ENDSELECT


@CARD_52185(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3500
	CASE "守備力"
		RETURN 2500
	CASE "특수制限"
		RETURN 1
	CASE "추가日"
		RETURN 250809
ENDSELECT
RETURN 0


@CARDSUMMON_52185(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 3
素材条件レベル = 10
;추가효과用
지정素材종류 = 엑시즈
IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,52185) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_52185(종류)
#DIM DYNAMIC 종류
PRINTL レベル１０モンスター×３
CALLFORM CARD_ABLE(0,EX덱起動)
PRINTL 자신は「괴감옥신 유피테르」を１ターンに１度しか특수소환できない。
PRINTL 「괴감옥신 유피테르」は１ターンに１度、装備カードを３枚以上装備している자신のモンスター上に重ねてＸ소환する事もできる。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：このカードがＸ소환した場合に발동できる。
PRINTL このカードに装備可能な装備마법カードを任意の数だけ자신の묘지から装備する。
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②：Ｘモンスターを素材としているこのカードの攻撃力は３０００アップする。
CALLFORM CARD_ABLE(10,필드起動)
PRINTL ③：１ターンに１度、このカードのＸ素材を１つ取り除いて발동できる。
PRINTL 자신の묘지から「둠즈」モンスター１体を특수소환する。
PRINTL その後、フィールドのカード１枚を破壊できる。

@CARDCAN_52185(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52185)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,52185)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	FOR カウンタ,0,5
		CALL FULLRESET
		효과条件CNT = 1
		CALL CARD_EQUIP_CHECK(듀얼리스트,カウンタ,"필드")
		IF RESULT >= 3
			RETURN 1
		ENDIF
	NEXT
	RETURN 0
ELSEIF 종류 == EX소환時
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 装備마법
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 둠즈
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52185(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード,5
#DIM DYNAMIC 素材カード
#DIM DYNAMIC 装備可能数
#DIMS DYNAMIC 出処
#DIM DYNAMIC 選択結果


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52185,0)
	IF RESULT == 0
		;装備カード３枚以上装備しているモンスターの場所を調べる
		FOR カウンタ,0,5
			CALL FULLRESET
			효과条件CNT = 1
			CALL CARD_EQUIP_CHECK(듀얼리스트,カウンタ,"필드")
			IF RESULT >= 3
				選択カード:カウンタB = カウンタ
				カウンタB ++
			ENDIF
		NEXT
		CALL FULLRESET
		효과範囲필드 = 1
		FOR カウンタ,0,カウンタB
			효과条件固定:カウンタ = 選択カード:カウンタ
		NEXT
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		素材カード = RESULT
		IF 素材カード != 9999
			CALL RESETCONDITION_MAT
			必要素材数 = 1
			素材条件固定 = 素材カード
			TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_필드:素材カード")},"종류"
			SIF RESULT == 엑시즈
				효과추가종류 = 効追条達
			重ねてＸ = 1
			TRYCALLFORM XYZSUMMON(듀얼리스트,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52185,1)
	IF RESULT == 0
		CALL FULLRESET
		;마함ゾーンの空き数カウント
		装備可能数 = 5 - 마함数(듀얼리스트)
		;묘지の装備마법の枚数カウント
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 装備마법
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 0 && RESULT < 装備可能数
			装備可能数 = RESULT
		FOR カウンタ,0,装備可能数
			CALL FULLRESET
			IF 듀얼리스트 == "자신"
				PRINTFORML \@ カウンタ == 0 ? 유피테르に # さらに\@装備마법を装備させますか？
				CALL YN_CHOICE(듀얼리스트)
				選択結果 = RESULT
			ENDIF
			IF 選択結果 == 0
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件종류 = 装備마법
				CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
				選択カード = RESULT
				IF 選択カード != 9999
					CALL CARD_SELECT(듀얼리스트,選択カード)
					選択カード = RESULT
					出処 = %参照先（CS）%
					CALL FULLRESET
					효과条件固定 = カード소환先
					CALLFORM CARD_EQUIP_ADV(듀얼리스트,選択カード,出処)
				ENDIF
			ELSE
				BREAK
			ENDIF
		NEXT
	ENDIF
ELSEIF 종류 == 攻守変動
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,3000)
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52185,10,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = モン全般
		효과条件カテゴリ = 둠즈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF 듀얼리스트 == "자신"
			PRINTL フィールドのモンスターを破壊しますか？
			CALL YN_CHOICE(듀얼리스트)
			選択結果 = RESULT
		ELSE
			CALL FULLRESET
			효과範囲상대필드 = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				選択結果 = 0
		ENDIF
		IF 選択結果 == 0
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"묘지")
		ENDIF
	ENDIF
ENDIF