@CARDNAME_71234,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 아르토메기아 무브먼트－혈통－
	CASE "略称"
		RESULTS = 신예학도－혈통－
	CASE "カテゴリ"
		RESULTS:0 = 아르토메기아
ENDSELECT


@CARD_71234(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 通常魔法
	CASE "追加日"
		RETURN 250726
ENDSELECT

RETURN 0

@CARD_EXPLANATION_71234(種類)
#DIM DYNAMIC 種類

PRINTL このカード名の①②の効果はそれぞれ１ターンに１度しか使用できない。
;この効果を発動するターン、自分は融合モンスターしかEXデッキから特殊召喚できない。
CALLFORM CARD_ABLE(1,魔罠起動,手札起動)
PRINTL ①：相手フィールドのモンスターの数が自分フィールドのモンスターより多い場合に発動できる。
PRINTL 同名カードが自分フィールドに存在しない「아르토메기아」モンスター１体をデッキから守備表示で特殊召喚する。
CALLFORM CARD_ABLE(2,手札→墓地)
PRINTL ②：このカードが手札から墓地へ送られた場合に発動できる。
PRINTL 自分の墓地・除外状態の「아르토메기아」罠カード１枚を手札に加える。


@CARDCAN_71234(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 存在カード名,100

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,71234)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 || 種類 == 手札起動
	SIF 発動済_1(決闘者,71234)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	SIF モンスター数(決闘者) >= モンスター数(対面者)
		RETURN 0
	CALL FULLRESET
	効果範囲魔罠 = 1
	効果条件名前 = 神芸学都아르토메기아
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	FOR カウンタ,0,5
		SIF GETVAR(@"%決闘者%_戦場:カウンタ") == -1
			CONTINUE
		TRYCALLFORM CARDNAME_{GETVAR(@"%決闘者%_戦場:カウンタ")},"カテゴリ"
		SIF !MATCH(RESULTS,"아르토메기아")
			CONTINUE
		TRYCALLFORM CARDNAME_{GETVAR(@"%決闘者%_戦場:カウンタ")},"名前"
		選択カード名 = %RESULTS%
		SIF MATCH(存在カード名,選択カード名)
			CONTINUE
		存在カード名:カウンタB = %選択カード名%
		カウンタB ++
		SIF カウンタB >= 50
			BREAK
	NEXT
	CALL FULLRESET
	効果範囲デッキ = 1
	効果条件カテゴリ = 아르토메기아
	効果条件種類 = モン全般
	効果条件固名前:0 = 仮置き
	FOR カウンタ,0,20
		効果条件固名前:カウンタ = %存在カード名:カウンタ%
	NEXT
	効果条件召喚可 = 1
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 手札→墓地
	SIF 発動済_2(決闘者,71234)
		RETURN 0
	CALL FULLRESET
	効果範囲墓地 = 1
	効果範囲除外 = 1
	効果条件カテゴリ = 아르토메기아
	効果条件種類 = 罠全般
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_71234(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 存在カード名,100

IF 種類 == 魔罠起動 || 種類 == 手札起動
	;カード発動確認
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,71234,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		FOR カウンタ,0,5
			SIF GETVAR(@"%決闘者%_戦場:カウンタ") == -1
				CONTINUE
			TRYCALLFORM CARDNAME_{GETVAR(@"%決闘者%_戦場:カウンタ")},"カテゴリ"
			SIF !MATCH(RESULTS,"아르토메기아")
				CONTINUE
			TRYCALLFORM CARDNAME_{GETVAR(@"%決闘者%_戦場:カウンタ")},"名前"
			選択カード名 = %RESULTS%
			SIF MATCH(存在カード名,選択カード名)
				CONTINUE
			存在カード名:カウンタB = %選択カード名%
			カウンタB ++
			SIF カウンタB >= 50
				BREAK
		NEXT
		CALL FULLRESET
		効果範囲デッキ = 1
		効果条件カテゴリ = 아르토메기아
		効果条件固名前:0 = 仮置き
		FOR カウンタ,0,20
			効果条件固名前:カウンタ = %存在カード名:カウンタ%
		NEXT
		CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所,"守備表示")
	ENDIF
ELSEIF 種類 == 手札→墓地
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,71234,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果範囲墓地 = 1
		効果範囲除外 = 1
		効果条件カテゴリ = 아르토메기아
		効果条件種類 = 罠全般
		CALL CARD_ADD_ADV(決闘者,ゾーン,場所)
	ENDIF
ENDIF
