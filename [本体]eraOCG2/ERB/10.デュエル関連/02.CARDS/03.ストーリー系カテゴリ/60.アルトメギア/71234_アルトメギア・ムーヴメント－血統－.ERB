@CARDNAME_71234,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 아르토메기아 무브먼트－혈통－
	CASE "略称"
		RESULTS = 신예학도－혈통－
	CASE "カテゴリ"
		RESULTS:0 = 아르토메기아
ENDSELECT


@CARD_71234(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250726
ENDSELECT

RETURN 0

@CARD_EXPLANATION_71234(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
;この효과を발동するターン、자신は융합モンスターしかEX덱から특수소환できない。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：상대フィールドのモンスターの数が자신フィールドのモンスターより多い場合に발동できる。
PRINTL 同名カードが자신フィールドに存在しない「아르토메기아」モンスター１体を덱から守備표시で특수소환する。
CALLFORM CARD_ABLE(2,손패→묘지)
PRINTL ②：このカードが손패から묘지へ送られた場合に발동できる。
PRINTL 자신の묘지・제외状態の「아르토메기아」함정カード１枚を손패に加える。


@CARDCAN_71234(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 存在カード名,100

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71234)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,71234)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= モンスター数(対面者)
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前 = 神芸学都아르토메기아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
			CONTINUE
		TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:カウンタ")},"カテゴリ"
		SIF !MATCH(RESULTS,"아르토메기아")
			CONTINUE
		TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:カウンタ")},"名前"
		選択カード名 = %RESULTS%
		SIF MATCH(存在カード名,選択カード名)
			CONTINUE
		存在カード名:カウンタB = %選択カード名%
		カウンタB ++
		SIF カウンタB >= 50
			BREAK
	NEXT
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 아르토메기아
	효과条件종류 = モン全般
	효과条件固名前:0 = 仮置き
	FOR カウンタ,0,20
		효과条件固名前:カウンタ = %存在カード名:カウンタ%
	NEXT
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 손패→묘지
	SIF 발동済_2(듀얼리스트,71234)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件カテゴリ = 아르토메기아
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_71234(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 存在カード名,100

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71234,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
				CONTINUE
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:カウンタ")},"カテゴリ"
			SIF !MATCH(RESULTS,"아르토메기아")
				CONTINUE
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:カウンタ")},"名前"
			選択カード名 = %RESULTS%
			SIF MATCH(存在カード名,選択カード名)
				CONTINUE
			存在カード名:カウンタB = %選択カード名%
			カウンタB ++
			SIF カウンタB >= 50
				BREAK
		NEXT
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 아르토메기아
		효과条件固名前:0 = 仮置き
		FOR カウンタ,0,20
			효과条件固名前:カウンタ = %存在カード名:カウンタ%
		NEXT
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
	ENDIF
ELSEIF 종류 == 손패→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71234,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件カテゴリ = 아르토메기아
		효과条件종류 = 함정全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
