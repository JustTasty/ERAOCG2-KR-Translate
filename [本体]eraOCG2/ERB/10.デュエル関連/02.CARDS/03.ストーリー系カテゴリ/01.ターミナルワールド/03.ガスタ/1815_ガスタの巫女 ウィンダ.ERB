@CARDNAME_1815,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 가스타의 무녀 윈다
	CASE "略称"
		RESULTS = 윈다
	CASE "カテゴリ"
		RESULTS:0 = 가스타
		RESULTS:1 = 윈다
ENDSELECT


@CARD_1815(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 400
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

