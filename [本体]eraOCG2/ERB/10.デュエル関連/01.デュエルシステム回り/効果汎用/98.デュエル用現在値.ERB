@素材条件レベル_ADV(지정レベル, 条件)
#DIM DYNAMIC 지정レベル
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIMS DYNAMIC 条件

SIF 지정レベル > 13
	지정レベル = 13

SELECTCASE 条件
	CASE "以上"
		FOR カウンタ,지정レベル,14
			素材条件レベル:カウンタB  = カウンタ
			カウンタB ++
		NEXT
	CASE "以下"
		FOR カウンタ,1,지정レベル + 1
			素材条件レベル:カウンタB = カウンタ
			カウンタB ++
		NEXT
	CASE "以外"
		FOR カウンタ,1,14
			SIF カウンタ == 지정レベル 
				CONTINUE
			효과条件レベル:カウンタB = カウンタ
			カウンタB ++
		NEXT
ENDSELECT


