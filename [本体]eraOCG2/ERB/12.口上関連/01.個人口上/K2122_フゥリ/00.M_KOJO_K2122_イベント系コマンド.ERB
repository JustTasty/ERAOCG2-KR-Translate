;"EVENT"系이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K2122_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:애처
			PRINTFORML 「お兄ちゃん！」 
			PRINTFORMW %CALLNAME:TARGET%는あなたを見て、嬉しそうにしていた。
		ELSE
			PRINTFORML 「ああ、主様。」
			PRINTFORMW %CALLNAME:TARGET%는あなたに気付いて手を振り、挨拶した。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:애처
			PRINTFORML 「お兄ちゃん！」 
			PRINTFORMW %CALLNAME:TARGET%는あなたを見て、嬉しそうにしていた。
		ELSE
			PRINTFORML 「ああ、主様。」
			PRINTFORMW %CALLNAME:TARGET%는あなたに気付いて手を振り、挨拶した。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが洗澡に入ってきて一緒に入る
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:애처
			PRINTFORMW 「お兄ちゃん❤！今日も一緒に入らない？」
		ELSE
			PRINTFORML 「主様、私と一緒に湯浴みをしましょう！」
			PRINTFORMW %CALLNAME:TARGET%는顔を真っ赤にしてそう言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが洗澡に入ってきて出て行った
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:애처
			PRINTFORMW 「お、お兄ちゃん！　入ってるならそう言ってよ！」
		ELSE
			PRINTFORML 「わああああ！？ 主様、入ってたんですね！！」
			PRINTFORMW %CALLNAME:TARGET%는叫びながら出て行った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが洗澡に入ってきて追い出された
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で遭遇した
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と约会时に遭遇した
	;ARG:1に데이트상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:애처
			PRINTFORML 「お兄ちゃん……바람……？」
			PRINTFORMW 恐ろしい顔をした %CALLNAME:TARGET%にあなたは小さく悲鳴を漏らした。
		ELSE
			PRINTFORML 「彼女との관계は、非常に良いようですね……」
			PRINTFORMW %CALLNAME:TARGET%는無表情でそう言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇구상、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		IF TALENT:애처
			PRINTFORML 「お兄ちゃん、あけましておめでとうございます！ 今年も一緒に頑張ろうね！」
			PRINTFORMW %CALLNAME:TARGET%と一年も過ごしていたようだ。
		ELSE
			PRINTFORML 「あけましておめでとうございます！ 主様、今日は美味しいものでも食べませんか？」
			PRINTFORMW %CALLNAME:TARGET%는、スーパーの広告にある割引牛肉を손가락してそう言った。
			PRINTFORMW 今日は奮発して、 %CALLNAME:TARGET%においしい식사をさせますね!
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で一日最初の挨拶
	;ARG:1に데이트상대、ARG:1が0の場合は非데이트
	;注、このあと通常の외출先で遭遇した구상、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1と约会时
		IF ARG:1
			PRINTFORMW 「一緒に出かける？いいですよ！」
		;一人で외출
		ELSE
			PRINTFORMW 「お金の使い過ぎには気を付けてくださいね！」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう구상）
;==================================================
@M_KOJO_EVENT_K2122_2(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;非同棲かつ상대가 先に起きてる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（연모時添い寝中睡觉時のみ？
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(연모無しでも入ってくる可能性有り
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML 「主様、朝稽古の時間ですよ！」
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 起きている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「もう朝ですよ、あ……兄――主様！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 寝ている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「ん……兄様……ふふ……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、상대가 起きてきた
	;添い寝時TARGET起床はこちら（TARGET쇠약or添い寝中TARGET睡魔負け後그대로 MASTERが起き続けた上で상대가 起床した場合限定
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「ふぁ……おはようございます、主様」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1
			IF FIRSTTIME(SELECTCOM)
				PRINTFORML 「ああ……もう朝稽古の時間なんですね……」
				PRINTFORMW %CALLNAME:TARGET%는慌てて起き上がり、時間を見ると %CALLNAME:MASTER%にも起きるよう促し始めた。
			ELSE
				PRINTFORML 	「ああ……また朝稽古の時間です……」
				PRINTFORMW	 %CALLNAME:TARGET%는急いで起き上がって時間を確認したが、またすぐに横になった。
				PRINTFORMW	「今日はもうダメですね、もう少し寝ましょう……」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;その他
	SIF RAND:5 == 0
	PRINTFORML %CALLNAME:TARGET%는今日はいつもより長く침대の上にいた。
	PRINTFORMW 普段は勤勉で有能なお여우様だが、こうした怠け者の一面もあるようだ。
	;;中国語では、「여우狸」（hú li）と「여우理」（hú lǐ）が響きが似ていて、読み方に違いがあります、この場合はMASTERがフゥリに対して使う愛称です。
ENDIF
