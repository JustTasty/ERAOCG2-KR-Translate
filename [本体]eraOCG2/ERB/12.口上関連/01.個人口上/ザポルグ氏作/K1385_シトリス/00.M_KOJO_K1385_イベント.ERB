			PRINTFORMW 心なしか植物の方も元気がない・・・
		ELSE
			PRINTFORML 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（연모のみ,그대로 一日が終了）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 「우후후❤今のうちに食べちゃおうかしら❤」
		ELSE
			PRINTFORML 「も～　お腹が空いてなかったら食べちゃってたわよ～？」
			PRINTFORMW %CALLNAME:TARGET%는どうにかこうにかして、気絶したあなたを運んだ・・・
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが寝るために自室まで戻ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
