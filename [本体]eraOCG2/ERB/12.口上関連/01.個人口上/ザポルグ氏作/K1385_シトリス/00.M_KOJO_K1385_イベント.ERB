;"EVENT"系이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K1385_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「마스터❤　こっちにきて～❤」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて一緒に入る
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「おじゃまするわよ～❤」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて出て行った
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「ちょっと合わないわ～」
		PRINTFORMW 環境が合わなかったのか、 %CALLNAME:TARGET%는목욕場から出ていった。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて追い出された
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で遭遇した
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;ARG:1に데이트상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇구상、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で一日最初の挨拶
	;ARG:1に데이트상대、ARG:1が0の場合は非데이트
	;注、このあと通常の외출先で遭遇した구상、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1と데이트중
		IF ARG:1
		;一人で외출
		ELSE
		ENDIF
		PRINTFORMW  
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう구상）
;==================================================
@M_KOJO_EVENT_K1385_2(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;非同棲かつ상대가 先に起きてる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（연모時添い寝中취침時のみ？
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(연모無しでも入ってくる可能性有り
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 起きている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「あら、もう起きちゃったの～？」
		PRINTFORMW  何かする気だったのだろうか・・・
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 寝ている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML %CALLNAME:TARGET%に挨拶したが反応がない。
		PRINTFORMW 目を開けたまま寝ているようだ・・・
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、상대가 起きてきた
	;添い寝時TARGET起床はこちら（TARGET쇠약or添い寝中TARGET睡魔負け後그대로 MASTERが起き続けた上で상대가 起床した場合限定
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORML 「・・・・・・あら、いたのね❤」
			PRINTFORMW %CALLNAME:TARGET%는今起きたようだ・・・
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML 「・・・・・・ふふ、おはよう❤」
			PRINTFORMW 挨拶を返すと %CALLNAME:TARGET%는嬉しそうにほほえんだ。
			;植物に話しかけるのってまさに独身ですね！
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORMW   
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ구상）
;==================================================
@M_KOJO_EVENT_K1385_3(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在땅】から【캐릭터の自室】に戻りました）
	;記入チェック（=0, 非표시、1, 표시）
