	SELECTCASE RAND:5 + 1
		CASE 1
			PRINTFORMW 「ふっ、うっ、ふぐっ！？」
		CASE 2
			PRINTFORMW 「中がっ、ずりずりっ、めくれてっ・・・！」
		CASE 3
			PRINTFORMW 「うぅぅぅ・・・❤　あぁぁぁ・・・❤」
		CASE 4
			PRINTFORMW 「お尻がっ、うっ❤　戻らなくなったらっ、ふっ❤　どうするつもりなんですかぁっ❤」
		CASE 5
			PRINTFORMW 「はっ❤　くぅっ❤　お腹っ❤　圧迫されてっ❤　くるしいのにっ❤　なんでっ❤」
	ENDSELECT
ELSE
	SELECTCASE RAND:3 + 1
		CASE 1
			PRINTFORMW 「ぎぃっ！？　やっ、やぁっ・・・！」
		CASE 2
			PRINTFORMW 「ぐっ！　ふっ！　おぇっ！」
		CASE 3
			PRINTFORMW 「くるじっ・・・！　や、やめっ・・・！」
	ENDSELECT
ENDIF

;체위변경
@M_KOJO_MESSAGE_COM_K1627_SEXCHANGE_1
IF TALENT:TARGET:연모
	SELECTCASE RAND:3 + 1
		CASE 1
			PRINTFORMW 「あっ・・・❤　今度は、こっちなんですね・・・❤」
		CASE 2
			PRINTFORMW 「んっ❤　%CSTR:TARGET:MASCALL%さんは、こっちの方が好きなんですか？」
		CASE 3
			PRINTFORMW 「んんっ❤　さっきとは、違う所が刺激されて・・・❤」
	ENDSELECT
ELSE
	SELECTCASE RAND:2 + 1
		CASE 1
			PRINTFORMW 「早く、終わって・・・」
		CASE 2
			PRINTFORMW 「誰か・・・助けて・・・」
	ENDSELECT
ENDIF

