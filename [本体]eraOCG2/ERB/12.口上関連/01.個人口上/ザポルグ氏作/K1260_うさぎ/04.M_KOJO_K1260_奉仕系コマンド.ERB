	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF BUST_SIZE(TARGET) >= 0
			PRINTFORMW 「んしょ・・・んしょ・・・　難しいね、これ・・・」
		ELSE
			PRINTFORMW 「・・・・・・痛くない？」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	IF BUST_SIZE(TARGET) > 0
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 「んしょ・・・んしょ・・・　どう？気持ちいい？」
			CASE 2
				PRINTFORMW 「おっぱい動かすの、結構難しいんだね・・・」
			CASE 3
				PRINTFORMW 「んー・・・れー・・・❤ ぬちゅぬちゅ・・・　唾を垂らしてみたけどどう？ さっきよりも気持ちいい？❤」
			CASE 4
				PRINTFORMW 「おお・・・ すごい気持ちよさそう・・・❤」
			CASE 5
				PRINTFORMW 「ふふ❤ %CALLNAME:MASTER%の弱い所は全部知ってるよ❤　こことか、おっぱいでゆるーく刺激されるの、好きでしょ？❤」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「・・・骨ズリ」
			CASE 1
				PRINTFORMW 「私、가슴無いからあんまり気持ちよくないんじゃない？」
			CASE 2
				PRINTFORMW 「もうちょっと大きかったらなあ・・・」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

