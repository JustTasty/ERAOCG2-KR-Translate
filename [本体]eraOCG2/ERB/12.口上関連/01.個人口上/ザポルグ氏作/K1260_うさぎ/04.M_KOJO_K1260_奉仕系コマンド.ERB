@M_KOJO_MESSAGE_COM_K1260_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K1260_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K1260_80_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「%CSTR:TARGET:DIKCALL%って、結構硬いんだね」
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 4
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「どう？ 気持ちいい？」
		CASE 2
			IF TALENT:MASTER:형태 >= 4
				PRINTFORMW 「溝とか出っ張ってるとことか・・・わわっ、触ったら暴れ出した。　気持ちいいの？」
			ELSE
				PRINTFORMW 「手のひらサイズで握りやすいね、ちょっとかわいいかも」
			ENDIF
		CASE 3
			PRINTFORMW 「おー・・・ビクビクしてる・・・」
		CASE 4
			PRINTFORMW 「すんすん・・・手に臭いが移っちゃった・・・❤」
		CASE 5
			PRINTFORMW 「・・・ん、ガマン汁出てきた❤」
		CASE 6
			PRINTFORMW 「出す時は手とか口とかで受け止めるから、イキたくなったら言ってね❤」
	ENDSELECT
ENDIF
RETURN 1

