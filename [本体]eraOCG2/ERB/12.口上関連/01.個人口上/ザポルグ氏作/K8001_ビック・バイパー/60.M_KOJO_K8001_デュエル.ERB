;==================================================
;5,효과발동
;==================================================
@M_KOJO_DUEL_K8001_5(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIM DYNAMIC 발동出処
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;無効にされた場合
	IF ARG:1 == 1
		IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
			PRINTFORMW 「無効にされましたか。それなら──」
		ELSE
			PRINTFORMW 「無効確認　展開　修正」
		ENDIF
	;それ以外
	ELSE
		SIF 발동出処 == 1 || 발동出処 == 2 || 발동出処 == 3 || 발동出処 == 4
			RETURN 1
		IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
			SELECTCASE RAND:2 + 1
				CASE 1
					PRINTFORMW 「%カード名%의 효과를 발동します！」
				CASE 2
					PRINTFORMW 「%カード名%의 효과발동！」
			ENDSELECT
		ELSE
			SELECTCASE RAND:1 + 1
				CASE 1
					PRINTFORMW 「발동　%カード名%」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

