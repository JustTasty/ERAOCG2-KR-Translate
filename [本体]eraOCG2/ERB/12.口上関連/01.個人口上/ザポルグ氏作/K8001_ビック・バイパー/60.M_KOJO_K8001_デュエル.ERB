;==================================================
;0,デュエルスタート！
;==================================================
@M_KOJO_DUEL_K8001_0(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		SELECTCASE RAND:3 + 1
			CASE 1
				PRINTFORMW 「デュエルモード起動完了。参ります」
			CASE 2
				PRINTFORMW 「덱の調整であればお任せください。全力で勝ちに行かせていただきます」
			CASE 3
				PRINTFORMW 「デュエルスタンバイ」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2 + 1
			CASE 1
				PRINTFORMW 「デュエルシステム　起動」
			CASE 2
				PRINTFORMW 「デュエル　開始」
		ENDSELECT
	ENDIF
ENDIF

;==================================================
;1,モンスター소환
;例：%カード名%を소환！
;==================================================
@M_KOJO_DUEL_K8001_1(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
		
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		IF カードID == 1674
			PRINTFORMW 「行きますよ！」
		ELSEIF カードID == 24801
			PRINTFORMW 「共闘開始！　ジェイドナイト！ＧＯ！」
		ELSE
			SELECTCASE RAND:3 + 1
				CASE 1
					PRINTFORMW 「出動！%カード名%！」
				CASE 2
					PRINTFORMW 「%カード名%をエントリー！」
				CASE 3
					PRINTFORMW 「%カード名%を소환！」
			ENDSELECT
		ENDIF
	ELSE
		IF カードID == 1674
			PRINTFORMW 「出動準備　開始」
		ELSEIF カードID == 24801
			PRINTFORMW 「Call...Jade Knight!」
		ELSE
			SELECTCASE RAND:1 + 1
				CASE 1
					PRINTFORMW 「소환　名前　%カード名%」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF


;==================================================
;2,モンスター특수소환
;==================================================
@M_KOJO_DUEL_K8001_2(カードID,소환出処)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIM DYNAMIC 소환出処
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		IF カードID == 1674
			PRINTFORMW 「行きますよ！」
		ELSEIF カードID == 24801
			PRINTFORMW 「共闘開始！　ジェイドナイト！ＧＯ！」
		ELSEIF カードID == 24800
			PRINTFORMW 「オプションを획득！」
		ELSEIF カードID == 24802
			PRINTFORMW 「行きなさい！　ダッカ―！」
		ELSEIF カードID == 172480
			PRINTFORMW 「バリアー展開！」
		ELSEIF カードID == 11674
			PRINTFORMW 「さあ、ここからが本番ですよ！」
		ELSE
			SELECTCASE 소환出処
				;패에서 
				CASE 召種손패
					PRINTFORMW 「%カード名%を패에서 出動！」
				;마법함정ゾーンから。　キリ番・シェイプシスターなどを出す場合
				CASE 召種마함
					PRINTFORMW 「마법＆함정존의 %カード名%をエントリー！」
				;덱에서 
				CASE 召種덱
					PRINTFORMW 「덱에서 %カード名%を招集！」
				;EX덱에서 
				CASE 召種EX덱
					PRINTFORMW 「緊急出動！%カード名%！」
				;묘지에서 
				CASE 召種묘지
					PRINTFORMW 「%カード名%を戦線復帰！」
				;제외状態から
				CASE 召種제외
					PRINTFORMW 「%カード名%！異次元より帰還！」
				;主にトークン生成
				CASE 10
					PRINTFORMW 「%カード名%を生成！」
			ENDSELECT
		ENDIF
	ELSE
		IF カードID == 1674
			PRINTFORMW 「出動準備　開始」
		ELSEIF カードID == 24801
			PRINTFORMW 「Call...Jade Knight!」
		ELSEIF カードID == 24800
			PRINTFORMW 「GET Option!」
		ELSEIF カードID == 172480
			PRINTFORMW 「GET Barrier!」
		ELSE
			SELECTCASE 소환出処
				;패에서 
				CASE 召種손패
					PRINTFORMW 「出処　손패　특수소환　%カード名%」
				;마법함정ゾーンから。　キリ番・シェイプシスターなどを出す場合
				CASE 召種마함
					PRINTFORMW 「出処　마함　특수소환　%カード名%」
				;덱에서 
				CASE 召種덱
					PRINTFORMW 「出処　덱　특수소환　%カード名%」
				;EX덱에서 
				CASE 召種EX덱
					PRINTFORMW 「出処　EX덱　특수소환　%カード名%」
				;묘지에서 
				CASE 召種묘지
					PRINTFORMW 「出処　묘지　특수소환　%カード名%」
				;제외状態から
				CASE 召種제외
					PRINTFORMW 「出処　제외　특수소환　%カード名%」
				;主にトークン生成
				CASE 10
					PRINTFORMW 「生成　%カード名%」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

