;==================================================
;0,デュエルスタート！
;==================================================
@M_KOJO_DUEL_K8001_0(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		SELECTCASE RAND:3 + 1
			CASE 1
PRINTFORMW 「듀얼 모드 기동 완료. 가겠습니다」
			CASE 2
PRINTFORMW 「덱 조정이라면 맡겨 주십시오. 전력으로 승리하겠습니다」
			CASE 3
PRINTFORMW 「듀얼 스탠바이」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2 + 1
			CASE 1
PRINTFORMW 「듀얼 시스템 기동」
			CASE 2
PRINTFORMW 「듀얼 개시」
		ENDSELECT
	ENDIF
ENDIF

;==================================================
;1,モンスター소환
;例：%カード名%を소환！
;==================================================
@M_KOJO_DUEL_K8001_1(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
		
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		IF カードID == 1674
PRINTFORMW 「가겠습니다!」
		ELSEIF カードID == 24801
PRINTFORMW 「공동 투쟁 개시! 제이드 나이트! GO!」
		ELSE
			SELECTCASE RAND:3 + 1
				CASE 1
PRINTFORMW 「출동!%カード名%!」
				CASE 2
PRINTFORMW 「%カード名%을 엔트리!」
				CASE 3
PRINTFORMW 「%カード名%을 소환!」
			ENDSELECT
		ENDIF
	ELSE
		IF カードID == 1674
PRINTFORMW 「출동 준비 개시」
		ELSEIF カードID == 24801
PRINTFORMW 「Call...Jade Knight!」
		ELSE
			SELECTCASE RAND:1 + 1
				CASE 1
PRINTFORMW 「소환 이름 %カード名%」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF


;==================================================
;2,モンスター특수소환
;==================================================
@M_KOJO_DUEL_K8001_2(カードID,소환出処)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIM DYNAMIC 소환出処
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		IF カードID == 1674
PRINTFORMW 「가겠습니다!」
		ELSEIF カードID == 24801
PRINTFORMW 「공동 투쟁 개시! 제이드 나이트! GO!」
		ELSEIF カードID == 24800
PRINTFORMW 「옵션을 획득!」
		ELSEIF カードID == 24802
PRINTFORMW 「가거라! 다카!」
		ELSEIF カードID == 172480
PRINTFORMW 「배리어 전개!」
		ELSEIF カードID == 11674
PRINTFORMW 「자, 여기부터가 본게임입니다!」
		ELSE
			SELECTCASE 소환出処
				;패에서 
				CASE 召種손패
