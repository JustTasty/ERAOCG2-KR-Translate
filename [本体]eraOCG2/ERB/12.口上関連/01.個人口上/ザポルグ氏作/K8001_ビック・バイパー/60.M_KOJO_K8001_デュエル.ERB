;==================================================
;10,ダメージ反応！（ダメージ）
;==================================================
@M_KOJO_DUEL_K8001_10(ダメージ量,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC ダメージ量
#DIMS DYNAMIC カード名
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨
		SELECTCASE ダメージ量
			;ダメージなし
			CASE 0
				PRINTFORMW 「ダメージはありませんね」
			;１～１９９９の時、小ダメージ
			CASE 1 TO 1999
				PRINTFORMW 「かすり傷ですね」
			;２０００～３９９９の時、結構なダメージ
			CASE 2000 TO 3999
				PRINTFORMW 「軽傷です！」
			;４０００～７９９９の時、超大ダメージ
			CASE 4000 TO 7999
				PRINTFORMW 「くっ……強い……！」
			;８０００以上、基本即死
			CASEELSE
				PRINTFORMW 「……I'll be back.」
		ENDSELECT
	ELSE
		SELECTCASE ダメージ量
			;ダメージなし
			CASE 0
				PRINTFORMW 「ダメージ算出　０」
			;１～１９９９の時、小ダメージ
			CASE 1 TO 1999
				PRINTFORMW 「ダメージ算出　{ダメージ量}」
			;２０００～３９９９の時、結構なダメージ
			CASE 2000 TO 3999
				PRINTFORM 「ダメージ算出　{ダメージ量}
				SIF ライフ_상대
					PRINTFORM 　要対処
				PRINTFORMW 」
			;４０００～７９９９の時、超大ダメージ
			CASE 4000 TO 7999
				PRINTFORMW 「ダメージ算出　{ダメージ量}」
				SIF ライフ_상대
					PRINTFORMW 「危険域　突入」
			;８０００以上、基本即死
			CASEELSE
				PRINTFORMW 「ダメージ算出　……死」
		ENDSELECT
	ENDIF
ENDIF


