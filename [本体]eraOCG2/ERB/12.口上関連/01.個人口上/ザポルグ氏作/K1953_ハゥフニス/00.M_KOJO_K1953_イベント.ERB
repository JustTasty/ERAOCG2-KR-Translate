;オナバレ系
;==================================================
@M_KOJO_EVENT_K1953_35(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	;共通部分
		;オナニーの声が聞こえる（オナバレ直前）
		CASE 1
			PRINTFORMW 『んっ❤　ふっ❤　 %CALLNAME:MASTER%・・・んっ❤』
	;一般
		;オナバレ
		CASE 2
			IF TALENT:TARGET:음란
				PRINTFORMW 「あっ・・・❤　 %CALLNAME:MASTER%・・・❤」
			ELSE
				PRINTFORMW 「！？？！？！！？？」
				PRINTFORMW 「あな、あ、ノ、ノックぐらいしなさいよ！？」
			ENDIF
		;上目遣いで訴える
		CASE 3
			PRINTFORMW 「・・・・・・スる？❤」
		;期待に答えて밀어넘어뜨린다
		CASE 4
			PRINTFORML 「あっ・・・ん・・・」
			PRINTFORMW %CALLNAME:TARGET%는力を抜き、 %CALLNAME:MASTER%に身を任せた・・・
		;断られた（部屋の청소を始める）
		CASE 5
			PRINTFORMW 「あ・・・うん・・・」
		;ノックしてから中に入る（オナバレ無し）
		CASE 6
			PRINTFORMW 「！？　ちょ、ちょっと待って！　なんなら入って来ないで！」
			PRINTFORMW ・・・・・・。
			PRINTFORMW 「お、お待たせ。　入っていいわよ？」
			PRINTFORMW 室内には消臭剤特有の匂いが漂っている・・・
	
	;雑魚雌のみ
		;オナバレ（縋りついてくる）
		CASE 10
			PRINTFORML 「ねえ、いつもみたいに・・・シないの？❤」
			PRINTFORMW %CALLNAME:TARGET%는股をもじもじとさせている・・・
		;見せ槍オナニー
		CASE 11
			PRINTFORMW 「あ・・・❤　すー・・・❤」
	
	;포식자のみ
		;問答無用역 레이프
		CASE 15
			PRINTFORMW 
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;MASTERを看病する
;==================================================
@M_KOJO_EVENT_K1953_36(ARG,ARG:1)
;구상主はARG
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;--------------------------------------------------
IF LOCAL
	SELECTCASE ARG
		;フリースペース、서술문でも何でも好きに書いていい
		CASE 0
			PRINTFORML 「・・・血とか、飲んでみる？」
			PRINTFORMW 腕を差し出してくる %CALLNAME:TARGET%にやんわりと断った・・・
		;汗を拭いて額に冷たいもの
		CASE 1
			PRINTFORMW 「ぬるかったりしたらちゃんと言うのよ？」
		CASE 2
			;療養食食べさせる
			PRINTFORMW 「あーん・・・どう？　食べられそう？」
		;薬を飲ませる
		CASE 3
			PRINTFORMW 「・・・そんなに不味いの？」
		;子守唄を歌う
		CASE 4
			PRINTFORMW 透き通るような声と緩慢なリズムに眠気が込み上げてくる・・・
		;密着して傍にいる
		CASE 5
			PRINTFORMW 「移るって？　その時は %CALLNAME:MASTER%が看病しなさいよ」
		;話し상대에게 なる
		CASE 22
			PRINTFORMW 「家事とかは全部やっておくから、今日くらいは休んでなさい」
		;薬を補充する
		CASE 23
			PRINTFORMW 「薬買ってきたわよー・・・종류、合ってるわよね？」
	ENDSELECT
ENDIF
RETURN 1
