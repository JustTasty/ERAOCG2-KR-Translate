@M_KOJO_MESSAGE_COM_K2061_81_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;ファースト키스
		IF EXP:TARGET:Ｍ경험 == 0 && EXP:TARGET:펠라경험 == 1 && CALLNAME:(CFLAG:TARGET:키스상실상대) == CALLNAME:MASTER
			PRINTFORMW 「ちゅ……え？　はい、키스はまだですが……」
			PRINTFORMW 「！？　えっ！？　これもファースト키스に入るんですか！？」
			PRINTFORMW 「……その、やり直しとかって、出来ませんか？」
		;それ以外
		ELSE
			PRINTFORMW 「ん……ちゅ❤　はむ……」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 1
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 0
		
	;お掃除펠라
	IF STAIN:MASTER:2 & 4 && ABL:TARGET:기교 >= 3
		PRINTFORMW 「はむっ❤　じゅるるるっ❤　ん……ぷっ、ふぅ……❤」
		PRINTFORMW 「お掃除、終わりましたよ❤　%CSTR:TARGET:MASCALL%❤」

	;通常
	ELSE
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 「ん……少し、塩気が強いですね♪」
			CASE 2
				PRINTFORMW 「ん……ちゅ❤　はむ……」
			CASE 3
				PRINTFORMW 「はぁ……ちゅっ❤　れろぉ……❤」
			CASE 4
				PRINTFORML 「ふっ❤　んっ❤　んっ……んふっ❤」
				PRINTFORMW %CALLNAME:TARGET%は上目遣いにこちらを見ながら、内頬に擦り付けている……
			CASE 5
				PRINTFORML 「ちゅっ……じゅるっ……んー……❤」
				PRINTFORMW %CALLNAME:TARGET%は咥えこんだ性器を口の中で転がしている……
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

