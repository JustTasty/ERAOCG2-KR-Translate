		SELECTCASE RAND:2 + 1
			CASE 1
				PRINTFORMW 「んっ… %CALLNAME:MASTER%の手…温かい…♡」
			CASE 2
				PRINTFORMW 「も、もう…どこ触ってるのよ…でも、嫌じゃ…ないわ」
		ENDSELECT
	ELSEIF TALENT:TARGET:연모 >= 1 || ABL:친밀 >= 5
		SELECTCASE RAND:2 + 1
			CASE 1
				PRINTFORMW 「はぁ…ん… %CALLNAME:MASTER%の触り方…上手…」
			CASE 2
				PRINTFORMW 「や、やだ……あっ♡」
				PRINTFORMW 「っ！　今のはナシよ！」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2 + 1
			CASE 1
				PRINTFORMW 「こ、こんなの…気持ち悪い…」
			CASE 2
				PRINTFORMW 「っ…触るな！」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
