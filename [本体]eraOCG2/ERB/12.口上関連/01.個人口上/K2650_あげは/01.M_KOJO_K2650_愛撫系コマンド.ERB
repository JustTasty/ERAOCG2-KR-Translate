	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V삽입
	IF LOCAL:1 && TEQUIP:50 == MASTER
		;사정한
		IF NOWEX:MASTER:사정
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A삽입
	ELSEIF LOCAL:1 && TEQUIP:51 == MASTER
		;사정한
		IF NOWEX:MASTER:사정
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF



	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 1
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 2
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 2
