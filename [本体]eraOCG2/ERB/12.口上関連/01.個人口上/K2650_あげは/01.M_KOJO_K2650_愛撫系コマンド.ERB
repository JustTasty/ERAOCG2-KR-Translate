			CASE 2
				PRINTFORMW 「ちゅぱ…れろ…♡ あぁ…혀が…♡」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2 + 1
			CASE 1
				PRINTFORMW 「んむっ！？ …はぁ…何するのよ、もう…」
			CASE 2
				PRINTFORMW 「ふぁ…は…急に何するのよ…バカ…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1


