		호감도修正 += 0
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 
		CASE 2
			PRINTFORMW 
		CASE 3
			PRINTFORMW 
	ENDSELECT
ENDIF
RETURN 1

