;==================================================
@M_KOJO_MESSAGE_COM_K2001_68
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2001_68_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2001_68_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;처녀
		IF TCVAR:파과 == 1
PRINTFORMW "……후후❤"
			CALL M_KOJO_MESSAGE_COM_K2001_SHOJO_BREAK
			RETURN 1
		ELSE
PRINTFORMW 상반신을 끌어안자, 로제는 기쁜 듯이 소리를 흘렸다……
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입계속
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:V삽입관리플래그 == MASTER
		;前回사정한
		IF TCVAR:질내사정플래그 
			CALL M_KOJO_MESSAGE_COM_K2001_INCHANGE
			RETURN 1
		ELSE
			CALL M_KOJO_MESSAGE_COM_K2001_INCHANGE
			RETURN 1
		ENDIF
	ENDIF
	호감도修正 = 2
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 3
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
PRINTFORMW "항문……이요? 아니요, 알겠습니다. 그럼, 가겠습니다……"
		CASE 4
PRINTFORML "……❤"
PRINTFORMW "흐읍, 으읍, 하앗……들어갔습니다. 그럼, 움직이겠습니다……"
		CASE 3
PRINTFORMW "후우우우우……마스터, 무겁지는, 아프지는 않으신가요?"
		CASE 5
PRINTFORML "마스터, 안아주셔도, 괜찮을까요……？"
PRINTFORMW 로제는 달콤한 목소리를 내면서, 장내를 꿈틀거리게 하고 있다……
		CASE 2
PRINTFORMW "저, 어떠신가요?"
		CASE 6
PRINTFORML "마스터, 머리를 쓰다듬어주셔도, 괜찮을까요……？"
PRINTFORMW 머리카락 냄새를 칭찬하자, 로제는 조금 기쁜 듯 보였다……
	ENDSELECT
	RETURN 1
ENDIF
RETURN 1

