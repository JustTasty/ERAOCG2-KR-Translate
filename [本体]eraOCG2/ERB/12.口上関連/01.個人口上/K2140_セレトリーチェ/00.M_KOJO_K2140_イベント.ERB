@M_KOJO_EVENT_K2140_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERまたはTARGETが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 
		IF CFLAG:TARGET:현재위치 == 場所_玄関				;玄关
			IF CFLAG:MASTER:전턴위치 <= 50
				PRINTFORMW 「どうしたんですか？　もう行かないといけないんじゃ……？」
			ELSE
				PRINTFORMW 「何してるんですか？　早く上がってください」
			ENDIF
		ELSEIF CFLAG:TARGET:현재위치 == 場所_廊下			;走廊
			PRINTFORMW 「あ、 %CALLNAME:MASTER%さん。　ここにいたんですね」
		ELSEIF CFLAG:TARGET:현재위치 == 場所_洗面所			;盥洗室
			PRINTFORMW 「 %CALLNAME:MASTER%さんも入りますか？　すぐ出るので少し待っていてください」
		ELSEIF CFLAG:TARGET:현재위치 == 場所_浴室			;浴室
			PRINTFORMW 「まだ入ってるんですけど……」
		ELSEIF CFLAG:TARGET:현재위치 == 場所_ダイニング		;餐厅
			PRINTFORMW 「もうご飯の時間ですか？」
