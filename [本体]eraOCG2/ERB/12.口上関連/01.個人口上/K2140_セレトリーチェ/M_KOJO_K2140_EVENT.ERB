@KOJO_K2140_DATE_FOREST()
    PRINTFORML 
	PRINTFORML  「森の奥にデュエルモンスターが経営している아로마スパがあるそうですけど、行ってみませんか？」
	PRINTFORMW
    PRINTFORML  [1] - はい
    PRINTFORML  [0] - いいえ
    PRINTFORML
    BINPUT
    IF RESULT == 1 && MONEY >= 8000
        PRINTFORML  %CALLNAME:MASTER%と %CALLNAME:TARGET%が森の中を進んで行くと、木造の小屋が見えてきた。
        PRINTFORML  「ここですね」
	    PRINTFORMW
        IF 依頼_完了횟수:443 > 0
            PRINTFORML  特別なサービスを추가してもらおうか？
	        PRINTFORMW
            PRINTFORML  [0] - 普通にマッサージしてもらう
            PRINTFORML  [1] - %CALLNAME:TARGET%の가슴を大きくしてもらう
            PRINTFORML  [2] - %CALLNAME:TARGET%の가슴を小さくしてもらう
            PRINTFORML  [3] - 아로마に미약を加える
            PRINTFORML
            BINPUT
            IF RESULT == 1
                PRINTFORML  %CALLNAME:MASTER%がこっそり合図を送ると、아로마モンスター達は命令を理解して動き始めた。
                PRINTFORMW
                PRINTFORML  一時間後
                PRINTFORMW
                PRINTFORML 「すごく気持ちよかったけど、가슴が大きくなった気がします。
                PRINTFORML 　マッサージってこういう효과도 あるんですか？」
                PRINTFORML  %CALLNAME:TARGET%는자신의 가슴を触り、その変化に驚いていた。
                PRINTFORMW
                IF TALENT:TARGET:가슴사이즈 < 3
                    TALENT:TARGET:가슴사이즈 += 1
				    CALL SET_BODYSIZE(TARGET,0b10,1)
                    ; CALL SHOP_BUST_RESET(TARGET)
                ENDIF
                TIME += 60
                BASE:TARGET:무드 += 100
	            BASE:TARGET:분노 -= 100
            ELSEIF RESULT == 2
                PRINTFORML  %CALLNAME:MASTER%がこっそり合図を送ると、아로마モンスター達は命令を理解して動き始めた。
                PRINTFORMW
                PRINTFORML  一時間後
                PRINTFORMW
                PRINTFORML 「すごく気持ちよかったけど、가슴が小さくなった気がします。
                PRINTFORML 　マッサージってこういう효과도 あるんですか？」
                PRINTFORML  %CALLNAME:TARGET%는자신의 가슴を触り、その変化に驚いていた。
                PRINTFORMW
                IF TALENT:TARGET:가슴사이즈 > -3
                    TALENT:TARGET:가슴사이즈 -= 1
				    CALL SET_BODYSIZE(TARGET,0b10,1)
                    ; CALL SHOP_BUST_RESET(TARGET)
                ENDIF
                TIME += 60
                BASE:TARGET:무드 += 100
	            BASE:TARGET:분노 -= 100
            ELSEIF RESULT == 3
                PRINTFORML  %CALLNAME:MASTER%がこっそり合図を送ると、아로마モンスター達は命令を理解して動き始めた。
                PRINTFORMW
                PRINTFORML  一時間後
                PRINTFORMW
                PRINTFORML 「すごく気持ちよかったけど……身体が、なんか変です……❤」
                PRINTFORML  %CALLNAME:TARGET%는顔を赤らめ、股を擦り合わせながら出てきた。
                PRINTFORMW
                PALAM:TARGET:욕정 += 10000
                BASE:TARGET:무드 += 500
	            BASE:TARGET:분노 -= 500
            ELSE
                PRINTFORML  一小时后
                PRINTFORMW
                PRINTFORML  %CALLNAME:TARGET%出来的时候、一副신清气爽的样子
                PRINTFORML  「太棒了、还想来体验一次」
                PRINTFORMW
                TIME += 60
                BASE:TARGET:무드 += 100
	            BASE:TARGET:분노 -= 100
            ENDIF
        ELSE
            PRINTFORML  一時間後
            PRINTFORMW
            PRINTFORML  %CALLNAME:TARGET%는すっきりとした顔で出て来た。
            PRINTFORML  「最高でした、また来たいですね」
            PRINTFORMW
            TIME += 60
            BASE:TARGET:무드 += 100
	        BASE:TARGET:분노 -= 100
        ENDIF
        IF MAXBASE:TARGET:체력 <= 4900
	        MAXBASE:TARGET:체력 += 100
	        PRINTFORML %CALLNAME:TARGET%の체력UP！！：【{MAXBASE:TARGET:체력-100}】→【{MAXBASE:TARGET:체력}】
	    ELSEIF MAXBASE:TARGET:체력 > 4900 && MAXBASE:TARGET:체력 < 5000
	        MAXBASE:TARGET:체력 = 5000
	        PRINTFORML %CALLNAME:TARGET%の체력UP！！：【{MAXBASE:TARGET:체력-100}】→【{MAXBASE:TARGET:체력}】
	    ENDIF
	    IF MAXBASE:TARGET:기력 <= 4900
	        MAXBASE:TARGET:기력 += 100
	        PRINTFORML %CALLNAME:TARGET%の기력UP！！：【{MAXBASE:TARGET:기력-100}】→【{MAXBASE:TARGET:기력}】
	    ELSEIF MAXBASE:TARGET:기력 > 4900 && MAXBASE:TARGET:기력 < 5000
	        MAXBASE:TARGET:기력 = 5000
	        PRINTFORML %CALLNAME:TARGET%の체력UP！！：【{MAXBASE:TARGET:기력-100}】→【{MAXBASE:TARGET:기력}】
	    ENDIF
	    BASE:TARGET:체력 = MAXBASE:TARGET:체력
	    BASE:TARGET:기력 = MAXBASE:TARGET:기력
    ELSEIF RESULT == 1
        PRINTFORML
        PRINTFORML  ８０００円を用意してからまた来よう。
        PRINTFORMW
    ELSE
        PRINTFORML
        PRINTFORML  また今度にしよう。
        PRINTFORMW
    ENDIF
RETURN