		RETURN 1
	ENDIF
	;기본 세트
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 0
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 0
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW "이거 싫어어……❤ 머리 이상해져어❤"
		CASE 2
			PRINTFORMW "이익❤ 이거 계속하면❤ 몸이 이상해져……❤"
		CASE 3
			PRINTFORMW "용서해줘❤ 이제 %CSTR:TARGET:SELFCALL%의 패배로도 좋으니까❤ 더 이상 개발되면 %CSTR:TARGET:MASCALL%에게 거역할 수 없게 되어버려❤❤"
			PRINTFORMW "뭐든지 해도 좋으니까❤ 잡스러운 암컷의 완전 타락한 암캐 보지에 자비로운 자지님 넣어주세요오❤❤❤"
	ENDSELECT
ENDIF
RETURN 1

