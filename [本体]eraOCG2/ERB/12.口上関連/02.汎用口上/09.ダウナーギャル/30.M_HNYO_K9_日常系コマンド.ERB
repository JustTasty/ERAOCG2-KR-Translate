CALL TRAIN_MESSAGE
;구상メイン呼出
CALL M_HNYO_MESSAGE_COM_K9_300_1
RETURN RESULT

@M_HNYO_MESSAGE_COM_K9_300_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;호감도修正、通常（연모系陥落なし）で５
	호감도修正 = 5
	;TARGETが【애처】だったら호감도修正に＋２
	SIF TALENT:TARGET:애처
		호감도修正 += 2
	;TARGETが【친애】だったら호감도修正に＋２
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 2
	;TARGETが【연모】を得ていたら호감도修正に＋１
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	;「RAND:n」n面ダイスを振る
	;우후후中の会話
	IF CFLAG:TARGET:우후후
		SELECTCASE RAND:3 + 1
			CASE 1
				PRINTFORMW 「%CSTR:TARGET:SELFCALL%の体、触ってて気持ちいい？　そう、%CSTR:TARGET:SELFCALL%는なんか落ち着く……」
			CASE 2
				PRINTFORMW 「……ペース落としてくれてるんだ、やさし……♪」
			CASE 3
				PRINTFORMW 「『%CALLNAME:MASTER%とエッチ中でーす❤』……っと、ふふんっ！　ん、いいのいいのどうせ%CSTR:TARGET:MASCALL%しか知らない裏垢だし」
		ENDSELECT
	;日常会話
	ELSE
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 「低燃費なんで今は話かけないで～。今こうしてる話してるだけでもバッテリー10％ぐらい減ったわ」
			CASE 2
				PRINTFORMW 「やる気～？　明日のために溜めてるから、今日はださな～い。てか、頑張るとかダルくない？」
			CASE 3
				PRINTFORMW 「なんかゲームとか遊ばない？　いやデュエルじゃなくて、頭使わなくていい奴」
			CASE 4
				PRINTFORMW 「うげっマジか、じゃあここで無効吐いて、はぁ！？　何それ都合よすぎでしょ！　はーもう、ネットデュエルやる気なくした！」
				SIF TALENT:TARGET:연모 >= 1
					PRINTFORMW 「イライラする、%CALLNAME:MASTER%デュエルしよ」
			CASE 5
				PRINTFORMW 「ふぅああ……なんか動くのめんどいし、このまま時間だけ流れてくれないかな」
			;연모
			CASE 6
				PRINTFORMW 「予定？　別にないけどあるってことにしとく～」
				SIF BASE:TARGET:기력 >= 1000
					PRINTFORMW 「……데이트の話とかならキャンセル考えとくけど」
			;친애
			CASE 7
				PRINTFORMW 「新ギャグ思いついた？　いいよ聞いたげる……あーいいじゃん、その感じ、なんかじわじわ来た特にふとんのとこ」
			CASE 8
				PRINTFORMW 「ちょい体密着させて、良いからいいから。は～%CSTR:TARGET:SELFCALL%と居るとよけいなこと考えなくて楽だわ……」
			;애처
			CASE 9
				PRINTFORMW 「そういやちょっと日用品足りなくなってたんだよね、外行くならついでにリスト送っとくね」
				SIF BASE:TARGET:기력 >= 1000
					PRINTFORMW 「ええっ、%CSTR:TARGET:SELFCALL%も行くの……？　まぁいいよ、데이트しようじゃん」
			CASE 10
				PRINTFORMW 「……もうなんか一緒に居すぎて、居ないの想像つかなくなってきたわ……約束だけど、危ないことはしないでね」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
