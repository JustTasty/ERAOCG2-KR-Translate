@M_HNYO_EVENT_K9_35(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	;共通部分
		;オナニーの声が聞こえる（オナバレ直前）
		CASE 1
			PRINTFORMW 
	;一般
		;オナバレ
		CASE 2
			PRINTFORMW 
		;上目遣いで訴える
		CASE 3
			PRINTFORMW 
		;期待に答えて押し倒す
		CASE 4
			PRINTFORMW 
		;断られた（部屋の掃除を始める）
		CASE 5
			PRINTFORMW 
		;ノックしてから中に入る（オナバレ無し）
		CASE 6
			PRINTFORMW 
	
	;雑魚雌のみ
		;オナバレ（縋りついてくる）
		CASE 10
			PRINTFORMW 
		;見せ槍オナニー
		CASE 11
			PRINTFORMW 
	
	;捕食者のみ
		;問答無用逆レイプ
		CASE 15
			PRINTFORMW 
	ENDSELECT
ENDIF
RETURN 1

