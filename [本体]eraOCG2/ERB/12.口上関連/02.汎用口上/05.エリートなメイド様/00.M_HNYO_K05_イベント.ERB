@M_HNYO_EVENT_K5_22(ARG,ARG:1)
;구상주는 ARG
;ARG:0에 자위 상황
;ARG:1에 자위 종류
;--------------------------------------------------
;기입 체크 (=0, 비표시, 1, 표시)
LOCAL = 0
;-------------
IF LOCAL
	;-------------------------------------------------
	;통상 자위
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW   
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;같은 방일 때 자위
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW   
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;질내 사정 정액 자위
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW   
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;무지 보유자 (자위 경험 (조교 자위 경험은 제외)에 따른 분기 있음)
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		LOCAL:2 = EXP:자위경험 - EXP:조교자위경험
		;무지 제거 이벤트 있음 (표시는 한 번만)
		IF LOCAL:1 >= 25
		;서술문 분기. 쓰고 싶다면 [SKIPSTART][SKIPEND]를 삭제해 주세요
		[SKIPSTART]
		ELSEIF LOCAL:1 >= 20
		ELSEIF LOCAL:1 >= 15
		ELSEIF LOCAL:1 >= 10
		ELSEIF LOCAL:1 >= 5
		[SKIPEND]
		ELSE
		ENDIF
		PRINTFORMW   
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;함락 소질 있음 자위 중독 높음
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;-------------------------------------------------
		;C 민감 or C 감각 10 이상
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW   
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;V 민감 or V 감각 10 이상
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORMW   
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;A 민감 or A 감각 10 이상
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 3
			PRINTFORMW   
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;B 민감 or B 감각 10 이상
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 4
			PRINTFORMW   
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;부위 지정 없음
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 5
			PRINTFORMW   
			RETURN 1
		ENDIF
		;기타
		;위의 분기를 전혀 작성하지 않고 여기에 작성함으로써 함락 소질 있음 자위 중독 높음 자위의 공통 구상을 표시할 수 있습니다
		PRINTFORMW   
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;자위 중독 높음
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;-------------------------------------------------
		;항문 질 동시
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW   
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;질만
		;기입 체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORMW   
			RETURN 1
		ENDIF
		;기타
		;위의 분기를 전혀 작성하지 않고 여기에 작성함으로써 자위 중독 높음 자위의 공통 구상을 표시할 수 있습니다
		PRINTFORMW   
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;참기
	;기입 체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;기타
	;위의 분기를 전혀 작성하지 않고 여기에 작성함으로써 자위의 공통 구상을 표시할 수 있습니다
	PRINTFORMW   
	RETURN 1
ENDIF
RETURN 1
