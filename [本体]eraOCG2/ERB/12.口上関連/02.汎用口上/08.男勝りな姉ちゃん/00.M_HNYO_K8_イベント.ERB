PRINTFORMW 「안녕~, %CSTR:TARGET:MASCALL%」
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
PRINTFORMW 「여!, %CSTR:TARGET:MASCALL%！」
PRINTFORMW %CALLNAME:TARGET%는 가볍게 목례했다.

		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
PRINTFORMW 「오~, %CSTR:TARGET:MASCALL%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
