;"EVENT"系이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_HNYO_EVENT_K8_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;一日初めての挨拶분기（MAIN/お外どちらも）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GROUPMATCH(ARG,1,2,6) && CFLAG:TARGET:인사했음 == 0
		PRINTFORMW 「おはよ～っ、%CSTR:TARGET:MASCALL%」
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「よっ、%CSTR:TARGET:MASCALL%！」
		PRINTFORMW %CALLNAME:TARGET%는会釈した。

		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「おー、%CSTR:TARGET:MASCALL%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「よっ、목욕沸いてるか～？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「うおっ！？　すまん！　今出る！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「よっ、はいハイ邪魔邪魔～%CSTR:TARGET:SELFCALL%の番だ！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「よっ、元気してるー？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1に데이트상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW 「おおっ……隅に置けないな」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

