;==================================================
;12,파이즈리펠라
;==================================================
@M_HNYO_MESSAGE_SCOM_K7_12
CALL TRAIN_MESSAGE
CALL M_HNYO_MESSAGE_SCOM_K7_12_1
RETURN 1
@M_HNYO_MESSAGE_SCOM_K7_12_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;처음
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「가슴으로 하면서요……? 으응……❤ 낼름❤ 이런 느낌으로 괜찮나요……?❤」
		CASE 2
			PRINTFORMW 「음츕…… 으응❤ 앗, 부르르 떨고 있어요……❤」
		CASE 3
			PRINTFORMW 「으응, 어잇……❤ 꿀꺽❤ 앗, 끝에서부터…… 낼름❤ 흐흣❤」
		CASE 4
			PRINTFORMW 「%CSTR:TARGET:SELFCALL%의, 굉장히 뜨겁고 단단하고……❤ 멋져요……❤❤」
		CASE 5
			PRINTFORMW 「%CSTR:TARGET:MASCALL%❤ 잔뜩 사정해주세요❤ 제 몸에 잔뜩 뿌려주세요❤」
	ENDSELECT
ENDIF
;미기입
;CALLFORM M_HNYO_MESSAGE_COM_K7_{SELECTCOM}_1
RETURN 1

