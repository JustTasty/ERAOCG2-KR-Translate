	RETURN 1
ELSEIF 종류 == ターン終了時予約
	RETURN 1
ENDIF

@CARDEFFECT_200522(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード_ID
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == ターン開始時予約
	IF ターンプレイヤー == 듀얼리스트
		選択カード_ID = GETVAR(@"%듀얼리스트%_덱:0")
		TRYCALLFORM CARD_UI(選択カード_ID)
		FOR 場所,0,1
			TRYCALLFORM CARD_{選択カード_ID},"소환불가"
			SIF RESULT == 1
				BREAK
			TRYCALLFORM CARD_{選択カード_ID},"특수불가"
			SIF RESULT == 1
				BREAK
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,"덱",0)
		NEXT
	ELSEIF ターンプレイヤー == 対面者
		選択カード_ID = GETVAR(@"%対面者%_덱:0")
		TRYCALLFORM CARD_UI(選択カード_ID)
		FOR 場所,0,1
			TRYCALLFORM CARD_{選択カード_ID},"종류"
			SIF RESULT > モン全般
				BREAK
			TRYCALLFORM CARD_{選択カード_ID},"소환불가"
			SIF RESULT == 1
				BREAK
			TRYCALLFORM CARD_{選択カード_ID},"특수불가"
			SIF RESULT == 1
				BREAK
			CALL FULLRESET
			CALL CARD_SP_SUMMON(対面者,"덱",0)
		NEXT
	ENDIF
	CALL CARD_RESERV(듀얼리스트,200522)

ELSEIF 종류 == ターン終了時予約
	CALL CARD_RESERV(듀얼리스트,200522,スタン予約)
ENDIF

