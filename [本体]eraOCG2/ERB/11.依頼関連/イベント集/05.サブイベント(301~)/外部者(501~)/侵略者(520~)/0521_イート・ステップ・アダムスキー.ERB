	효과範囲상대필드 = 1
	CALLFORM 효과条件속성_ADV(물속성,"以外")
	CALL CARD_CHECK_ADV("자신",종류,ゾーン,場所)
	IF RESULT != 1
		CALL FULLRESET
		CALL STATCHANGE_EFFECT("자신","필드",場所,"直攻")
	ENDIF
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALLFORM 효과条件속성_ADV(물속성,"以外")
	CALL CARD_CHECK_ADV("상대",종류,ゾーン,場所)
	IF RESULT != 1
		CALL FULLRESET
		CALL STATCHANGE_EFFECT("상대","필드",場所,"直攻")
	ENDIF
	
ENDIF

