			효과範囲덱 = 1
			효과条件종류 = モン全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종류 = モン全般
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT >= 10
				BREAK
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 0
				RETURN 0
		REND
		CALL FULLRESET
		CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
	IF GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所") < 1
		CALL FLASH(1)
		INPUTMOUSEKEY 100
		CALL FLASH(2)
		INPUTMOUSEKEY 200
		SETCOLOR C_YELLOW
		PRINTW 《열화 거대요새 빅 코어ＭｋーＥ.Ｒ.Ａ》　굉침！
		RESETCOLOR
		CALL CARD_RECONSTRUCT(듀얼리스트,"필드",場所,152360)
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,完全묘지行き)
		ライフ変数値 = ライフ_상대 * 9 / 10
		CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",ライフ変数値)
	ELSE
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",-1)
		PRINTFORMW ビッグコアのシールドが１つ剥がれた！（残り{GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所")}枚）
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,288004,10)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"제외")
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == ターン終了時強制
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",1000)
	SETCOLOR C_YELLOW
	PRINTW 빅・코어의 눈동자의 반짝임이 더욱 강해졌다！
	RESETCOLOR

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"モ耐")
ENDIF
