@CARDCAN_268004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,268004)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時 || 종류 == EX소환時
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF GETVAR(@"%対面者%_戦闘カード") == 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_268004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 選択カード名前
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 조교대상名前

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 소환時 || 종류 == 특수소환時 || 종류 == EX소환時
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",6)

ELSEIF 종류 == 攻撃後強制誘発
	IF GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所") < 1
		CALL FLASH(1)
		INPUTMOUSEKEY 30
		CALL FLASH(2)
		SETCOLOR C_YELLOW
		PRINTW 《거대전함 빅 코어Mk-E.R.A》　굉침！
		RESETCOLOR
		CALL CARD_RECONSTRUCT(듀얼리스트,"필드",場所,152360)
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,完全묘지行き)
	ELSE
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",-1)
		PRINTFORMW ビッグコアのシールドが１つ剥がれた！（残り{GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所")}枚）
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,268004,10)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"제외")
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == ターン終了時
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",500)
	SETCOLOR C_YELLOW
	PRINTW ビッグ・コアの瞳の輝きが強くなっていく…
	RESETCOLOR

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"モ耐")
ENDIF
