	;쾌락検知
	CFLAG:이벤트主:500 = 7
ELSEIF 依頼進行度 >= 11
	;おはだ
	CFLAG:이벤트主:500 = 6
ELSEIF 依頼進行度 >= 9
	;太もも
	CFLAG:이벤트主:500 = 5
ELSEIF 依頼進行度 >= 7
	;口
	CFLAG:이벤트主:500 = 4
