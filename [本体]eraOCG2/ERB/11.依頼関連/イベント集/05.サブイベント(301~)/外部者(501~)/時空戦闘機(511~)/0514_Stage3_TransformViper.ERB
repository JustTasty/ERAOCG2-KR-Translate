#DIM DYNAMIC 이벤트主

VARSET RESULT
VARSET RESULTS

이벤트主 = NO_TO_CHARANUM(8001)

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (VV3)Stage3 TransformViper
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 前回の戦いは少し危なかった。
		PRINTL そのことでバイパーから話があるようだ。
		PRINTL
		SETCOLOR C_YELLOW
		PRINTL （注意！　５連戦＋덱변경불가！）
		RESETCOLOR
		PRINTL 
	CASE "条件"
		;前回이벤트クリア
		SIF 依頼_完了횟수:513 == 0
			RETURN 0
		;천년도어の解放が必要
		SIF ITEM:천년도어 == 0
			RETURN 0
		;バイパーが연모か굴복
		SIF TALENT:이벤트主:연모 == 0 && TALENT:이벤트主:굴복됨 == 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

