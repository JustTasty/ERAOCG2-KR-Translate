@IRAI_EVENT_435(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS



SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-DMR4)음교의 낙인
	CASE "이벤트種"
		RETURN 이벤트_施設
	CASE "依頼先"
		RETURN 場所_教会
	CASE "詳細"
		IF 依頼_完了횟수:435 == 0
			PRINTL 信者が増えたある日、クエムから新しい提案を受けた
		ELSE
			PRINTL 임신している女に카드를 産ませようか…？
		ENDIF
		PRINTL 
		PRINTL 
	CASE "条件"
		SIF TALENT:MASTER:성별 == 1
			RETURN 0
		CALL COUNT_EVENT433
		SIF 12 > RESULT
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

