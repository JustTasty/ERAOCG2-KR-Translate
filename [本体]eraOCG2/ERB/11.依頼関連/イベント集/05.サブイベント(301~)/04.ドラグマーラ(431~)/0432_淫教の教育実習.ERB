#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,279777,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		カウンタ = RESULT
		IF TALENT:TARGET:성별 == 1
			PRINTFORML
			PRINTFORML
			SETCOLOR C_PINK
			PRINTFORMW %CALLNAME:조교대상%는음교の信者としての교육《ちょうきょう》の仕上げに聖液をバーストした…！
			RESETCOLOR
			PRINTFORML
			CALL EROTIC_MOVE("前穴凌辱",TARGET, MASTER, カウンタ * 300)
		ENDIF
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",カウンタ * 2000,1)

	ENDIF
ENDIF




