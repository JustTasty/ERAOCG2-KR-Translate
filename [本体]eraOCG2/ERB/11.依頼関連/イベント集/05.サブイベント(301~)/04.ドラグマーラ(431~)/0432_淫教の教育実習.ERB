	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 常在無効
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_279778(듀얼리스트,종류,ゾーン,場所)
