@IRAI_EVENT_483(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-NGA3)-반전시동- 아누스피어
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 瞳に改竄した印を刻んだ피리카を가스타の集落に送り届けてから
		PRINTL 一週間が経過しようとしていた…
		PRINTL そろそろ様子を見に行くべきだろうか？
		PRINTL 
		PRINTL 報酬：「-반전시동-아누스피어"など
	CASE "条件"
		SIF 依頼_完了횟수:482 == 0
			RETURN 0
		SIF 依頼_完了済 != 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

