@IRAI_DUEL_CONDVIC_485
VARSET RESULT

;상대のターン
IF ターンプレイヤー == "상대"

	SIF 상대ターン終了 == 1
		RETURN 6
	SIF ライフ_상대 == 7500
		RETURN 5
	SIF ライフ_자신 == 8000
		RETURN 2
	SIF ライフ_자신 == 9000
		RETURN 3
	SIF ライフ_자신 == 10000
		RETURN 4
ENDIF

;피리카が패에 居る時
CALL FULLRESET
효과範囲손패 = 1
효과条件ID = 1818
효과条件CNT = 1
CALL CARD_CHECK_ADV("자신")
SIF RESULT >= 1
	RETURN 7

;피리카が필드에 居る時
CALL FULLRESET
효과範囲필드 = 1
효과条件ID = 1818
효과条件CNT = 1
CALL CARD_CHECK_ADV("자신")
IF RESULT >= 1
	;촉수が居ない、居る場合
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件カテゴリ = 아누스피어
	효과条件CNT = 1
	CALL CARD_CHECK_ADV("자신")
	IF RESULT == 0
		;촉수が居ない
		RETURN 8
	ELSE
		;촉수が居る
		RETURN 9
	ENDIF
ENDIF

;npnp피리카が필드에 居る時
CALL FULLRESET
효과範囲필드 = 1
효과条件ID = 59554
효과条件CNT = 1
CALL CARD_CHECK_ADV("자신")
IF RESULT >= 1
	;盤面の카운터を見るのが一番なんだけど、ライフを見るのが楽なので…
	IF ライフ_상대 == 7500
		RETURN 10
	ELSEIF ライフ_상대 > 3500
		RETURN 11
	ELSE
		RETURN 12
	ENDIF
ENDIF


RETURN 0

;メッセージの설정
