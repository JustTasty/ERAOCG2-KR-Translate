@IRAI_EVENT_482(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-NGA2)반전하는 눈동자로의 접촉
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 바람の谷の誘拐事件について
		PRINTL 貴方はぼんやりと思考を巡らせる…
		PRINTL 
		PRINTL 「新着メッセージ:一件」
		PRINTL 
		PRINTL 報酬：「반전하는 눈동자의 접촉」など
	CASE "条件"
		SIF 依頼_完了횟수:481 == 0
			RETURN 0
		SIF 依頼_完了済 != 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

