@IRAI_EVENT_50(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (ST5-1)꿈의 나라의 카이바랜드
	CASE "이벤트種"
		RETURN 이벤트_物語
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL あれからまたしばらく時が過ぎた。
		PRINTL 新たな次元との접촉で精霊世界側はまた大きな広がりと書類仕事にいそしむ中……
		PRINTL 戦士たちは休息を必要としていた。
		PRINTL
		PRINTL 
	CASE "条件"
		SIF 依頼_完了횟수:34 == 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0



