	CFLAG:대상:몸무게초기치 = CALC_BODYSIZE_WEIST(대상)
ENDIF

@CALC_BODYSIZE_WEIST(대상)
#FUNCTION
#DIM DYNAMIC 대상
#DIM DYNAMIC 基礎値
基礎値 = (CFLAG:대상:신장초기치 * (IS_MALE(대상) ? 42 # 37 + TALENT:대상:몸두께)) / 100

SELECTCASE TALENT:대상:체형
	CASE -3 ;小人
		TIMES 基礎値, 1.07
	CASE -2 ;幼児체형
		TIMES 基礎値, 1.07
	CASE -1 ;小柄체형
		TIMES 基礎値, 1.03
	CASE 0 ;普通
	CASE 1 ;長身
	CASE 2 ;大柄
		TIMES 基礎値, 1.03
	CASE 3 ;巨人
		TIMES 基礎値, 1.10
ENDSELECT

SELECTCASE TALENT:대상:몸두께
	CASE 0 ;標準
	CASE 1 ;太り気味
		TIMES 基礎値, 1.08
	CASE 2 ;むちむち
		TIMES 基礎値, 1.15
	CASE 3 ;むっちり
		TIMES 基礎値, 1.2
	CASE 4 ;ぽってり
		TIMES 基礎値, 1.25
	CASE 5 ;운동しろ
		TIMES 基礎値, 1.3
ENDSELECT

基礎値 = 基礎値 * RAND(97, 104) / 100

RETURNF 基礎値

