@TROPHY_NO_1(参照先)
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 메인스토리 １장 클리어
	CASE "詳細"
		PRINTL 
		PRINTL 메인 스토리 1장 클리어의 증거
		PRINTL 
		PRINTL 보수: 빈 카드 1번째 슬롯 (나중에 해금되는 기능에서 사용 가능한 아이템)
		PRINTL 
	CASE "条件"
		SIF 依頼_完了횟수:4 > 0
			RETURN 1
		RETURN 0
	CASE "報酬"
		CALL CARD_GET_SINGLE(201,,3)
ENDSELECT
RETURN 0


