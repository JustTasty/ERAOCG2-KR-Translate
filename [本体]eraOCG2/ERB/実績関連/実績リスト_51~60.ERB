@TROPHY_NO_51(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성数

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = E.R.A카드를 사용하도록……
	CASE "詳細"
		PRINTL 
		PRINTL 의뢰 "뒷골목의 듀얼장" 또는 "월드 챔피언십、인터벌1"을 클리어하고
		PRINTL 뒷골목 샵을 해방한 증거
		PRINTL 
		PRINTL 보수: 조교 세계와의 접촉《E・R・A 카논 이벤트》1장
		PRINTL 
	CASE "条件"
		SIF パック_解放:99 > 0
			RETURN 1
		RETURN 0
	CASE "報酬"
		CALL CARD_GET_SINGLE(79669,,1)
ENDSELECT
RETURN 0


@TROPHY_NO_52(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성数

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 듀얼리스트가 아니다、조교사다！
	CASE "詳細"
		PRINTL 
		PRINTL 카드 매입 감정액 100,000엔 이상을 달성한 증거
		PRINTL 
		PRINTL 보수: 10,000DP
		PRINTL 
	CASE "条件"
		SIF 実績_進行보존:52 > 0
			RETURN 1
		RETURN 0
	CASE "報酬"
		PRINTFORMW DP + 10,000
		FLAG:듀얼포인트 += 10000
ENDSELECT
RETURN 0


