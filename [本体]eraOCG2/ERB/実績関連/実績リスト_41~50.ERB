@TROPHY_NO_41(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성数

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 카드 컬렉터
	CASE "詳細"
		PRINTL 
		PRINTL 보유 카드 종류 1,000개 이상 달성의 증거
		PRINTL 
		PRINTL 보수: 1,000DP
		PRINTL 
	CASE "条件"
		FOR カウンタ,1000,100000
			SIF ITEM:カウンタ >= 1
			条件달성数 ++ 
		NEXT
		SIF 条件달성数 >= 1000
			RETURN 1
		RETURN 0
	CASE "報酬"
		PRINTFORMW DP + 1,000
		FLAG:듀얼포인트 += 1000
ENDSELECT
RETURN 0


@TROPHY_NO_42(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성数

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 덱 마이스터
	CASE "詳細"
		PRINTL 
		PRINTL 만든 덱이 10개 이상인 증거
		PRINTL 
		PRINTL 보수: 500DP
		PRINTL 
	CASE "条件"
		FOR カウンタ,0,20
			CALL DECK_ABLE_CHECK(カウンタ)
			SIF RESULT == 1
				条件달성数 ++
		NEXT
		SIF 条件달성数 >= 10
			RETURN 1
		RETURN 0
	CASE "報酬"
		PRINTFORMW DP + 500
		FLAG:듀얼포인트 += 500
ENDSELECT
RETURN 0


@TROPHY_NO_43(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성数

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 레어카드 컬렉터
	CASE "詳細"
		PRINTL 
		PRINTL 보유 UR 카드 종류 100개 이상 달성의 증거
		PRINTL 
		PRINTL 보수: 1,000DP
		PRINTL 
	CASE "条件"
		FOR カウンタ,1000,100000
			SIF ITEM:カウンタ == 0
				CONTINUE
			TRYCALLFORM CARDPRICE(カウンタ,"レア度")
			SIF RESULT != ULR
				CONTINUE
			条件달성数 ++ 
		NEXT
		SIF 条件달성数 >= 100
			RETURN 1
		RETURN 0
	CASE "報酬"
		PRINTFORMW DP + 1,000
		FLAG:듀얼포인트 += 1000
ENDSELECT
RETURN 0


