@EVENT_COUNTER_REACTION(ARG,ARG:1)

;질삽입
IF MASTER_POSE(6,1) == ARG
	SELECTCASE RAND:5
		CASE 1
			SIF ABL:ARG:질 < 2
				RETURN 0
			TCVAR:ARG:CT파생 = 1
		;だいしゅきホールド
		CASE 2
			SIF !TALENT:ARG:연모
				RETURN 0
			SIF TEQUIP:ARG:체위 == 1 || TEQUIP:ARG:체위 == 4
				TCVAR:ARG:CT파생 = 2
		;성교見せつけ
		CASE 3
			SIF ABL:ARG:노출증 < 2
				RETURN 0
			SIF TEQUIP:ARG:체위 == 3
				TCVAR:ARG:CT파생 = 3
	ENDSELECT
	SIF TCVAR:ARG:CT파생
		TCVAR:ARG:CT행동 = 500
ENDIF

;수음
IF MASTER_POSE(3,1) == ARG
	SELECTCASE RAND:4
		CASE 1
			SIF ABL:ARG:손가락 < 2
				RETURN 0
			TCVAR:ARG:CT행동 = 502
		;玉揉み
		CASE 2
			SIF ABL:ARG:손가락 < 2
				RETURN 0
			IF ARG == TARGET && SELECTCOM == 80
				TCVAR:ARG:CT파생 = 1
				TCVAR:ARG:CT행동 = 600
			ENDIF
		;애널애무
		CASE 3
			SIF BASE:MASTER:발기 < 1000
				RETURN 0
			SIF ARG == TARGET && SELECTCOM == 80
				TCVAR:ARG:CT행동 = 601
	ENDSELECT
ENDIF
;펠라치오
IF MASTER_POSE(4,1) == ARG
	SELECTCASE RAND:4
		CASE 1
			SIF ABL:ARG:혀 < 2
				RETURN 0
			TCVAR:ARG:CT행동 = 503
			TCVAR:ARG:CT파생 = 1
		;玉舐め
		CASE 2
			SIF ABL:ARG:혀 < 2
				RETURN 0
			SIF ARG == TARGET
				RETURN 0
			TCVAR:ARG:CT파생 = 2
			TCVAR:ARG:CT행동 = 600
		;ディープスロート
		CASE 3
			SIF ABL:ARG:혀 < 4
				RETURN 0
			SIF BASE:MASTER:발기 < 1000
				RETURN 0
			IF ARG == TARGET && SELECTCOM == 81
				TCVAR:ARG:CT행동 = 503
				TCVAR:ARG:CT파생 = 3
			ENDIF
	ENDSELECT
ENDIF
;파이즈리
IF MASTER_POSE(5,1) == ARG
	SELECTCASE RAND:4
		CASE 1
			SIF ARG == TARGET || SELECTCOM == 82
				RETURN 0
			SIF ABL:ARG:가슴 < 2
				RETURN 0
			TCVAR:ARG:CT파생 = 1
		;縦파이즈리
		CASE 2
			SIF ABL:ARG:가슴 < 3
				RETURN 0
			SIF BUST_SIZE(ARG) < 1
				RETURN 0
			SIF ARG == TARGET && SELECTCOM == 82
				TCVAR:ARG:CT파생 = 2
		;頑張って谷間をつくる
		CASE 3
			SIF ABL:ARG:가슴 < 3
				RETURN 0
			SIF BUST_SIZE(ARG) != -1
				RETURN 0
			SIF ARG == TARGET && SELECTCOM == 82
				TCVAR:ARG:CT파생 = 3
	ENDSELECT
	SIF TCVAR:ARG:CT파생
		TCVAR:ARG:CT행동 = 504
ENDIF
;스마타
IF MASTER_POSE(2,1) == ARG
	SELECTCASE RAND:3
		CASE 1
			SIF ABL:ARG:허리 < 2
				RETURN 0
			TCVAR:ARG:CT파생 = 1
		;수음추가
		CASE 2
			SIF ABL:ARG:허리 < 2
				RETURN 0
			SIF ABL:ARG:손가락 < 2
				RETURN 0
			SIF ARG == TARGET && SELECTCOM == 83
				TCVAR:ARG:CT파생 = 2
	ENDSELECT
	SIF TCVAR:ARG:CT파생
		TCVAR:ARG:CT행동 = 505
ENDIF
