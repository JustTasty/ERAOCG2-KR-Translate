@EVENT_COUNTER_POSE(ARG,ARG:1)
VARSET LOCAL
SIF TCVAR:ARG:30 > 1
	RETURN 0
SELECTCASE TCVAR:ARG:CT행동
	;키스
	CASE 30
		TFLAG:60 ++
	;가슴を揉ませる
	CASE 33
		TFLAG:61 ++
	;股間をまさぐる
	CASE 34
		TFLAG:62 ++
	;가슴を揉む
	CASE 36
		TFLAG:65 ++
	;수음
	CASE 50
		TFLAG:62 ++
	;펠라치오
	CASE 51
		TFLAG:62 ++
	;스마타
	CASE 52
		TFLAG:62 ++
	;파이즈리
	CASE 53
		TFLAG:62 ++
	;풋잡
	CASE 54
		TFLAG:62 ++
	;애널애무
	CASE 55
		TFLAG:64 ++

	;애널舐め
	CASE 56
		TFLAG:64 ++
	;손가락삽입
	CASE 57
		TFLAG:63 ++
	;가슴애무
	CASE 58
		TFLAG:65 ++
	;유두빨기
	CASE 59
		TFLAG:65 ++
	;ディープ키스
	CASE 60
		TFLAG:60 ++
	;역 레이프
	CASE 61
		TFLAG:62 ++

	;커닐링구스 강제
	CASE 70
		TFLAG:60 ++
	;펠라치오強制
	CASE 71
		TFLAG:60 ++
	;애널봉사
	CASE 72
		TFLAG:60 ++
	;ぱふぱふ
	CASE 73
		TFLAG:60 ++
	;乳合わせ
	CASE 74
		TFLAG:65 ++
	;タワシ洗い
	CASE 75
		TFLAG:61 ++


	CASE 500
		TFLAG:62 ++
ENDSELECT
;69자세
SELECTCASE TCVAR:ARG:CT행동
	CASE 50
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;펠라치오
	CASE 51
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;파이즈리
	CASE 53
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;애널애무
	CASE 55
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;손가락삽입
	CASE 57
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;커닐링구스 강제
	CASE 70
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;펠라치오強制
	CASE 71
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	;애널봉사
	CASE 72
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,2)
	CASEELSE
		CALL EVENT_COUNTER_POSE_69(ARG,ARG:1,0)
ENDSELECT

;引数1 대상의 캐릭터番号 2 행동順 3 식스나인移行への確率
@EVENT_COUNTER_POSE_69(ARG,ARG:1,ARG:2)
IF ARG:2 == 0
	TEQUIP:ARG:식스나인 = 0
	RETURN 0
ENDIF
IF TEQUIP:ARG:식스나인
	TFLAG:60 ++
	RETURN 0
ENDIF
SIF TFLAG:60
	RETURN 0
IF RAND:(ARG:2) == 0
	TEQUIP:ARG:식스나인 = 1
	TCVAR:ARG:27 = 1
ENDIF
