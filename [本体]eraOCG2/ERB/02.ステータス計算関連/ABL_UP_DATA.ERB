@JUEL_DEMAND_33(ARG,ARG:1,ARG:2)
#FUNCTION
;호모끼が必要
SIF ABL:(ARG:2):호모끼 < ABL:(ARG:2):게이중독 + 1
	RETURNF -2
SIF ARG > 0
	RETURNF -1
VARSET LOCAL
;快C(LOCAL:0),욕정(LOCAL:11),굴복(LOCAL:12)が必要
SELECTCASE ABL:(ARG:2):게이중독
	CASE 0
		LOCAL:0 = 3000
		LOCAL:11 = 10000
		LOCAL:12 = 10000
	CASE 1
		LOCAL:0 = 8000
		LOCAL:11 = 25000
		LOCAL:12 = 25000
	CASE 2
		LOCAL:0 = 15000
		LOCAL:11 = 50000
		LOCAL:12 = 50000
	CASE 3
		LOCAL:0 = 30000
		LOCAL:11 = 100000
		LOCAL:12 = 100000
	CASE 4
		LOCAL:0 = 55000
		LOCAL:11 = 300000
		LOCAL:12 = 300000
	CASE 5
		LOCAL:0 = 60000
		LOCAL:11 = 350000
		LOCAL:12 = 350000
	CASE 6
		LOCAL:0 = 65000
		LOCAL:11 = 400000
		LOCAL:12 = 400000
	CASE 7
		LOCAL:0 = 70000
		LOCAL:11 = 450000
		LOCAL:12 = 450000
	CASE 8
		LOCAL:0 = 75000
		LOCAL:11 = 500000
		LOCAL:12 = 500000
	CASE 9
		LOCAL:0 = 80000
		LOCAL:11 = 550000
		LOCAL:12 = 550000
	CASEELSE
		LOCAL:0 = 10000 * (ABL:(ARG:2):게이중독 - 1)
		LOCAL:11 = 60000 * (ABL:(ARG:2):게이중독 + 1)
		LOCAL:12 = 60000 * (ABL:(ARG:2):게이중독 + 1)
