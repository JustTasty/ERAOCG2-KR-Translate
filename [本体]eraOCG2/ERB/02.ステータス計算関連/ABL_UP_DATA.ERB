;JUEL:(ARG:1)が必要ならLOCAL:(ARG:1)に代入してその値を返す
;ついでにパターンの認識や必要なABL等の処理
;--------------------------------------------------
@JUEL_DEMAND_0(ARG,ARG:1,ARG:2)
#FUNCTION
SIF ARG > 0
	RETURNF -1
VARSET LOCAL
SELECTCASE ABL:(ARG:2):Ｃ감각
	CASE 0
		LOCAL:0 = 1
	CASE 1
		LOCAL:0 = 20
	CASE 2
		LOCAL:0 = 400
	CASE 3
		LOCAL:0 = 5000
	CASE 4
		LOCAL:0 = 15000
	CASE 5
		LOCAL:0 = 30000
	CASE 6
		LOCAL:0 = 40000
	CASE 7
		LOCAL:0 = 50000
	CASE 8
		LOCAL:0 = 60000
	CASE 9
		LOCAL:0 = 70000
	CASEELSE
		LOCAL:0 = 20000 * (ABL:(ARG:2):Ｃ감각 - 6)
ENDSELECT
;경험보정
LOCAL:0 = LOCAL:0 * 10 / (5 + GETEXPLV(EXP:(ARG:2):Ｃ경험,8))
LOCAL:0 = LOCAL:0 * (1 + ABL:(ARG:2):Ｃ감각) / (1 + GETEXPLV(EXP:(ARG:2):Ｃ절정경험,8))
;일선넘지않음
SIF TALENT:(ARG:2):일선넘지않음 && !TALENT:(ARG:2):연모
	TIMES LOCAL:0 , 2.00
RETURNF LOCAL:(ARG:1)

