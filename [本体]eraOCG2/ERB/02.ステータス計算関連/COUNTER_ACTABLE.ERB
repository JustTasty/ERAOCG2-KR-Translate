@ACTABLE(ARG,ARG:1)
#FUNCTION
VARSET LOCAL
;前の캐릭터が何をしたかをLOCAL:100～に출력
FOR LOCAL,1,CHARANUM
	SIF TARGET:LOCAL <= 0
		CONTINUE
	SIF TCVAR:(TARGET:LOCAL):CT행동
		LOCAL:(100 + TCVAR:(TARGET:LOCAL):CT행동) ++
NEXT

SELECTCASE ARG:1
	;色っぽい仕草
	CASE 10
		RETURNF 1
	;見つめる
	CASE 11
		RETURNF 1
	;体を摺り寄せる
	CASE 12
		RETURNF 1
	;치마を捲る
	CASE 13
		;치마着用
		SIF !EQUIP:MASTER:9
			RETURNF 0
		SIF TEQUIP:MASTER:0 > 3
			RETURNF 0
		;全裸
		SIF !EQUIP:MASTER:0
			RETURNF 0
		SIF !ONCE("치마めくられ",1,ARG)
			RETURNF 0
		RETURNF 1
	;耳元に息を吹きかける
	CASE 14
		;대화中はダメ
		SIF SELECTCOM == 300
			RETURNF 0
		RETURNF 1
	;엉덩이를 쓰다듬는다
	CASE 15
		;포식자じゃないとダメ
		SIF TALENT:ARG:포식자 == 0
			RETURNF 0
		;プレイヤーが男だとダメ
		SIF TALENT:MASTER:성별 == 2
			RETURNF 0
		RETURNF 1
	;耳舐め
	CASE 16
		;포식자じゃないとダメ
		SIF TALENT:ARG:포식자 == 0
			RETURNF 0
		RETURNF 1

	;발기の上からすりすり
	CASE 17
		SIF BASE:MASTER:발기 < 500
			RETURNF 0
		SIF HAS_PENIS(MASTER) == 0
			RETURNF 0
		RETURNF 1
	;키스
	CASE 30
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	;後ろから抱きつく
	CASE 31
		SIF LOCAL:131
			RETURNF 0
		RETURNF 1
	;前から抱きつく
	CASE 32
		SIF LOCAL:132
			RETURNF 0
		;키스
		SIF LOCAL:131
			RETURNF 0
		RETURNF 1
	;가슴を揉ませる
	CASE 33
		SIF TFLAG:61
			RETURNF 0
		RETURNF 1
	;股間をまさぐる
	CASE 34
		SIF TEQUIP:MASTER:0 <= 3
			RETURNF 0
		SIF TFLAG:62
			RETURNF 0
		RETURNF 1
	;치마の中を見せる
	CASE 35
		;치마着用
		SIF !EQUIP:ARG:9
			RETURNF 0
		SIF TEQUIP:ARG:0 > 3
			RETURNF 0
		;全裸
		SIF !EQUIP:MASTER:0
			RETURNF 0
		RETURNF 1
	;가슴を揉む
	CASE 36
		SIF !(TALENT:MASTER:성별 & 1)
			RETURNF 0
		SIF TEQUIP:MASTER:1 < 1
			RETURNF 0
		SIF TFLAG:65
			RETURNF 0
		RETURNF 1
	;수음
	CASE 50
		SIF TFLAG:62
			RETURNF 0
		SIF ABL:ARG:손가락 < 1
			RETURNF 0
		RETURNF 1
	;펠라치오
	CASE 51
		SIF TFLAG:62
			RETURNF 0
		SIF ABL:ARG:혀 < 1
			RETURNF 0
		RETURNF 1
	;스마타
	CASE 52
		SIF TFLAG:62
			RETURNF 0
		SIF ABL:ARG:허리 < 1
			RETURNF 0
		;윤활
		SIF PALAM:ARG:윤활 < 2000
			RETURNF 0
		RETURNF 1
	;파이즈리
	CASE 53
		SIF TFLAG:62
			RETURNF 0
		SIF ABL:ARG:가슴 < 1
			RETURNF 0
		SIF HAS_PENIS(MASTER) == 0
			RETURNF 0
		;[絶壁]だと불가
		SIF BUST_SIZE(ARG) == -2
			RETURNF 0
		;[爆乳]なら기교LV1以上、[巨乳]でなければ기교3以上、[貧乳]だと기교4以上必要
		SIF BUST_SIZE(ARG) + ABL:ARG:기교 < 3
			RETURNF 0
		RETURNF 1
	;풋잡
	CASE 54
		SIF TCVAR:MASTER:120 != 1
			RETURNF 0
		SIF HAS_PENIS(MASTER) == 0
			RETURNF 0
		SIF TFLAG:62
			RETURNF 0
		SIF ABL:ARG:새드끼 < 1
			RETURNF 0
		RETURNF 1
	;애널애무
	CASE 55
		SIF TFLAG:64
			RETURNF 0
		SIF ABL:ARG:손가락 < 1
			RETURNF 0
		RETURNF 1
	;애널舐め
	CASE 56
		SIF TFLAG:64
			RETURNF 0
		SIF STAIN:MASTER:4 & 8 && TALENT:ARG:악취내성 < 1
			RETURNF 0
		SIF ABL:ARG:혀 < 1
			RETURNF 0
		RETURNF 1
	;손가락삽입
	CASE 57
		SIF TFLAG:63
			RETURNF 0
		SIF TALENT:MASTER:처녀
			RETURNF 0
		SIF !(TALENT:MASTER:성별 & 1)
			RETURNF 0
		RETURNF 1
	;가슴애무
	CASE 58
		SIF TFLAG:65
			RETURNF 0
		SIF ABL:ARG:손가락 < 1
			RETURNF 0
		RETURNF 1
	;유두빨기
	CASE 59
		SIF TFLAG:65
			RETURNF 0
		SIF ABL:ARG:혀 < 1
			RETURNF 0
		RETURNF 1
	;딥키스
	CASE 60
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	;역 레이프
	CASE 61
		SIF TFLAG:62
			RETURNF 0
		SIF BASE:MASTER:발기 < 500
			RETURNF 0
		SIF HAS_PENIS(MASTER) == 0
			RETURNF 0
		RETURNF 1
	;커닐링구스 강제
	CASE 70
		SIF !(TALENT:ARG:2 & 1)
			RETURNF 0
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	;펠라치오強制
	CASE 71
		SIF !(TALENT:ARG:2 & 2)
			RETURNF 0
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	;애널봉사
	CASE 72
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	;ぱふぱふ
	CASE 73
		SIF !(TALENT:MASTER:성별 & 1)
			RETURNF 0
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	;乳合わせ
	CASE 74
		SIF !(TALENT:MASTER:성별 & 1)
			RETURNF 0
		SIF TFLAG:65
			RETURNF 0
		RETURNF 1
	;タワシ洗い
	CASE 75
		SIF !(TALENT:ARG:2 & 1)
			RETURNF 0
		;윤활
		SIF PALAM:ARG:윤활 < 500
			RETURNF 0
		;口が空いてれば커닐링구스させる
		SIF !TFLAG:60
			RETURNF 0
		SIF TFLAG:61 > 1
			RETURNF 0
		RETURNF 1
ENDSELECT

