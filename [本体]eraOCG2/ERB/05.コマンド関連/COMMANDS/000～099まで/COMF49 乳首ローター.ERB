;-------------------------------------------------
;유두로터
;도구사용コマンド、レベル1
;-------------------------------------------------
@COM49
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 10
DOWNBASE:기력 += 80

CALL SOURCE_CALC_쾌Ｂ(TARGET, MASTER,250)
CALL SOURCE_CALC_노출(TARGET, MASTER, 50)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 60)
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 1
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 1
ENDIF

EXP:MASTER:도구사용경험 ++
RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------

@COM_ABLE49
;로터実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(49)
	RETURN RESULT
;아이템を持っているか、아이템なし설정でないとダメ
SIF ITEM:0 == 0 && NOITEM == 0
	RETURN 0

;상반신착의
SIF TEQUIP:1 > 1
	RETURN 0
;착유기사용중は불가
SIF TEQUIP:17
	RETURN 0
;유방전극装着中はダメ
SIF TEQUIP:24
	RETURN 0
RETURN 1