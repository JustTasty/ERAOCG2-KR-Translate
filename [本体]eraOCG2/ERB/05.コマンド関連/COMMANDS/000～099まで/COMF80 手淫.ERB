;-------------------------------------------------
;수음
;봉사계커맨드、レベル1
;-------------------------------------------------
@COM80
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
TCVAR:MASTER:사정대상캐릭터_1 = TARGET
TCVAR:MASTER:사정부위플래그 = 3
TCVAR:MASTER:사정쾌락강도 = 1500

;봉사쾌락
TFLAG:봉사쾌락플래그 = 1
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 10
DOWNBASE:기력 += 100

CALL SOURCE_CALC_노출(TARGET, MASTER, 20)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 50)
CALL SOURCE_CALC_봉사(TARGET, MASTER, 250)

;애널、파과血、촉수の汚れ
IF (STAIN:MASTER:0 & 8) || (STAIN:MASTER:0 & 32) || (STAIN:MASTER:0 & 64)
	CALL SOURCE_CALC_불결(TARGET, MASTER, 400)
;愛液,정액汚れ
ELSEIF STAIN:MASTER:0 & 1 || STAIN:MASTER:0 & 4
	CALL SOURCE_CALC_불결(TARGET, MASTER, 200)
ELSE
	CALL SOURCE_CALC_불결(TARGET, MASTER, 100)
ENDIF
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;사정に관계なく行われる処理はここから
;奴隷の손가락⇔조교者のＰの汚れが이동
STAIN:1 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:1

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;수음경험
EXP:수음경험 ++

;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 4
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 4
ENDIF

RETURN 1


@CAN_COM80(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
SKIPDISP ARG
PRINTL 수음
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------


;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER
素質確認 = RESULT:1
판정値 += RESULT
;ABL:욕망
IF ABL:욕망
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 1,')
	素質確認 = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV,ABL:봉사정신,'(,ABL:봉사정신 * 4,')
	素質確認 = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:정액중독 * 1
	PRINT 정액중독
	PRINTV 'LV,ABL:정액중독,'(,ABL:정액중독 * 1,')
	素質確認 = 1
ENDIF

IF MARK:쾌락각인
	SIF 素質確認
		PRINT  + 
	판정値 += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 1,')
	素質確認 = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정,5)
	SIF 素質確認
		PRINT  + 
	판정値 += GETPALAMLV(PALAM:욕정,5) * 1
	PRINT 욕정
	PRINTV 'LV,GETPALAMLV(PALAM:욕정,5),'(,GETPALAMLV(PALAM:욕정,5) * 1,')
	素質確認 = 1
ENDIF
;부끄럼쟁이
IF TALENT:수치심 > 0
	PRINT  - 
	판정値 -= 1
	PRINT 부끄러움
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;악취둔감
IF TALENT:악취내성 > 0
	SIF 素質確認
		PRINT  + 
	판정値 += 1
	PRINT 악취둔감
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;악취민감
IF TALENT:악취내성 < 0
	판정値 -= 3
	PRINT 악취민감
	PRINTV '(,3,')
	素質確認 = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF 素質確認
		PRINT  + 
	판정値 += 6
	PRINT 헌신적
	PRINTV '(,6,')
	素質確認 = 1
ENDIF
;쾌감의 부정（음란、쾌락광で無効）
IF TALENT:쾌감반응 < 0 && !TALENT:음란
	PRINT  - 
	판정値 -= 1
	PRINT 쾌감의 부정
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;男嫌い（연모、친애で無効）
IF TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1) && (!TALENT:연모 || MASTER != MASTER)
	PRINT  - 
	판정値 -= 5
	PRINT 남성혐오
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;연모
IF TALENT:연모
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 연모
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;친애
IF TALENT:연모 > 1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 친애
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;マイペース
IF TALENT:페이스배분 == -1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 마이페이스
	PRINTV '(,5,')
	素質確認 = 1
ENDIF


;조교者が후타나리
IF (TALENT:MASTER:성별 & 3) == 3
	SIF 素質確認
		PRINT  + 
	판정値 += 8
	PRINT 후타나리
	PRINTV '(,8,')
	素質確認 = 1
ENDIF


;愛液の汚れ
SIF STAIN:MASTER:2 & 1
	汚れ値 += 1
;정액の汚れ
SIF STAIN:MASTER:2 & 4
	汚れ値 += 3
;애널の汚れ
SIF STAIN:MASTER:2 & 8
	汚れ値 += 7
SIF STAIN:MASTER:2 & 16
	汚れ値 += 15
;手扱きでは汚れがそれほど影響しない
Y /= 3
汚れ値 = 汚れ値 * (2 - TALENT:악취내성) / 2

;더러움있음
IF 汚れ値
 	PRINT  - 
	판정値 -= 汚れ値
	PRINT 더러움 있음
	;악취둔감
	IF TALENT:악취내성 > 0
		PRINT 、악취둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT 、악취민감
	ENDIF
	PRINTV '(,汚れ値,')
	素質確認 = 1
ENDIF


;合計を표시(14以上で実行)
PRINT  = 
PRINTV 판정値

実行値 = 14
SIF 판정値 < 実行値
	PRINT  < 
SIF 판정値 == 実行値
	PRINT  = 
SIF 판정値 > 実行値
	PRINT  > 
PRINT 실행치
PRINTV 実行値

WAIT
SKIPDISP 0
;実行できない
SIF 판정値 < 実行値
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE80
;수음実行판정
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(80)
	RETURN RESULT
;조교者がオトコか半人半妖か후타나리じゃないとダメ
SIF !(TALENT:MASTER:성별 & 2)
	RETURN 0
;밧줄사용중/手足が使えないと불가
SIF TEQUIP:밧줄 || TEQUIP:사지없음
	RETURN 0
RETURN 1