	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(425)
	RETURN RESULT
;詰所
SIF CFLAG:MASTER:현재위치 != 場所_キッチン
	RETURN 0
SIF CFLAG:우후후 == 2
	RETURN 0
RETURN 1
