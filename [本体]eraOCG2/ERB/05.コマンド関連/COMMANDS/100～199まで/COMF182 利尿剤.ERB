;-------------------------------------------------
;이뇨제
;특수コマンド
;-------------------------------------------------
@COM182
ITEM:42 -= 1

TEQUIP:이뇨제 = 1
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
;行為者
TCVAR:116 = MASTER
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
;조교者が조합지식を持っていれば消費を抑えられる
IF TALENT:MASTER:조합지식
	DOWNBASE:체력 += 100
	DOWNBASE:기력 += 100
ELSE
	DOWNBASE:체력 += 200
	DOWNBASE:기력 += 200
ENDIF

CALL SOURCE_CALC_불결(TARGET, MASTER, 800)


;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 1
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 1
ENDIF

RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE182
;이뇨제実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(182)
	RETURN RESULT
;아이템を持っているか、아이템なし설정でないとダメ
SIF ITEM:42 == 0 && NOITEM == 0
	RETURN 0
;약독내성の경우、直接注入以外ダメ
SIF TALENT:약독내성
	RETURN 0
;새댁플레이 중はダメ
SIF TEQUIP:새댁플레이
	RETURN 0
RETURN 1