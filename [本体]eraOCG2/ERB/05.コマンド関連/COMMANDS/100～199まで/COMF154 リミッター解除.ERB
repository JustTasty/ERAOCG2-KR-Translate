;-------------------------------------------------
;리미터해제
;カード使用コマンド
;-------------------------------------------------
@COM154
#DIM DYNAMIC 노출강도
#DIM DYNAMIC 정복강도
#DIM DYNAMIC 가슴倍率
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 100
DOWNBASE:기력 += 150

가슴倍率 = 3 + TALENT:TARGET:가슴사이즈


IF 가슴倍率 >= 1
	CALL SOURCE_CALC_쾌Ｂ(TARGET, MASTER, 150 * 가슴倍率)
	CALL SOURCE_CALC_노출(TARGET, MASTER, 200 * 가슴倍率)
ENDIF
CFLAG:TARGET:추가가슴사이즈 = 2
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 5
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 5
ENDIF

RETURN 1
;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE154
;土下座実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(154)
	RETURN RESULT
;オンナか후타나리じゃないとダメ
SIF HAS_VAGINA(TARGET) == 0
	RETURN 0
;1かいぽっきりだよー
SIF CFLAG:TARGET:추가가슴사이즈 != 0
	RETURN 0
;금단의지식が無いとできない
SIF TALENT:MASTER:금단의지식 == 0
	RETURN 0
;정신조작！！【추가】
SIF ITEM:70015 <= 0
	RETURN 0

SIF CFLAG:수면
	RETURN 0
;상대가 いないとできない
SIF TARGET:1 <= 0
	RETURN 0
RETURN 1
