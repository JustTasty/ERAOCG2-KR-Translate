;-------------------------------------------------
;乳スパンキング
;SM系コマンド、レベル1
;-------------------------------------------------
@COM108
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 60
DOWNBASE:기력 += 20
CALL SOURCE_CALC_쾌Ｂ(TARGET, MASTER, 50)
CALL SOURCE_CALC_고통(TARGET, MASTER, 300)
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 2
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 2
ENDIF

RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE108
;スパンキング実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(108)
	RETURN RESULT
RETURN 1
