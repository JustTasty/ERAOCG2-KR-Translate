IF LOCAL
	;同時사정
	IF NOWEX:사정 && NOWEX:MASTER:사정
		PRINTFORMW
		RETURN 1
	;TARGETだけ사정
	ELSEIF NOWEX:사정
		;PLAYERについてる
		IF GETBIT(TALENT:MASTER:성별,1)
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	;PLAYERだけ사정
	ELSE
		;TARGETについてる
		IF GETBIT(TALENT:성별,1)
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

