	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ구상）
;==================================================
@M_KOJO_EVENT_KX_3(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在땅】から【캐릭터の自室】に戻りました）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:우후후
			;우후후ダウン後に何も言わせない場合はここに何も書かない
			PRINTFORML
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;수면中（～は眠っている）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;쇠약して寝る（체력500以下,우후후時は우후후強制終了）
	;（캐릭터の自室以外：～を【現在땅】から【캐릭터の自室】に運びました）
	;（캐릭터の自室　　：～は疲れたと言って、横になりました）
	;この後ARG == 1 が流れる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（연모のみ,그대로 一日が終了）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

