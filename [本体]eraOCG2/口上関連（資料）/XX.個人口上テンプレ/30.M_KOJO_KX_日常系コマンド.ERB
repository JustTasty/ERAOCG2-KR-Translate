RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_300_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;호감도修正、通常（연모系함락なし）で５
	호감도修正 = 5
	;TARGETが【애처】だったら호감도修正に＋２
	SIF TALENT:TARGET:애처
		호감도修正 += 2
	;TARGETが【친애】だったら호감도修正に＋２
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 2
	;TARGETが【연모】を得ていたら호감도修正に＋１
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	;「RAND:n」n面ダイスを振る
	;우후후中の대화
	IF CFLAG:TARGET:우후후
		SELECTCASE RAND:3 + 1
			CASE 1
				PRINTFORMW 
			CASE 2
				PRINTFORMW 
			CASE 3
				PRINTFORMW 
		ENDSELECT
	;日常대화
	ELSE
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 
			CASE 2
				PRINTFORMW 
			CASE 3
				PRINTFORMW 
			CASE 4
				PRINTFORMW 
			CASE 5
				PRINTFORMW 
			;연모
			CASE 6
				PRINTFORMW 
			;친애
			CASE 7
				PRINTFORMW 
			CASE 8
				PRINTFORMW 
			;애처
			CASE 9
				PRINTFORMW 
			CASE 10
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
