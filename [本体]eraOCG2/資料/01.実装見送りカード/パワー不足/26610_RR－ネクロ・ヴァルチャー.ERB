@CARDNAME_26610,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－ネクロ・ヴァルチャー
	CASE "略称"
		RESULTS = ネクロ・Ｖ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1600
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26610(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①：１ターンに１度、자신フィールドの「RR」 몬스터 １장을 リリースし、
PRINTL 자신の묘지の「RUM」마법카드 １枚を대상として발동できる。
PRINTL そのカードを손패に加える。

@CARDCAN_26610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件達成
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26610)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = RUM
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26610,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = レイドラプターズ
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件カテゴリ = RUM
			효과条件종류 = 마법全般
			CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF