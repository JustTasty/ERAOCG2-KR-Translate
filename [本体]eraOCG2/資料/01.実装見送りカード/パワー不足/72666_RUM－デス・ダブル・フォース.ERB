@CARDNAME_72666,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RUM－デス・더블・フォース
	CASE "略称"
		RESULTS = RUM デス더블
	CASE "カテゴリ"
		RESULTS:0 = RUM
ENDSELECT


@CARD_72666(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72666(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動,손패起動,フリー誘発)
PRINTL ①：자신의 「RR」モンスターが戦闘로 파괴되어 묘지로 보내진 ターンに、
PRINTL 자신의 묘지の「RR」X몬스터 １体を대상として발동할 수 있다。
PRINTL 그 몬스터를 특수 소환하고、その倍のランクのX몬스터 １体を、
PRINTL 대상의 몬스터 위에 겹쳐서 X소환扱いとしてEX덱에서 특수  소환한다。


@CARDCAN_72666(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72666)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,72666) == 0
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,0,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = レイドラプターズ
	효과条件종류 = 엑시즈
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	CALL FULLRESET
	효과条件カテゴリ = レイドラプターズ
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"戦闘破壊")
	SIF RESULT != 1
		RETURN 0
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72666,1)
ELSEIF 종류 == ターン終了時強制
	RETURN 1
ENDIF



@CARDEFFECT_72666(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 설정レベル
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72666)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;通常마법汎用処理
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 엑시즈
		효과条件カテゴリ = レイドラプターズ
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			설정レベル = GETVAR(@"%듀얼리스트%_MZ_星:カード소환先") * 2
			CALL FULLRESET
			효과条件レベル = 설정レベル
			효과条件종류 = 엑시즈
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			選択カード = RESULT
			;돌아간다
			IF 選択カード != 9999
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
				CALL RESETCONDITION_MAT(1)
				代替素材数 = 1
				重ねてＸ = 1
				素材条件固定 = カード소환先
				TRYCALLFORM XYZSUMMON(듀얼리스트,選択カード)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72666,1)
ENDIF
