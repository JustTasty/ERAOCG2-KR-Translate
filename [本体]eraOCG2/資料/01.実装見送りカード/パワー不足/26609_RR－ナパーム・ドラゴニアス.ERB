@CARDNAME_26609,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－ナパーム・ドラゴニアス
	CASE "略称"
		RESULTS = ナパーム・Ｄ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26609(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26609(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①：１ターンに１度、발동できる。相手に６００ダメージを与える。
CALLFORM CARD_ABLE(0,破壊→묘지 ,戦闘→묘지)
PRINTL ②：このカードが戦闘・효과で破壊され묘지へ送られた場合に발동できる。
PRINTL 덱から「RR」 몬스터 １장을 효과を無効にして특수소환する。

@CARDCAN_26609(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件達成
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26609)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_SS_CHECK(듀얼리스트,0,"덱")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26609(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26609,10)
	IF RESULT == 0
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",600,1)
	ENDIF
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26609)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = レイドラプターズ
		효과추가종류 = 効追無効
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF