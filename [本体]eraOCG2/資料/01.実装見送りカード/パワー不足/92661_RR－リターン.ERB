@CARDNAME_92661,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－リターン
	CASE "略称"
		RESULTS = RR－リターン
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_92661(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92661(종류)
#DIM DYNAMIC 종류
PRINTL このカード名の②の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ①：자신フィールドの「RR」モンスターが戦闘で破壊された場合、
PRINTL 자신の묘지の「RR」 몬스터 １장을 대상として발동できる。
PRINTL そのモンスターを손패に加える。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：자신フィールドの「RR」モンスターが효과で破壊された場合、
PRINTL 묘지のこのカードを제외して발동できる。
PRINTL 덱から「RR」카드 １枚を손패に加える。

@CARDCAN_92661(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 효과先
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92661)
SIF RESULT == 1
	RETURN 0
IF 종류 == 효과後誘発
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = レイドラプターズ
		효과条件종류 = モン全般
		CALL CARD_CHECK_FIN_DATA(듀얼리스트,"戦闘破壊")
		SIF RESULT != 1
			RETURN 0
		RETURN 1
	ELSEIF ゾーン == "묘지"
		SIF 발동済_2(듀얼리스트,92661)
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = レイドラプターズ
		효과条件종류 = モン全般
		CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과破壊")
		SIF RESULT != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_92661(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 효과後誘発
	IF ゾーン == "마함"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92661)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = レイドラプターズ
			효과条件종류 = モン全般
			CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		ENDIF
	ELSEIF ゾーン == "묘지"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92661,2)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과条件カテゴリ = レイドラプターズ
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF
