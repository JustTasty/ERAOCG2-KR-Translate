@CARDNAME_26618,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－ワイルド・ヴァルチャー
	CASE "略称"
		RESULTS = ワイルド・Ｖ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26618(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 2000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26618(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の②の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,효과소환)
PRINTL ①：자신필드에 モンスターが存在しない場合、このカードはリリースなしで소환できる。
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②：このカードをリリースして발동できる。レベルの合計が６以下になるように、
PRINTL 자신の손패・덱・묘지から「RR」モンスターを２장까지 특수소환する。

@CARDCAN_26618(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26618)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과소환
	SIF モンスター数(듀얼리스트) != 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,26618)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,0,"",1)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26618(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 残りレベル

IF 종류 == 효과소환
	RETURN 1
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26618,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件カテゴリ = レイドラプターズ
			효과条件종류 = レベル持ち
			CALLFORM 효과条件レベル_ADV(6, "以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_필드:カード소환先")}, "レベル"
			残りレベル = 6 - RESULT
			IF 残りレベル >= 1 && モンスター数(듀얼리스트) < 5
				CALL FULLRESET
				효과範囲손패 = 1
				효과範囲덱 = 1
				효과範囲묘지 = 1
				효과条件カテゴリ = レイドラプターズ
				효과条件종류 = レベル持ち
				효과条件任意 = 1
				CALLFORM 효과条件レベル_ADV(残りレベル, "以下")
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF
ENDIF