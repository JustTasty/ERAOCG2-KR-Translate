@CARDNAME_26605,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－インペイル・レイニアス
	CASE "略称"
		RESULTS = インペイル・Ｌ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26605(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1700
	CASE "守備力"
		RETURN 1000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26605(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の②の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,소환時 ,특수소환時)
PRINTL ①：このカードが소환・특수소환した場合、
PRINTL フィールドの攻撃표시 몬스터 １장을 대상として발동できる。
PRINTL そのモンスターを守備표시にする。
CALLFORM CARD_ABLE(2,攻撃後誘発)
PRINTL ②：このカードが攻撃したダメージステップ終了時に、
PRINTL 자신の묘지の「RR」 몬스터 １장을 대상として발동できる。
PRINTL そのモンスターを守備표시で특수소환する。

@CARDCAN_26605(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26605)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件표시 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,26605)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,0,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = レイドラプターズ
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26605(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26605)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件표시 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			CALL CARD_SELECT(듀얼리스트,RESULT)
			選択カード = RESULT
			CALL SET_FIELD_INFO(대상者（CS）,参照先（CS）,選択カード,"表",2,1)
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26605,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = レイドラプターズ
		효과条件종류 = モン全般
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
	ENDIF
ENDIF
