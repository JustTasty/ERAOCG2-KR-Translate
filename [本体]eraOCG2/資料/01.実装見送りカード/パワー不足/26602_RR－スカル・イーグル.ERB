@CARDNAME_26602,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－スカル・イーグル
	CASE "略称"
		RESULTS = スカル・Ｅ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26602(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 500
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26602(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,X素材時효과)
PRINTL ①：이 카드를 소재로 하고 X소환된 경우、자신 필드의 X 몬스터 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 카드의 공격력은 ３００올린다。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：자신 필드의 X몬스터의 효과가 발동した경우、묘지의 이 카드를 제외하고、
PRINTL 자신 묘지의 「RR」카드 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 카드를 패에 넣는다。

@CARDCAN_26602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26602)
SIF RESULT == 1
	RETURN 0

IF 종류 == X素材時효과
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,26602)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = レイドラプターズ
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

IF 종류 == X素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26602)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 엑시즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永攻",300)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26602,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ = レイドラプターズ
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
	ENDIF
ENDIF
