@CARDNAME_xxxx,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS
;TFのウリアちゃんは別カードにする
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 聖斬のコード アシッド
	CASE "略称"
		RESULTS = アシッド
	CASE "カテゴリ"
		RESULTS:0 = アンノウン
		RESULTS:1 = セレス
ENDSELECT


@CARD_xxxx(参照先)
;名前専用
#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2300
	CASE "守備力"
		RETURN 2300
ENDSELECT

RETURN 0

