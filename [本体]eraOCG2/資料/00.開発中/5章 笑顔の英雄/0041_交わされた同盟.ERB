@IRAI_EVENT_40(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (ST5-1)夢の国のカイバランド
	CASE "이벤트種"
		RETURN 이벤트_物語
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL あれからまたしばらく時が過ぎた。
		PRINTL 何かと
		PRINTL 
		PRINTL
		PRINTL 
	CASE "条件"
		SIF 依頼_完了횟수:34 == 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0



