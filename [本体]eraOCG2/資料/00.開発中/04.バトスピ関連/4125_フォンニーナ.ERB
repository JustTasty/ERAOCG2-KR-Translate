@CARDNAME_4125,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4125)

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = フォンニーナ
	CASE "略称"
		RESULTS = フォンニーナ
	CASE "カテゴリ"
		RESULTS:0 = 시희
		RESULTS:4 = 배틀 스피리츠
		IF TALENT:カード主:애처
			RESULTS:1 = 계약
		ENDIF
ENDSELECT


@CARD_4125(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 800
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_4125(종류)
#DIM DYNAMIC 종류
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4125)

IF TALENT:カード主:애처
	PRINTL 이 카드명의 ①②③의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
	CALLFORM CARD_ABLE(1,손패起動)
	PRINTL ①：자신 필드의 コア카운터を１개 제거하고 발동할 수 있다。
	PRINTL 이 카드를 패에서 특수 소환한다。
	CALLFORM CARD_ABLE(2,효과後誘発)
	PRINTL ②：자신필드에 「시희」モンスター 또는 곤충족 몬스터가 소환 / 특수 소환된 경우에 발동할 수 있다。
	PRINTL 자신은 덱에서 １장 드로우한다。
	CALLFORM CARD_ABLE(3,필드起動)
	PRINTL ③：자신의 마법 / 함정존의 「創界神」にコア카운터を１つ置くことで발동할 수 있다。
	PRINTL 자신의 덱의 위에서 카드를 １장 めくる。
	PRINTL 그 안에서 곤충족モンスター 또는 「시희」카드 １장을 고르고 패에 넣는다事ができる。
	PRINTL 残りのカードは덱으로 되돌린다。
ELSE
	PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
	CALLFORM CARD_ABLE(2,효과後誘発)
	PRINTL ①：자신필드에 「시희」モンスター 또는 곤충족 몬스터가 소환 / 특수 소환된 경우에 발동할 수 있다。
	PRINTL 자신은 덱에서 １장 드로우한다。
ENDIF

@CARDCAN_4125(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4125)

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4125)
SIF RESULT == 1
	RETURN 0


IF 종류 == 손패起動
	IF TALENT:カード主:애처
		SIF 발동済_1(듀얼리스트,4125)
			RETURN 0
		SIF メインフェイズ != 1
			RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
		CALL CARD_SS_CHECK(듀얼리스트,4125,"손패")
		SIF RESULT == 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 배틀 스피리츠
		효과条件표시 = 表側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT:9 < 1
			RETURN 0
	ELSE
		RETURN 0	
	ENDIF
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,4125)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종족 = 곤충족
	효과条件スーパーカテゴリ = 시희
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 필드起動
	IF TALENT:カード主:애처
		SIF メインフェイズ != 1
			RETURN 0
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
		SIF 발동済_3(듀얼리스트,4125)
			RETURN 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件カテゴリ = 創界神
		효과条件표시 = 表側표시
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		RETURN 1
	ELSE
		RETURN 0	
	ENDIF
ENDIF

@CARDEFFECT_4125(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタ数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 軽減数
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC カード主

カード主 = NO_TO_CHARANUM(4125)


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4125,1)
	IF RESULT == 0
		PRINTL コア카운터を取り除く카드를 選択
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 배틀 스피리츠
		효과条件표시 = 表側표시
		효과条件個Ｃ所持 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			PRINTL コアが取り除かれました
			CALL CARD_SELECT(듀얼리스트,RESULT)
			CALL SET_FIELD_INFO(대상者（CS）,参照先（CS）,選択カード（CS）,"個Ｃ",-1)

			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4125,2)
	IF RESULT == 0
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4125,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件カテゴリ = 創界神
		효과条件표시 = 表側표시
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			CALL SET_FIELD_INFO(듀얼리스트,参照先（CS）,選択カード（CS）,"個Ｃ",2)
		ENDIF

		CALL FULLRESET
		효과条件종족 = 곤충족
		효과条件スーパーカテゴリ = 시희
		효과条件任意 = 1
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所,1)
	ENDIF
ENDIF

