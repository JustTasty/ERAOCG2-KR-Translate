@CARDNAME_71635,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = “Ω”・ワールドエンド
	CASE "略称"
		RESULTS = Ωワールドエンド
	CASE "カテゴリ"
		RESULTS:0 = グレンディオス
		RESULTS:1 = 星輝兵
		RESULTS:2 = 링크ジョーカー
		RESULTS:4 = 뱅가드
ENDSELECT

@CARD_71635(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
		RETURN フィールド
ENDSELECT
RETURN 0

@CARDSUMMON_71635(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
RETURN 0

@CARD_EXPLANATION_71635(종류)
#DIM DYNAMIC 종류

PRINTL 자신フィールドは「Ωワールドエンド」となる。
PRINTL このカード名の①②③の효과はそれぞれ１ターンに１度しか使用できない。
CALL CARD_ABLE(1,손패起動,마함起動)
PRINTL ①：このカードの발동時の효과処理として、덱から「星輝兵」 또는 「Я」 몬스터 １장을 손패に加える。
CALL CARD_ABLE(2,마함起動)
PRINTL ②：자신メインフェイズに以下の효과から１つを選択して발동できる。
PRINTL ●손패から「星輝兵」카드 １枚を捨てて、덱から「Я」 몬스터 １장을 손패に加える。
PRINTL ●손패から「Я」 몬스터 １장을 捨てて、덱からこのカード名以外の「星輝兵」카드 １枚を손패に加える。
CALL CARD_ABLE(3,ターン開始時)
PRINTL ③：자신スタンバイフェイズに、자신のＬＰが１０００以下で、
PRINTL 자신필드에 「グレンディオス」モンスターが存在し、상대필드에 呪縛状態のモンスターが５장存在する場合に발동できる。
PRINTL 자신はデュエルに勝利する。

@CARDCAN_71635(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

#DIM DYNAMIC 条件達成

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71635)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_2(듀얼리스트,71635)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 星輝兵
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = Я
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件達成++
	ENDIF
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = Я
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 星輝兵
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件達成++
	ENDIF
	SIF 条件達成 < 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,71635)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ:0 = 星輝兵
	효과条件カテゴリ:1 = Я
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_3(듀얼리스트,71635)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = グレンディオス
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF GETVAR(@"ライフ_%듀얼리스트%") > 1000
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 裏側표시
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 5
		RETURN 0
    RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_71635(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71635,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 星輝兵
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = Я
			효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件Ａ++
		ENDIF
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = Я
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = グレンディオス
			효과条件종류 = 마함全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件Ｂ++
		ENDIF
		IF 条件Ａ == 1 && 条件Ｂ == 1
			CALL YN_CHOICE(듀얼리스트,"●손패から「星輝兵」카드 １枚を捨てて、덱から「Я」 몬스터 １장을 손패に加える。。","●손패から「Я」 몬스터 １장을 捨てて、덱からこのカード名以外の「星輝兵」카드 １枚を손패に加える。")
			選択효과 = RESULT
		ELSEIF 条件Ａ == 1 && 条件Ｂ == 0
			選択효과 = はい
		ELSEIF 条件Ａ == 0 && 条件Ｂ == 1
			選択효과 = いいえ
		ENDIF
		IF 選択효과 == はい
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = 星輝兵
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件カテゴリ = Я
				효과条件종류 = モン全般
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ELSEIF 選択효과 == いいえ
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = Я
			효과条件종류 = モン全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件カテゴリ = 星輝兵
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71635,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ:0 = 星輝兵
		효과条件カテゴリ:1 = Я
		효과条件종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71635,3)
	IF RESULT == 0
		CALL SET_PLAYER_INFO(듀얼리스트,"듀얼패배플래그",4,1)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트,"Ωワールドエンド")
ENDIF