	CASE "종류"
		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_77511(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：이 카드는 カードの「惑乱魔」モンスター1体を捨てることもできるに발동할 수 있다。
PRINTL 상대는 덱에서 持有성별の 몬스터 １장을 특수 소환한다,자신덱에서 「落とし穴」일반 함정 카드나 「蜜穴」通常함정カード1장을 패에 넣는다。
PRINTL 「惑乱魔」몬스터를 버리고 이 카드를 발동した場合,상대で특수 소환한 몬스터의 효과는 無効化され。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：상대ターン,묘지의 이 카드를 제외し,자신 필드의 「惑乱魔」モンスタ2枚と상대 필드持有성별のモンスター1장을 대상으로 하고 발동할 수 있다。
SETCOLOR C_PINK
PRINTL 대상となる상대몬스터는 、자신이 選択した2体の몬스터의 攻撃力合計値を下げる。
PRINTL 이 효과로 대상의 牝性몬스터의 공격력이 0に低下した場合,그 몬스터를 「惑乱魔的宠物」(攻撃力900·守備力1900)に書き換え。
RESETCOLOR
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_77511(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,77511)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,77511)
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF 발동済_2(듀얼리스트,77511)
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 惑乱魔
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0

	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 성별在
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_77511(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 选择卡片1
#DIM DYNAMIC 选择卡片2
#DIM DYNAMIC 攻撃力修正1
#DIM DYNAMIC 攻撃力修正2
#DIM DYNAMIC 合计下降攻撃力
#DIM DYNAMIC 原本攻撃力修正3
#DIM DYNAMIC 攻撃力修正3
#DIM DYNAMIC 选择对方卡片
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 选择卡片_ID
#DIMS DYNAMIC 选择卡名称
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 조교대상1
#DIM DYNAMIC 조교대상2

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,77511,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 惑乱魔
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT != 0
			CALL FULLRESET
			PRINTL 要从手中丢弃1张「惑乱魔」怪兽来发动这张卡吗?
			PRINTL [1] - 是
			PRINTL [2] - 但是我拒绝
			BINPUT
			IF RESULT == 1
				CALL FULLRESET
				효과範囲손패 = 1
				효과条件カテゴリ = 惑乱魔
				효과条件종류 = モン全般
				CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					효과範囲덱 = 1
					효과条件성별 = 성별在
					효과추가종류 = 22
					CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
					IF RESULT == 1
						CALL FULLRESET
						효과条件종류 = 通常함정
						효과条件カテゴリ:0 = 落とし穴
						효과条件カテゴリ:1 = 蜜穴
						CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
					ENDIF
				ELSE
					CALL FULLRESET
					효과範囲덱 = 1
					효과条件성별 = 성별在
					효과条件종류 = モン全般
					CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
					IF RESULT == 1
						CALL FULLRESET
						효과条件종류 = 通常함정
						효과条件カテゴリ:0 = 落とし穴
						효과条件カテゴリ:1 = 蜜穴
						CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
					ENDIF
				ENDIF
			ELSE
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件성별 = 성별在
				효과条件종류 = モン全般
				CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					효과条件종류 = 通常함정
					효과条件カテゴリ:0 = 落とし穴
					효과条件カテゴリ:1 = 蜜穴
					CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
				ENDIF
			ENDIF
		ELSE
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件성별 = 성별在
			효과条件종류 = モン全般
			CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과条件종류 = 通常함정
				효과条件カテゴリ:0 = 落とし穴
				효과条件カテゴリ:1 = 蜜穴
				CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,77511,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ = 惑乱魔
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		选择卡片1 = RESULT
		IF 选择卡片1 != 9999
			攻撃力修正1 = GETVAR(@"%듀얼리스트%_MZ_攻:选择卡片1")
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:选择卡片1")}, "名前"
			选择卡名称 = %RESULTS%
			CALL FULLRESET
			효과条件カテゴリ = 惑乱魔
			효과条件固名前 = %选择卡名称%
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			选择卡片2 = RESULT
			IF 选择卡片2 != 9999
				攻撃力修正2 = GETVAR(@"%듀얼리스트%_MZ_攻:选择卡片2")
				合计下降攻撃力 = 攻撃力修正1 + 攻撃力修正2
				CALL FULLRESET
				효과条件성별 = 성별在
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
				选择对方卡片 = RESULT
				IF 选择对方卡片 != 9999
					攻撃力修正3 = GETVAR(@"%対面者%_MZ_攻:选择对方卡片")

					TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:选择对方卡片")}("名前")
                	조교대상 = GETVAR(@"%対面者%_필드:选择对方卡片")
					TRYCALLFORM CARD_{GETVAR(@"%対面者%_필드:选择对方卡片")}, "성별"
					IF RESULT == 牝性 && 攻撃力修正3 - 合计下降攻撃力 <= 0
						CALL FULLRESET
						PRINTL 选择的怪兽攻撃力降至了0,要将其替换为「惑乱魔的宠物」吗?
						PRINTL [1] - 是
						PRINTL [2] - 但是我拒绝
						BINPUT
						IF RESULT == 1
							CALL CARD_EROTICS(조교대상)
							IF RESULT != -1
								조교대상 = RESULT
								SETCOLOR C_PINK
								PRINTFORML
								PRINTFORMW 惑乱魔们刺激着%CALLNAME:조교대상%的性器，又在%CALLNAME:조교대상%即将高潮之际突然停止……！
								PRINTFORMW 直到彻底굴복于惑乱魔的一刻,%CALLNAME:조교대상%才从残酷的寸止调教中迎来片刻喘息。
								PRINTFORML
								RESETCOLOR
								CALL EROTIC_MOVE("조교部屋",조교대상, -1,100)
								CALL TOKEN_TRAPSEX(듀얼리스트,选择对方卡片)
							ENDIF
						ENDIF
					ELSEIF RESULT == ふたなり || RESULT == 雄性 ;&& (GETVAR(@"%対面者%_MZ_攻:选择对方卡片") - 合计下降攻撃力) > 0
						CALL CARD_EROTICS(GETVAR(@"%듀얼리스트%_필드:选择卡片1"))
						조교대상1 = RESULT
						CALL CARD_EROTICS(GETVAR(@"%듀얼리스트%_필드:选择卡片2"))
						조교대상2 = RESULT

						IF 조교대상1 >= 0
							CALL CARD_EQUIP_MONSTER(対面者,选择对方卡片,"상대필드",选择卡片1,(1 + 2))
							CALL CARD_EQUIP_MONSTER(対面者,选择对方卡片,"상대필드",选择卡片2,(1 + 2))
							SETCOLOR C_PINK
							PRINTFORML
							PRINTFORMW %CALLNAME:조교대상1%和%CALLNAME:조교대상2%误判了猎物的实力,反而被对手捕获了……！
							PRINTFORMW %CALLNAME:조교대상1%被当作肉铠固定在对方的肉棒上，%CALLNAME:조교대상2%则被捆绑好拎在手中。
							PRINTFORML
							RESETCOLOR
							CALL EROTIC_MOVE("前穴凌辱",조교대상1,-1,100)
							CALL EROTIC_MOVE("구속晒し",조교대상2,-1,100)
						ENDIF
					ELSE
						CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",选择对方卡片,"永攻DOWN",77511,合计下降攻撃力)
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
	


