SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件カテゴリ = 惑乱魔
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	CALL CAT_CHECK("惑乱魔",듀얼리스트,戦闘場所(듀얼리스트,1),1)
	SIF RESULT != 1
		RETURN 0	
    RETURN 1
ENDIF

