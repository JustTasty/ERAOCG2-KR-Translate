IF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦狂")

ELSEIF 종류 == 攻撃後強制誘発
	CALL FULLRESET
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦",1)
ENDIF


