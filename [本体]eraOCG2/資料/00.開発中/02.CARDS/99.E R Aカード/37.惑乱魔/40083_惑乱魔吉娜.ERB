@CARDNAME_40083,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 惑乱魔吉娜
	CASE "略称"
		RESULTS = 吉娜(惑)
	CASE "カテゴリ"
		RESULTS:0 = 惑乱魔
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_40083(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 700
	CASE "守備力"
		RETURN 2100
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_40083(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①②의 효과는 1ターンに1度、いずれか1つしか使用できない。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：이 카드는 패에서 「惑乱魔」モンスタ·일반 함정 카드 １장을 捨てに발동할 수 있다。
PRINTL 이 카드를 패에서 특수 소환한다,その後,덱에서 이 카드명 이외의 「惑乱魔」 몬스터 １장을 묘지로 보낸다。
CALLFORM CARD_ABLE(2,묘지起動,フリー誘発)
PRINTL ②：묘지의 이 카드를 제외し,상대묘지의 「性别を有する怪兽」 몬스터 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 카드를 주인의 마법＆함정 존에 「蜜罐」にして앞면 표시로 置く事ができる,이 효과는 상대ターンでも발동할 수 있다。
SETCOLOR C_PINK
CALLFORM CARD_ABLE(3,攻撃時誘発)
PRINTL ③：공격 표시의 이 카드가 공격 대상으로 선택된 경우에 발동한다。
PRINTL 攻撃몬스터의 공격력은 800ポイントダウンする,이 카드를 수비 표시로 한다。
RESETCOLOR
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ④：이 카드는 함정내성을 持つ。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_40083(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,40083)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF ゾーン != "손패"
    	RETURN 0
    SIF 발동済_1(듀얼리스트,40083)
    	RETURN 0
    SIF 발동済_2(듀얼리스트,40083)
    	RETURN 0
    SIF ターンプレイヤー != 듀얼리스트
    	RETURN 0
    SIF メインフェイズ != 1
    	RETURN 0

    CALL FULLRESET
    효과範囲손패 = 1
    효과条件종류 = モン全般
    효과条件カテゴリ = 惑乱魔
    효과条件固名前 = 惑乱魔吉娜
    CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
    SIF RESULT == 1
    	条件달성++

    CALL FULLRESET
    효과範囲손패 = 1
    효과条件종류 = 通常함정
	효과条件カテゴリ:0 = 落とし穴
	효과条件カテゴリ:1 = 蜜穴
    CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
    SIF RESULT == 1
    	条件달성++
    SIF 条件달성 == 0
		RETURN 0
    RETURN 1

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF ゾーン != "묘지"
    	RETURN 0
    SIF 발동済_1(듀얼리스트,40083)
    	RETURN 0
    SIF 발동済_2(듀얼리스트,40083)
    	RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0

    CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件종류 = モン全般
	효과条件성별 = 성별在
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
    
ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_40083(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 自己卡片
#DIM DYNAMIC 自己守備力修正
#DIM DYNAMIC 对方卡片
#DIM DYNAMIC 对方攻撃力修正
#DIM DYNAMIC 选择묘지卡片
#DIM DYNAMIC 조교대상


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40083,1)
	IF RESULT == 0
    	CALL FULLRESET
        효과範囲손패 = 1
        효과条件종류:0 = モン全般
        효과条件종류:1 = 通常함정
        효과条件カテゴリ:0 = 落とし穴
        효과条件カテゴリ:1 = 蜜穴
        효과条件カテゴリ:2 = 惑乱魔
        효과条件固名前 = 惑乱魔吉娜
        CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
        IF RESULT == 1
        	CALL FULLRESET
            CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
            CALL FULLRESET
            효과条件종류 = モン全般
            효과条件カテゴリ = 惑乱魔
            효과条件固名前 = 惑乱魔吉娜
            CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
        ENDIF
    ENDIF

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40083,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件종류 = モン全般
		효과条件성별 = 성별在
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대묘지",ゾーン,場所)
		选择묘지卡片 = RESULT
		IF 选择묘지卡片 != 9999
			CALL FULLRESET
			효과추가カテ = 蜜罐
			CALL CARD_ADD(対面者,"묘지","마함",选择묘지卡片,"表側표시")
		ENDIF
    ENDIF

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40083)
	IF RESULT == 0
		自己卡片 = 戦闘場所(듀얼리스트)
		自己守備力修正 = GETVAR(@"%듀얼리스트%_MZ_守:自己卡片")
		对方卡片 = 戦闘場所(듀얼리스트,1)
		对方攻撃力修正 = GETVAR(@"%対面者%_MZ_攻:对方卡片")
		TRYCALLFORM CARD_{戦闘カードID(듀얼리스트,1)}("성별")
		IF RESULT == 牝性
			조교대상 = GETVAR(@"%対面者%_필드:对方卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW %CALLNAME:조교대상%的身体接受调教的同时被压在半透明的叶片上,屈辱的暴露在众人的视线之中。
				RESETCOLOR
				CALL EROTIC_MOVE("구속晒し",조교대상)
			ENDIF
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",对方卡片,"永攻DOWN",800)

		ELSEIF RESULT == 雄性 || RESULT == ふたなり && (对方攻撃力修正 - 800 ) - 自己守備力修正 >= 0
			조교대상 = GETVAR(@"%対面者%_필드:对方卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW 吉娜沉溺于交欢的쾌감，被%CALLNAME:조교대상%的肉棒彻底정복了……！
				RESETCOLOR
				CALL EROTIC_MOVE("前穴凌辱",NO_TO_CHARANUM(1379), -1)
				IF 마함数(対面者) < 5
					SETCOLOR C_PINK
					PRINTFORMW 随后就那样被%CALLNAME:조교대상%当作肉铠带走了……
					RESETCOLOR
					CALL CARD_EQUIP_MONSTER(対面者,对方卡片,"상대필드",自己卡片,(1 + 2))
				ENDIF
			ELSE
				CALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:对方卡片")}("名前")
				SETCOLOR C_PINK
				PRINTFORMW 吉娜沉溺于交欢的쾌감，被%RESULTS%的肉棒彻底정복了……！
				RESETCOLOR
				CALL EROTIC_MOVE("前穴凌辱",NO_TO_CHARANUM(1379), -1)
				IF 마함数(対面者) < 5
					SETCOLOR C_PINK
					PRINTFORMW 随后就那样被%RESULTS%当作肉铠带走了……
					RESETCOLOR
					CALL CARD_EQUIP_MONSTER(対面者,对方卡片,"상대필드",自己卡片,(1 + 2))
				ENDIF
			ENDIF

		ELSEIF RESULT == 雄性 || RESULT == ふたなり
			조교대상 = GETVAR(@"%対面者%_필드:对方卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW %CALLNAME:조교대상%的身体接受调教的同时被压在半透明的叶片上,屈辱的暴露在众人的视线之中。
				RESETCOLOR
				CALL EROTIC_MOVE("全身애무",조교대상,NO_TO_CHARANUM(1379))
			ENDIF
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",对方卡片,"永攻DOWN",800)
			
		ELSE
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",对方卡片,"永攻DOWN",800)
		ENDIF
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表",2,1)
	ENDIF

ELSEIF 종류 == 攻撃時強制誘発
	CALL FULLRESET
	IF RESULT == 0
		IF 戦闘場所(듀얼리스트,1) != 5
			CALL CARD_EROTICS(戦闘カードID(듀얼리스트,1))
			IF RESULT != 0
				조교대상 = RESULT
			ENDIF
		ENDIF
		IF 조교대상 > 0
			PRINTFORML
			SETCOLOR C_PINK 
			PRINTFORML %CALLNAME:조교대상%触碰到绒毛的瞬间就被关了起来。
			RESETCOLOR
		ENDIF		
	ENDIF		

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
ENDIF
