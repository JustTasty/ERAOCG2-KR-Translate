@CARDNAME_99071,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 蜜穴的吸引
	CASE "略称"
		RESULTS = 蜜穴吸引
	CASE "カテゴリ"
		RESULTS:0 = 蜜穴
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_99071(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99071(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL ①：자신필드の「惑乱魔」モンスタ1枚と상대필드のモンスター1枚を대상として발동できる。
PRINTL 대상となる상대モンスターの攻撃力は、자신が대상とするモンスターの攻撃力の数値を下げる。
PRINTL この효과で대상の持有性别的モンスターの攻撃力が0に低下した場合,そのモンスタ주인의 마법＆함정ゾーンに「蜜罐」にして表側표시で置く。
PRINTL 성별なし的场合,そのモンスタ破坏。
CALLFORM CARD_ABLE(1,묘지起動,フリー誘発)
PRINTL ②：묘지のこのカードを제외し상대フィールド1枚「蜜罐」和자신필드1只「惑乱魔」怪兽为对象才能发动。
PRINTL このターン、その자신の「惑乱魔」モンスターは戦闘では破壊されない。
CALL TEXT_DECORATION("ROGUE")



@CARDCAN_99071(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 效果限制达成
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99071)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 惑乱魔
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1     
    
ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
		
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件カテゴリ = 蜜罐
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
    SIF RESULT != 1
		RETURN 0
    RETURN 1
ENDIF

@CARDEFFECT_99071(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 选择卡片
#DIM DYNAMIC 选择对方卡片
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 守備力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99071)
	IF RESULT == 0
        CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
        CALL FULLRESET
        효과範囲필드 = 1
        효과条件カテゴリ = 惑乱魔
        CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
        选择卡片 = RESULT
        IF 选择卡片 != 9999
        	攻撃力修正 = GETVAR(@"%듀얼리스트%_MZ_守:选择卡片")
        	CALL FULLRESET
            효과範囲상대필드 = 1
            CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
            选择对方卡片 = RESULT
            IF 选择对方卡片 != 9999
            	CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",选择对方卡片,"永攻DOWN",99071,攻撃力修正)
                TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:选择对方卡片")}("名前")
                조교대상 = GETVAR(@"%対面者%_필드:选择对方卡片")
				TRYCALLFORM CARD_{GETVAR(@"%対面者%_필드:选择对方卡片")}, "성별"
                IF RESULT == 牝性 || RESULT == ふたなり || RESULT == 雄性 ;&& (GETVAR(@"%対面者%_MZ_攻:选择对方卡片") - 攻撃力修正) <= 0
                	CALL FULLRESET
					PRINTL 选择的怪兽攻撃力降至了0,要让那只怪兽作为「蜜罐」在对方마함区放置吗?
					PRINTL [1] - 是
					PRINTL [2] - 但是我拒绝
					BINPUT
					IF RESULT == 1
                		효과추가カテ = 蜜罐
                		CALL CARD_ADD(対面者,"필드","마함",选择对方卡片,"表側표시")
                
                    	CALL CARD_EROTICS(조교대상)
                    	IF RESULT != -1
                        	조교대상 = RESULT
                        	SETCOLOR C_PINK
                        	PRINTFORML
                        	PRINTFORMW %CALLNAME:조교대상%在惑乱魔的调教下굴복了,被改造成了生产"花蜜"的蜜罐……！
                        	PRINTFORML
                        	RESETCOLOR
                        	CALL EROTIC_MOVE("묘상捕縛",조교대상, -1, 100)
                    	ENDIF
					ELSE
						CALL CARD_EROTICS(조교대상)
						IF RESULT != -1
							조교대상 = RESULT
							SETCOLOR C_PINK
							PRINTFORML
							PRINTFORMW %CALLNAME:조교대상%的力量在调教中被惑乱魔吸走了！
							PRINTFORML
							RESETCOLOR
							CALL EROTIC_MOVE("全身레즈愛撫",조교대상, -1, 50)
						ENDIF
					ENDIF
                ELSEIF (GETVAR(@"%対面者%_MZ_攻:选择对方卡片") - 攻撃力修正) <= 0
					CALL FULLRESET
					CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",选择卡片,"묘지",99071)
                ENDIF
            ENDIF
        ENDIF
    ENDIF

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99071)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
    	CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		효과条件カテゴリ = 蜜罐
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"对方마함",ゾーン,場所)
		选择对方卡片 = RESULT
		IF 选择对方卡片 != 9999
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件カテゴリ = 惑乱魔
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			选择卡片 = RESULT
			IF 选择卡片 != 9999
				TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_마함:选择对方卡片")}("名前")
				조교대상 = GETVAR(@"%対面者%_마함:选择对方卡片")
				CALL CARD_EROTICS(조교대상)
				IF RESULT != -1
					조교대상 = RESULT
					SETCOLOR C_PINK
					PRINTFORML
					PRINTFORMW 惑乱魔从%CALLNAME:조교대상%的股间汲取了花蜜,체력得到了补充。
					PRINTFORML
					RESETCOLOR
					CALL EROTIC_MOVE("指入れ",조교대상, -1, 300)
				ENDIF
				CALL STATCHANGE_EFFECT(듀얼리스트,"自身",选择卡片,"戦耐",1)
			ENDIF
		ENDIF
    ENDIF
ENDIF