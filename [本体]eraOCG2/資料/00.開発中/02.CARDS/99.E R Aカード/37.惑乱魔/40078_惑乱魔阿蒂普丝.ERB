@CARDNAME_40078,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 惑乱魔阿蒂普丝
	CASE "略称"
		RESULTS = 阿蒂普丝(惑)
	CASE "カテゴリ"
		RESULTS:0 = 惑乱魔
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_40078(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 900
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_40078(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
	必要링크数 = 3
	素材条件カテゴリ:0 = 蟲惑魔
	素材条件カテゴリ:1 = 惑乱魔


IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,40078) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_40078(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 「蟲惑魔」モンスター 또는 「惑乱魔」モンスタ2장 이상
PRINTL このカード名の③④の효과はそれぞれ1ターンに1度しか使用できない。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：このカードは함정내성을 持つ。
PRINTL 자신·相手フィールド攻撃力为0的モンスタ或「蜜罐」は戦闘では破壊されない。
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②: 相手フィールド攻撃力为0的モンスタ或「蜜罐」が守備표시の場合,那些卡的守備力变为0。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ③：相手フィールドの表側표시 몬스터 １장을 대상として발동できる。
PRINTL 자신の묘지から通常함정카드 １枚を제외し,そのモンスタの攻撃力变为0,효과を턴 종료시까지 無効にする,この효과は相手ターンでも발동できる。
PRINTL 那之后可以从자신の묘지から通常함정카드 １枚を제외し,选相手フィールド1只攻撃力为0的怪兽제외。
PRINTL そのカードが「성별を有するモンスター」した場合,作为제외的代替把那只怪兽当作「蜜罐」给这张卡装备。
CALLFORM CARD_ABLE(1,バトル開始時)
PRINTL ④：자신バトルフェイズ開始時に발동できる。
PRINTL 자신·相手フィールド攻撃力为0的持有성별的怪兽视为「蜜罐」并变为守備표시,没有성별的モンスタ送去묘지。
PRINTL 自己フィールドの「惑乱魔」怪兽直到ターン終了時攻撃力上升자신·相手フィールド「蜜罐」的数量*500,貫通攻撃を得る。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_40078(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件達成

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,40078)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,40078)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0

	CALL FULLRESET
    효과範囲相手필드 = 1
    효과条件표시 = 表側표시
    CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 通常함정
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == バトル開始時
	SIF 발동済_2(듀얼리스트,40078)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
 	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲相手필드 = 1
	효과条件攻撃力 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
    	条件達成 ++

 	CALL FULLRESET
	효과範囲필드 = 1
	효과条件攻撃力 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
    SIF RESULT == 1
    	条件達成 ++

	CALL FULLRESET
	효과範囲相手필드 = 1
	효과範囲相手마함 = 1
	효과条件カテゴリ = 蜜罐
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
    	条件達成 ++

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 蜜罐
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
    	条件達成 ++
	SIF 条件達成 == 0
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과 || 종류 == 攻守変動 || 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_40078(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 蜜罐数量
#DIM DYNAMIC 选择제외陷阱
#DIM DYNAMIC 选择제외怪兽
#DIM DYNAMIC 选择卡片
#DIM DYNAMIC 追击卡片
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40078,1)
    IF RESULT == 0
    	효과条件종류 = 通常함정
    	CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
    	选择제외陷阱 = RESULT
    	IF 选择제외陷阱 != 9999 && GETVAR(@"%듀얼리스트%_제외NG置換") == 0
			CALL CARD_DISPOSE(듀얼리스트,"묘지",选择제외陷阱)
			CALL FULLRESET
			효과条件표시 = 表側표시
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"相手필드",ゾーン,場所)
			选择卡片 = RESULT
			IF 选择卡片 != 9999
        		CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",选择卡片,"無効")
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",选择卡片,"永攻DOWN",100000)

				CALL CARD_LOADUP

				CALL FULLRESET
				PRINTL 要제외묘지的「惑乱魔」怪兽以发动额外的效果吗？
				PRINTL [1] - 是
				PRINTL [100] - 但是我拒绝
				BINPUT
				IF RESULT != 100
					CALL FULLRESET
					효과条件종류 = モン全般
					효과条件カテゴリ = 惑乱魔
					CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
					选择제외怪兽 = RESULT
					IF 选择제외怪兽 != 9999
						CALL CARD_DISPOSE(듀얼리스트,"묘지",选择제외怪兽)
						CALL FULLRESET
						효과範囲相手필드 = 1
						효과条件攻撃力 = 1
						CALL CARD_CHOOSE_BASIC(듀얼리스트,"相手필드",ゾーン,場所)
						追击卡片 = RESULT
						IF 追击卡片 != 9999
							TRYCALLFORM CARD_{GETVAR(@"%対面者%_필드:追击卡片")}, "성별"
							IF RESULT == 牝性 || RESULT == ふたなり || RESULT == 雄性
								TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:追击卡片")}("名前")
								조교대상 = GETVAR(@"%対面者%_필드:追击卡片")
								CALL FULLRESET
								효과추가カテ = 蜜罐
								CALL CARD_EQUIP_MONSTER(듀얼리스트,場所,"相手필드",追击卡片)

								CALL CARD_EROTICS(조교대상)
								IF RESULT != -1
									조교대상 = RESULT
									SETCOLOR C_PINK
									PRINTFORML
									PRINTFORMW 无力反抗的%CALLNAME:조교대상%成为了阿蒂普斯中意的玩物，被轻易的拖走了……！
									PRINTFORML
									RESETCOLOR
									CALL EROTIC_MOVE("묘상捕縛",조교대상, -1, 100)
								ENDIF

							ELSE
								CALL FULLRESET
								CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",追击卡片,"제외")
							ENDIF
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40078,2)
    IF RESULT == 0
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
				CONTINUE
			SIF GETVAR(@"%듀얼리스트%_MZ_表:カウンタ") == 3
				CONTINUE
			IF GETVAR(@"%듀얼리스트%_MZ_性:カウンタ") == 牝性 || GETVAR(@"%듀얼리스트%_MZ_性:カウンタ") == ふたなり || GETVAR(@"%듀얼리스트%_MZ_性:カウンタ") == 雄性 && GETVAR(@"%듀얼리스트%_MZ_攻:カウンタ") <= 0
				CALL CARD_SET_CATEGORY(듀얼리스트,"필드",カウンタ,"蜜罐")
			ELSEIF GETVAR(@"%듀얼리스트%_MZ_性:カウンタ") != 牝性 || GETVAR(@"%듀얼리스트%_MZ_性:カウンタ") != ふたなり || GETVAR(@"%듀얼리스트%_MZ_性:カウンタ") != 雄性 && GETVAR(@"%듀얼리스트%_MZ_攻:カウンタ") <= 0
				CALL CARD_DISPOSE(듀얼리스트,"필드",カウンタ)
			ENDIF
			CALL FULLRESET
			효과条件カテゴリ = 蜜罐
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",カウンタ,"表",2)
		NEXT

		FOR カウンタ,0,5
			SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
				CONTINUE
			SIF GETVAR(@"%対面者%_MZ_表:カウンタ") == 3
				CONTINUE
			IF GETVAR(@"%対面者%_MZ_性:カウンタ") == 牝性 || GETVAR(@"%対面者%_MZ_性:カウンタ") == ふたなり || GETVAR(@"%対面者%_MZ_性:カウンタ") == 雄性 && GETVAR(@"%対面者%_MZ_攻:カウンタ") <= 0
				CALL CARD_SET_CATEGORY(対面者,"필드",カウンタ,"蜜罐")
			ELSEIF GETVAR(@"%対面者%_MZ_性:カウンタ") != 牝性 || GETVAR(@"%対面者%_MZ_性:カウンタ") != ふたなり || GETVAR(@"%対面者%_MZ_性:カウンタ") != 雄性 && GETVAR(@"%対面者%_MZ_攻:カウンタ") <= 0
				CALL CARD_DISPOSE(対面者,"필드",カウンタ)
			ENDIF
			CALL FULLRESET
			효과条件カテゴリ = 蜜罐
			CALL STATCHANGE_EFFECT(듀얼리스트,"相手필드",カウンタ,"表",2)
		NEXT

		CALL CARD_LOADUP

		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲相手필드 = 1
		효과範囲相手마함 = 1
		효과条件カテゴリ = 蜜罐
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과範囲相手필드 = 1
			효과範囲相手마함 = 1
			효과条件カテゴリ = 蜜罐
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			蜜罐数量 = RESULT
			攻撃力修正 = RESULT * 500
			효과条件カテゴリ = 惑乱魔
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一攻",攻撃力修正)
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"貫通",1)
		ENDIF
	ENDIF

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲相手필드 = 1
	효과条件攻撃力 = 1
	효과条件표시 = 15
	CALL STATCHANGE_BASIC(듀얼리스트,"相手필드",場所,0,-100000,40078)

	CALL FULLRESET
	효과範囲相手필드 = 1
	효과条件カテゴリ = 蜜罐
	효과条件표시 = 15
	CALL STATCHANGE_BASIC(듀얼리스트,"相手필드",場所,0,-100000,40078)

ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
	CALL FULLRESET
	효과範囲相手필드 = 1
	효과条件攻撃力 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"相手필드",場所,"戦耐")

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件攻撃力 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦耐")

	CALL FULLRESET
	효과範囲相手필드 = 1
	효과条件カテゴリ = 蜜罐
	CALL STATCHANGE_EFFECT(듀얼리스트,"相手필드",場所,"戦耐")

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 蜜罐
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦耐")

ELSEIF 종류 == リアクション효과
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_마함:カウンタ") == -1
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_装:カウンタ") != 場所
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_リ数:カウンタ") >= 10
			CONTINUE
			
		CALL CARD_EROTICS(GETVAR(@"%듀얼리스트%_마함:カウンタ"))
		IF RESULT != -1
			조교대상 = RESULT
			SETCOLOR C_PINK
			PRINTFORML
			PRINTFORML %CALLNAME:조교대상%的体内被阿蒂普斯种入了满满当当的卵,随后被作为产床，以双腿大开的羞耻姿势挂在了空中。
			PRINTFORMW 阿蒂普斯用蛛丝将植入了卵的腔内与屁穴细心的封装好，腹部传来的异动感促使%CALLNAME:조교대상%拼命将这些卵排出体外，却无济于事。
			PRINTFORML
			RESETCOLOR
			CALL EROTIC_MOVE("疑似出産",조교대상,-1,100)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",カウンタ,"リ数",1,0,1)
		ENDIF
	NEXT

	FOR カウンタ,0,5
		SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
			CONTINUE
		SIF GETVAR(@"%対面者%_MZ_リ数:カウンタ") >= 10
			CONTINUE
		SIF GETVAR(@"%対面者%_MZ_攻:カウンタ") >= 1
			CONTINUE

		CALL CARD_EROTICS(GETVAR(@"%対面者%_필드:カウンタ"))
		IF RESULT != -1
			조교대상 = RESULT
			SETCOLOR C_PINK
			PRINTFORML
			PRINTFORMW %CALLNAME:조교대상%的力量不足以挣脱蛛丝,如同收藏品般挂在蛛网上等待着主人的把玩……！
			PRINTFORML
			RESETCOLOR
			CALL EROTIC_MOVE("拘束晒し",조교대상)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",カウンタ,"リ数",1,0,1)
		ENDIF
	NEXT
ENDIF