@CARDNAME_40076,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 惑乱魔芙莉西亚
	CASE "略称"
		RESULTS = 芙莉西亚(惑)
	CASE "カテゴリ"
		RESULTS:0 = 惑乱魔
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_40076(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 150
	CASE "守備力"
		RETURN 2650
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_40076(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 4
素材条件カテゴリ:0 = 蟲惑魔
素材条件カテゴリ:1 = 惑乱魔

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,40076) 
	RETURN RESULT
ENDIF




@CARD_EXPLANATION_40076(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL レベル4の「蟲惑魔」モンスター 또는 「惑乱魔」モンスタx2
PRINTL 이 카드명의 ③④의 효과는 각각 1ターンに1度しか使用できない。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：Ｘ素材を持っている이 카드는 他のカード의 효과를 受けない。
PRINTL 이 카드의 X素材の数*200だけ,상대 필드の몬스터의 攻撃力はダウンする。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②：이 카드가 モンスターゾーンに存在する限り。
PRINTL 戦闘阶段以外的场合,このカード以外の자신의 「惑乱魔」モンスターは効破を持つ。
CALLFORM CARD_ABLE(1,필드起動,フリー誘発)
PRINTL ③：자신・상대ターン,자신필드에 このカード以外の「惑乱魔」と名のついた몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 得到自己フィールド其他「惑乱魔」モンスター数量的Ｘ素材,さらにデッ상대 필드の攻撃力0のモンスター1体を選を파괴한다。
PRINTL ：その카드가「성별を有するモンスター」した場合,破壊する대신에 そのモンスタ주인의 마법＆함정ゾーンに「蜜罐」にして表側표시で置く。
CALLFORM CARD_ABLE(1,攻撃時誘発)
PRINTL ④：자신 필드 위에 このカード与「惑乱魔塞拉」と名のついた몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 守備표시に변경する,이 카드의 守備力は턴 종료시까지 、자신 필드의 「惑乱魔」몬스터의 攻撃力合計した数値올린다。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_40076(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 必要的卡
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,40076)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,40076)
		RETURN 0
	CALL FULLRESET
	효과条件固名前 =  惑乱魔芙莉西亚
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 惑乱魔
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,40076)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과条件固名前:0 = 惑乱魔芙莉西亚
	효과条件固名前:1 = 惑乱魔塞拉
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 惑乱魔
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과条件固名前:0 =  惑乱魔芙莉西亚
	효과条件固名前:1 =  惑乱魔塞拉
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 惑乱魔
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1		

ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_40076(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 守備力修正
#DIM DYNAMIC X消費
#DIM DYNAMIC X素材数
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 选择卡片
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 蜜罐


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40076,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件표시 = 表側표시
		효과条件固名前 =  惑乱魔芙莉西亚
		효과条件カテゴリ = 惑乱魔
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		X消費 = RESULT
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",X消費)
		PRINTFORMW 芙莉西亚的엑시즈素材{X消費}増加了！

		CALL CARD_LOADUP

		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件攻撃力 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			PRINTL 要选상대필드1只攻撃力为0的怪兽当作「蜜罐」在对方마함表側표시放置吗?
			PRINTL [1] - 是
			PRINTL [2] - 但是我拒绝
			BINPUT
			IF RESULT == 1
				CALL FULLRESET
				효과範囲상대필드 = 1
				효과条件攻撃力 = 1
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
				选择卡片 = RESULT
				IF 选择卡片 != 9999
					TRYCALLFORM CARD_{GETVAR(@"%対面者%_필드:选择卡片")}, "성별"
					IF RESULT == 牝性 || RESULT == ふたなり || RESULT == 雄性
						TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:选择卡片")}("名前")
						조교대상 = GETVAR(@"%対面者%_필드:选择卡片")
						CALL FULLRESET
						효과추가カテ = 蜜罐
						CALL CARD_ADD(対面者,"필드","마함",选择卡片,"表側표시")

						CALL CARD_EROTICS(조교대상)
						IF RESULT != -1
							조교대상 = RESULT
							SETCOLOR C_PINK
							PRINTFORML
							PRINTFORMW 沉醉于芙莉西亚气味的%CALLNAME:조교대상%无力做出反抗,被改造成了生产"花蜜"的蜜罐……！
							PRINTFORML
							RESETCOLOR
							CALL EROTIC_MOVE("묘상捕縛",조교대상, -1, 100)
						ENDIF
					ELSE
						CALL FULLRESET
						CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",选择卡片,"묘지")
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40076,2)
	IF RESULT == 0
		选择卡片 = 戦闘場所(듀얼리스트,1)
		TRYCALLFORM CARD_{戦闘カードID(듀얼리스트,1)}("성별")
		IF RESULT == 牝性
			TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:选择卡片")}("名前")
			조교대상 = GETVAR(@"%対面者%_필드:选择卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW 大王花浓烈的雌臭味令%CALLNAME:조교대상%意识迷离,全身任由惑乱魔们肆意把玩。
				RESETCOLOR
				CALL EROTIC_MOVE("全身레즈愛撫",조교대상,NO_TO_CHARANUM(1371),50)
			ENDIF
		ENDIF
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件표시 = 表側표시
		효과条件カテゴリ = 惑乱魔
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		守備力修正 = RESULT:3
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"一守",守備力修正)
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表",2,1)
	ENDIF

ELSEIF 종류 == 攻撃時強制誘発
	CALL FULLRESET
	IF RESULT == 0
		IF 戦闘場所(듀얼리스트,1) != 5
			CALL CARD_EROTICS(戦闘カードID(듀얼리스트,1))
			IF RESULT != 0
				조교대상 = RESULT
			ENDIF
		ENDIF
		IF 조교대상 > 0
			PRINTFORML
			SETCOLOR C_PINK 
			PRINTFORMW %CALLNAME:조교대상%想要直接对芙莉西亚进行攻击,却被附近的惑乱魔一起包围了。
			RESETCOLOR
		ENDIF
	ENDIF

ELSEIF 종류 == 攻守変動
	IF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") >= 0
		CALL FULLRESET
		X素材数 = GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所")
		攻撃力修正 = (1 + X素材数) * 200
		효과条件固カテゴリ = 蜜罐
		CALL STATCHANGE_BASIC(듀얼리스트,"상대필드",場所,-攻撃力修正,0,40076)
	ENDIF

ELSEIF 종류 == 지속효과
	IF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") >= 0
		CALL FULLRESET
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"魔耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
	ENDIF

	IF バトルフェイズ != 1
		CALL FULLRESET
		효과条件カテゴリ = 惑乱魔
		효과条件他所 = 1
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF
ENDIF










