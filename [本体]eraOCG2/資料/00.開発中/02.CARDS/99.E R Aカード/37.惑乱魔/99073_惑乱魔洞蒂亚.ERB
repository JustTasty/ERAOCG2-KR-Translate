@CARDNAME_99073,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 惑乱魔洞蒂亚
	CASE "略称"
		RESULTS = 洞蒂亚(惑)
	CASE "カテゴリ"
		RESULTS:0 = 惑乱魔
		RESULTS:1 = 蜜穴
        RESULTS:2 = E.R.A
ENDSELECT


@CARD_99073(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 200
	CASE "守備力"
		RETURN 2600
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 효과なし
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99073(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①②의 효과는 각각 1ターンに1度しか使用できない。
PRINTL 이 카드는 패에서 「惑乱魔」モンスタ或通常함정카드 １枚を捨て、セットしたターンに발동する事もできる。
CALLFORM CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：이 카드는 발동後、モンスターンに守備표시で특수 소환한다。
PRINTL 那之后可以用자신 필드의 モンスタ为素材,从EX덱把1只「惑乱魔」モンスタ엑시즈召唤或링크召唤。
CALLFORM CARD_ABLE(1,묘지起動,フリー誘発)
PRINTL ②：자신 필드有「惑乱魔」엑시즈モンスタ或link2以上的「惑乱魔」링크モンスタ,묘지의 이 카드를 제외하고 발동할 수 있다。
PRINTL 상대는 묘지에서 持有성별の 몬스터 １장을 특수 소환한다,이 효과로 특수 소환한 モンスター视为「蜜罐」。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_99073(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99073)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	SIF 발동済_1(듀얼리스트,99073)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,99073,"마함")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ:0 = 惑乱魔
	효과条件カテゴリ:1 = 蜜穴
	효과条件종류:0 = モン全般
	효과条件종류:1 = 通常함정
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3 && RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF 발동済_2(듀얼리스트,99073)
		RETURN 0
	SIF モンスター数(対面者) >= 5
		RETURN 0

	CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件성별 = 성별在
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류:0 = 엑시즈
	효과条件종류:1 = 링크
	효과条件レベル:0 = 2
	효과条件レベル:1 = 3
	효과条件レベル:2 = 4
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_99073(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 选择卡片
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 소환カード
#DIM DYNAMIC 소환カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF (종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")) && GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99073,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ:0 = 惑乱魔
		효과条件カテゴリ:1 = 蜜穴
		효과条件종류:0 = モン全般
		효과条件종류:1 = 通常함정
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트, "마함", 場所, "守備표시")
			CALL FULLRESET
			PRINTL 要用自己필드的怪兽进行엑시즈召唤或링크召唤吗？
			PRINTL [1] - 是
			PRINTL [2] - 但是我拒绝
			BINPUT
			IF RESULT == 1
				CALL FULLRESET
				효과範囲EX덱 = 1
				효과条件カテゴリ = 惑乱魔
				효과条件종류:0 = 엑시즈
				효과条件종류:1 = 링크
				효과条件소환可 = 1
				CALL CARD_CHOOSE_BASIC(듀얼리스트, "",ゾーン,場所)
				소환カード = RESULT
				IF 소환カード != 9999
					소환カードID = RESULT:99
					CALLFORM CARD_{소환カードID}, "종류"
					IF RESULT == 엑시즈
						TRYCALLFORM CARDSUMMON_{소환カードID}(듀얼리스트,소환カード)
						CALL XYZSUMMON(듀얼리스트, 소환カード)
					ELSEIF RESULT == 링크
						TRYCALLFORM CARDSUMMON_{소환カードID}(듀얼리스트,소환カード)
						CALL LINSUMMON(듀얼리스트, 소환カード)
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 마함起動 || (종류 == フリー誘発 && GROUPMATCH(ゾーン,"마함"))
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99073,1)
	IF RESULT == 0
		CALL RESETCONDITION		
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所, "守備표시")
		CALL FULLRESET
		PRINTL 要用自己필드的怪兽进行엑시즈召唤或링크召唤吗？
		PRINTL [1] - 是
		PRINTL [2] - 但是我拒绝
		BINPUT
		IF RESULT == 1
			CALL FULLRESET
			효과範囲EX덱 = 1
			효과条件カテゴリ = 惑乱魔
			효과条件종류:0 = 엑시즈
			효과条件종류:1 = 링크
			효과条件소환可 = 1
			CALL CARD_CHOOSE_BASIC(듀얼리스트, "EX덱",ゾーン,場所)
			소환カード = RESULT
			IF 소환カード != 9999
				소환カードID = RESULT:99
				CALLFORM CARD_{소환カードID}, "종류"
				IF RESULT == 엑시즈
					TRYCALLFORM CARDSUMMON_{소환カードID}(듀얼리스트,소환カード)
					CALL XYZSUMMON(듀얼리스트, 소환カード)
				ELSEIF RESULT == 링크
					TRYCALLFORM CARDSUMMON_{소환カードID}(듀얼리스트,소환カード)
					CALL LINSUMMON(듀얼리스트, 소환カード)
				ENDIF
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99073,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件성별 = 성별在
		효과条件종류 = モン全般
		效果추가カテ = 蜜罐
		CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
	ENDIF
ENDIF