@CARDNAME_79558,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 【旨乳】ホルスタイン-デリシャス
	CASE "略称"
		RESULTS = 암소登録【旨乳】
	CASE "カテゴリ"
		RESULTS:0 = ホルス
		RESULTS:1 = ホルスタイン
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_79558(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250325
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79558(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드는 １턴에 １번밖에 발동할 수 없다。
PRINTL 이 카드의 ①의 효과는 デュエル中１度しか適用할 수 없으며、①の효과로 選択したカードは「암소」の登録情報として지속마법가 된다。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：패 / 덱에서 「ホルスタイン」이외의 牝性몬스터를 １장選択し、그 몬스터를 지속마법 카드扱いで자신의 마법＆함정 존에 앞면 표시로 둔다。
PRINTL 그 후、덱外から「【旨乳】ホルスタイン」１장을 패에 넣고、
PRINTL 자신의 덱 / 패 / 몬스터 존 / 묘지에 存在する모든 同名카드를 「【旨乳】ホルスタイン」に書き換える。
PRINTL 이 효과의 処理後、이하의 효과에서 選択する。
PRINTL ●패에서 「【旨乳】ホルスタイン」１장을 특수 소환한다。
PRINTL ●덱에서 「ホルスタイン」함정카드를 １장 고르고 패에 넣는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79558(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

;戦闘終了플래그を拾うため
IF 종류 == 永攻紋章起動 && (자신_듀얼패배플래그 > 0 || 상대_듀얼패배플래그 > 0)
	RETURN 1
ENDIF

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79558)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_D1(듀얼리스트,79558) 
		RETURN 0
	SIF 발동済_1(듀얼리스트,79558) 
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件성별 = 牝性
	효과条件固カテゴリ = ホルスタイン
	
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 永紋章起動
	RETURN 1
ELSEIF 종류 == 永攻紋章起動
	RETURN 1
ENDIF

@CARDEFFECT_79558(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 所有者
#DIM DYNAMIC 参照場所
#DIM DYNAMIC 암소대상
#DIM DYNAMIC 암소所有
#DIMS DYNAMIC 암소名称
#DIMS DYNAMIC 암소略称
#DIM DYNAMIC 암소속성
#DIM DYNAMIC 암소レベル
#DIM DYNAMIC 암소攻撃力
#DIM DYNAMIC 암소守備力
#DIM DYNAMIC 암소튜너
#DIM DYNAMIC 암소乳サイズ
#DIM DYNAMIC 암소ミルクml
#DIM DYNAMIC 암소ミルク売却額
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC カテゴリ名称

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

書換先カードID = 179558
カテゴリ名称 = 登録情報：ホルスタイン-旨乳

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79558,1)
	IF RESULT == 0 && 마함数(듀얼리스트) < 5
		
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件성별 = 牝性
		효과条件固カテゴリ = ホルスタイン
		효과추가カテ = %カテゴリ名称%
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
		
		選択カードID = RESULT:1
		
		IF RESULT != 0
			;마함に置く(デュエル中１度)
			CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79558,20)
			
			;ホルスタイン情報の취득(カードIDから各種情報を취득)
			CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,書換先カードID)
			암소名称 = %RESULTS:0%
			암소略称 = %RESULTS:1%
			参照場所 = RESULT:0
			암소대상 = RESULT:1
			参照カードID = RESULT:99
			CALL HORUSTEIN_GET_BUST(参照カードID)
			암소乳サイズ = RESULT:2
			
			;耐性と발동無効の付与
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"破耐",2)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"対耐",2)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"発無",2)
			
			;カードの書き換え
			;필드의 카드
			FOR カウンタ,0,5
				SIF GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"필드",カウンタ,書換先カードID)
			NEXT
			
			;패의 カード
			FOR カウンタ,0,現손패(듀얼리스트)
				SIF GETVAR(@"%듀얼리스트%_손패:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"손패",カウンタ,書換先カードID)
			NEXT
			
			;덱의 カード
			FOR カウンタ,0,現山札数(듀얼리스트)
				SIF GETVAR(@"%듀얼리스트%_덱:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"덱",カウンタ,書換先カードID)
			NEXT
			
			;묘지의 カード
			FOR カウンタ,0,現묘지数(듀얼리스트)
				SIF GETVAR(@"%듀얼리스트%_묘지:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"묘지",カウンタ,書換先カードID)
			NEXT
			
			PRINTFORMW 登録証に名前が刻まれると、どこからともなくホルスの守護者が現れた！
			PRINTFORML %암소名称%は抵抗する間もなく三人の守護者に押さえつけられると、身体に관리タグを印字されてしまった…
			PRINTFORML 
			PRINTFORML 「…モぉー！…モぉォ…？」
			PRINTFORMW 
			PRINTFORML %암소名称%は驚いて声を上げるが、その声は암소の鳴き声に変わってしまう。
			PRINTFORML あの관리タグ…印字された部分を中心に、身体が熱く変わっていく감각に襲われるらしい。
			PRINTFORML 熱く火照った%암소名称%の身体は汗でしっとりと湿っているようだ…
			PRINTFORMW %암소名称%を무시하고、守護者はそのガッチリとした手で乳を丁寧に암소に相応しい"商品"へと"調整"していく…
			SETCOLOR C_PINK
			PRINTFORML 
			PRINTFORML 「モぉっー…」「モぉぉう…❤」
			PRINTFORMW 
			RESETCOLOR
			PRINTFORML …調整が終わったようだ。
			PRINTFORML 先ほどまでと様子が変わり、声を上げるたびに熱を帯びた암소の鳴き声へと変わっていく…
			PRINTFORML 
			IF 암소乳サイズ >= 0
				;元々普乳以上の경우、1サイズアップ
				
				PRINTFORM %암소名称%の乳は
				SETCOLOR C_PINK
				PRINTFORM  [%GET_TALENTNAME(121,암소乳サイズ)%] 
				RESETCOLOR
				PRINTFORM から
				SETCOLOR C_H_PINK
				PRINTFORM  [%GET_TALENTNAME(121,암소乳サイズ+1)%] 
				RESETCOLOR
				PRINTFORML になってしまった…
				PRINTFORMW
				
				;着替え
				PRINTFORML %암소名称%は押さえつけていた守護者が何時の間にか居なくなったのを確認すると、
				PRINTFORMW しっとりと濡れた身体に張り付いていた服をぺちゃり、と脱ぎ捨てた…
			ELSE
				;普乳未満の경우、巨乳にする
				;一気に2サイズ以上アップする
				
				PRINTFORM %암소名称%の乳は
				SETCOLOR C_PINK
				PRINTFORM  [%GET_TALENTNAME(121,암소乳サイズ)%] 
				RESETCOLOR
				PRINTFORM から
				SETCOLOR C_H_PINK
				PRINTFORM  [%GET_TALENTNAME(121,1)%] 
				RESETCOLOR
				PRINTFORML になってしまった…
				PRINTFORMW
				
				;着替え
				PRINTFORML 守護者は%암소名称%の一気に大きくなった乳を逃がすように服を脱がせてくれた…
				PRINTFORMW %암소名称%は身体の重心が変わってしまった影響か、歩きにくそうだ…
			ENDIF
			
			PRINTFORML 
			PRINTFORML %암소名称%は足元に用意してあった衣装を拾い、それが当然というように牛柄ビキニに着替え始めた…
			
			;服装の변경(牛柄ビキニと角)
			IF 암소대상 != -1 
				;アクセサリ&帽子：つけ角・大(2枠目を변경)
				EQUIP:암소대상:2 = 21
				CFLAG:암소대상:(200 + 2) = 21
				;靴、靴下は触らない
				;ズボン～インナーまでは全剥き
				FOR カウンタ,6,13
					EQUIP:암소대상:カウンタ = 0
					CFLAG:암소대상:(200 + カウンタ) = 0
				NEXT
				;ワンピース：牛柄ビキニ
				EQUIP:암소대상:15 = 33
				CFLAG:암소대상:(200 + 15) = 33
				
				;조교
				CALL EROTIC_MOVE("バスト조교",암소대상,-1,300)
			ENDIF
			
			;書き換えた카드를 패에 넣는다。
			CALL CARD_GENERATE_HAND(듀얼리스트,書換先カードID)
			
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = ホルスタイン
			효과条件종류 = 함정全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			
			IF RESULT == 1
				;덱にある場合
				CALL YN_CHOICE(듀얼리스트,"「ホルスタイン」함정카드를 패에 넣는다","「" + 암소名称 + "」を패에서 특수 소환한다")
			ELSE
				;덱に존재하지 않는 경우、選択肢は표시しない
				PRINTFORML
				PRINTFORMW 「ホルスタイン」함정카드가덱に존재하지 않는 為、「%암소名称%」を패에서 특수 소환한다…が選択されます。
				RESULT = 1
			ENDIF
			
			IF RESULT == 0
				;덱에서 「ホルスタイン」함정を패에 넣는다
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件カテゴリ = ホルスタイン
				효과条件종류 = 함정全般
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ELSE
				;패에서 특수소환
				CALL FULLRESET
				;選択時に【〇乳】ホルスタイン…と표시されるとｷﾞｮｯっとするので、패에서 探して특수 소환한다
				FOR カウンタ,0,現손패(듀얼리스트)
					SIF GETVAR(@"%듀얼리스트%_손패:カウンタ") != 書換先カードID
						CONTINUE
					CALL CARD_SP_SUMMON(듀얼리스트,"손패",カウンタ)
					BREAK
				NEXT
				IF RESULT != 0
					PRINTFORMW %암소名称%の思わず吸い付きたくなる乳がしっとりと艶めいている…
					CALL HORUSTEIN_RAND_COW_TXT(1,2)
				ENDIF
				
			ENDIF
			
			PRINTFORML 
			CALL FULLRESET
			효과範囲마함 = 1
			효과範囲손패 = 1
			효과条件ID = 79555
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				;패 / 필드에 ホルスタインファームが存在する場合
				SETCOLOR C_PINK
				PRINTFORML %암소名称%はしっとりと艶めいた乳を、何も言わずに乳をこちらに突き出してきた…
				PRINTFORML %암소名称%は搾られる準備が出来ているようだ…
				PRINTFORMW 「…モぉ…っ❤ モォゥー…❤」
				PRINTFORML 
				RESETCOLOR
			ENDIF
			
			PRINTFORML 
			;永紋章の표시をスキップ
			SKIPDISP 1
			CALL CARD_RESERV(듀얼리스트,79558,永紋章予約)
			CALL CARD_RESERV(듀얼리스트,79558,永攻紋章予約)
			SKIPDISP 0
			SETCOLOR C_YELLOW
			PRINTFORML 「【旨乳】ホルスタイン」は필드에 存在する場合「%암소名称%」として扱われます。
			RESETCOLOR
			PRINTFORMW
			
			IF 암소乳サイズ <= 0
				;普乳以下なら巨乳にする
				암소乳サイズ = 1 -(암소乳サイズ)
			ELSE
				;巨乳以上なら1サイズUpする
				암소乳サイズ = 1
			ENDIF
			;旨乳は카운터を巨乳以上になるように調整してスタート(乳サイズ：UP)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"個Ｃ",암소乳サイズ)
			
		ELSE
			PRINTFORMW 마법＆함정 존에 카드를 置けなかったため、登録できませんでした…
		ENDIF
		
	ENDIF
	
	SKIPDISP 1
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	SKIPDISP 0
	
ELSEIF 종류 == 永紋章起動 || 종류 == 永攻紋章起動
	;紋章を使用し、패 / 필드의 암소の情報を書き換え
	CALL FULLRESET
	CALL HORUSTEIN_GET_DATA(듀얼리스트,,179558)
	암소名称 = %RESULTS:0%
	암소略称 = %RESULTS:1%
	参照場所 = RESULT:0
	암소대상 = RESULT:1
	암소所有 = RESULT:2
	암소ミルクml = RESULT:4		;搾乳量(듀얼中総量)
	参照カードID = RESULT:99
	암소乳サイズ = GETVAR(@"%듀얼리스트%_魔_個Ｃ:参照場所");ここで落ちている：多分後から情報を手に入れたのかな？

	;参照先카드가吹っ飛んでたら処理しない
	IF 参照場所 != -1
		
		IF 듀얼리스트 == "자신"
			所有者 = 1
		ELSEIF 듀얼리스트 == "상대"
			所有者 = 2
		ENDIF
		
		;속성
		TRYCALLFORM CARD_{参照カードID},"속성"
		암소속성 = RESULT
		
		;レベルの취득
		TRYCALLFORM CARD_{参照カードID},"レベル"
		암소レベル = RESULT
		
		;攻守
		TRYCALLFORM CARD_{参照カードID},"攻撃力"
		암소攻撃力 = RESULT
		TRYCALLFORM CARD_{参照カードID},"守備力"
		암소守備力 = RESULT

		;旨乳の攻守は1500に近いほど高くなる
		IF INRANGE (암소攻撃力, 0, 3000) 
			ABS 암소攻撃力 - 1500
			암소攻撃力 = 3000 - RESULT * 2
		ELSE 
			암소攻撃力 = 0
		ENDIF

		IF INRANGE (암소守備力, 0, 3000) 
			ABS 암소守備力 - 1500
			암소守備力 = 3000 - RESULT * 2
		ELSE 
			암소守備力 = 0
		ENDIF

		;튜너
		TRYCALLFORM CARD_{参照カードID},"튜너"
		암소튜너 = RESULT
		
		;패의 カードのレベル변경(손패は설정分Lvが下がる)
		IF 종류 == 永攻紋章起動
			CALL FULLRESET
			효과条件ID = 書換先カードID
			CALL STATCHANGE_HAND(듀얼리스트,"全体",場所,"永星",12 - 암소レベル)
		ENDIF
		
		;자신 필드의 カードで、所有者が자신
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ") != 書換先カードID
				CONTINUE
			SIF GETVAR(@"%듀얼리스트%_MZ_所有:カウンタ") != 所有者
				CONTINUE

			IF 종류 == 永紋章起動 
				CALL CARD_SET_RENAME(듀얼리스트,"필드",カウンタ,암소略称)
				CALL STATCHANGE_EFFECT(듀얼리스트,"自身",カウンタ,"変化元ID",参照カードID)
			ELSEIF 종류 == 永攻紋章起動 
				
				CALL STATCHANGE_OTHER(듀얼리스트,"自身",カウンタ,"属",암소속성)
				CALL STATCHANGE_EFFECT(듀얼리스트,"自身",カウンタ,"永星",암소レベル)
				CALL STATCHANGE_BASIC(듀얼리스트,"自身",カウンタ,암소攻撃力,암소守備力)
				CALL CARD_SET_CATEGORY(듀얼리스트,"필드",カウンタ,"E.R.A")
				;カテゴリ2は未설정なら변경する
				SIF GETVARS(@"%듀얼리스트%_MZ_追カテ2:カウンタ") == ""
					CALL CARD_SET_CATEGORY(듀얼리스트,"필드",カウンタ,"ホルスタイン",1)
				SIF 암소튜너 == 1
					CALL STATCHANGE_EFFECT(듀얼리스트,"自身",カウンタ,"一Ｔ",2)
				
				;지속마법扱いの자신に乗ってる카운터の数だけ乳を盛る
				CALL STATCHANGE_OTHER(듀얼리스트,"自身",カウンタ,"가슴追",암소乳サイズ)
			ENDIF
		NEXT
		
		;상대 필드의 카드で、所有者が자신
		FOR カウンタ,0,5
			SIF GETVAR(@"%対面者%_MZ_ID:カウンタ") != 書換先カードID
				CONTINUE
			SIF GETVAR(@"%対面者%_MZ_所有:カウンタ") != 所有者
				CONTINUE

			IF 종류 == 永紋章起動 
				CALL CARD_SET_RENAME(対面者,"필드",カウンタ,암소略称)
				CALL STATCHANGE_EFFECT(対面者,"自身",カウンタ,"変化元ID",参照カードID)
			ELSEIF 종류 == 永攻紋章起動 
				
				CALL STATCHANGE_OTHER(対面者,"自身",カウンタ,"属",암소속성)
				CALL STATCHANGE_EFFECT(対面者,"自身",カウンタ,"永星",암소レベル)
				CALL STATCHANGE_BASIC(対面者,"自身",カウンタ,암소攻撃力,암소守備力)
				CALL CARD_SET_CATEGORY(対面者,"필드",カウンタ,"E.R.A")
				;カテゴリ2は未설정なら변경する
				SIF GETVARS(@"%対面者%_MZ_追カテ2:カウンタ") == ""
					CALL CARD_SET_CATEGORY(対面者,"필드",カウンタ,"ホルスタイン",1)
				
				SIF 암소튜너 == 1
					CALL STATCHANGE_EFFECT(対面者,"自身",カウンタ,"一Ｔ",2)
				
				CALL STATCHANGE_OTHER(対面者,"自身",カウンタ,"가슴追",암소乳サイズ)
				
			ENDIF
		NEXT
	ENDIF
	IF (자신_듀얼패배플래그 > 0 || 상대_듀얼패배플래그 > 0) && 암소ミルクml > 0
	
		;基本は마함見るけど、자신필드에 존재하는 경우、更に別の효과로 乳がデカくなってるかチェック
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ") != 書換先カードID
				CONTINUE
			SIF GETVAR(@"%듀얼리스트%_MZ_가슴追:カウンタ") > 암소乳サイズ
				암소乳サイズ = GETVAR(@"%듀얼리스트%_MZ_가슴追:カウンタ")
		NEXT
		
		CALL HORUSTEIN_GET_BUST(参照カードID,암소乳サイズ)
		암소乳サイズ = RESULT:2
		암소ミルク売却額 = 암소ミルクml *10
		
		CUSTOMDRAWLINE —
		PRINTFORML
		IF 암소乳サイズ >= 5
			;規格外な
			TIMES 암소ミルク売却額 , 0.15
		
			SETCOLOR C_H_PINK
			PRINTFORM [%GET_TALENTNAME(121,암소乳サイズ)%] 
			RESETCOLOR
			PRINTFORML の乳は買取が難しいようだ…
		ELSE
			;旨乳は何もしなくてもたくさん搾れるので価格が抑えめ
			TIMES 암소ミルク売却額 , 0.65
			SETCOLOR C_PINK
			PRINTFORML [%GET_TALENTNAME(121,암소乳サイズ)%] 
		ENDIF
		RESETCOLOR
		
		PRINTFORML %암소名称%から搾った{암소ミルクml}mlのミルクを{암소ミルク売却額}円で売却した…
		CALL MONEY_GET(암소ミルク売却額)

		
		SETCOLOR C_GRAY
		TIMES 암소ミルク売却額 , 0.1875;元の金額の0.15倍
		;なんか計算で出したけど、しょっぱい金額引かれてるとみみっちいので、適当にデカくすることにした
		PRINTFORML 機材のメンテナンス費として{암소ミルク売却額*10}円程度、金額が引かれています…
		PRINTFORML 
		RESETCOLOR
		CUSTOMDRAWLINE —
		
		;リセットも兼ねて、ミルクの情報を削除
		CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"T1汎数",0,上書きモード)
		CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"汎数",0,上書きモード)
	ENDIF
ENDIF


