	CASE "レベル"
		RETURN 12
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN デュアル
ENDSELECT

RETURN 0

@CARD_EXPLANATION_179558(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 通常몬스터와 して扱い、마법＆함정 존에 置かれたカードによってステータスが決定する。
PRINTL 자신필드에 「ホルスタイン-ミルクファーム」가 존재하는 경우、이하의 효과를 얻는다。
PRINTL 
PRINTL 이 카드의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
PRINTL ①：메인 페이즈에 발동할 수 있다。이 카드의 乳を計測する。
;CALLFORM CARD_ABLE(0,필드起動)
PRINTL ②：バトルフェイズに발동할 수 있다。이 카드의 乳を搾り発情させる。
PRINTL 그 후、이하의 효과를 적용한다。
PRINTL ●이 턴、이 카드가 존재하는 한、상대는 このカードしか공격 대상으로 할 수 없다。
PRINTL ●이 카드가 전투 / 효과로 파괴된 경우、자신의 묘지 / 제외상태인 「ホルスタイン」카드를 ２장까지 골라、손패か덱に戻す事が出来る。
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ③：이 카드의 攻撃が中断된 경우에 발동할 수 있다。
PRINTL 攻撃대상의 몬스터를 「ホルスタイン」カードとして扱い、그 몬스터의 컨트롤을 얻는다。

@CARDCAN_179558(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 別途カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,179558)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF GETVARS(@"%듀얼리스트%_フィールド名") != "ミルクファーム"
		RETURN 0
	IF メインフェイズ == 1
		SIF ターンプレイヤー == "상대" ;ただ情報を표시するだけなので、NPCは使用しない(ターン1付けてないから多分連打するし…)
			RETURN 0
	ELSEIF バトルフェイズ == 1
		SIF 발동済_1(듀얼리스트,179558) 
			RETURN 0
	ENDIF
	
	RETURN 1
ELSEIF 종류 == 攻撃後強制誘発;本当はミルクファームなきゃダメだけど、ただ致すだけだし良いか…
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_発情:場所") == 0
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_攻撃無効_保") != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF GETVARS(@"%듀얼리스트%_フィールド名") != "ミルクファーム"
		RETURN 0
	;発情時に攻撃中断された場合
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_発情:場所") == 0
		RETURN 0	
	SIF GETVAR(@"%듀얼리스트%_攻撃無効") != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_SS_CHECK(듀얼리스트,0,"묘지")
	SIF RESULT == 1
		RETURN 0
	
	;バトルフェイズに乳を搾った
	SIF !발동済_1(듀얼리스트,179558) 
		RETURN 0
	
	;전투 / 효과破壊時의 효과를 使っていない
	SIF 발동済_2(듀얼리스트,179558) 
		RETURN 0
	
	;특수소환可否
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ  = ホルスタイン
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	;守備표시の場合
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 1 
		RETURN 0
	;자신이 특수状態：ドリップ(99554)の場合
	SIF GETVAR(@"%듀얼리스트%_MZ_특수状態:場所") != 99554
		RETURN 0
	
	CALL FULLRESET
	효과条件固カテゴリ = ホルスタイン
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT == 1
		RETURN 1
	
	CALL FULLRESET
	효과条件固カテゴリ = ホルスタイン
	CALL CARD_CHECK_FIN_DATA(対面者,"효과발동")
	SIF RESULT == 1
		RETURN 1
	
	RETURN 0
ENDIF

@CARDEFFECT_179558(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 参照場所
#DIM DYNAMIC 암소대상
#DIMS DYNAMIC 암소名称
#DIM DYNAMIC 攻撃대상
#DIM DYNAMIC 攻撃대상カードID
#DIMS DYNAMIC 상대名称
#DIMS DYNAMIC 搾手名称
#DIM DYNAMIC 搾手
#DIM DYNAMIC 절정횟수
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC カテゴリ名称
#DIM DYNAMIC 現乳サイズ
#DIM DYNAMIC T1ミルクml
#DIM DYNAMIC ミルクml

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

書換先カードID = 179558
カテゴリ名称 = 登録情報：ホルスタイン-旨乳

;ホルスタイン情報の취득
CALL HORUSTEIN_GET_DATA(듀얼리스트,場所,179558)
암소名称 = %RESULTS:0%
参照場所 = RESULT:0
암소대상 = RESULT:1
T1ミルクml = RESULT:3	;搾乳量(1ターン中)
ミルクml = RESULT:4		;搾乳量(듀얼中総量)
参照カードID = RESULT:99

;乳サイズ취득
CALL HORUSTEIN_GET_BUST(参照カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:場所"))
現乳サイズ = RESULT:2 

;実行者によって文章の상대を切り替える
IF 듀얼리스트 == "자신"
	搾手名称 = %CALLNAME:MASTER%
	搾手 = MASTER
ELSE
	;対戦상대
	CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
	IF RESULT != -1
		搾手名称 = %CALLNAME:RESULT%
	ELSE
		搾手名称 = "対戦者"
	ENDIF
	搾手 = RESULT
ENDIF

;攻撃대상의 名前を취득
IF 종류 == 攻撃後強制誘発 || 종류 == 효과後誘発
	攻撃대상 = 戦闘場所(対面者)
	;名称の설정
	IF 攻撃대상 == 99
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			상대名称 = %CALLNAME:RESULT%
		ELSE
			상대名称 = "対戦상대"
		ENDIF
	ELSE
		攻撃대상カードID = GETVAR(@"%対面者%_MZ_ID:攻撃대상")
		TRYCALLFORM CARDNAME_{攻撃대상カードID},"略称"
		상대名称 = %RESULTS%
	ENDIF
ENDIF

IF 종류 == 필드起動 && メインフェイズ == 1
	;ステータス표시させるだけなので、無効にさせない
	효과無効不能 = 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID)
	IF RESULT == 0 && 参照カードID != -1
		
		PRINTFORML 
		PRINTFORMW ホルスの従者達が%암소名称%を取り囲んでいる…
		
		;名前から가슴のサイズの変動まで표시
		CALL HORUSTEIN_PRINT_STATE(듀얼리스트,場所,179558, GETVAR(@"%듀얼리스트%_MZ_가슴追:場所"))
		

	ENDIF
ELSEIF 종류 == 필드起動 && バトルフェイズ == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID,1)
	
	IF RESULT == 0 && 参照カードID != -1
		;가슴を搾る
		IF GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所") == 0
			;1回目
			PRINTFORML %搾手名称%は%암소名称%の乳に手を伸ばすと、その輪郭が変わらないように指を滑らせていく…
			PRINTFORMW 肌から乳へと変化していく感触を楽しみながら、徐々にその中心へと搾るような動きで가슴を意識させる。
			PRINTFORM %암소名称%から
			SETCOLOR C_PINK
			PRINTFORM 「モー…❤」
			RESETCOLOR
			PRINTFORML と声が漏れるころには、%암소名称%の乳はピンっと張り、その先端は搾って欲しそうに艶めいていた…
			PRINTFORML %搾手名称%はキュッと指を搾ると、%암소名称%は喜ぶように白い液体を滴らせた…
			SETCOLOR C_PINK
			PRINTFORMW 「もぉっ❤ んっぁっ❤ ―…❤」
			RESETCOLOR
			IF 암소대상 != -1
				CALL EROTIC_MOVE("バスト조교",암소대상,搾手,300)
			ENDIF
			
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"発情",175)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",1,上書きモード)
		ELSE
			;2回目
			PRINTFORML %搾手名称%は%암소名称%の乳に再び手を伸ばすと、その乳により白く濡れた指を滑らせた…
			PRINTFORML 既に一度搾られているというのに、指が触れただけでも尖らせた先端からはポタポタと漏れてしまっている…
			SETCOLOR C_PINK
			PRINTFORML 「モー…❤ モー…❤」
			RESETCOLOR
			PRINTFORMW 優しく撫でるような刺激では物足りないのか、%암소名称%は%搾手名称%の手に가슴を押し付け始めた…
			PRINTFORML 
			PRINTFORML %搾手名称%は期待を裏切るかのように丁寧に優しくなぞると、先端をパチンっと弾き、高まった性感を一気に解放させた！
			PRINTFORML %암소名称%の乳は突然の刺激に耐えきれず、壊れた蛇口のように白い液体を零れさせていく…
			SETCOLOR C_PINK
			PRINTFORMW 「…ぁっ！？ ぉぉおっ～～～❤❤」
			RESETCOLOR
			IF 암소대상 != -1
				CALL EROTIC_MOVE("バスト조교",암소대상,搾手,300)
			ENDIF
			
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"発情",185)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",0,上書きモード)
		ENDIF
		;ターゲット集中
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"攻誘",1)

		;효과破壊の適用は발동済2で판정
		
		CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:場所"))
		
		;発情
		SETCOLOR C_PINK
		PRINTFORML 
		PRINTFORML %암소名称%は乳を搾られた影響か、酷く発情している…
		PRINTFORML
		RESETCOLOR

		IF ターンプレイヤー != 듀얼리스트
			;상대 턴에 발동した場合
			PRINTFORML %암소名称%は乳を搾られた余韻に浸っている…

			;암소鳴き声：2回、ねっとり鳴き
			CALL HORUSTEIN_RAND_COW_TXT(2,2)
		ENDIF
		
		PRINTFORMW
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
	;発情時に攻撃が中断された場合
	
	;ひとりえっち(ここは一回触ってるので短めに。효과処理もあるしね)
	PRINTFORML …発情した%암소名称%は%상대名称%にふらふらと近寄るが、視線を足元に向けると途中で足を止めてしまった。
	PRINTFORML ふぅふぅと呼吸を荒げている%암소名称%は、自身の乳から目が離せなくなってしまう…
	PRINTFORMW %암소名称%は%搾手名称%に搾って貰った記憶を呼び覚ますように、乳をゆっくりと擦り始めた…
	
	;암소の鳴き声を鳴らす
	CALL HORUSTEIN_RAND_COW_TXT(1,2)
	
	IF 암소대상 != -1
		CALL EROTIC_MOVE("バスト조교",암소대상,-1,500)
	ENDIF
	절정횟수 += RESULT:1
	IF 절정횟수 > 0
		PRINTFORML %암소名称%は{절정횟수}回절정したようだ。
		PRINTFORMW 余程気持ち良かったのか、%암소名称%の周りには小さな白い斑点が飛び散ってしまっている…
	ENDIF
	
	PRINTFORML 
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID)
	
	IF RESULT == 0
		IF 攻撃대상 == 5 
			;直接攻撃失敗時
			;再び手を伸ばすと、상대필드에 他に戦闘を行える몬스터가 いない為か、タガが外れたように…
			
			PRINTFORMW %암소名称%は상대필드에 戦闘を行えるモンスター居ないことを確認すると、さらなる쾌락を求めて指を動かし始めた…
			절정횟수 += 2
			SETCOLOR C_PINK
			
			;암소の鳴き声を鳴らす
			CALL HORUSTEIN_RAND_COW_TXT(절정횟수,2)
			
			IF 암소대상 != -1
				REPEAT 절정횟수
					CALL EROTIC_MOVE("バスト조교",암소대상,-1,200)
				REND
			ENDIF
			
			RESETCOLOR
			PRINTFORML 鳴き声が響き終わると、%암소名称%は「ぱちゃんっ」と音を立てて白い水たまりに倒れ込んだ…
			PRINTFORMW 好き放題気持ちよくなった%암소名称%は、만족そうな笑みを浮かべている…
			;절정後の암소鳴き声
			CALL HORUSTEIN_RAND_COW_TXT(3,2)
			PRINTFORMW 
		ELSE
			;モンスター攻撃時に失敗
			PRINTFORML 発情した%암소名称%は%상대名称%に大きな乳を押し付け、そのまま押し倒した！
			PRINTFORM %암소名称%は
			SETCOLOR C_PINK
			;旨乳
			PRINTFORM 「モォゥ…❤」
			RESETCOLOR
			
			PRINTFORML と鳴くと、乳を押し付けたまま자신의 乳を搾り始めた…
			PRINTFORMW 白い液体があっという間に상대を包み込み、%상대名称%に濃厚な匂いが染みついていく…
			PRINTFORML 
			PRINTFORMW %상대名称%は母乳によってマーキングされ、正気を失ってしまった…
			PRINTFORMW フラフラとこちらに向かってきた%상대名称%は、%암소名称%の母乳にすっかり夢中のようだ…貪るように%암소名称%の乳を味わっている…
			
			;NTR
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",攻撃대상,"強奪")
			CALL CARD_SET_CATEGORY(듀얼리스트,"필드",コントロール先,"ホルスタイン")
		ENDIF
		
	ENDIF
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,書換先カードID,2)
	IF RESULT == 0
		;묘지 / 제외のカード回収
		REPEAT 2
			CALL FULLRESET
			효과範囲묘지 = 1
			효과範囲제외 = 1
			효과条件カテゴリ  = ホルスタイン
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT != 9999
				CALL CARD_SELECT(듀얼리스트,RESULT)
				PRINTL 카드를 패에 加えますか？
				CALL YN_CHOICE(듀얼리스트)

				IF RESULT == 0
					CALL CARD_DISPOSE(듀얼리스트,参照先（CS）,選択カード（CS）,손패行き)
				ELSEIF RESULT == 1
					CALL CARD_DISPOSE(듀얼리스트,参照先（CS）,選択カード（CS）,덱行き)
				ENDIF
			ELSE
				BREAK
			ENDIF
		REND
	ENDIF
ELSEIF 종류 == 효과後強制誘発
	;ドリップ状態(守備표시時、ホルスタイン이외의 카드가발동する度に搾られる)
	
	SETCOLOR C_PINK
	CUSTOMDRAWLINE —
	PRINTFORML
	PRINTFORML %암소名称%はドリップ装置に繋がれている…
	PRINTFORML 
	CUSTOMDRAWLINE —
	RESETCOLOR
	
	PRINTFORML
	PRINTFORML %암소名称%の가슴に取り付けられた装置が僅かに振動し、零れ落ちたモノがミルクサーバーに溜まっていく…
	PRINTFORML
	
	;1回、ねっとり鳴き、短め鳴き
	CALL HORUSTEIN_RAND_COW_TXT(1,2,10)
	PRINTFORML 
	
	;搾乳する(通常よりも少量)
	CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,GETVAR(@"%듀얼리스트%_MZ_가슴追:場所")-5)
	PRINTFORML 
	
	;乳を吸ってLP회복
	CALL LIFE_CHANGE(듀얼리스트,"増加","효과",100)
ENDIF
