@CARD_EXPLANATION_99556(종류)
#DIM DYNAMIC 종류
#DIMS DYNAMIC 듀얼리스트

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の②の효과は１ターンに１度しか발동できない。
CALLFORM CARD_ABLE(0,마함起動,손패起動)
PRINTL ①：자신の마법＆함정ゾーンに「ホルスタイン」カードの효과によって置かれたカードが存在する場合、バトルフェイズ・자신メインフェイズに발동できる。
PRINTL 「ホルスタイン」カード以外の、자신フィールドの表側표시のカードを묘지に送る。
PRINTL その後、選択したカードが牝性モンスターだった場合、以下の효과を適用できる。
PRINTL ●：손패・묘지・덱・EX덱・상대の묘지から、묘지に送ったカードと同じカードを지속마법カード扱いで자신の마법＆함정ゾーンに表側표시で置き
PRINTL 　　この듀얼中、まだ발동していない「ホルスタイン」通常마법を손패・덱から제외し、その효과を全て適用する。
PRINTL 자신の마법＆함정ゾーンにセットされたカードが存在しない場合、このカードは손패から捨てて발동でき
PRINTL １ターンに１度、자신の마법＆함정ゾーンにカードが２枚以上存在する場合、상대フィールドのカードも選択することができる。
CALLFORM CARD_ABLE(1,묘지起動)
PRINTL ②：자신メインフェイズに발동できる。
PRINTL 묘지のこのカードを제외し、묘지の「ホルスタイン」カードを손패に加える。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 마함数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99556)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 
	SIF !(メインフェイズ == 1 && ターンプレイヤー == 듀얼리스트) && バトルフェイズ != 1
		RETURN 0
	
	IF ゾーン == "손패"
		;セットカードがあれば使えない
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件표시 = 裏側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT > 0
			RETURN 0
	ELSEIF ゾーン == "마함"
		;セットしたターンには使えない
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	
	;자신필드에 ホルスタインカードの효과で置いたカードが無ければ使えない。
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件カテゴリ:0 = 登録情報：ホルスタイン-美乳
	효과条件カテゴリ:1 = 登録情報：ホルスタイン-愛乳
	효과条件カテゴリ:2 = 登録情報：ホルスタイン-旨乳
	효과条件カテゴリ:3 = 登録情報：ホルスタイン-EX乳
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 0
		RETURN 0
	
	;필드에 「ホルスタイン」以外の表側のカードがなければ使えない
	;마법함정ゾーンにカードが2枚上あれば상대のカードも대상にできる。
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	마함数 = RESULT
	
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	IF 마함数 >= 2 && !발동済_1(듀얼리스트,99556)
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
	ENDIF
	효과条件표시 = 表側표시
	효과条件固カテゴリ:0 = ホルスタイン
	효과条件固カテゴリ:1 = 登録情報：ホルスタイン-美乳
	효과条件固カテゴリ:2 = 登録情報：ホルスタイン-愛乳
	효과条件固カテゴリ:3 = 登録情報：ホルスタイン-旨乳
	효과条件固カテゴリ:4 = 登録情報：ホルスタイン-EX乳
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 0
		RETURN 0
	
	RETURN 1
ELSEIF 종류 == 묘지起動
	
	SIF 발동済_2(듀얼리스트,99556)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	
	RETURN 1
ENDIF

@CARDEFFECT_99556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 参照場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 마함数
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 攻め암소参照カードID
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 암소대상
#DIMS DYNAMIC 암소名称
#DIM DYNAMIC 암소乳サイズ
#DIM DYNAMIC 被암소대상
#DIMS DYNAMIC 被암소名称
#DIM DYNAMIC 被암소乳サイズ
#DIMS DYNAMIC カテゴリ名称
#DIM DYNAMIC 美乳チェック
#DIM DYNAMIC 愛乳チェック
#DIM DYNAMIC 旨乳チェック
#DIM DYNAMIC EX乳チェック


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99556)
	IF RESULT == 0
		
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件カテゴリ:0 = 登録情報：ホルスタイン-美乳
		효과条件カテゴリ:1 = 登録情報：ホルスタイン-愛乳
		효과条件カテゴリ:2 = 登録情報：ホルスタイン-旨乳
		효과条件カテゴリ:3 = 登録情報：ホルスタイン-EX乳
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			;選択肢なし
			RESULT = 0
		ELSE
			PRINTFORML 誰が乳圧を掛けますか？
			PRINTFORML 
				
			美乳チェック = -1
			愛乳チェック = -1
			旨乳チェック = -1
			EX乳チェック = -1
			
			FOR カウンタ,0,5
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-美乳"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179556)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【美乳】%RESULTS:0%
					美乳チェック = カウンタ
					CONTINUE
				ENDIF
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-愛乳"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179557)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【愛乳】%RESULTS:0%
					愛乳チェック = カウンタ
					CONTINUE
				ENDIF
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-旨乳"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179558)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【旨乳】%RESULTS:0%
					旨乳チェック = カウンタ
					CONTINUE
				ENDIF
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-EX乳"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179559)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【EX乳】%RESULTS:0%
					EX乳チェック = カウンタ
					CONTINUE
				ENDIF
				
			NEXT
			
			PRINTFORML 
			PRINTFORML [9] - おまかせ
			IF 듀얼리스트 == "자신"
				BINPUT
			ELSE
				RESULT = 9
			ENDIF
		ENDIF
		
		IF RESULT == 9
			SIF 美乳チェック != -1
				RESULT = 美乳チェック
			SIF 愛乳チェック != -1
				RESULT = 愛乳チェック
			SIF 旨乳チェック != -1
				RESULT = 旨乳チェック
			SIF EX乳チェック != -1
				RESULT = EX乳チェック
			
			;雑ランダム処理
			SIF 旨乳チェック != -1 && RAND:2 == 1
				RESULT = 旨乳チェック
			SIF 愛乳チェック != -1 && RAND:2 == 1
				RESULT = 愛乳チェック
			SIF 美乳チェック != -1 && RAND:2 == 1
				RESULT = 美乳チェック
		ENDIF
		
		FOR カウンタ,RESULT,5
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-美乳"
				RESULT = カウンタ
				攻め암소参照カードID = 179556
				BREAK
			ENDIF
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-愛乳"
				RESULT = カウンタ
				攻め암소参照カードID = 179557
				BREAK
			ENDIF
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-旨乳"
				RESULT = カウンタ
				攻め암소参照カードID = 179558
				BREAK
			ENDIF
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：ホルスタイン-EX乳"
				RESULT = カウンタ
				攻め암소参照カードID = 179559
				BREAK
			ENDIF
		NEXT
		
		;ホルスタイン情報の취득
		CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,攻め암소参照カードID)
		암소名称 = %RESULTS:0%
		参照場所 = RESULT:0
		암소대상 = RESULT:1
		암소乳サイズ = GETVAR(@"%듀얼리스트%_魔_個Ｃ:参照場所");마법함정の乳サイズ
		CALL HORUSTEIN_GET_BUST(RESULT:99,암소乳サイズ)
		암소乳サイズ = RESULT:2
		
		美乳チェック = 0
		愛乳チェック = 0
		旨乳チェック = 0
		EX乳チェック = 0
		
		;묘지送り
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		마함数 = RESULT
		
		효과範囲필드 = 1
		효과範囲마함 = 1
		IF 마함数 >= 2 && !발동済_1(듀얼리스트,99556)
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
		ENDIF
		효과条件표시 = 表側표시
		효과条件固カテゴリ:0 = ホルスタイン
		효과条件固カテゴリ:1 = 登録情報：ホルスタイン-美乳
		효과条件固カテゴリ:2 = 登録情報：ホルスタイン-愛乳
		효과条件固カテゴリ:3 = 登録情報：ホルスタイン-旨乳
		효과条件固カテゴリ:4 = 登録情報：ホルスタイン-EX乳
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	
		IF RESULT != -1
			選択カードID = RESULT:1
			
			;상대のカードを選んだら、使用済みにする
			SIF 듀얼리스트 != 대상者（CS）
				CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99556,1)
			
			;ホルスタイン情報の취득(カードIDから名前を취득)
			CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,選択カードID)
			被암소名称 = %RESULTS:0%
			
			;今回は乳の大きさは마법함정を参照して出している。
			;MZの乳は「ホルスタイン」以外のカードの효과で変動してれば値がずれてしまうが、面倒なので今回はふわっとした表現に
			;貧乳(-1)以下、巨乳(1)以下、巨乳より大きい
			
			PRINTFORM %암소名称%は%被암소名称%の上にのしかかると、
			IF 암소乳サイズ <= -1
				PRINTFORMW 可愛らしい乳で一生懸命%被암소名称%を押しつぶした…
			ELSEIF 암소乳サイズ <= 1
				;愛乳以外は巨乳以上になる
				PRINTFORMW 大きな乳で%被암소名称%を押しつぶしてしまった…
			ELSE
				PRINTFORMW とても大きな乳で%被암소名称%を押しつぶしてしまった…
			ENDIF
			PRINTFORML
			
			;選択カードIDが자신の덱・EX덱・손패に存在する場合
			;かつ、마법함정ゾーンに空きがある場合
			
			CALL FULLRESET
			효과範囲상대묘지 = 1
			효과範囲손패 = 1
			효과範囲묘지 = 1
			효과範囲덱 = 1
			효과範囲EX덱 = 1
			효과条件성별 = 牝性
			효과条件ID = 選択カードID
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT != 0 && 마함数(듀얼리스트) < 5
				;存在する
				
				FOR カウンタ,0,5
					;손패と덱に발동していない〇〇登録があるか
					CALL FULLRESET
					효과範囲손패 = 1
					효과範囲덱 = 1
					효과条件ID = 79556 + カウンタ
					CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
					
					;발동可能ﾁｪｯｸと存在チェック
					IF RESULT != -1
						SELECTCASE 79556 + カウンタ
						CASE 79556
							美乳チェック = 1
							SIF 발동済_D1(듀얼리스트,79556)
								美乳チェック = -1
						CASE 79557
							愛乳チェック = 1
							SIF 발동済_D1(듀얼리스트,79557)
								愛乳チェック = -1
						CASE 79558
							旨乳チェック = 1
							SIF 발동済_D1(듀얼리스트,79558)
								旨乳チェック = -1
						CASE 79559
							EX乳チェック = 1
							SIF 발동済_D1(듀얼리스트,79559)
								EX乳チェック = -1
						ENDSELECT
					ENDIF
				NEXT
				
				IF 美乳チェック == 1 || 愛乳チェック == 1 || 旨乳チェック == 1 || EX乳チェック == 1
					
					;발동可能な登録カードがある
					PRINTFORML 
					PRINTFORML 손패・덱・EX덱に암소に登録可能なカードが存在します。登録しますか？
					PRINTFORML 
					IF  美乳チェック == 1
						PRINTFORML [0] - 【美乳】登録
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【美乳】登録
						RESETCOLOR
					ENDIF
					IF  愛乳チェック == 1
						PRINTFORML [1] - 【愛乳】登録
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【愛乳】登録
						RESETCOLOR
					ENDIF
					IF  旨乳チェック == 1
						PRINTFORML [2] - 【旨乳】登録
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【旨乳】登録
						RESETCOLOR
					ENDIF
					IF  EX乳チェック == 1
						PRINTFORML [3] - 【EX乳】登録
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【EX乳】登録
						RESETCOLOR
					ENDIF
					PRINTFORML 
					PRINTFORML [9] - 登録しない
							
					IF 듀얼리스트 == "자신"
						BINPUT
					ELSE
						SIF  EX乳チェック == 1
							RESULT = 3
						SIF  旨乳チェック == 1
							RESULT = 2
						SIF  愛乳チェック == 1
							RESULT = 1
						SIF  美乳チェック == 1
							RESULT = 0
					ENDIF
				ELSE
					;選択肢が出なければ終わり
					RESULT = -1
				ENDIF
			ELSE 
				;자신の손패덱EX덱にカードが無ければ、カードを潰して終わり
				RESULT = -1
			ENDIF
		ELSE
			;潰すのに失敗したら終わり
			RESULT = -1
		ENDIF
		
		SELECTCASE RESULT
		CASE 0
			美乳チェック = 2
			書換先カードID = 179556
			カテゴリ名称 = 登録情報：ホルスタイン-美乳
		CASE 1
			愛乳チェック = 2
			書換先カードID = 179557
			カテゴリ名称 = 登録情報：ホルスタイン-愛乳
		CASE 2
			旨乳チェック = 2
			書換先カードID = 179558
			カテゴリ名称 = 登録情報：ホルスタイン-旨乳
		CASE 3
