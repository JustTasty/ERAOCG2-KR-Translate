@CARDNAME_79555,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = ホルスタイン-ミルクファーム
	CASE "略称"
		RESULTS = ミルクファーム
	CASE "カテゴリ"
		RESULTS:0 = ホルス
		RESULTS:1 = ホルスタイン
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_79555(参照先)

#DIMS DYNAMIC 参照先
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カードIDチェック
#DIM DYNAMIC 마법함정数チェック

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
	CASE "추가日"
		RETURN 250325
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79555(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 자신 필드는 「ホルスタイン-ミルクファーム」가 된다。
PRINTL 이 카드의 효과の발동に対して、상대는 카드의 효과를 발동할 수 없다。
CALLFORM CARD_ABLE(1,마함起動)
PRINTL ①：バトルフェイズ、자신 메인 페이즈에 발동할 수 있다。
PRINTL 패를 １장 버리고、捨てたカードによって이하의 효과를 적용한다。
PRINTL ●通常モンスター：그 카드가야수족牝性몬스터의 場合、１０００ＬＰ회복する。
PRINTL ●효과モンスター：묘지의 「ホルスタイン」몬스터를 １장選んで특수 소환한다。
PRINTL ●마법カード：덱에서 「ホルスタイン」通常마법카드를 １枚고르고 패에 넣는다。
PRINTL ●함정カード：덱에서 「ホルスタイン」함정카드를 １枚고르고 패에 넣는다。


CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79555)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 
	SIF !(メインフェイズ == 1 && ターンプレイヤー == 듀얼리스트) && バトルフェイズ != 1
		RETURN 0
	;他、各種효과の발동可否をチェックする(下記の内、一つでもOKなら발동可能)
	;通常モンスター(패의 通常)
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류 = 通常モン
	;효과条件종족 = 야수족
	;효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 1
	
	;효과モンスター(패의 효과+묘지의 ホルスタインモンスター)
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류:0 = 효과モン
	효과条件종류:1 = 의식
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 야수족
		효과条件カテゴリ = ホルスタイン
		효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 1
	ENDIF
	
	;마법カード(패의 마법+덱의 ホルスタイン마법カード)
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종류 = 通常마법
		효과条件カテゴリ = ホルスタイン
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 1
	ENDIF
	
	;함정(패의 함정+덱의 ホルスタイン함정カード)
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종류 = 함정全般
		효과条件カテゴリ = ホルスタイン
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			RETURN 1
	ENDIF
	
	RETURN 0
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0	
	RETURN 1
ELSEIF 종류 == チェーン可否
	SIF !GROUPMATCH(チェーン可否종류, 마함起動)
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_79555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 효과条件_1
#DIM DYNAMIC 효과条件_2
#DIM DYNAMIC 효과条件_3
#DIM DYNAMIC 효과条件_4
#DIM DYNAMIC 選択カード종류
#DIMS DYNAMIC カテゴリ名称
#DIMS DYNAMIC 암소名称
#DIM DYNAMIC 암소대상
#DIM DYNAMIC 암소가슴追
#DIM DYNAMIC 参照場所;마법함정に置かれたホルスタインの参照先
#DIM DYNAMIC 参照カードID;마법함정に置かれたホルスタインの参照先

IF  종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79555)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 마함起動	
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79555)
	IF RESULT == 0
;＞＞条件설정開始―――――――――――――――――――――――――――――――――
		;通常モンスター(패의 通常)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = 通常モン
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			효과条件_1 = 1

		;효과モンスター(패의 효과+ホルスタイン몬스터의 チェック)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류:0 = 효과モン
		효과条件종류:1 = 의식
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족 = 야수족
			효과条件カテゴリ = ホルスタイン
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				효과条件_2 = 1
		ENDIF
		
		;마법カード(패의 마법+덱의 ホルスタイン마법カード)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = 마법全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = 通常마법
			효과条件カテゴリ = ホルスタイン
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				효과条件_3 = 1
		ENDIF
		
		;함정(패의 함정+덱의 ホルスタイン함정カード)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = 함정全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = 함정全般
			효과条件カテゴリ = ホルスタイン
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				효과条件_4 = 1
		ENDIF
;＞＞条件설정終了―――――――――――――――――――――――――――――――――

		カウンタ = 0
		;0から始まる連番でなければならない
		CALL FULLRESET
		효과範囲손패 = 1
		IF 효과条件_1
			효과条件종류:カウンタ = 通常モン
			カウンタ++
		ENDIF
		IF 효과条件_2
			효과条件종류:カウンタ = 효과モン
			カウンタ++
			효과条件종류:カウンタ = 의식
			カウンタ++
		ENDIF
		IF 효과条件_3
			효과条件종류:カウンタ = 通常마법
			효과条件종류:(カウンタ+1) = 装備마법
			효과条件종류:(カウンタ+2) = 速攻마법
			효과条件종류:(カウンタ+3) = 지속마법
			효과条件종류:(カウンタ+4) = フィールド
			カウンタ += 5
		ENDIF
		IF 효과条件_4
			효과条件종류:カウンタ = 通常함정
			효과条件종류:(カウンタ+1) = 지속함정
			カウンタ++
		ENDIF
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			;★デバッグ用チェッカー
			;PRINTFORMW 효과1：{효과条件_1} 효과2：{효과条件_2} 효과3：{효과条件_3} 효과4：{효과条件_4}
			選択カードID = RESULT:1
			CALLFORM CARD_{選択カードID},"종류"
			選択カード종류 = RESULT

			SELECTCASE 選択カード종류
			;●通常モンスター：그 카드가야수족牝性몬스터의 場合、LPを1000ポイント회복する。
			CASE 通常モン
				CALLFORM CARD_{選択カードID},"성별"
				IF RESULT == 牝性
					CALLFORM CARD_{選択カードID},"종족"
					IF RESULT == 야수족
						;ランダム효과로 も良いかも、LP회복、ドロー、소환権추가みたいな
						CALL HORUSTEIN_GET_DATA(듀얼리스트,,選択カードID)
						암소名称 = %RESULTS:0%
						参照場所 = RESULT:0
						参照カードID = RESULT:99
						암소가슴追 = GETVAR(@"%듀얼리스트%_魔_個Ｃ:参照場所")						
						PRINTFORML %암소名称%から搾ったミルクを飲み干した！
						;ホルスタイン몬스터의 場合、きっちり搾乳する
						IF INRANGE (選択カードID, 179556, 179558) 
							;搾乳する
							CALL HORUSTEIN_GET_MILK(듀얼리스트,参照カードID,암소가슴追);★
						ENDIF						
						;乳を吸ってLP회복
						CALL LIFE_CHANGE(듀얼리스트,"増加","효과",1000)
					ELSE
						;獣じゃないなら乳を揉むだけとか？						
					ENDIF
				ENDIF
			;●효과モンスター：묘지의 「ホルスタイン」몬스터를 특수 소환한다。
			CASE 효과モン,의식
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件カテゴリ  = ホルスタイン
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
				
				IF RESULT != -1
					選択カードID = RESULT:1
					CALL HORUSTEIN_GET_DATA(듀얼리스트,カード소환先,選択カードID);★
					암소名称 = %RESULTS:0%
					;◆小さな가슴でも違和感のない描写にする
					SELECTCASE 選択カードID
					CASE 179556
						;美乳
						PRINTFORMW %암소名称%の美しい乳がしなやかに揺れた…
						CALL HORUSTEIN_RAND_COW_TXT(1,0)
					CASE 179557
						;愛乳
						PRINTFORMW %암소名称%の可愛らしい유두がぷるんと震えた…
						CALL HORUSTEIN_RAND_COW_TXT(1,1)
					CASE 179558
						;旨乳
						PRINTFORMW %암소名称%の思わず吸い付きたくなる乳がしっとりと艶めいている…
						CALL HORUSTEIN_RAND_COW_TXT(1,2)
					CASE 179559
						;EXは明確にデカイ
						PRINTFORMW %암소名称%の頭より大きな乳がどぷんっと音を立てて震えた…
						CALL HORUSTEIN_RAND_COW_TXT(1,3)
					ENDSELECT
				ENDIF
				PRINTFORMW 
			;●마법カード：「ホルスタイン」마법카드를 패에 넣는다。
			CASE 20 TO 29
				CALL FULLRESET
				효과条件カテゴリ = ホルスタイン
				효과条件종류 = 마법全般
				CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
			;●함정カード：「ホルスタイン」함정카드를 패에 넣는다。
			CASE 30 TO 39	
				CALL FULLRESET
				효과条件カテゴリ = ホルスタイン
				효과条件종류 = 함정全般
				CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
			ENDSELECT
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "ミルクファーム")
ENDIF