@CARDNAME_30999,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = レベル・スティーラー
	CASE "略称"
		RESULTS = スティーラー
	CASE "カテゴリ"
		RESULTS:0 = なし
ENDSELECT


@CARD_30999(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 600
	CASE "守備力"
		RETURN 0
	CASE "制限"
		RETURN 制限
	CASE "レア度"
		RETURN ULR
ENDSELECT

RETURN 0

